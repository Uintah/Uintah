            <object class="Grid">
              <var name="ToolDialog::button_height" 
                   propagate="yes"
                   overwrite="no">40</var>
              
              <var name="row">4</var>
              <var name="col">1</var>
              <var name="cell-height">$ToolDialog::button_height</var>
              
              <var name="rows">1</var>
              <var name="cols">5</var>
              
              
              <object class="Button">
                <var name="row">1</var>
                <var name="col">1</var>
              
                <var name="Button::color" propagate="yes">#EEEEEEFF</var>
                <var name="Button::Text::text" propagate="yes">Set Seed Layer</var>

                <signal name="Button::clicked" 
                        target="Painter::SetLayer"></signal>


              </object>
              
              <object class="VCRControls">
                <var name="row">1</var>
                <var name="col">3</var>
                <var name="cell-width">120</var>
                <signal name="VCRControls::play"
                        target="Painter::FinishTool">
                  <var name="threaded">1</var>
                </signal>

                <signal name="VCRControls::stop"
                        target="Painter::AbortFilterOn"/>
                
              </object>

              <object class="Button">
                <var name="row">1</var>
                <var name="col">5</var>
              
                <var name="Button::color" propagate="yes">#EEEEEEFF</var>
                <var name="Button::Text::text" propagate="yes">Done</var>

                <signal name="Button::clicked" 
                        target="Animation::AnimateVariableDescending">
                  <var name="varnum">4</var>
                </signal>

              </object>
            </object>
          </object>
        </object>
      </object>
