<definition class="ITKThresholdDialog">
  <object class="ShadowedBox">
    <definition class="ShadowedBox::child">
      <object class="Box">
        <var name="focus_mode">1</var>
        <var name="color">#AAAAAAAA</var>
        
        <object class="Texture">
          <var name="file">BackgroundTile04.png</var>
          <var name="color">#AAAAAAFF</var>
          <var name="repeatx">1</var>
          <var name="repeaty">1</var>
        </object>
        
        <object class="Texture">
          <var name="file">BackgroundTile04.png</var>
          <var name="color">#000000</var>
          <var name="repeatx">1</var>
          <var name="repeaty">1</var>
        </object>
        


        <object class="Grid">
          <var name="rows">1</var>
          <var name="cols">2</var>

          
          <object class="Grid">
            <var name="rows">5</var>
            <var name="cols">1</var>

            <var name="cell-width">300</var>
            
            <var name="row">1</var>
            <var name="col">2</var>
            
            <object class="LabeledSpinnerBox">                
              <var name="row">2</var>
              <var name="col">1</var>
              <var name="LabeledSpinnerBox::text" propagate="yes">Curvature Scaling:</var>
              <var name="LabeledSpinnerBox::variable" propagate="yes">ITKThresholdTool::curvatureScaling</var>
            </object>
            
            <object class="LabeledSpinnerBox">                
              <var name="row">3</var>
              <var name="col">1</var>
              <var name="LabeledSpinnerBox::text" propagate="yes">Propagation Scaling:</var>
              <var name="LabeledSpinnerBox::variable" propagate="yes">ITKThresholdTool::propagationScaling</var>
            </object>
            
            <object class="LabeledSpinnerBox">                
              <var name="row">4</var>
              <var name="col">1</var>
              <var name="LabeledSpinnerBox::text" propagate="yes">Edge Weight:</var>
              <var name="LabeledSpinnerBox::variable" propagate="yes">ITKThresholdTool::edgeWeight</var>
            </object>
          </object>

          <object class="Grid">
            <var name="row">1</var>
            <var name="col">1</var>
            
            <var name="rows">4</var>
            <var name="cols">1</var>
            
            
            <object class="Text">
              <var name="row">1</var>
              <var name="col">1</var>
              <var name="text">$ToolDialog::text</var>
              <var name="size">30</var>
              <var name="fgcolor">#000000FF</var>
              <var name="bgcolor">#FFFFFF80</var>
              <var name="anchor">W</var>
              <var name="offsetx">-2</var>
              <var name="offsety">2</var>
              <var name="shadow">1</var>
            </object>
            
