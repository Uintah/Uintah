<?xml version="1.0" encoding="iso-8859-1"?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->

<!-- @version: Updated 7/31/00 -->

<Uintah_specification>

   <Meta>
	<title>Heat conduction problem</title>
   </Meta>

   <Time>
	<maxTime>.1</maxTime>
	<initTime>0.0</initTime>
	<delt_min>0.001</delt_min>
	<delt_max>0.01</delt_max>
	<timestep_multiplier>0.8</timestep_multiplier>
   </Time>

   <DataArchiver>
        <filebase>heatcond.uda</filebase>
        <outputInterval>.01</outputInterval>
	<save label = "KineticEnergy"/>
	<save label = "TotalMass"/>
	<save label = "StrainEnergy"/>
	<save label = "CenterOfMassPosition"/>
	<save label = "CenterOfMassVelocity"/>
	<save label = "p.x"/>
	<save label = "p.volume"/>
	<save label = "p.stress"/>
	<save label = "g.mass"/>
	<save label = "g.stressFS"/>
	<checkpoint cycle = "2" interval = "0.01"/>
   </DataArchiver>

   <MPM>
	<time_integrator>explicit</time_integrator>
   </MPM>


    <PhysicalConstants>
	<gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
	<MPM>
	    <material>
                <burn type = "null" />
                <density>10000.0</density>
                <thermal_conductivity>237.0</thermal_conductivity>
                <specific_heat>903</specific_heat>
                <heat_transfer_coefficient>111</heat_transfer_coefficient>
                <constitutive_model type="comp_neo_hook">
                    <bulk_modulus>2000.0</bulk_modulus>
                    <shear_modulus>1500.0</shear_modulus>
                </constitutive_model>
		<geom_object>
		    <box label="gp1">
			<min>[0,0,0]</min>
			<max>[0.01,0.004,0.004]</max>
		    </box>
		    <res>[2,2,2]</res>
		    <velocity>[0.,0.,0.]</velocity>
		    <temperature>300.</temperature>
 		</geom_object>

		<geom_object>
		    <box label="gp2">
			<min>[0.01,0,0]</min>
			<max>[0.02,0.004,0.004]</max>
		    </box>
		    <res>[2,2,2]</res>
		    <velocity>[0.,0.,0.]</velocity>
		    <temperature>0.</temperature>
 		</geom_object>
		<velocity_field>0</velocity_field>

	    </material>

            <contact>
               <type>single_velocity</type>
               <vel_fields>[1,2,3]</vel_fields>
            </contact>


	</MPM>

    </MaterialProperties>
	
    <Grid>
	<Level>
	    <Box label="1">
		<lower>[0,0,0]</lower>
		<upper>[0.02,0.004,0.004]</upper>
	        <resolution>[20,4,4]</resolution>
		<patches>[1,1,1]</patches>
	    </Box>
	</Level>
    </Grid>

    <PhysicalBC>
	<MPM>
	</MPM>
    </PhysicalBC>
    
</Uintah_specification>
