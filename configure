echo "$ac_t""-L$XML_LIB_DIR $XML_LIB_FLAG" 1>&6






##-- Find Teem ------------------------------------------------------

teem_pkg="no"
for i in $package; do
  if test "$i" = "Teem" ; then
    teem_pkg="yes"
    if test -e "$thirdparty/include/nrrd.h" ; then
      TEEM_PREFIX=$thirdparty
    fi
  fi
done

if test "${teem_pkg}" = "yes" ; then
  if test "${with_teem+set}" = set; then
    withval="$with_teem"
    if test "$withval" != "yes" ; then
      if test -e "${withval}/include/nrrd.h"; then
        TEEM_PREFIX="$withval"
      else
        { echo "configure: error: could not find ${withval}/include/nrrd.h" 1>&2; exit 1; }
        teem_pkg="no"
      fi
    else
      echo "configure: warning: ignoring pathless with-teem flag" 1>&2
    fi
  fi
fi

if test "${teem_pkg}" = "yes" ; then
  echo $ac_n "checking for teem""... $ac_c" 1>&6
echo "configure:2326: checking for teem" >&5
  if test "$TEEM_PREFIX" ; then
    TEEM_INCLUDE="-I${TEEM_PREFIX}/include"
    TEEM_LIB_DIR="${TEEM_PREFIX}/lib"
    echo "$ac_t""${TEEM_PREFIX}" 1>&6
  else
    echo "$ac_t""no" 1>&6
  fi
fi





