   { (exit 1); exit 1; }; }
  fi
  # and only one include path:
  ### Determine if there is only one item in
  __sci_pass=false
  __sci_first_time=true
  for value in "" ; do
    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
