       ac_try="$theCompiler $CFLAGS conftest.$theExt $LIBRARY -lF77 $_SCI_OTHER_LIBS_ >/dev/null >> conftest.out 2>> conftest.out"
       (eval $ac_try) 2>&5

       if test "$debugging" = "yes"; then
          echo ac_try is $ac_try
