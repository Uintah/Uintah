QT_MOC_BIN=""
{ echo "$as_me:$LINENO: checking for moc in $LIB_DIR_QT_MT/../bin..." >&5
echo "$as_me: checking for moc in $LIB_DIR_QT_MT/../bin..." >&6;}
# Extract the first word of "moc", so it can be a program name with args.
