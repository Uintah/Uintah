    _SCI_NOTFOUND_='echo -n ""'
    _SCI_DECL_="char main();"
    _SCI_CALL_="main();"
    _SCI_VAR_LIBNAME_=`echo petsc | sed 'y%abcdefghijklmnopqrstuvwxyz./-%ABCDEFGHIJKLMNOPQRSTUVWXYZ___%'`
    _SCI_VAR_NAME_=LIB$_SCI_VAR_LIBNAME_
    _SCI_VAR_HAVE_="HAVE_$_SCI_VAR_NAME_"
    _SCI_VAR_LIBRARY_="$_SCI_VAR_LIBNAME_"_LIBRARY

    if test "$i/lib" = "/usr/lib" ||
       test "$i/lib" = "/lib" ||
       test "$i/lib" = "/usr/lib/" ||
       test "$i/lib" = "/lib/"; then
      LIBRARY=
    else
      LIBRARY=-L$i/lib
    fi

    if test ""; then
      _SCI_FOUND_=''
    fi

    if test "continue"; then
      _SCI_NOTFOUND_='continue'
    fi

    if test "main" = "main"; then
      _SCI_DECL_=""
      _SCI_CALL_=""
      echo $ac_n "checking for -lpetsc""... $ac_c" 1>&6
echo "configure:9472: checking for -lpetsc" >&5
    else
      echo $ac_n "checking for main in -lpetsc""... $ac_c" 1>&6
echo "configure:9475: checking for main in -lpetsc" >&5
    fi

    cat > conftest.$ac_ext <<EOF
    #include "confdefs.h"
    extern "C" {
    $_SCI_DECL_
    }
    int main() {
      $_SCI_CALL_
      return 0;
    }
EOF

    rm -f conftest.out a.out
    ac_try="$ac_link $LIBRARY -lpetsc -lpetscsles -lpetscmat -lpetscvec \
      -lpetscdm -lpetsccontrib $LAPACK_LIBRARY -llapack \
      $BLAS_LIBRARY -lblas $X_LIBRARY -lX11 -lmpiuni $FLIBS -lm >/dev/null 2>conftest.out"
    (eval $ac_try) 2>&5
#   part of this is a hack to prevent the bizarre libg2c "errors"
    ac_err=`grep -v '^ *+' conftest.out | grep -v "^conftest.${ac_ext}\$" | grep -v "libg2c\.a(open\.o" | grep -v "open\.o(\.text+"`
    if test -z "$ac_err"; then
      if test "$i/lib" != "/usr/lib" &&
         test "$i/lib" != "/usr/lib/" &&
         test "$i/lib" != "/lib" &&
         test "$i/lib" != "/lib/"; then
        eval $_SCI_VAR_NAME_='$i/lib'
        eval $_SCI_VAR_HAVE_=yes
        eval $_SCI_VAR_LIBRARY_='-L$i/lib'
      else
        eval $_SCI_VAR_NAME_='\ '
        eval $_SCI_VAR_HAVE_=yes
        eval $_SCI_VAR_LIBRARY_=''
      fi
      echo "$ac_t""yes" 1>&6
      eval $_SCI_FOUND_
    else
      eval $_SCI_VAR_NAME_=''
      eval $_SCI_VAR_HAVE_=''
      eval $_SCI_VAR_LIBRARY_=''
      echo "$ac_t""no" 1>&6
      eval $_SCI_NOTFOUND_
    fi
#    rm -f conftest* a.out
      
  fi
  
    ##  SCI_CHECK_HEADERS
    _SCI_FOUND_='echo -n ""'
    _SCI_NOTFOUND_='echo -n ""'
    _SCI_LOOPTEST_=true

    if test ""; then
      _SCI_FOUND_=''
    fi

    if test "break"; then
      _SCI_NOTFOUND_='break'
    fi

    _SCI_FIRST_TIME_=yes
    for _SCI_LOOP_VAR_ in petsc.h petscsles.h; do

      if test "$_SCI_FIRST_TIME_" = "yes"; then
        _SCI_VAR_NAME_=INC`echo $_SCI_LOOP_VAR_ | sed 'y%abcdefghijklmnopqrstuvwxyz./-%ABCDEFGHIJKLMNOPQRSTUVWXYZ___%'`
        _SCI_FIRST_TIME_=no      
      fi

      echo $ac_n "checking for $_SCI_LOOP_VAR_""... $ac_c" 1>&6
echo "configure:9544: checking for $_SCI_LOOP_VAR_" >&5
      cat > conftest.$ac_ext <<EOF
      #include "confdefs.h"
      #include <$_SCI_LOOP_VAR_>
      int main() {
        return 0;
      }
EOF

      ac_try="$ac_cpp conftest.$ac_ext -I$i/include -I$i/bmake/$PETSC_ARCH -I$i/src/sys/src/mpiuni >/dev/null 2>conftest.out"
      (eval $ac_try) 2>&5
      ac_err=`grep -v '^ *+' conftest.out | grep -v "WARNING" | grep -v "^conftest.${ac_ext}\$"`
      if test -z "$ac_err"; then
        echo "$ac_t""yes" 1>&6
      else
        eval $_SCI_VAR_NAME_=''
        echo "$ac_t""no" 1>&6
        _SCI_LOOPTEST_=false
        eval $_SCI_NOTFOUND_
      fi

#      rm -f conftest* a.out
    done

    if test "$_SCI_LOOPTEST_" = "true"; then
      if test "$i/include" != "/usr/include" &&
         test "$i/include" != "/usr/include/"; then
        eval $_SCI_VAR_NAME_='$i/include'
      else
        eval $_SCI_VAR_NAME_='\ '
      fi
      eval $_SCI_FOUND_
    fi
  
  if test "$LIBPETSC" &&
     test "$INCPETSC_H" &&
     test -x "$i/bin/petscarch"; then 
    FOUND_ALL_PETSC=yes
    UNIPETSC_DIR=$i
    if test "$debug" = "yes"; then
      UNIPETSC_SUB=libg
    else
      UNIPETSC_SUB=libO
    fi
    UNIPETSC_EXTRA_LIBS="-lpetscsles -lpetscmat -lpetscvec \
      -lpetscdm -lpetsccontrib $LAPACK_LIBRARY -llapack \
      $BLAS_LIBRARY -lblas $X_LIBRARY -lX11 -lmpiuni $FLIBS -lm"
    UNIPETSC_ARCH=`$i/bin/petscarch`
    if test "$is_64bit" != "yes" -a "$UNIPETSC_ARCH" = "IRIX64"; then
      UNIPETSC_ARCH="IRIX"
    fi
    HAVE_LIBUNIPETSC=yes
    LIBUNIPETSC=$LIBPETSC
    INCUNIPETSC_H=$INCPETSC_H
    UNI_PETSC_DEF="#define UNI_PETSC 1"
    break
  fi 
done

if test "$FOUND_ALL_PETSC" != "yes"; then
  LIBUNIPETSC=""
  INCUNIPETSC_H=""
  HAVE_LIBUNIPETSC=''
