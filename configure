    #######################################################
    ##  Begin: SCI_CHECK_HEADERS macro...

    if test "$debugging" = "yes"; then
       echo "SCI CHECK HEADERS:"
       echo "   paths:       $LIBGLOBUS_DC/../include"
       echo "   headers:     globus_nexus.h"
