  if test -z "TCL_MAJOR_VERSION" ; then
    { echo "configure: error: not found -- must build.  Implement me." 1>&2; exit 1; }
  else
    { echo "configure: error: incorrect tcl version $TCL_MAJOR_VERSION$TCL_MINOR_VERSION$TCL_PATCH_LEVEL found" 1>&2; exit 1; }
  fi
