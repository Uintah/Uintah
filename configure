    if test "$debugging" = "yes"; then
       echo "SCI CHECK LIB:"
       echo "   paths:      "
       echo "   lib name:   m"
       echo "   function:   main"
       echo "   other-libs: "
       echo "   c++ lib:    "
       echo "   optional:   not-optional"
