      echo $ac_n "checking for main in -lpetsc""... $ac_c" 1>&6
echo "configure:9393: checking for main in -lpetsc" >&5
    fi

    cat > conftest.$ac_ext <<EOF
    #include "confdefs.h"
    extern "C" {
    $_SCI_DECL_
    }
    int main() {
      $_SCI_CALL_
      return 0;
    }
EOF

    rm -f conftest.out a.out
    ac_try="$ac_link $LIBRARY -lpetsc -lpetscsles -lpetscmat -lpetscvec \
    -lpetscdm -lpetsccontrib $LAPACK_LIBRARY -llapack \
    $BLAS_LIBRARY -lblas $X_LIBRARY -lX11 \
    -L$i/lib/$PETSC_SUB/$PETSC_ARCH -lmpiuni $FLIBS -lm >/dev/null 2>conftest.out"
    (eval $ac_try) 2>&5
#   part of this is a hack to prevent the bizarre libg2c "errors"
    ac_err=`grep -v '^ *+' conftest.out | grep -v "^conftest.${ac_ext}\$" | grep -v "libg2c\.a(open\.o" | grep -v "open\.o(\.text+"`
    if test -z "$ac_err"; then
      if test "$i/lib/$PETSC_SUB/$PETSC_ARCH" != "/usr/lib" &&
         test "$i/lib/$PETSC_SUB/$PETSC_ARCH" != "/usr/lib/" &&
         test "$i/lib/$PETSC_SUB/$PETSC_ARCH" != "/lib" &&
         test "$i/lib/$PETSC_SUB/$PETSC_ARCH" != "/lib/"; then
        eval $_SCI_VAR_NAME_='$i/lib/$PETSC_SUB/$PETSC_ARCH'
        eval $_SCI_VAR_HAVE_=yes
        eval $_SCI_VAR_LIBRARY_='-L$i/lib/$PETSC_SUB/$PETSC_ARCH'
      else
        eval $_SCI_VAR_NAME_='\ '
        eval $_SCI_VAR_HAVE_=yes
        eval $_SCI_VAR_LIBRARY_=''
      fi
      echo "$ac_t""yes" 1>&6
      eval $_SCI_FOUND_
    else
      eval $_SCI_VAR_NAME_=''
      eval $_SCI_VAR_HAVE_=''
      eval $_SCI_VAR_LIBRARY_=''
      echo "$ac_t""no" 1>&6
      eval $_SCI_NOTFOUND_
    fi
#    rm -f conftest* a.out
  
  if test "$HAVE_LIBPETSC" != "yes"; then
    
    ##  SCI_CHECK_LIB
