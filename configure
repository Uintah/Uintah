                if test -f "$_CHECK_PATH_/$_HEADER_"; then
                   echo "$ac_t""$_CHECK_PATH_" 1>&6
                   eval $_SCI_INCLUDE_NAME_=-I$_CHECK_PATH_
                else
                   # Even though the compilation was successful,
