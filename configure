       echo "   optional:   not-optional"
    fi

    ##########################
    ## CXX and OPTIONAL parameters

    _SCI_USING_CXX_="yes"
    _SCI_LIB_OPTIONAL_="not-optional"

    if test "$_SCI_LIB_OPTIONAL_" != "not-optional" -a \
       "$_SCI_LIB_OPTIONAL_" != "optional"; then
       
      ## SCI_MSG_ERROR
      if test -z "$debugging"; then
