       _SCI_CALL_="main();"

       echo $ac_n "checking for main in -lF77""... $ac_c" 1>&6
echo "configure:6572: checking for main in -lF77" >&5
       if test "$_SCI_USING_CXX_" != "yes"; then
          EXTERN_BEGIN="extern \"C\" {"
          EXTERN_END="}"
       fi
    fi

    ##########################
    ## OTHER-LIBRARIES parameter

