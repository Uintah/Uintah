<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE component SYSTEM "../../../../Dataflow/XML/component.dtd">
<?xml-stylesheet href="../../../../Dataflow/XML/component.xsl" type="text/xsl"?>
<?cocoon-process type="xslt"?>

<component name="TensorFieldExtractor" category="Selectors">
  <overview>
    <authors>
      <author>Packages/Kurt Zimmerman</author>
    </authors>
    <summary>Select tensor data from a Uintah Data Archive.</summary>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>Data Archive</name>
        <datatype>Uintah::Archive</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>Field</name>
        <datatype>SCIRun::Field</datatype>
      </port>
   </outputs>
  </io>
</component>
