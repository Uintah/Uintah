<?xml version='1.0' encoding='ISO-8859-1' ?>
<Uintah_Include>

  <diffusion_model         spec="OPTIONAL NO_DATA"
                           attribute1="type REQUIRED STRING 'jg, rf1,
                                       non_linear1, non_linear2, constant_rate, bazant'">
    <diffusivity           spec="REQUIRED DOUBLE" />
    <max_concentration     spec="REQUIRED DOUBLE" />
    <initial_concentration spec="OPTIONAL DOUBLE" />
    
    <!-- Variables specific to the Bazant model (for now) -->
    <Boltzmann             spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <solutionEnergy        spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <penaltyCahnHilliard   spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <sitesPerVolume        spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <volPerIntercSite      spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <initialChemicalPotential spec = "OPTIONAL DOUBLE" />
    <mismatchStrain        spec="OPTIONAL NO_DATA" >
      <v1                  spec="REQUIRED VECTOR" />
      <v2                  spec="REQUIRED VECTOR" />
      <v3                  spec="REQUIRED VECTOR" />
    </mismatchStrain>
    <!-- Bazant model variables -->

    <!-- Required for constant_rate model -->
    <constant_rate         spec="REQUIRED DOUBLE" need_applies_to="type constant_rate" />
    
    <!-- Required for non-linear diffusion models -->
    <tuning1               spec="REQUIRED DOUBLE" need_applies_to="type non_linear1, non_linear2" />
    <tuning2               spec="REQUIRED DOUBLE" need_applies_to="type non_linear1, non_linear2" />

    <!-- Non_linear1 implemented only -->
    <use_pressure          spec="OPTIONAL NO_DATA" >
      <tuning3               spec="REQUIRED DOUBLE"   />
      <tuning4               spec="REQUIRED DOUBLE"   />
      <tuning5               spec="REQUIRED DOUBLE"   />
    </use_pressure>
    
    <!-- Diff curves only implemented for type non_linear1 -->
    <diff_curve            spec="OPTIONAL NO_DATA" need_applies_to="type non_linear1" >
      <time_point          spec="MULTIPLE NO_DATA" >
        <time              spec="REQUIRED DOUBLE" />
        <flux_direction    spec="REQUIRED STRING 'in, out, transition'" />
      </time_point>
    </diff_curve>

    <!-- non-linear diffusion models -->
    
    
    
    <!-- Tags not currently used -->
    <ramp_time             spec="OPTIONAL DOUBLE" />
    <operating_temp        spec="OPTIONAL DOUBLE" />
    <partial_atomic_vol    spec="OPTIONAL DOUBLE" />
    <init_conc             spec="OPTIONAL DOUBLE" />

    
    <conductivity_equation   spec="OPTIONAL NO_DATA"
                             attribute1="type REQUIRED STRING 'fixed, binary'">
      <!-- For fixed conductivity model -->
      <conductivity          spec="REQUIRED DOUBLE" need_applies_to="type fixed" />
      
      <!-- For binary conductivity model -->
      <min_conc              spec="REQUIRED DOUBLE" need_applies_to="type binary" />
      <max_conc              spec="REQUIRED DOUBLE" need_applies_to="type binary" />
      <min_conductivity      spec="REQUIRED DOUBLE" need_applies_to="type binary" />
      <max_conductivity      spec="REQUIRED DOUBLE" need_applies_to="type binary" />
    </conductivity_equation>
  </diffusion_model>
</Uintah_Include>
