<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- HMX -->
<!-- Density: 1.9 g/cm^3 -->
<!-- From: Benson, D.J. and Conley, P. Eulerian finite-element simulations 
           of experimentally acquired HMX microstructures. Modelling Simul. 
           Mater. Sci. Eng. 7 (1999), pp 333-354.
-->
<Uintah_Include>
  <flow_model type="steinberg_cochran_guinan">
    <mu_0>   10.0e9  </mu_0>     <!-- Pa^-1 -->
    <sigma_0> 0.6e9  </sigma_0>  <!-- Pa^-1 -->
    <Y_max>   1.0e9 </Y_max>    <!-- Pa^-1 -->
    <beta>    0.0    </beta>
    <n>       1.0    </n>   
    <A>       0.0    </A>        <!-- Pa^-1 -->
    <B>       0.0    </B>        <!-- K^-1 -->
    <T_m0>  520.0    </T_m0>     <!-- K -->
    <Gamma_0> 1.1    </Gamma_0>   
    <a>       0.0    </a>
    <epsilon_p0> 0.0 </epsilon_p0>
  </flow_model>
</Uintah_Include>
