<?xml version="1.0" encoding="iso-8859-1"?>

<Uintah_specification> 
<!--Please use a consistent set of units, (mks, cgs,...)-->

   <Meta>
       <title>block of water moving back and forth</title>
   </Meta>
    <Solver type="hypre"/>
   <SimulationComponent type="ice" />
    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
      <!-- <maxTime>            1.0         </maxTime> -->
       <maxTime>            0.1         </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1.0         </delt_max>
       <delt_init>          1.0e-9      </delt_init>
<!--   <max_Timesteps>     1000    </max_Timesteps> -->

       <max_delt_increase>    1.0       </max_delt_increase>
       <timestep_multiplier>1         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
       <Face side = "x-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      
      <Face side = "x+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      
          
      <Face side = "y-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>                  

      <Face side = "y+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      
      <Face side = "z-">
          <BCType id="all" label="Symmetric" var="symmetry"> </BCType>
      </Face>
      
      <Face side = "z+">
          <BCType id="all" label="Symmetric" var="symmetry"> </BCType>
      </Face>

    </BoundaryConditions>
       <Level>
         <Box label="1">
            <lower>        [-0.5, 0.0,-0.025]   </lower>
            <upper>        [ 0.5, 0.5, 0.025]   </upper>
            <extraCells>   [1,1,1]          </extraCells>
            <patches>      [2,2,1]          </patches>
         </Box>
         <spacing>        [0.025,0.025,0.05]     </spacing>
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>WaterAirOscillator.uda</filebase>
      <outputInterval>0.005</outputInterval>
      <save label="press_CC"/>
      <save label="delP_Dilatate"/>
      <save label="vol_frac_CC"/>
      <save label="rho_CC"/>
      <save label="vel_CC"/>
      <save label="temp_CC"/>
      <save label="sp_vol_CC"/>
      <save label="TotalMass"/>
      <save label="TotalIntEng"/>
      <save label="TotalMomentum"/>
      <checkpoint interval="0.01" cycle="2"/>
   </DataArchiver>

    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.1</cfl>
       <ICE>
        <advection type = "FirstOrder" />

        <TimeStepControl>
          <Scheme_for_delT_calc> aggressive   </Scheme_for_delT_calc>
          <knob_for_speedSound>  0.01          </knob_for_speedSound>
        </TimeStepControl>

        <ClampSpecificVolume>true</ClampSpecificVolume>

        <ImplicitSolver>
           <max_outer_iterations>      20    </max_outer_iterations>
           <outer_iteration_tolerance> 1e-8  </outer_iteration_tolerance>
           <iters_before_timestep_restart> 10 </iters_before_timestep_restart>
           
           <Parameters variable="implicitPressure">
              <tolerance>     1.e-14   </tolerance>
              <solver>         cg      </solver>
              <preconditioner> pfmg    </preconditioner>
              <maxiterations>  7500    </maxiterations>
              <npre>           1       </npre>
              <npost>          1       </npost>
              <skip>           1       </skip>
              <jump>           0       </jump>
              <relax_type>     2       </relax_type>
           </Parameters>
        </ImplicitSolver>


      </ICE>        
    </CFD>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>


    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <ICE>
          <material name = "Water">
            <EOS type="Thomsen_Hartka_water">
              <a>  2.0e-7     </a>    <!-- (K/Pa)     -->    
              <b>  2.6        </b>    <!-- (J/kg K^2) -->
              <co> 4205.7     </co>   <!-- (J/Kg K)   -->
              <ko> 5.0e-10    </ko>   <!-- (1/Pa)     -->
              <To> 277.0      </To>   <!-- (K)        -->
              <L>  8.0e-6     </L>    <!-- (1/K^2)    -->
              <vo> 1.00008e-3 </vo>   <!-- (m^3/kg)   -->
            </EOS>
                              <!-- 855 -->
            <dynamic_viscosity>    0           </dynamic_viscosity>
            <thermal_conductivity> 0           </thermal_conductivity>
                   
            <specific_heat>        4145.37627765 </specific_heat>
            <gamma>                1.000009      </gamma>
            <geom_object>
               <box label="centerBlock">
                   <min>           [ -0.3,-10,-10]      </min>  
                   <max>           [ 0.3,  10, 10 ]    </max>  
               </box>
               <res>                [2,2,2]            </res>  
               <velocity>           [0.0,0.0,0.0]      </velocity>
              <density>              995.749014615     </density>      
              <pressure>             101325.0          </pressure>     
              <temperature>          300.0             </temperature>  
            </geom_object>
          </material>       
   
         <material name = "Air">
           <EOS type = "ideal_gas">                     </EOS>
                                  <!-- 1e-4  -->
           <dynamic_viscosity>            0           </dynamic_viscosity>
           <thermal_conductivity>       0.0             </thermal_conductivity>
           <specific_heat>              716.0           </specific_heat>
           <gamma>                      1.4             </gamma>           
           <geom_object>
              <box label="leftblock">
                  <min>           [-0.5, -10, -10]       </min>
                  <max>           [ -0.3, 10,  10 ]      </max>
              </box>
               <res>                      [2,2,2]       </res>
               <velocity>           [0.0,0.0,0.0]       </velocity>
               <temperature>        300.0               </temperature>
               <density>         1.29722416201117       </density>
               
               <!--      2.35858938547486 = 2 atm    
                         2.06376571229050 = 1.75 atm
                         1.76894203910615 = 1.5 atm
                         1.29722416201117 = 1.1 atm  -->
               
               <pressure>            202650             </pressure>
           </geom_object>
           <geom_object>
              <box label="rightblock">
                  <min>           [ 0.3, -10, -10]       </min>
                  <max>           [ 0.5, 10,  10 ]      </max>
              </box>
               <res>                      [2,2,2]       </res>
               <velocity>           [0.0,0.0,0.0]       </velocity>
               <temperature>        300.0               </temperature>
               <density>    1.1792946927374306000e+00   </density>
               <pressure>           101325.0            </pressure>
           </geom_object>
         </material>
      </ICE>       
       
      <exchange_properties> 
        <exchange_coefficients>
              <momentum>           [1e10]          </momentum>
              <heat>                1e5]           </heat>
        </exchange_coefficients>
      </exchange_properties> 
    </MaterialProperties>
    
<!--____________________________________________________________________-->
    <DataAnalysis>
       <Module name="lineExtract">
       
        <material>Water</material>
        <samplingFrequency> 1e3 </samplingFrequency>
        <timeStart>          0   </timeStart>
        <timeStop>          100  </timeStop>
       
        <Variables>
          <analyze label="press_CC" matl="0"/> 
          <analyze label="rho_CC"/>
          <analyze label="temp_CC"/>
          <analyze label="vel_CC"/>
          <analyze label="delP_Dilatate"/>
        </Variables>

        <lines>
          <line name="P"> 
                <startingPt>  [0.0, 0.25, 0]   </startingPt>
                <endingPt>    [0.025, 0.25, 0] </endingPt>
                <stepSize> 0.1 </stepSize>
          </line>
        </lines>
      </Module>
  </DataAnalysis>
       
</Uintah_specification>

