<?xml version="1.0" encoding="iso-8859-1"?>

<Uintah_specification> 
<!--Please use a consistent set of units, (mks, cgs,...)-->

   <Meta>
       <title>Micro_OKC</title>
   </Meta>

   <SimulationComponent type="mpmice" />
    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            0.01       </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1.0         </delt_max>
       <delt_init>          1.0e-4      </delt_init>
       <max_Timesteps>      10         </max_Timesteps>
       <timestep_multiplier>0.8         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    
      <BoundaryConditions>     
          <inletVelocity type="powerLawProfile">
            <verticalDirection> 1           </verticalDirection>
            <exponent>          0.1571        </exponent>
            <maxHeight>         131.0          </maxHeight>
            <material>          atmosphere </material>
          </inletVelocity> 

          <LODI>
            <press_infinity>    101325.0    </press_infinity>
            <sigma>             0.27        </sigma>
            <material>          atmosphere  </material>
            <Li_scale>          1           </Li_scale>
          </LODI>

          <Face side = "x-">
            <BCType id = "0" var = "Neumann"    label = "Pressure">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "1" var = "Dirichlet"  label = "Velocity">
                                  <value> [9.3,0.0,0.0] </value>
            </BCType>
            <BCType id = "0" var = "Neumann"    label = "Velocity">
                                  <value> [0.0,0.0,0.0] </value>
            </BCType>
            <BCType id = "1" var = "Dirichlet"  label = "Temperature">
                                  <value> 298.0  </value>
            </BCType>
            <BCType id = "0" var = "Neumann"    label = "Temperature">
                                  <value> 0.0  </value>
            </BCType>
            <BCType id = "1" var = "Dirichlet"  label = "Density">
                                  <value> 1.178721  </value>
            </BCType>
            <BCType id = "0" var = "Neumann"    label = "Density">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "1" var = "Dirichlet"  label = "scalar-f" >
                                  <value> 0.0   </value>
            </BCType>
          </Face>   
          <Face side = "x+">
            <BCType id = "0" var = "LODI"       label = "Pressure">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "1" var = "LODI"       label = "Velocity">
                                  <value> [0.,0.,0.] </value>
            </BCType>
            <BCType id = "0" var = "Neumann"    label = "Velocity">
                                  <value> [0.,0.,0.] </value>
            </BCType>
            <BCType id = "1" var = "LODI"       label = "Temperature">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "0" var = "Neumann"    label = "Temperature">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "1" var = "LODI"       label = "Density">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "0" var = "Neumann"    label = "Density">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "1" var = "Dirichlet"  label = "scalar-f" >
                                  <value> 0.0   </value>
            </BCType>
          </Face>

          <Face side = "y-">
            <BCType id = "0"    var = "Neumann"    label = "Pressure">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "all"  var = "Dirichlet" label = "Velocity">
                                  <value> [0.,0.,0.] </value>
            </BCType>
            <BCType id = "all"  var = "Neumann"  label = "Temperature">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "all"  var = "Neumann"  label = "Density">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "1"    var = "Dirichlet"  label = "scalar-f" >
                                  <value> 0.0   </value>
            </BCType>
          </Face>                  
          <Face side = "y+">
            <BCType id = "0"    var = "Neumann"     label = "Pressure">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "all"  var = "Dirichlet" label = "Velocity">
                                  <value> [8.475,0.0,0.0] </value>
            </BCType>
            <BCType id = "all"  var = "Neumann"  label = "Temperature">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "all"  var = "Neumann"  label = "Density">
                                  <value> 0.0   </value>
            </BCType>
            <BCType id = "1"    var = "Dirichlet"  label = "scalar-f" >
                                  <value> 0.0   </value>
            </BCType>
          </Face> 
          <Face side = "z-">
            <BCType id = "all"  var = "symmetry" label = "Symmetric">
            </BCType>
          </Face>                  
          <Face side = "z+">
            <BCType id = "all"  var = "symmetry" label = "Symmetric">
            </BCType>
          </Face> 
       </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0.0,0.0,-1.0]       </lower>
              <upper>        [1.0,0.5,-0.5]          </upper>
              <extraCells> [1,1,1]            </extraCells>
              <patches>    [4,4,2]            </patches>
           </Box>
           <spacing>        [0.002,0.002,0.002]     </spacing>
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>micro_OKC.uda</filebase>
      <outputInitTimestep/>
      <outputTimestepInterval>100</outputTimestepInterval>
      <save label="press_CC"/>
      <save label="vel_CC"            material = "1"/>
      <save label="vol_frac_CC"/>
      <save label="p.volume"/>
      <save label="scalar-f"/>
      <save label="p.x"/>
      
<!--        
      <save label="variance_press_CC" material="0"/>
      <save label="skewness_press_CC" material="0"/>
      <save label="kurtosis_press_CC" material="0"/>
      <save label="variance_scalar-f" material="1"/>
      <save label="skewness_scalar-f" material="1"/>
      <save label="kurtosis_scalar-f" material="1"/>
-->
      <save label="variance_vel_CC" material="1"/>
      <save label="skewness_vel_CC" material="1"/>
      <save label="kurtosis_vel_CC" material="1"/>
      <save label="uv_prime" />
      <save label="uw_prime" />
      <save label="vw_prime" />
      <save label="mean_vel_CC"     material="1"/>

     <outputDoubleAsFloat/>
     <checkpoint cycle = "2" interval = "24.0"/>
   </DataArchiver>

    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.5</cfl>
       <ICE>
        <advection type = "FirstOrder" />
        <ClampSpecificVolume>false</ClampSpecificVolume>
      </ICE>        
    </CFD>
    <MPM>
        <time_integrator>explicit</time_integrator>
    </MPM>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>


    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <MPM>
         <material>
           <density>             8900.0           </density>
           <constitutive_model type = "comp_neo_hook_plastic">
             <useModifiedEOS>     true          </useModifiedEOS>
             <bulk_modulus>      117.0e7        </bulk_modulus>
             <shear_modulus>      43.8e7        </shear_modulus>
             <yield_stress>       70.0e6        </yield_stress>
             <hardening_modulus>  43.8e5        </hardening_modulus>
             <alpha>               0.0          </alpha>
             <strain_limit>       3.05          </strain_limit>
             <reduction_factor>   1.0           </reduction_factor>
           </constitutive_model>
           <thermal_conductivity>  400.0         </thermal_conductivity>
           <specific_heat>         386.0         </specific_heat>
           <geom_object>
           
              <include href="OKC_mpm.xml"/>
              
               <res>               [2,2,2]         </res>
               <velocity>          [0.0,0.0,0.0]   </velocity>
               <temperature>       300             </temperature>
           </geom_object>
         </material>

            <contact>
              <type>null</type>
              <materials>              [0]         </materials>
            </contact>
      </MPM>
       <ICE>
         <material name="atmosphere">
           <EOS type = "ideal_gas">                     </EOS>
           <dynamic_viscosity>          0.0             </dynamic_viscosity>
           <thermal_conductivity>       0.0             </thermal_conductivity>
           <specific_heat>              716.0           </specific_heat>
           <gamma>                      1.4             </gamma>           
           <geom_object>
              <difference>
                <box label="domain">
                    <min>           [-1.0,-1.0,-1.0]     </min>
                    <max>           [  4.0,4.0,4.0 ]     </max>
                </box>
                <include href="OKC_ice.xml"/>
              </difference>
               <res>                      [2,2,2]       </res>
               <velocity>           [0.0,0.0,0.0]       </velocity>
               <temperature>        300.0               </temperature>
               <density>    1.1792946927374306000e+00   </density>
               <pressure>           101325.0            </pressure>
           </geom_object>
         </material>
      </ICE>       
       
            <exchange_properties> 
         <exchange_coefficients>
              <momentum>               [1e10]          </momentum>
              <heat>                  [1e10]           </heat>
        </exchange_coefficients>
            </exchange_properties> 
    </MaterialProperties>
    
    <!--__________________________________-->
     <!--____________________________________________________________________-->
    <!--Transport a passiv scalar around                -->
    <Models>
      <Model type="PassiveScalar">
        <material>atmosphere</material>
        <scalar name="f">
          <constants>
            <diffusivity>  0.0   </diffusivity>
          </constants>

          <initialization>
            <geom_object>
              <box label="domain"> </box>
              <scalar>    0.00   </scalar>
            </geom_object>
          </initialization>

          <interiorSources>
            <geom_object>
              <sphere label = "sphere1">
                <origin> [0.5, 0.25, -0.75] </origin>
                <radius> 0.001   </radius>
              </sphere>
              <maxScalar> 1.0   </maxScalar>
              <scalar> 2.0 </scalar>
            </geom_object>
          </interiorSources>
        </scalar> 
      </Model>
    </Models>     
    
    <!--__________________________________-->
    <DataAnalysis>
      <Module name = "statistics">
        <material>atmosphere</material>
      <!--  <samplingFrequency>  0.5      </samplingFrequency> -->
          <timeStart>   1E-5  </timeStart>
         <timeStop>   4400  </timeStop>
        <computeHigherOrderStats> true </computeHigherOrderStats>
        <Variables>
          <analyze label="scalar-f"  matl="1"/>
          <analyze label="press_CC"  matl="0"/>
          <analyze label="vel_CC"    matl="1" computeReynoldsStress="true" /> 
          
<!--  <analyze label="variance_press_CC" matl="0"/>
          <analyze label="skewness_press_CC" matl="0"/>
          <analyze label="kurtosis_press_CC" matl="0"/>
        
          <analyze label="variance_scalar-f" matl="1"/>
          <analyze label="skewness_scalar-f" matl="1"/>
          <analyze label="kurtosis_scalar-f" matl="1"/>

          <analyze label="variance_vel_CC" matl="1"/>
          <analyze label="skewness_vel_CC" matl="1"/>
          <analyze label="kurtosis_vel_CC" matl="1"/>  -->
        </Variables>
      </Module>
      
      <!--__________________________________-->
      <Module name="planeExtract">
        <material>    atmosphere    </material>
        <samplingFrequency>  0.5    </samplingFrequency>
        <timeStart>   2e-5  </timeStart>
        <timeStop>   4400  </timeStop>

        <Variables>
          <analyze label="press_CC"     matl="0"/>
          <analyze label="scalar-f"/>
          <analyze label="vel_CC"/>
          <analyze label="mean_vel_CC"  matl="1"/>

<!--          
          <analyze label="variance_press_CC" matl="0"/>
          <analyze label="skewness_press_CC" matl="0"/>
          <analyze label="kurtosis_press_CC" matl="0"/>
          <analyze label="variance_scalar-f" matl="1"/>
          <analyze label="skewness_scalar-f" matl="1"/>
          <analyze label="kurtosis_scalar-f" matl="1"/>
-->
          <analyze label="variance_vel_CC" matl="1"/>
          <analyze label="skewness_vel_CC" matl="1"/>
          <analyze label="kurtosis_vel_CC" matl="1"/>
          <analyze label="uv_prime" matl="1"/>
          <analyze label="uw_prime" matl="1"/>
          <analyze label="vw_prime" matl="1"/>
        </Variables> 
        
        <planes>
          <plane name="centerLine">
            <startingPt> [ 0.0, 0.0,   -0.75]  </startingPt>
            <endingPt>   [ 1.0, 0.5,   -0.75]  </endingPt>
          </plane>
        </planes>     
      </Module>         
    </DataAnalysis>
      
</Uintah_specification>
