<?xml version="1.0" encoding="iso-8859-1"?>

<Uintah_specification> 
<!--Please use a consistent set of units, (mks, cgs,...)-->

   <Meta>
       <title>none</title>
   </Meta>

   <SimulationComponent type="mpmice" />
    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>           1.0       </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1.0         </delt_max>
       <delt_init>          1.0e-4      </delt_init>
       <timestep_multiplier>0.8         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <LODI>
           <press_infinity> 1.0132500000010138e+05  </press_infinity>
           <sigma>          0.27                    </sigma>
           <material> atmosphere                   </material>
      </LODI>
    
    
    
      <Face side = "x-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"      var = "Dirichlet">
                              <value> [1.,1.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"    var = "Dirichlet">
                              <value> 300.0  </value>
        </BCType>
        <BCType id = "0" label = "Density"          var = "Dirichlet">
                              <value> 8900   </value>
        </BCType>
        <BCType id = "1" label = "Density"          var = "Dirichlet">
                              <value> 1.1792946927374306000e+00   </value>
        </BCType>
        <BCType id = "all"   label = "SpecificVol"  var = "computeFromDensity">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      
      <Face side = "x+">
        <BCType id = "0"   label = "Pressure"   var = "LODI">
                              <value> 0.0   </value>
        </BCType>
        
        <BCType id = "0" label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "0" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        
        
        <BCType id = "1" label = "Velocity"     var = "LODI">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "1" label = "Temperature"  var = "LODI">
                              <value> 0  </value>
        </BCType>
        <BCType id = "1" label = "Density"      var = "LODI">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all"   label = "SpecificVol"  var = "computeFromDensity">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      
      <Face side = "y-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Dirichlet">
                              <value> [1.,1.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Dirichlet">
                              <value> 300.0  </value>
        </BCType>
        <BCType id = "0" label = "Density"        var = "Dirichlet">
                              <value> 8900   </value>
        </BCType>
        <BCType id = "1" label = "Density"        var = "Dirichlet">
                              <value> 1.1792946927374306000e+00   </value>
        </BCType>
        <BCType id = "all"   label = "SpecificVol"  var = "computeFromDensity">
                              <value> 0.0  </value>
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0"   label = "Pressure"     var = "LODI">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "0" label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0" label = "Temperature"  var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "0" label = "Density"      var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        
        <BCType id = "1" label = "Velocity"     var = "LODI">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "1" label = "Temperature"  var = "LODI">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "1" label = "Density"      var = "LODI">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all"   label = "SpecificVol"  var = "computeFromDensity">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "all"   label = "Symmetric" var = "symmetry">
        </BCType>
      </Face> 
      <Face side = "z+">
        <BCType id = "all"   label = "Symmetric" var = "symmetry">
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>      [0,   0,   0]     </lower>
              <upper>      [1.0, 1.0, 0.001] </upper>
              <extraCells> [1,   1,   1]     </extraCells>
              <patches>    [1,   1,   1]     </patches>
           </Box>
           <spacing>        [0.01,0.01,0.001]     </spacing>
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>ramp.uda</filebase>
      <outputTimestepInterval>1</outputTimestepInterval>
      <save label="press_equil_CC"/>
      <save label="uvel_FC"/>
      <save label="vvel_FC"/>
      <save label="wvel_FC"/>
      <save label="delP_Dilatate"/>
      <save label="press_CC"/>
      <save label="rho_CC"/>
      <save label="vel_CC"/>
      <save label="Li1"/>
      <save label="Li2"/>
      <save label="Li3"/>
      <save label="Li4"/>
      <save label="Li5"/>
      <save label="p.x"/>
      <save label="p.volume"/>
      <save label="p.velocity"/>
      <save label="TotalIntEng"/>
      <save label="TotalMass"/>
      <save label="TotalMomentum"/>
      <save label="eng_exch_error"/>
      <save label="mom_exch_error"/>
      <checkpoint interval="0.005" cycle="2"/>
   </DataArchiver>

    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
      <cfl>0.25</cfl>
      <ICE>
        <advection type = "SecondOrder" />
        <ClampSpecificVolume>false</ClampSpecificVolume>
        
        <TimeStepControl>
          <Scheme_for_delT_calc> aggressive     </Scheme_for_delT_calc>
          <knob_for_speedSound>  0.0            </knob_for_speedSound>
        </TimeStepControl>
        
        <ImplicitSolver>
           <max_outer_iterations>      20      </max_outer_iterations>
           <outer_iteration_tolerance> 1.0e-7  </outer_iteration_tolerance>
           <iters_before_timestep_restart> 5   </iters_before_timestep_restart>
           <Parameters variable="implicitPressure">
            <!-- Common options -->
              <tolerance>1.e-10    </tolerance>

            <!-- CGSolver options -->
              <norm>     LInfinity  </norm>
              <criteria> Absolute   </criteria>

            <!-- Hypre options -->
            <solver>         cg      </solver>
            <preconditioner> pfmg    </preconditioner>
            <maxiterations>  7500    </maxiterations>
            <npre>           1       </npre>
            <npost>          1       </npost>
            <skip>           0       </skip>
            <jump>           0       </jump>
           </Parameters>
        </ImplicitSolver>
      </ICE>        
    </CFD>
    <MPM>
      <time_integrator>explicit</time_integrator>
    </MPM>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>

    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <MPM>
         <material>
              <density>             8900.0           </density>
              <constitutive_model type = "comp_neo_hook_plastic">
                <useModifiedEOS>     true            </useModifiedEOS>
                <bulk_modulus>      117.0e7          </bulk_modulus>
                <shear_modulus>      43.8e7          </shear_modulus>
                <yield_stress>       70.0e6          </yield_stress>
                <hardening_modulus>  43.8e5          </hardening_modulus>
                <alpha>               0.0            </alpha>
                <strain_limit>       3.05            </strain_limit>
                <reduction_factor>   1.0             </reduction_factor>
              </constitutive_model>
              <thermal_conductivity>  400.0          </thermal_conductivity>
              <specific_heat>         386.0          </specific_heat>
              <geom_object> 
                <difference>   
                  <cone label = "ramp">
                    <bottom>            [0.3,0.0,0.0]   </bottom>
                    <top>               [0.7,0.0,0.0]  </top>
                    <bottom_radius>0.31</bottom_radius>
                    <top_radius>   0.71</top_radius>
                  </cone>
                  
                  <cone label = "ramp2">
                    <bottom>            [0.3,0.0,0.0]   </bottom>
                    <top>               [0.7,0.0,0.0]   </top>
                    <bottom_radius>0.29</bottom_radius>
                    <top_radius>   0.69</top_radius>
                  </cone>
                </difference>
               <res>               [2,2,2]         </res>
               <velocity>          [0.0,0.0,0.0]   </velocity>
               <temperature>       300             </temperature>
             </geom_object>
         </material>

         <contact>
           <type>null</type>
           <materials>              [0]         </materials>
         </contact>
      </MPM>
      <!--_______________________________________________________-->
       <ICE>
         <material name="atmosphere">
           <EOS type = "ideal_gas">                     </EOS>
           <dynamic_viscosity>          0.0             </dynamic_viscosity>
           <thermal_conductivity>       0.0             </thermal_conductivity>
           <specific_heat>              716.0           </specific_heat>
           <gamma>                      1.4             </gamma>           
           <geom_object>
             <difference>
               <box label="wholeDomain">
                 <min>           [-1.0,-1.0,-1.0]    </min>
                 <max>           [4.0,4.0,4.0 ]      </max>
               </box>
               <difference>
                 <cone label="ramp" />
                 <cone label="ramp2" />
               </difference>
             </difference>
             <res>                      [2,2,2]       </res>
             <velocity>           [1.0,1.0,0.0]       </velocity>
             <temperature>        300.0               </temperature>
             <density>    1.1792946927374306000e+00   </density>
             <pressure>           101325.0            </pressure>
           </geom_object>
         </material>
      </ICE>       
       
      <exchange_properties> 
         <exchange_coefficients>
              <momentum>       [1e10]           </momentum>
              <heat>           [1e10]           </heat>
         </exchange_coefficients>
      </exchange_properties> 
    </MaterialProperties> 
</Uintah_specification>
