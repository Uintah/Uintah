       as_fn_error $? "For specific SCI-TRY-LINK test for BOOST only one include path may be specified for arg 4 (you had: $BOOST_INCLUDE_DIR).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
  fi
fi

