      if test -z "$has_minus"; then
         has_minus_l=`echo $lib | sed 's/-l.*//'`
         has_minus_framework=`echo $lib | sed 's/-framework.*//'`

         if test -z "$has_minus_framework"; then
            # Two rounds for this loop with respect to frameworks.
