fi


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for xml2 library and headers (required)" >&5
$as_echo_n "checking for xml2 library and headers (required)... " >&6; }
_sci_savelibs=$LIBS
_sci_saveldflags=$LDFLAGS
_sci_savecflags=$CFLAGS
