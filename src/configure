$as_echo "$as_me: WARNING: Only -I options are allowed in arg 4 ($_sci_inc) of Z check.  Skipping $inc." >&2;}
        fi
        continue
     fi
  fi

  the_inc=`echo $inc | grep "^\-I"`
  if test -z "$the_inc" && test "$has_minus_faltivec" = "no"; then
