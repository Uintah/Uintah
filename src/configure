  #echo specific
  # Make sure the exact includes were found
  for i in "" mkl_cblas.h mkl_lapack.h; do
    #echo looking for $_sci_inc/$i
    if test ! -e $_sci_inc/$i; then
     as_fn_error $? "Specifically requested BLAS include file '$_sci_inc/$i' was not found" "$LINENO" 5
    fi
  done
  # Make sure the exact libraries were found
  for i in "" mkl mkl_core mkl_intel_lp64 guide; do
    if test -z "$i"; then
       continue
    fi
    has_minus=`echo $i | sed 's/-.*//'`
    if test -z "$has_minus"; then
       i=`echo $i | sed 's/-l//g'`
    fi
    if test ! -e $_sci_lib/lib$i.so && test ! -e $_sci_lib/lib$i.a && test ! -e $_sci_lib/$i.lib && test ! -e $_sci_lib/lib$i.dylib; then
     as_fn_error $? "Specifically requested BLAS library file '$_sci_lib/lib$i' (.so, .a, or .dylib) was not found" "$LINENO" 5
    fi
  done
fi


#restore variables
CFLAGS=$_sci_savecflags
