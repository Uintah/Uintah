<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->
<!-- @version: Updated 7/31/00-->
<Uintah_specification>

   <Meta>
       <title>Constitutive Model Tester</title>
   </Meta>

   <SimulationComponent type="mpm" />
   <Time>
       <maxTime>.10</maxTime>
       <initTime>0.0</initTime>
       <delt_min>2.e-6</delt_min>
       <delt_max>2.e-3</delt_max>
       <timestep_multiplier>0.4</timestep_multiplier>
       <max_Timesteps>4</max_Timesteps>
   </Time>
   <DataArchiver>
        <filebase>const_test_hypo.uda</filebase>
       <outputInterval>.01</outputInterval>
        <save label = "BndyForce_zminus"/>
        <save label = "BndyForce_zplus"/>
        <save label = "BndyContactArea_zminus"/>
        <save label = "BndyContactArea_zplus"/>
<!--
       <save label = "KineticEnergy"/>
       <save label = "TotalMass"/>
       <save label = "StrainEnergy"/>
       <save label = "CenterOfMassPosition"/>
       <save label = "CenterOfMassVelocity"/>
-->
       <save label = "p.x"/>
       <save label = "p.velocity"/>
       <save label = "p.volume"/>
       <save label = "p.mass"/>
       <save label = "p.stress"/>
       <save label = "g.mass"/>
       <save label = "g.stressFS"/>
       <checkpoint cycle = "2" interval = "0.01"/>
   </DataArchiver>

    <MPM>
       <time_integrator>explicit</time_integrator>
        <boundary_traction_faces>[zminus,zplus]</boundary_traction_faces> 
    </MPM>

    <PhysicalConstants>
       <gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
       <MPM>
           <material>
              <density>1.e9</density>
              <constitutive_model type="hypo_elastic">
                 <G>12.e6</G>
                 <K>32.e6</K>
               </constitutive_model>
              <thermal_conductivity>1.0</thermal_conductivity>
              <specific_heat>5</specific_heat>
              <geom_object>
                  <box label = "piston">
                     <min>[0.0,0.0,.01]</min>
                     <max>[0.01,0.01,.02]</max>
                  </box>
                  <res>[1,1,1]</res>
                  <velocity>[0.0,0.0,-1.e-2]</velocity>
                  <temperature>12</temperature>
               </geom_object>
           </material>

           <material>
              <density>1.e3</density>
              <constitutive_model type="kayenta">
                 <B0>161000000000.0</B0>
                 <G0>79300000000.0</G0>
                 <A1>843020000.0</A1>
                 <A2>2.731e-10  </A2>
                 <A3>821920000.0</A3>
                 <A4>1.0e-10    </A4>
                 <P0>-9.99e98   </P0>
                 <CR>1.00e-3    </CR>
                 <RK>0.72       </RK>
                 <CTI1>9.99999e98 </CTI1>
                 <CTPS>9.99999e98 </CTPS>
                 <J3TYPE>3.0    </J3TYPE>
               </constitutive_model>
<!--
              <constitutive_model type="hypo_elastic_fortran">
                 <G>12.e6</G>
                 <K>32.e6</K>
               </constitutive_model>
-->
              <thermal_conductivity>1.0</thermal_conductivity>
              <specific_heat>5</specific_heat>
              <geom_object>
                  <box label = "box">
                     <min>[0.0,0.0,0.0]</min>
                     <max>[0.01,0.01,0.01]</max>
                  </box>
                  <res>[1,1,1]</res>
                  <velocity>[0.0,0.0,-.5e-2]</velocity>
                  <temperature>12</temperature>
               </geom_object>
           </material>

           <contact>
             <type>rigid</type>
             <materials>[0,1]</materials>
              <stop_time>999999.9 </stop_time>
           </contact>
       </MPM>

    </MaterialProperties>
       
    <Grid>
       <BoundaryConditions>
          <Face side = "x-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>
           <Face side = "x+">
            <BCType id = "all" var = "Dirichlet" label = "Velocity">
                <value> [0.0,0.0,0.0] </value>
                 </BCType>
           </Face>
           <Face side = "y-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
          <Face side = "y+">
            <BCType id = "all" var = "Dirichlet" label = "Velocity">
                    <value> [0.0,0.0,0.0] </value>
            </BCType>
           </Face>                  
           <Face side = "z-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
          <Face side = "z+">
            <BCType id = "all" var = "Dirichlet" label = "Velocity">
                      <value> [0.0,0.0,0.0] </value>
           </BCType>
           </Face>                 
       </BoundaryConditions>
       <Level>
           <Box label = "1">
              <lower>[0.0,0.0,0.0]</lower>
              <upper>[.02,.02,0.03]</upper>
               <resolution>[2,2,3]</resolution>
              <patches>[1,1,1]</patches>
           </Box>
       </Level>
    </Grid>

    
</Uintah_specification>

