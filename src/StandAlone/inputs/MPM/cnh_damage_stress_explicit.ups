<?xml version="1.0" encoding="iso-8859-1"?>
<Uintah_specification>

   <Meta>
       <title> uniaxial plane stress tension  - failure due to stress </title>
   </Meta>

   <SimulationComponent type="mpm" />

   <Time>
       <maxTime>0.00002</maxTime>
       <initTime>0.0</initTime>
       <delt_min>0.000000001</delt_min>
       <delt_max>0.000005</delt_max>
       <max_Timesteps>3000000</max_Timesteps>
       <timestep_multiplier>0.4</timestep_multiplier>
   </Time>
   <DataArchiver>
       <filebase>cnh_damage_stress_explicit.uda</filebase>
       <outputInterval>0.000001</outputInterval>
       <save label="p.x"/>
       <save label="p.velocity"/>
       <save label="p.volume"/>
       <save label="p.mass"/>
       <save label="p.temperature"/>
       <save label="p.deformationMeasure"/>
       <save label="p.stress"/>
       <save label="p.particleID"/>
       <save label="p.localizedMPM"/>
       <save label = "BndyForce_yminus"/>
   </DataArchiver>

    <MPM>
       <time_integrator>explicit</time_integrator>
       <boundary_traction_faces>[yminus]</boundary_traction_faces>        
    </MPM>

    <PhysicalConstants>
       <gravity>[0, 0.0,  0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
       <MPM>

         <material>
            <density>1000.0</density>
            <constitutive_model type="comp_neo_hook">
                <bulk_modulus> 8.9e9 </bulk_modulus>
                <shear_modulus> 3.52e9</shear_modulus>
            </constitutive_model>
            <erosion algorithm="ZeroStress"/>
            <is_rigid> true </is_rigid>
            <thermal_conductivity>1.0</thermal_conductivity>
            <specific_heat>5</specific_heat>
            <geom_object>
                <box label = "piston">
                   <min>[0.0, 0.02,  0.0]</min>
                   <max>[0.01,0.03, 0.01]</max>
                </box>
                <res>[1,1,1]</res>
                <velocity>[0.0, 0.0001, 0.0]</velocity>
                <temperature>12</temperature>
             </geom_object>
         </material>

        <material>
          <density>1000.0</density>

          <constitutive_model type="cnh_damage">
               <bulk_modulus> 8.9e9  </bulk_modulus>
               <shear_modulus>3.52e9  </shear_modulus>
            </constitutive_model>
            <damage_model type="Threshold">
                <failure_mean> 900        </failure_mean>
                <failure_std>  0.1        </failure_std>
                <failure_distrib>constant </failure_distrib>
                <failure_criteria>MaximumPrincipalStress</failure_criteria>
            </damage_model>
            
            <erosion algorithm="ZeroStress"/>

            <is_rigid> false </is_rigid>
            <thermal_conductivity>0.0</thermal_conductivity>
            <specific_heat>5</specific_heat>

            <geom_object>
              <box label = "box">
                 <min>[0.0, 0.0, 0.0]</min>
                 <max>[0.01, 0.02, 0.01]</max>
              </box>
              <res>[1,2,1]</res>
              <velocity>[0.0, 0.0, 0.0]</velocity>
              <temperature>12</temperature>
            </geom_object>
        </material>

       <contact>
         <type>rigid</type>
         <materials>[0,1]</materials>
         <direction>[0,1,0]</direction>
         <stop_time>999999.9 </stop_time>
        </contact>

       </MPM>
    </MaterialProperties>
       
    <Grid>

       <BoundaryConditions>
         <Face side = "x-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>
         <Face side = "x+">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>
           <Face side = "y-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
           <Face side = "y+">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
           <Face side = "z-">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
           <Face side = "z+">
            <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
           </Face>                  
       </BoundaryConditions>

        <Level>
           <Box label = "1">
              <lower>[-0.01, 0.0,  -0.01]</lower>
              <upper>[0.02,  0.04, 0.02]</upper>
               <resolution>[6, 8, 6]</resolution>
              <patches>[1,1,1]</patches>
           </Box>
        </Level>
    </Grid>

</Uintah_specification>
