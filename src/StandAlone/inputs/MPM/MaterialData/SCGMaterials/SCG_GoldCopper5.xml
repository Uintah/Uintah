<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- SCG Gold with 5% Copper -->
<!-- 
    From: 
          Steinberg, D.J. Equation of State and Strength Properties of Selected Materials.
           UCRL-MA-106439: Change 1, Feb. 1996.
-->
<Uintah_Include>

    <density>     18100.0 </density>       <!-- kg/m^3 -->
    <thermal_conductivity> 318.0 </thermal_conductivity>  <!-- W/mK -->
    <specific_heat> 141.0 </specific_heat> <!-- J/kgK -->
    <room_temp>     298.0 </room_temp>     <!-- K -->
    <melt_temp>    1880.0 </melt_temp>     <!-- K -->

    <!-- Constitutive Model -->
    <constitutive_model type="elastic_plastic">
      <!-- Plasticity Model -->
      <plasticity_model type="steinberg_cochran_guinan">
        <mu_0>   30.7e9   </mu_0>     <!-- Pa -->
        <sigma_0> 0.63e9  </sigma_0>  <!-- Pa -->
        <Y_max>   1.10e9  </Y_max>    <!-- Pa -->
        <beta>   1000     </beta>
        <n>       0.072   </n>
        <A>       3.91    </A>        <!-- Pa^-1 -->
        <B>       3.26e-4 </B>        <!-- K^-1 -->
        <T_m0> 1880.0     </T_m0>     <!-- K -->
        <Gamma_0> 2.99    </Gamma_0>
        <a>       2.4     </a>
        <epsilon_p0> 0.0  </epsilon_p0>
      </plasticity_model>


      <!-- Shear Modulus Model -->
      <shear_modulus_model type="scg_shear">
        <mu_0> 30.7e9   </mu_0>       <!-- Pa -->
        <A>     3.91    </A>          <!-- Pa^-1 -->
        <B>     3.26e-4 </B>          <!-- K^-1 -->
      </shear_modulus_model>

      <!-- Melting Temperature Model -->
      <melting_temp_model type="scg_Tm">
        <T_m0> 1880.0  </T_m0>        <!-- K -->
        <Gamma_0> 2.99 </Gamma_0> 
        <a>       2.4  </a>
      </melting_temp_model>

      <!-- Equation of State -->
      <equation_of_state type="mie_gruneisen">
        <C_0>    3050.0  </C_0>      <!-- m/s -->
        <Gamma_0>   2.99 </Gamma_0>   
        <S_alpha>   1.56 </S_alpha>   
        <S_2>       0.0  </S_2>       
        <S_3>       0.0  </S_3>       
        <!-- <b>         0.59 </b> -->
      </equation_of_state>

      <initial_material_temperature>  298.0 </initial_material_temperature> <!-- K -->
    </constitutive_model>
</Uintah_Include>
