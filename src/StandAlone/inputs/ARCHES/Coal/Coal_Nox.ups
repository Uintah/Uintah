                  <bottom>[0.0, 0.0, 0.0]</bottom>
                  <top>   [0.0, 0.0, 0.05]</top>
                  <radius>0.01</radius>
                  <cylinder_end>false</cylinder_end>
                  <axisymmetric_end>false</axisymmetric_end>
                  <axisymmetric_side>false</axisymmetric_side>
