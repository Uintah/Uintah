<?xml version='1.0' encoding='ISO-8859-1' ?><Uintah_specification>   <Meta>        <title>Wasatch-Arches with Wasatch Convection Diffusion Scalar Transport</title>   </Meta>   <SimulationComponent type="arches" />   <Solver type="hypre" />   <Time>        <maxTime>10.0</maxTime>        <initTime>0.0</initTime>        <delt_min>0.0001</delt_min>        <delt_max>0.0001</delt_max>        <timestep_multiplier>0.35</timestep_multiplier>   </Time>   <DataArchiver>        <filebase>wasatch_arches_scalar_test.uda</filebase>        <outputTimestepInterval>10</outputTimestepInterval>        <save label = "totalKineticEnergy"/>        <save label = "pressurePS"/>        <save label = "newCCUVelocity"/>                <save label = "newCCVVelocity"/>                <save label = "newCCWVelocity"/>                <save label = "scalarSP"/>        <save label = "densityCP"/>        <save label = "viscosityCTS"/>        <save label = "cellType"/>        <save label = "wasatch_scalar"/>        <checkpoint cycle = "2" interval = "0.01"/>        <compression>gzip</compression>   </DataArchiver>    <Grid>      <BoundaryConditions>      <!-- BOUNDARY CONDITIONS -->      <Face side="x-">           <BCType id = "all" label = "wasatch_scalar" var = "Dirichlet">                <value> 0 </value>           </BCType>      </Face>        <Face circle = "x-" origin = "0 1.5 1.5" radius = ".5">           <BCType id = "all" label = "wasatch_scalar" var = "Dirichlet">                <value> 10 </value>           </BCType>         </Face>      <Face side="x+">           <BCType id = "all" label = "wasatch_scalar" var = "Neumann">                <value> 0.0 </value>           </BCType>      </Face>      <!-- BOUNDARY CONDITIONS -->      <Face side="y-">           <BCType id = "all" label = "wasatch_scalar" var = "Dirichlet">                <value> 10.0 </value>           </BCType>      </Face>      <Face side="y+">           <BCType id = "all" label = "wasatch_scalar" var = "Dirichlet">                <value> 10.0 </value>           </BCType>      </Face>      <!-- BOUNDARY CONDITIONS -->      <Face side="z-">           <BCType id = "all" label = "wasatch_scalar" var = "Dirichlet">                <value> 0.0 </value>           </BCType>      </Face>      <Face side="z+">           <BCType id = "all" label = "wasatch_scalar" var = "Dirichlet">                <value> 0.0 </value>           </BCType>      </Face>    </BoundaryConditions>        <Level>            <Box label = "1">                <lower>[0,0,0]</lower>                <upper>[3,3,3]</upper>                <resolution>[50,50,50]</resolution>                <extraCells>[1,1,1]</extraCells>                <patches>[1,1,1]</patches>            </Box>        </Level>    </Grid>   <PhysicalConstants>       <gravity>[-9.8,0,0]</gravity>       <reference_point>[-1,-1,-1]</reference_point>       <viscosity>0.000020</viscosity>   </PhysicalConstants>     <Wasatch>    <Density>      <IsConstant>true</IsConstant>    </Density>    <!-- Set the initial condition for non-staggered scalar -->    <BasicExpression type="SVOL">      <TaskList>initialization</TaskList>      <NameTag name="wasatch_scalar" state="STATE_N" />    <Constant>0.0</Constant>    </BasicExpression>    <!-- Build a non-staggered scalar transport equation -->    <TransportEquation equation="generic">      <SolutionVariable> wasatch_scalar </SolutionVariable>      <StrongForm>true</StrongForm>      <ConvectiveFluxExpression>        <Method>UPWIND</Method>         <Direction>X</Direction>        <AdvectiveVelocity>            <NameTag name="uVelocitySPBC" state="STATE_N"/>        </AdvectiveVelocity>      </ConvectiveFluxExpression>      <ConvectiveFluxExpression>        <Method>UPWIND</Method>        <Direction>Y</Direction>        <AdvectiveVelocity>            <NameTag name="vVelocitySPBC" state="STATE_N"/>        </AdvectiveVelocity>      </ConvectiveFluxExpression>      <ConvectiveFluxExpression>        <Method>UPWIND</Method>        <Direction>Z</Direction>        <AdvectiveVelocity>            <NameTag name="wVelocitySPBC" state="STATE_N"/>        </AdvectiveVelocity>      </ConvectiveFluxExpression>      <DiffusiveFluxExpression>        <Direction>X</Direction>        <ConstantDiffusivity>0.2</ConstantDiffusivity>      </DiffusiveFluxExpression>      <DiffusiveFluxExpression>        <Direction>Y</Direction>        <ConstantDiffusivity>0.2</ConstantDiffusivity>      </DiffusiveFluxExpression>      <DiffusiveFluxExpression>        <Direction>Z</Direction>        <ConstantDiffusivity>0.2</ConstantDiffusivity>      </DiffusiveFluxExpression>    </TransportEquation>    </Wasatch>   <CFD>        <ARCHES>      <!-- TIME INTEGRATOR -->      <TimeIntegrator>        <ExplicitIntegrator order="first"/>      </TimeIntegrator>            <Turbulence model="compdynamicprocedure">                <turbulentPrandtlNumber>0.4</turbulentPrandtlNumber>            </Turbulence>            <Properties>                <use_mixing_model>false</use_mixing_model>                <ColdFlowMixingModel>                  <Stream>                    <density>0.133</density>                    <temperature>298</temperature>                  </Stream>                  <Stream>                    <density>1.184344</density>                    <temperature>298</temperature>                  </Stream>                </ColdFlowMixingModel>            </Properties>            <BoundaryConditions>                <FlowInlet>                    <InletVelocity>0.05</InletVelocity>                    <mixture_fraction>0.0</mixture_fraction>                    <geom_object>                      <box label = "botwall">                        <min>[-1,-1,-1]</min>                        <max>[0,5,5]</max>                      </box>                    </geom_object>                </FlowInlet>                <FlowInlet>                    <InletVelocity>0.351</InletVelocity>                    <mixture_fraction>1.0</mixture_fraction>                    <geom_object>                      <cylinder label = "inlet">                        <bottom>[-5,1.5,1.5]</bottom>                        <top>[0,1.5,1.5]</top>                        <radius> .5 </radius>                      </cylinder>                    </geom_object>                </FlowInlet>                <PressureBC>                    <mixture_fraction>0.0</mixture_fraction>                    <geom_object>                      <box label = "posY_xzWall">                        <min>[0,3,-1]</min>                        <max>[3,4,4]</max>                      </box>                      <box label = "zeroY_xzWall">                        <min>[0,-1,-1]</min>                        <max>[3,0,4]</max>                      </box>                      <box label = "posZ_xyWall">                        <min>[0,-1,3]</min>                        <max>[3,4,4]</max>                      </box>                      <box label = "zeroZ_xyWall">                                <min>[0,-1,-1]</min>                        <max>[3,4,0]</max>                      </box>                    </geom_object>                </PressureBC>                <OutletBC>                    <mixture_fraction>0.0</mixture_fraction>                    <geom_object>                      <box label = "topWall">                        <min>[3,-1,-1]</min>                        <max>[4,4,4]</max>                      </box>                    </geom_object>                </OutletBC>            </BoundaryConditions>            <ExplicitSolver>            <initial_dt>0.0001</initial_dt>            <variable_dt>false</variable_dt>                <!-- timeIntegratorType>FE</timeIntegratorType -->                <PressureSolver>                    <Parameters>                      <solver>           cg     </solver>                      <preconditioner>  pfmg    </preconditioner>                      <tolerance>      1.0e-10  </tolerance>                      <maxiterations>    75     </maxiterations>                      <skip>              1     </skip>                    </Parameters>                </PressureSolver>                <MomentumSolver>                    <convection_scheme>central</convection_scheme>                </MomentumSolver>                <MixtureFractionSolver>                    <convection_scheme>central-upwind</convection_scheme>                </MixtureFractionSolver>            </ExplicitSolver>        </ARCHES>    </CFD></Uintah_specification>