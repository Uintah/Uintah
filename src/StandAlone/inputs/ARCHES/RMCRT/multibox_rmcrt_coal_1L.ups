            <weight>1</weight>
          </src>
          <scaling_const>1</scaling_const>
        </Eqn>
        <Eqn label="enthalpy" type="CCscalar">
          <doDiff>true</doDiff>
          <doConv>true</doConv>
          <stage value="0"/>
          <turbulentPrandtlNumber>0.4</turbulentPrandtlNumber>
          <D_mol_constant value="5e-5"/>
          <conv_scheme>roe_minmod</conv_scheme>
          <initialization type="tabulated">
            <depend_varname>table_enthalpy</depend_varname>
          </initialization>
          <src label="divQ">
            <weight>1</weight>
          </src>
          <src label="heat_source">
            <weight>1</weight>
          </src>
          <scaling_const>1</scaling_const>
        </Eqn>
        <Eqn label="coal_gas_mix_frac" type="CCscalar">
          <doDiff>true</doDiff>
          <doConv>true</doConv>
          <stage value="0"/>
          <turbulentPrandtlNumber>0.4</turbulentPrandtlNumber>
          <D_mol_constant value="5e-5"/>
          <conv_scheme>roe_minmod</conv_scheme>
          <initialization type="constant">
            <constant>0.4956539416</constant>
          </initialization>
          <Clipping>
            <low>0.0</low>
            <high>1.0</high>
            <tol>1e-10</tol>
          </Clipping>
          <src label="eta_source1">
            <weight>1</weight>
          </src>
          <src label="eta_source2">
            <weight>1</weight>
          </src>
          <scaling_const>1</scaling_const>
        </Eqn>
        <Sources>
          <src label="res_source" type="constant_src">
            <constant>1</constant>
            <multiply_by_variable>
              <variable_string_name>density</variable_string_name>
              <NewDW_only>true</NewDW_only>
            </multiply_by_variable>
          </src>
          <!--__________________________________-->
          <src label="divQ" type="rmcrt_radiation">
            <calc_frequency>3</calc_frequency>
            <abskt label="abskg"/>
            <RMCRT type="float">
              <randomSeed>         false           </randomSeed>
              <nDivQRays>          100             </nDivQRays>
              <nFluxRays>          50              </nFluxRays>
              <Threshold>          0.0025          </Threshold>
              <CCRays>             true            </CCRays>
              <solveBoundaryFlux>  true            </solveBoundaryFlux>
              <allowReflect>       false           </allowReflect>
              <rayDirSampleAlgo>   Naive           </rayDirSampleAlgo>
              <StefanBoltzmann>    5.67051e-8      </StefanBoltzmann>
            </RMCRT>
          </src>
          <src label="eta_source1" type="coal_gas_devol">
            <devol_model_name>devol</devol_model_name>
          </src>
          <src label="momentum_eta_source1" type="coal_gas_devol_mom">
            <devol_model_name>devol</devol_model_name>
          </src>
          <src label="momentum_eta_source2" type="coal_gas_oxi_mom">
