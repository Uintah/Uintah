<?xml version="1.0" encoding="iso-8859-1"?>



<Uintah_specification> 
<!--Please use a consistent set of units, (mks, cgs,...)-->

   <Meta>
       <title>none</title>
   </Meta>    

   <SimulationComponent type="ice" />
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            3.48e-5    </maxTime>
       <initTime>           0.0        </initTime>
       <delt_min>           2.4e-14    </delt_min>
       <delt_max>           2.4e-05    </delt_max>
       <delt_init>          1.8e-09    </delt_init>
       <timestep_multiplier>0.5        </timestep_multiplier>
   </Time>
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face side = "x-">
        <BCType id = "all" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "all"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "all"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "all"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
        <BCType id = "all"   label = "SpecificVol"  var = "computeFromDensity">
                              <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "all" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "all" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "all" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "z+">
        <BCType id = "all" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0.,   0.,   0.]    </lower>
              <upper>        [1.e-1,1.e-2,1.e-2] </upper>
              <extraCells>   [1,1,1]             </extraCells>
              <patches>      [1,1,1]             </patches>
              <resolution>   [100, 1, 1]         </resolution>
           </Box>
       </Level>
    </Grid>    
    <!--____________________________________________________________________-->
    <!--      O  U  P  U  T     V  A  R  I  A  B  L  E  S                   -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>IandG_1d.uda</filebase>
      <outputTimestepInterval>10</outputTimestepInterval>
      <save label="press_equil_CC"/>
      <save label="delP_Dilatate"/>
      <save label="delP_MassX"/>
      <save label="press_CC"/>
      <save label="vol_frac_CC"/>  
      <save label="mom_source_CC"/>
      <save label="mass_L_CC"/> 
      <save label="mom_L_ME_CC"/>
      <save label="eng_L_ME_CC"/>
      <save label="mom_L_CC"/>
      <save label="int_eng_L_CC"/>
      <save label="vel_CC"/>
      <save label="rho_CC"/>
      <save label="sp_vol_CC"/>
      <save label="temp_CC"/>
      <save label="speedSound_CC"/>
      <save label="modelMass_src"/>
      <save label="modelEng_src"/>
      <save label="modelVol_src"/>
      <save label="modelMom_src"/>
      <save label="IandG:F"/>
      <checkpoint interval="0.005" cycle="2"/>
   </DataArchiver>

    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.2</cfl>
       <ICE>
        <advection type = "SecondOrder" />
        <TimeStepControl>
          <Scheme_for_delT_calc> conservative   </Scheme_for_delT_calc>
          <knob_for_speedSound>  1.0            </knob_for_speedSound>
        </TimeStepControl>
      </ICE>        
    </CFD>
    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>

    <!--____________________________________________________________________-->
    <!--     Material Properties and Initial Conditions                     -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <ICE>
         <material name = "reactant">
           <EOS type = "JWL">
             <A>9.5322e14</A>
             <B>-5.944e9</B>
             <R1>14.1</R1>
             <R2>1.41</R2>
             <om>0.8867</om>
             <rho0>1835.0</rho0>
           </EOS>
           <dynamic_viscosity>      0.0                 </dynamic_viscosity>
           <thermal_conductivity>   0.0                 </thermal_conductivity>
           <specific_heat>          1515.31             </specific_heat>
           <gamma>                  1.4                 </gamma>
           <geom_object>
                <box label="wholegrid">
                    <min>           [-1.0, -1.0, -1.0 ]   </min>
                    <max>           [ 3.0,  2.0,  2.0 ]   </max>
                </box> 
               <res>            [1,1,1]                 </res>
               <velocity>       [-65.4,0.0,0.0]        </velocity>
               <temperature>    300.0                   </temperature>
               <density>        1833.83248097926        </density>
               <pressure>       101325.0                </pressure>
           </geom_object>
         </material>

         <material name = "product">
           <EOS type = "JWL">
             <A>1.6689e12</A>
             <B>5.969e10</B>
             <R1>5.9</R1>
             <R2>2.1</R2>
             <om>0.45</om>
             <rho0>1835.0</rho0>
           </EOS>
           <dynamic_viscosity>      0.0                 </dynamic_viscosity>
           <thermal_conductivity>   0.0                 </thermal_conductivity>
           <specific_heat>          544.96               </specific_heat>
           <gamma>                  1.4                 </gamma>
           <geom_object>
                <box label="out_of_domain">
                    <min>           [ 99.0,  99.0,  99.0 ]   </min>
                    <max>           [100.0, 100.0, 100.0 ]   </max>
                </box>
               <res>            [1,1,1]                 </res>
               <velocity>       [0.0,0.0,0.0]           </velocity>
               <temperature>    300.0                   </temperature>
               <density>        1.37726724081686        </density>
               <pressure>       101325.0                </pressure>
           </geom_object>
         </material>
      </ICE>       

      <exchange_properties> 
         <exchange_coefficients>
            <momentum>          [1.e15]       </momentum>
            <heat>              [1.e15]       </heat>
        </exchange_coefficients>
      </exchange_properties> 
    </MaterialProperties>  
                     
    <Models>
      <Model type="IandG">
        <IandG>
          <fromMaterial> reactant   </fromMaterial>
          <toMaterial>   product    </toMaterial>
          <I>    7.43e17   </I>
          <G1> 175.0e-16   </G1>
          <G2>  10.0e-5   </G2>
          <a>    0.    </a>
          <b>    0.667 </b>
          <c>    0.667 </c>
          <d>    0.111 </d>
          <e>    0.667 </e>
          <g>    0.667 </g>
          <x>   20.    </x>
          <y>    2.    </y>
          <z>    1.    </z>
          <Figmax>    0.02      </Figmax>
          <FG1max>    0.8       </FG1max>
          <FG2min>    0.8       </FG2min>
          <rho0>   1835.0       </rho0>
          <E0>        5.558e6   </E0>
          <ThresholdPressure>      200000000.      </ThresholdPressure>
        </IandG>
      </Model>
    </Models>
<!--
-->

</Uintah_specification>
