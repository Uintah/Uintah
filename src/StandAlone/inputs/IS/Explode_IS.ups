           <Box label = "3D">      
              <lower>        [0.0,-0.1,0.0]     </lower>
              <upper>        [0.1, 0.1,0.1]     </upper>
              <extraCells>   [1,1,1]            </extraCells>
              <patches>      [4,8,4]            </patches>
              <resolution>   [150,300,150]      </resolution>
           </Box> 
       </Level>
