          <var name="cell-height">30</var>
          <var name="size">$ImageButton::Text::size</var>
          <var name="fgcolor">$ImageButton::Text::fgcolor</var>
          <var name="bgcolor">$ImageButton::Text::bgcolor</var>
          <var name="shadow">1</var>
          <var name="offsetx">-1</var>
          <var name="offsety">1</var>
          <var name="anchor">C</var>
        </object>
-->
      </object>
    </object>
  </definition>
