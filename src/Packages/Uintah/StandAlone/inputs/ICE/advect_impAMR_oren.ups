    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0,0,0]            </lower>
              <upper>        [1,1,1]            </upper>
              <extraCells>   [1,1,1]            </extraCells>
              <patches>      [1,1,1]            </patches>
           </Box>
           <spacing>         [.25,.25,.25]      </spacing>
       </Level>
       <time_refinement_ratio>    1 </time_refinement_ratio>       

