<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- @version: -->
<Uintah_specification>

   <Meta>
       <title>Annulus problem with 1/4 symmetry</title>
   </Meta>    
    <!--____________________________________________________________________-->
    <!--   T  I  M  E     V  A  R  I  A  B  L  E  S                         -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            0.005         </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1e-5        </delt_max>
       <delt_init>          1.0e-9      </delt_init>
       <timestep_multiplier>0.8         </timestep_multiplier>
   </Time>    
    <!--____________________________________________________________________-->
    <!--   G  R  I  D     V  A  R  I  A  B  L  E  S                         -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face side = "x-">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0" var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0" var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>                  
      <Face side = "z+">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label = "1">
              <lower>        [0,0,0]    </lower>
              <upper>        [1.2,1.2,0.02]    </upper>
               <extraCells>  [1,1,1]     </extraCells>
              <patches>      [2,2,1]     </patches>
           </Box>
           <spacing>   [0.025,0.025,0.02]  </spacing>
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>symAnnulus.uda</filebase>
      <outputInterval> 2.5e-4 </outputInterval>
     <!--
       <compression>gzip</compression>
     -->
      <save label = "vol_frac_CC"/>
      <save label = "rho_CC"/>
      <save label = "temp_CC"/>
      <save label = "delP_Dilatate"/>
      <save label = "vel_CC"/>
      <save label = "sp_vol_CC"/>
      <save label = "press_CC"/>
      <save label = "press_equil_CC"/>
      <save label = "int_eng_L_CC"/>
      <save label = "intE_source_CC"/>
      <save label = "Tdot"/>
      <save label = "eng_L_ME_CC"/>
      <save label = "mom_L_CC"/>
      <save label = "mom_source_CC"/>
      <save label = "mom_L_ME_CC"/>
      <save label = "p.x"/>
      <save label = "p.volume"/>
      <save label = "p.stress"/>
      <save label = "p.pressure"/>
     
     <!--
      <save label = "p.velocity"/>
      <save label = "TotalMass"/>
      <save label = "StrainEnergy"/>
      <save label = "CenterOfMassPosition"/>
      <save label = "CenterOfMassVelocity"/>
      <save label = "g.stressFS"/>
     -->
   </DataArchiver>

   <Debug>
      <dbg_timeStart>       1.0        </dbg_timeStart>
      <dbg_timeStop>        1.0        </dbg_timeStop>
      <dbg_outputInterval>  1e-9       </dbg_outputInterval>
      <dbg_BeginIndex>      [31,18,0]  </dbg_BeginIndex>
      <dbg_EndIndex>        [35,22,1]  </dbg_EndIndex>
      <dbg_SigFigs>         5          </dbg_SigFigs>
     <!--
     <debug label = "switchDebugInitialize"/>
     <debug label = "switchDebug_EQ_RF_press"/>
     <debug label = "switchDebug_vel_FC"/>           
     <debug label = "switchDebug_Exchange_FC"/>           
     <debug label = "switchDebug_explicit_press"/>   
     <debug label = "switchDebug_PressFC"/>          
     <debug label = "switchDebugLagrangianValues"/> 
     <debug label = "switchDebugMomentumExchange_CC"/> 
            
     <debug label = "switchDebug_advance_advect"/>  
     <debug label = "switchDebugSource_Sink"/>
     -->     
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.25</cfl>
       <ICE>
        <max_iteration_equilibration> 100 </max_iteration_equilibration>
        <advection type = "FirstOrderCE"/>
        <solution technique = "EqForm"/>
        <ADD_HEAT>
          <add_heat_matls>   [2]      </add_heat_matls>
          <add_heat_coeff>   [8.0e10] </add_heat_coeff>         
          <add_heat_t_start>  0       </add_heat_t_start>
          <add_heat_t_final>  0       </add_heat_t_final>  <!-- this needs to be changed -->
        </ADD_HEAT>
        
      </ICE> 
    </CFD>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>


    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <MPM>
         <material>
                <burn type = "null"/>
                <density>   8900.0   </density>
                <constitutive_model type = "comp_neo_hook">
                  <useModifiedEOS>     true             </useModifiedEOS>
                  <shear_modulus>      43.8e9           </shear_modulus>
                  <bulk_modulus>       117.0e9          </bulk_modulus>
                </constitutive_model>
                <toughness>             10.e6           </toughness>
                <thermal_conductivity>  300.0           </thermal_conductivity>
                <specific_heat>         716.0           </specific_heat>
               <gamma>                  1.4             </gamma>           
                <geom_object>
                <difference>
                   <cylinder label = "outer cylinder">
                     <bottom>           [0.0,0.0,0.0]   </bottom>
                     <top>              [0.0,0.0,.02]   </top>
                     <radius>           1.0             </radius>
                  </cylinder>
                  <cylinder label = "inner cylinder">
                     <bottom>           [0.0,0.0,0.0]   </bottom>
                     <top>              [0.0,0.0,.02]   </top>
                     <radius>           0.5             </radius>
                  </cylinder>
                 </difference>
                  <res>                 [2,2,1]         </res>
                  <velocity>            [0.0,0.0,0.0]   </velocity>
                  <temperature>         300             </temperature>
                </geom_object>
         </material>
            <contact>
              <type>null</type>
              <vel_fields>              [0,1,0]         </vel_fields>
            </contact>
      </MPM>
       <ICE>
         <material>
           <burn type = "null"/>
           <EOS type = "ideal_gas">                     </EOS>
           <viscosity>                  1.0e-2         </viscosity>
           <thermal_conductivity>       0.0            </thermal_conductivity>
           <specific_heat>              716.0          </specific_heat>
           <speed_of_sound>             376            </speed_of_sound>
           <gamma>                      1.4            </gamma>
           <geom_object>
             <difference>
                  <box label = "surrounding">
                    <min>           [-1.0,-1.0,-1.0]    </min>
                    <max>           [3.0,3.0,1.0]       </max>
                  </box>
                 <cylinder label = "outer cylinder">
                     <bottom>       [0.0,0.0,0.0]       </bottom>
                     <top>          [0.0,0.0,.02]       </top>
                     <radius>       1.0                 </radius>
                 </cylinder>
               </difference>
             <res>                      [2,2,1]        </res>
             <velocity>           [0.0,0.0,0.0]        </velocity>
             <temperature>        300.0                </temperature>
             <density>    1.1792946927374306000e+00    </density>
             <pressure>           101325.0             </pressure>
           </geom_object>
         </material>
         <material>
           <burn type = "null"/>
           <EOS type = "ideal_gas">                     </EOS>
           <viscosity>                  1.0e-2         </viscosity>
           <thermal_conductivity>       0.0            </thermal_conductivity>
           <specific_heat>              716.0          </specific_heat>
           <speed_of_sound>             376            </speed_of_sound>
           <gamma>                      1.4            </gamma>
           <geom_object>
              <cylinder label = "inner cylinder">
                     <bottom>       [0.0,0.0,0.0]       </bottom>
                     <top>          [0.0,0.0,.02]       </top>
                     <radius>       0.5                 </radius>
              </cylinder>
             <res>                      [2,2,1]        </res>
             <velocity>           [0.0,0.0,0.0]        </velocity>
             <temperature>        300.0                </temperature>
             <density>  1.1792946927374306000e+00      </density>
             <pressure>           101325.0             </pressure>
           </geom_object>
         </material>
      </ICE>       
       
       <exchange_properties>  
          <exchange_coefficients>
             <momentum>           [1e15, 1e15, 1e15]    </momentum>
             <heat>               [0, 0, 0]             </heat>
          </exchange_coefficients>
        </exchange_properties> 
    </MaterialProperties>
            

    <PhysicalBC>
       <MPM>
      </MPM>
    </PhysicalBC>
       
</Uintah_specification>

