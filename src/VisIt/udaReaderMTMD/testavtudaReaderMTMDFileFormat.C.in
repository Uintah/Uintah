  vecY = vecY = vecZ = 0;
  tenX = tenY = tenZ = 0;

  levelPatchVecPtr = NULL;
  patchInfoVecPtr = NULL;
  udaVarsPtr = NULL;

  debug5 << "Out avtudaReaderFileFormat\n";
}


