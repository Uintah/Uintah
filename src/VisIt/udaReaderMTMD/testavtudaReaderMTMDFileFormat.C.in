    // debug5 << varName << endl;
    // debug5 << matl << endl;

    int level, local_patch;

    GetLevelAndLocalPatchNumber(domain, timestate, level, local_patch);

    unsigned int count = 0;

    if ((currLevel != level) || (currVar.find(tmpVarName) == string::npos)) { 
