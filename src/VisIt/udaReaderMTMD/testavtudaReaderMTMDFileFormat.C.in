	      // debug5 << loX << " " << hiX << endl;
	      // debug5 << loY << " " << hiY << endl;

	      for (int j = loY; j < hiY; j++) {
		for (int k = loX; k < hiX; k++) {
		  if (ncomps == 3) {
		    vecValMatrix[k][j][0] = vecValMatrix[k][j][extraXYZ[2]];
		    vecValMatrix[k][j][cellValColln.z+extraXYZ[2]] = vecValMatrix[k][j][cellValColln.z];
