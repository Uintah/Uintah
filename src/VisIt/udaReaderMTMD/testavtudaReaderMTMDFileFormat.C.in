		tenVal& tenValRef = tenValMatrix[k][j][i];
		// tenValRef.name = varName;
		for (int p = 0; p < 3; p++) {
		  for (int q = 0; q < 3; q++) {
		    tenValRef.mat[p][q] = cellValVec[count++];
		  }
		}   
	      }
	    }
	  }
	}

	// Re-sized matrix
	for (int m = 0; m < 3; m++) {
