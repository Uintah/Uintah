# SCI Network 1.0

if {[llength [array get env SCIRUN_DATA]] == 0} {
   puts ""
   puts "*** Error: this net depends on the SCIRUN_DATA enviroment variable."
   puts "*** Please exit, set it in your shell, and rerun."
   puts "*** (e.g. setenv SCIRUN_DATA /local/data/SCIRunData/)"
   puts ""
   ::netedit scheduleok
   return
}
set DATADIR $env(SCIRUN_DATA)
puts "*** Using SCIRUN_DATA $DATADIR"

if {[llength [array get env SCIRUN_DATASET]] == 0} {
   puts ""
   puts "*** Environment variable 'SCIRUN_DATASET' was not set -- "
   puts "*** using 'sphere' as the default."
   puts ""
   set DATASET sphere
} else {
   set DATASET $env(SCIRUN_DATASET)
   puts "*** Using DATASET $DATASET"
}

source $DATADIR/$DATASET/$DATASET-settings.net

::netedit dontschedule

global userName
set userName "dmw"

global runDate
set runDate " Tue  Mar 13 2001"

global runTime
set runTime " 0:22:14"

set bbox {8.0 8.0 715 646}

set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 8 8]
set m1 [addModuleAtPosition "SCIRun" "Render" "Viewer" 202 602]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 8 392]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 151 95]
set m4 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 202 366]
set m5 [addModuleAtPosition "SCIRun" "Visualization" "StreamLines" 373 268]
set m6 [addModuleAtPosition "SCIRun" "Fields" "SampleField" 391 166]
set m7 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 429 514]
set m8 [addModuleAtPosition "SCIRun" "Fields" "Gradient" 373 88]
set m9 [addModuleAtPosition "SCIRun" "Fields" "BuildInterpolant" 596 347]
set m10 [addModuleAtPosition "SCIRun" "Fields" "ApplyInterpolant" 578 421]

addConnection $m3 0 $m2 1
addConnection $m0 0 $m2 0
addConnection $m2 0 $m1 0
addConnection $m0 0 $m4 0
addConnection $m3 0 $m4 1
addConnection $m4 1 $m1 1
addConnection $m6 1 $m1 2
addConnection $m7 0 $m1 3
addConnection $m3 0 $m7 1
addConnection $m6 0 $m5 1
addConnection $m0 0 $m8 0
addConnection $m8 0 $m6 0
addConnection $m9 0 $m10 1
addConnection $m5 0 $m9 1
addConnection $m10 0 $m7 0
addConnection $m0 0 $m9 0
addConnection $m0 0 $m10 0
addConnection $m8 0 $m5 0

set $m0-notes {}
set $m0-show_status 1
set $m0-filename $DATADIR/$DATASET/$DATASET-voltage.tvd.fld
set $m1-notes {}
set $m1-show_status 1
