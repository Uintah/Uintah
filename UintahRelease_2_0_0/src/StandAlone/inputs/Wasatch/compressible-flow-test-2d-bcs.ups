<?xml version="1.0"?>
<Uintah_specification>
  <Meta>
    <title>Compressible flow in 2D</title>
  </Meta>

  <SimulationComponent type="wasatch"/>

  <Wasatch>
    <TimeIntegrator> FE </TimeIntegrator>
    <Density method="COMPRESSIBLE">
      <NameTag name="density"/>
    </Density>
    <!-- Set the initial condition expression for mixture fraction -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f0"/>
      <GaussianFunction amplitude="10.0" deviation="0.5" mean="0" baseline="0.0">
        <NameTag name="XSVOL"/>
      </GaussianFunction>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f1"/>
      <GaussianFunction amplitude="10.0" deviation="0.5" mean="0" baseline="0.0">
        <NameTag name="YSVOL"/>
      </GaussianFunction>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f01"/>
      <ExprAlgebra algebraicOperation="PRODUCT">
        <!-- specify the list of tags of the source expressions -->
        <NameTag name="f0"/>
        <NameTag name="f1"/>
      </ExprAlgebra>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f3"/>
      <Constant>273.0</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="Temperature"/>
      <ExprAlgebra algebraicOperation="SUM">
        <!-- specify the list of tags of the source expressions -->
        <NameTag name="f01"/>
        <NameTag name="f3" />
      </ExprAlgebra>
    </BasicExpression>
    <!-- Set the initial condition expression for mixture fraction -->
    <!-- 
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="Temperature"/>
      <GaussianFunction amplitude="1000.0" deviation="1.0" mean="0.0" baseline="273.0">
        <NameTag name="XSVOL"/>
      </GaussianFunction> 
    </BasicExpression>
 -->
    <!-- Set the initial condition expression for x-velocity -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="u"/>
      <Constant>0</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="v"/>
      <Constant>0</Constant>
    </BasicExpression>
    <!-- Set the initial condition expression for x-velocity -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="pressure"/>
      <Constant>101325</Constant>
    </BasicExpression>
    <!-- Set the initial condition expression for x-velocity -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="Mixture_MW"/>
      <Constant>28.966</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="Mixture_MW"/>
      <Constant>28.966</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="viscosity"/>
      <Constant>1.95e-5</Constant>
    </BasicExpression>
    <!-- Momentum transport equations -->
    <MomentumEquations>
      <X-Velocity>u</X-Velocity>
      <X-Momentum>x-mom</X-Momentum>
      <Y-Velocity>v</Y-Velocity>
      <Y-Momentum>y-mom</Y-Momentum>
      <Viscosity>
        <NameTag name="viscosity"/>
      </Viscosity>
    </MomentumEquations>
    <EnergyEquation>
      <SolutionVariable>rhoet</SolutionVariable>
      <PrimitiveVariable>et</PrimitiveVariable>
      <DiffusiveFlux direction="XY" coefficient="0.025"/>
      <ConvectiveFlux direction="X" method="CENTRAL">
        <AdvectiveVelocity>
          <NameTag name="u"/>
        </AdvectiveVelocity>
      </ConvectiveFlux>
      <ConvectiveFlux direction="Y" method="CENTRAL">
        <AdvectiveVelocity>
          <NameTag name="v"/>
        </AdvectiveVelocity>
      </ConvectiveFlux>
    </EnergyEquation>
  </Wasatch>
  <Time>
    <maxTime>5.0</maxTime>
    <initTime>0.0</initTime>
    <delt_min>1.0e-6</delt_min>
    <delt_max>1.0e-6</delt_max>
    <max_Timesteps> 200 </max_Timesteps>
    <timestep_multiplier>1.0</timestep_multiplier>
  </Time>
  <DataArchiver>
    <filebase>compressible-2d-test-bcs.uda</filebase>
    <outputTimestepInterval>10</outputTimestepInterval>
    <outputInitTimestep/>
    <checkpoint cycle="2" timestepInterval="20"/>
    <save label="Temperature"/>
    <save label="u"/>
    <save label="v"/>
    <save label="pressure"/>
    <save label="density"/>
    <save label="et"/>
    <save label="rhoet"/>
  </DataArchiver>
  <Grid>
    <BoundaryConditions>
      <DefaultMaterial>all</DefaultMaterial>
			<!-- BOUNDARY CONDITIONS FOR MOMENTUM -->
			<Face side="x-" type="Wall">
			  <BCType label="rhoet" var="Neumann" value="0.0"/>        			  
			</Face>
			<Face side="x+" type="Wall">
			  <BCType label="rhoet" var="Neumann" value="0.0"/>        			  
			</Face>
			<Face side="y-" type="Wall">
			  <BCType label="rhoet" var="Neumann" value="0.0"/>        			  
			</Face>
			<Face side="y+" type="Wall">
			  <BCType label="rhoet" var="Neumann" value="0.0"/>        			  
			</Face>      
    </BoundaryConditions>
    <Level>
      <Box label="1">
        <lower>[-2,-2,0]</lower>
        <upper>[2,2,1]</upper>
        <resolution>[100,100,1]</resolution>
        <patches>[2,2,1]</patches>
      </Box>
      <periodic>[0,0,1]</periodic>
    </Level>
  </Grid>
</Uintah_specification>
