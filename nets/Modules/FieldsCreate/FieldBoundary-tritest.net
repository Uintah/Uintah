# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {32 3 540 637}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 87 3]

# Create a SCIRun->Visualization->ShowField Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 375 342]

# Create a SCIRun->FieldsCreate->FieldBoundary Module
set m3 [addModuleAtPosition "SCIRun" "FieldsCreate" "FieldBoundary" 133 83]

# Create a SCIRun->Render->Viewer Module
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 32 593]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 288 258]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 317 39]

# Create a SCIRun->FieldsCreate->ClipField Module
set m7 [addModuleAtPosition "SCIRun" "FieldsCreate" "ClipField" 54 257]

# Create a SCIRun->FieldsCreate->FieldBoundary Module
set m8 [addModuleAtPosition "SCIRun" "FieldsCreate" "FieldBoundary" 141 345]

# Create a SCIRun->Visualization->ShowField Module
set m9 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 120 505]

# Create a SCIRun->FieldsData->ApplyInterpMatrix Module
set m10 [addModuleAtPosition "SCIRun" "FieldsData" "ApplyInterpMatrix" 100 170]

# Create a SCIRun->FieldsData->ApplyInterpMatrix Module
set m11 [addModuleAtPosition "SCIRun" "FieldsData" "ApplyInterpMatrix" 83 425]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m3 0]
set c2 [addConnection $m1 0 $m10 0]
set c3 [addConnection $m7 0 $m4 0]
set c4 [addConnection $m7 1 $m8 0]
set c5 [addConnection $m7 1 $m11 0]
set c6 [addConnection $m7 1 $m2 0]
set c7 [addConnection $m10 0 $m7 0]
set c8 [addConnection $m11 0 $m9 0]
set c9 [addConnection $m6 0 $m5 0]
set c10 [addConnection $m3 0 $m10 1]
set c11 [addConnection $m8 0 $m11 1]
set c12 [addConnection $m10 0 $m5 1]
set c13 [addConnection $m5 0 $m2 1]
set c14 [addConnection $m5 0 $m9 1]
set c15 [addConnection $m9 0 $m4 1]
set c16 [addConnection $m3 1 $m10 2]
set c17 [addConnection $m8 1 $m11 2]
set c18 [addConnection $m2 0 $m4 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m2-nodes-on {0}
setGlobal $m2-edges-on {0}
setGlobal $m2-normalize-vectors {}
setGlobal $m2-has_scalar_data {1}
setGlobal $m2-node_display_type {Spheres}
setGlobal $m2-active_tab {Faces}
setGlobal $m2-show_progress {}

