set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {61 11 602 510}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 94 11]

# Create a SCIRun->FieldsCreate->SampleField Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleField" 198 188]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 61 466]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 63 383]

# Create a SCIRun->FieldsData->DirectInterpolate Module
set m5 [addModuleAtPosition "SCIRun" "FieldsData" "DirectInterpolate" 64 275]

# Create a SCIRun->FieldsData->TransformScalarData Module
set m6 [addModuleAtPosition "SCIRun" "FieldsData" "TransformScalarData" 170 91]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 353 287]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m8 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 407 166]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m5 0]
set c2 [addConnection $m1 0 $m6 0]
set c3 [addConnection $m5 0 $m4 0]
set c4 [addConnection $m6 0 $m2 0]
set c5 [addConnection $m8 0 $m7 0]
set c6 [addConnection $m4 0 $m3 0]
set c7 [addConnection $m1 0 $m7 1]
set c8 [addConnection $m2 0 $m5 1]
set c9 [addConnection $m7 0 $m4 1]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->FieldsCreate->SampleField Module
setGlobal $m2-endpoints {1}
setGlobal $m2-endpoint0x {        -0.855352}
setGlobal $m2-endpoint0y {        -0.268087}
setGlobal $m2-endpoint0z {        -0.216361}
setGlobal $m2-endpoint1x {         0.817528}
