<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- @version: -->
<Uintah_specification>
  <Meta>
    <title>Pipe Emitting Heptane</title>
  </Meta>

  <SimulationComponent type="arches"/>

  <Solver type="hypre"/>
  <Time>
    <maxTime>2.0</maxTime>
    <initTime>0.0</initTime>
    <delt_min>1.e-8</delt_min>
    <delt_max>0.1</delt_max>
    <timestep_multiplier>0.4</timestep_multiplier>
    <max_Timesteps>25</max_Timesteps>
  </Time>

  <DataArchiver>
    <filebase>heptane_pipe.uda</filebase>
    <outputTimestepInterval>2</outputTimestepInterval>
    <save label="uVelocitySPBC"/>
    <save label="vVelocitySPBC"/>
    <save label="wVelocitySPBC"/>
    <save label="enthalpy"/>
    <save label="volFraction"/>
    <save label="cellType"/>
<!-- Table Variables -->
<!-- iv's -->
    <save label="mixture_fraction"/>
    <save label="scalar_var"/>
    <save label="heat_loss"/>
<!-- dv's -->
    <save label="radiation_temperature"/>
    <save label="density" table_lookup="true"/>
    <save label="CO2" table_lookup="true"/>
    <save label="O2" table_lookup="true"/>
    <save label="H2O" table_lookup="true"/>
    <save label="CO" table_lookup="true"/>
    <save label="NC7H16" table_lookup="true"/>
    <save label="sensibleenthalpy" table_lookup="true"/>
    <save label="adiabaticenthalpy" table_lookup="true"/>

    <checkpoint cycle="2" interval="0.02"/>
    <compression>gzip</compression>
  </DataArchiver>

  <Grid>
    <BoundaryConditions>
      <Face side="x-" name="floor">
        <BCType id="all" label="bottom wall" var="WallBC">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="enthalpy" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="passive_scalar" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="heat_loss" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="scalar_var" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="radiation_temperature" var="Dirichlet">
          <value>298.0</value>
        </BCType>
        <BCType id="0"  label= "abskg"              var="ForcedDirichlet">
                            <value> 1.0 </value>
        </BCType>
      </Face>
      <Face side="x+" name="x+ wall">
        <BCType id="all" label="top" var="OutletBC">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="enthalpy" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="passive_scalar" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="heat_loss" var="Dirichlet">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="scalar_var" var="Dirichlet">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="radiation_temperature" var="Dirichlet">
          <value>298.0</value>
        </BCType>
        <BCType id="0"  label= "abskg"              var="ForcedDirichlet">
                            <value> 1.0 </value>
        </BCType>
      </Face>
      <Face side="y-" name="y- wall">
        <BCType id="all" label="y-pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="enthalpy" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="passive_scalar" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="heat_loss" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="scalar_var" var="Dirichlet">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="radiation_temperature" var="Dirichlet">
          <value>298.</value>
        </BCType>
        <BCType id="0"  label= "abskg"              var="ForcedDirichlet">
                            <value> 1.0 </value>
        </BCType>
      </Face>
      <Face side="y+" name="y+ wall">
        <BCType id="all" label="y+pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="enthalpy" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="passive_scalar" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="heat_loss" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="scalar_var" var="Dirichlet">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="radiation_temperature" var="Dirichlet">
          <value>298</value>
        </BCType>
        <BCType id="0"  label= "abskg"              var="ForcedDirichlet">
                            <value> 1.0 </value>
        </BCType>
      </Face>
      <Face side="z-" name="z- wall">
        <BCType id="all" label="z-pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="enthalpy" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="passive_scalar" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="heat_loss" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="scalar_var" var="Dirichlet">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="radiation_temperature" var="Dirichlet">
          <value>298</value>
        </BCType>
        <BCType id="0"  label= "abskg"              var="ForcedDirichlet">
                            <value> 1.0 </value>
        </BCType>
      </Face>
      <Face side="z+" name="z+ wall">
        <BCType id="all" label="z+pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="enthalpy" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="passive_scalar" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="heat_loss" var="Neumann">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="scalar_var" var="Dirichlet">
          <value>0.0</value>
        </BCType>
        <BCType id="all" label="radiation_temperature" var="Dirichlet">
          <value>298</value>
        </BCType>
        <BCType id="0"  label= "abskg"              var="ForcedDirichlet">
                            <value> 1.0 </value>
        </BCType>
      </Face>
    </BoundaryConditions>

    <Level>
      <Box label="1">
        <lower>[0,0,0]</lower>
        <upper>[3,3,3]</upper>
        <resolution>[15,15,15]</resolution>
        <extraCells>[1,1,1]</extraCells>
        <patches>[1,1,1]</patches>
      </Box>
    </Level>
  </Grid>

  <PhysicalConstants>
    <gravity>[-9.8,0,0]</gravity>
    <reference_point>[-1,-1,-1]</reference_point>
    <viscosity>0.000020</viscosity>
  </PhysicalConstants>

  <CFD>
<!-- ARCHES specification -->
    <ARCHES>

<!-- TIME INTEGRATOR -->
      <TimeIntegrator>
        <ExplicitIntegrator order="first"/>
      </TimeIntegrator>

<!-- SCALARS -->
      <TransportEqns>
        <Eqn label="mixture_fraction" type="CCscalar">
          <doDiff>true</doDiff>
          <doConv>true</doConv>
          <conv_scheme>upwind</conv_scheme>
          <determines_properties/>
          <initialization type="geometry_fill">
            <geom_object>
              <cylinder label="cylinder_fill">
                <bottom>          [-1.0, 1.5, 1.5]   </bottom>
                <top>              [1.0, 1.5, 1.5]   </top>
                <radius>           0.5          </radius>
              </cylinder>
            </geom_object>
            <constant_inside>1.0</constant_inside>
          </initialization>
          <Clipping>
            <low>0.0</low>
            <high>1.0</high>
          </Clipping>
        </Eqn>

        <Eqn label="enthalpy" type="CCscalar">
          <doDiff>true</doDiff>
          <doConv>true</doConv>
          <conv_scheme>upwind</conv_scheme>
          <determines_properties/>
          <initialization type="tabulated">
            <depend_varname>adiabaticenthalpy</depend_varname>
          </initialization>
          <src label="divQ"/>
        </Eqn>

        <Sources>
          <src label="divQ" type="do_radiation">
            <calc_frequency>5</calc_frequency>
            <calc_on_all_RKsteps>false</calc_on_all_RKsteps>
            <abskg label="abskg"/>
            <DORadiationModel>
              <LinearSolver type="hypre">
                <res_tol>1.0e-10</res_tol>
                <ksptype>gmres</ksptype>
                <pctype>jacobi</pctype>
                <max_iter>75</max_iter>
              </LinearSolver>
            </DORadiationModel>
          </src>
        </Sources>

      </TransportEqns>

      <PropertyModels>
        <model type="radiation_properties" label="abskg">
          <initialization type="constant">
            <constant>0.0</constant>
          </initialization>
          <calculator type="hottel_sarofim">
            <abskg label="gas_abskg"/>
            <opl>3.0</opl>
          </calculator>
        </model>
        <model type="heat_loss" label="heat_loss">
          <initialization type="constant">
            <constant>0</constant> 
          </initialization>
          <enthalpy_label>enthalpy</enthalpy_label>
        </model>
        <model type="scalsim_variance" label="scalar_var">
          <initialization type="constant">
            <constant>0.0</constant>
          </initialization>
          <mixture_fraction_label>mixture_fraction</mixture_fraction_label>
          <density_label>density</density_label>
          <variance_coefficient>0.14</variance_coefficient> 
        </model>
        <model type="cc_constant" label="soot">
          <initialization type="constant">
            <constant>0.0</constant>
          </initialization>
          <constant>0.0</constant>
        </model>
      </PropertyModels>

      <Turbulence model="smagorinsky">
        <cf>0.17</cf>
        <fac_mesh>1.0</fac_mesh>
        <filterl>0.000001</filterl>
      </Turbulence>

      <Properties>
        <ClassicTable>
          <inputfile>inputs/ARCHES/ClassicMixingTables/heptane/C7H16_flmlt_nonadiabatic.mix.gz</inputfile>
          <standard_flamelet f_label="mixture_fraction" var_label="scalar_var" hl_label="heat_loss"/>
          <reference_state f="0.0" var="0.0" hl="0.0"/>
        </ClassicTable>
      </Properties>

      <BoundaryConditions>
        <suppress_corner_recirculation/>
        <intrusions>
          <intrusion label="flare">
            <geom_object>
              <cylinder label="inner cylinder">
                <bottom>          [-1.0, 1.5, 1.5]   </bottom>
                <top>              [1.0, 1.5, 1.5]   </top>
                <radius>           0.5          </radius>
              </cylinder>
            </geom_object>
            <flux_dir>x+</flux_dir>
            <velocity type="massflow">
              <flow_rate>0.25</flow_rate>
            </velocity>
            <scalars>
              <scalar label="mixture_fraction" type="flat">
                <constant>1.0</constant>
              </scalar>
              <scalar label="scalar_var" type="flat">
                <constant>0.0</constant>
              </scalar>
              <scalar label="heat_loss" type="flat">
                <constant>0.0</constant>
              </scalar>
              <scalar label="enthalpy" type="tabulated">
                <depend_varname>adiabaticenthalpy</depend_varname>
              </scalar>
              <scalar label="passive_scalar" type="flat">
                <constant>0.25</constant>
              </scalar>
            </scalars>

          </intrusion>
        </intrusions>
      </BoundaryConditions>

      <ExplicitSolver>
        <restartOnNegativeDensityGuess>false</restartOnNegativeDensityGuess>
        <PressureSolver>
          <Parameters>
            <tolerance>     1.0e-16  </tolerance>
            <solver>          cg    </solver>
            <preconditioner>  pfmg  </preconditioner>
            <maxiterations>   250   </maxiterations>
          </Parameters>
        </PressureSolver>

        <MomentumSolver>
          <convection_scheme>upwind</convection_scheme>
        </MomentumSolver>

      </ExplicitSolver>
    </ARCHES>

  </CFD>

</Uintah_specification>
