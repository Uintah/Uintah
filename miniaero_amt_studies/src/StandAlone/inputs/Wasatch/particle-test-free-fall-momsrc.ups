<Uintah_specification>

  <Meta>
    <title>Lid Driven Cavity Problem</title>
  </Meta>

  <SimulationComponent type="wasatch"/>
   <Solver type="hypre"/>
  <Wasatch>
      <TimeIntegrator> FE </TimeIntegrator>            


<PostProcessingExpression type="SVOL">
		<TaskList>advance_solution</TaskList>
		<NameTag name="xvelcc"/>
		<InterpolateExpression type="XVOL">
				<NameTag name="u"/>
		</InterpolateExpression>
</PostProcessingExpression>

<PostProcessingExpression type="SVOL">
		<TaskList>advance_solution</TaskList>
		<NameTag name="yvelcc"/>
		<InterpolateExpression type="YVOL">
				<NameTag name="v"/>
		</InterpolateExpression>
</PostProcessingExpression>

<PostProcessingExpression type="SVOL">
		<TaskList>advance_solution</TaskList>
		<NameTag name="zvelcc"/>
		<InterpolateExpression type="ZVOL">
				<NameTag name="w"/>
		</InterpolateExpression>
</PostProcessingExpression>

<ForceOnGraph tasklist="advance_solution">
	<NameTag name="xvelcc"/>
	<NameTag name="yvelcc"/>
	<NameTag name="zvelcc"/>	
</ForceOnGraph>


    <PostProcessingExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="vel_magnitude"/>
      <VelocityMagnitude>
      <XVelocity>
        <NameTag name="u"/>
      </XVelocity>
      <YVelocity>
        <NameTag name="v"/>
      </YVelocity>
      <ZVelocity>
        <NameTag name="w"/>
      </ZVelocity>      
      </VelocityMagnitude>
    </PostProcessingExpression>

    <ForceOnGraph tasklist="advance_solution">
      <NameTag name="vel_magnitude"/>                
    </ForceOnGraph>

    <ParticleTransportEquations>
      <ParticlesPerCell>1</ParticlesPerCell>
      <MaximumParticles>1</MaximumParticles>
      <ParticleDensity name="p.rho"/>            
      <ParticleMass name="p.m" state="STATE_DYNAMIC">
      	<SourceTerm name="nothing"/>	
      </ParticleMass>      
      <ParticleSize name="p.size" state="STATE_DYNAMIC">      
      	<SourceTerm name="nothing"/>
      </ParticleSize>
			<ParticlePosition x="p.xw" y="p.yw" z="p.zw">
				<Velocity>
					<XVel name="p.u" state="STATE_DYNAMIC"/>
					<YVel name="p.v" state="STATE_DYNAMIC"/>
					<ZVel name="p.w" state="STATE_DYNAMIC"/>	
				</Velocity>
			</ParticlePosition>
			<ParticleMomentum x="p.u" y="p.v" z="p.w">
<!-- 				<DisableDragForce/> -->
				<Gravity direction="Z"/>
				<GasProperties>
					<GasViscosity name="viscosity"/>
					<GasDensity name="density"/>					
					<GasVelocity>
						<XVel name="u"/>
						<YVel name="v"/>
						<ZVel name="w"/>		
					</GasVelocity>				
				</GasProperties>
			</ParticleMomentum>
    </ParticleTransportEquations>

    <!-- Momentum transport equations -->
    <MomentumEquations>
 	
      <X-Velocity>u</X-Velocity>
      <X-Momentum>x-mom</X-Momentum>
      <Y-Velocity>v</Y-Velocity>
      <Y-Momentum>y-mom</Y-Momentum>
      <Z-Velocity>w</Z-Velocity>
      <Z-Momentum>z-mom</Z-Momentum>
      <Viscosity>
        <NameTag name="viscosity"/>
      </Viscosity>
      
      <Pressure>
        <Parameters>
 	 	 	 	 	 	 <skip>1</skip>
            <solver>         cg      </solver>
            <preconditioner> pfmg    </preconditioner>
            <tolerance>      1e-10  </tolerance>
            <maxiterations>  100   </maxiterations>
            <relax_type>     2       </relax_type>
           </Parameters>

      </Pressure>
    </MomentumEquations>    
    <Density method="CONSTANT">
      <Constant value="1.225" name="density"/>
    </Density>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.size"/>
<!--        <Constant>0.01</Constant> -->
       <Constant>0.067</Constant>
<!--        <RandomField low="0.01" high="0.02" seed="11"/> -->
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.rho"/>
       <Constant>369</Constant>
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.xw"/>
       <Constant>0.25</Constant>
<!-- 
      <ParticlePositionIC coordinate="X" bounds="PATCHBASED">
        <Random seed="1"/>
      </ParticlePositionIC>
 -->
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.yw"/>
       <Constant>0.1</Constant>
<!-- 
      <ParticlePositionIC coordinate="Y" bounds="PATCHBASED">
        <Random seed="2"/>
      </ParticlePositionIC>
 -->
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.zw"/>
       <Constant>0.95</Constant>
    </BasicExpression>


    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.u"/>
       <Constant>0.0</Constant>
    </BasicExpression>
    
    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.v"/>
       <Constant>0.3</Constant>
    </BasicExpression>
    
    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.w"/>
       <Constant>0.0</Constant>
    </BasicExpression>


    <!-- Set the initial condition expression for x-momentum -->
    <BasicExpression type="XVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="u"/>
       <Constant>0.0</Constant>
    </BasicExpression>

    <BasicExpression type="YVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="v"/>
       <Constant>0.0</Constant>
    </BasicExpression>

    <BasicExpression type="ZVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="w"/>
       <Constant>0.0</Constant>
    </BasicExpression>
    
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="viscosity"/>
      <Constant>1e-3</Constant>
    </BasicExpression>

  </Wasatch>


  <Time>
    <maxTime>10</maxTime>
    <initTime>0.0</initTime>
    <delt_min>1e-4</delt_min>
    <delt_max>1e-4</delt_max>
<!--     <max_Timesteps> 10</max_Timesteps> -->
    <timestep_multiplier>0.1</timestep_multiplier>
  </Time>


  <DataArchiver>
    <filebase>particle-test-free-fall-momsrc.uda</filebase>
    <outputTimestepInterval>100</outputTimestepInterval>
    <outputInitTimestep/>
    <checkpoint cycle="2" interval="0.0001"/>
    <save label="p.x"/>
    <save label="x-mom"/>    
    <save label="vel_magnitude"/>        
    <save label="y-mom"/>        
    <save label="z-mom"/>        
    
        <save label="xvelcc"/>        
    <save label="yvelcc"/>        
    <save label="zvelcc"/>        
    <save label="p.xw_rhs"/>    
    <save label="p.yw_rhs"/>    
    <save label="p.zw_rhs"/>            

    <save label="p.re"/>    
    <save label="p.cd"/>        
    <save label="p.tau"/>    
    <save label="p.drag_x"/>    
    <save label="p.drag_y"/>    
		<save label="p.drag_z"/>    

		<save label="p.body_z"/>        
    <save label="p.u_rhs"/>    
    <save label="p.v_rhs"/>    
    <save label="p.w_rhs"/>            
    <save label="p.m"/>
    <save label="p.u"/>    
    <save label="p.v"/>        
    <save label="p.w"/>        
    
    <save label="p_momsrc_x"/>    
    <save label="p_momsrc_y"/>    
    <save label="p_momsrc_z"/>        
    
    <save label="p.size"/>    
    <save label="p.xw"/>        
    <save label="p.yw"/>
    <save label="p.zw"/>    
  </DataArchiver> 

	<Grid>
		<BoundaryConditions>
			<DefaultMaterial>all</DefaultMaterial>
			<!-- BOUNDARY CONDITIONS FOR MOMENTUM -->
			<Face side="x-" type="Wall"/>
			<Face side="x+" type="Wall"/>
			<Face side="y-" type="Wall"/>
			<Face side="y+" type="Wall"/>
			<Face side="z-" type="Wall"/>			
			<Face side="z+" type="Wall"/>			
		</BoundaryConditions>

		<Level>
			<Box label="1">
				<lower>[0,0,0]</lower>
				<upper>[0.5,1,1]</upper>
				<resolution>[16,32,32]</resolution>
				<patches>[1,1,1]</patches>
			</Box>
			<periodic>[0,0,0]</periodic>
		</Level>
	</Grid>

</Uintah_specification>
