<?xml version="1.0" encoding="UTF-8"?>
<inputs>
  <CoalFeedRate>
    <value>0.0</value>
    <var>Neumann</var>
    <notes>[kg/s] mass flow rate of coal, var sets the bcs for all of the coal equations </notes>
  </CoalFeedRate>
  <Diameters>
    <value>8.3009e+00,8.2135e+01,2.6695e+02</value>
    <notes>[micrometers] particle diameter for each quadrature node</notes>
  </Diameters>
  <Diameter_weights>
    <value>0.34,0.33,0.33</value>
    <notes>[-] particle diameter weights (number density) for each quadrature node</notes>
  </Diameter_weights>
  <particle_temperature>
    <value>350</value>
    <notes>[K] temperature of particles at inlet</notes>
  </particle_temperature>
  <gas_phase>
    <xoutlet>
      <value>0</value>
      <var>OutletBC</var>
      <notes>[-] outlet bc 0 Neumann</notes>
    </xoutlet>
    <mixture_fraction_2>
      <value>0.0</value>
      <var>Neumann</var>
      <notes>[-] mixture fraction between primary and secondary streams</notes>
    </mixture_fraction_2>
    <coal_gas_mix_frac>
      <value>0.0</value>
      <var>Neumann</var>
      <notes>[-] mixture fraction of coal off gas</notes>
    </coal_gas_mix_frac>
    <heat_loss>
      <value>0.0</value>
      <var>Neumann</var>
      <notes>[-] heat loss</notes>
    </heat_loss>
    <enthalpy>
      <value>0.0</value>
      <var>Neumann</var>
      <notes>[-] enthalpy</notes>
    </enthalpy>
    <radiation_temperature>
      <value>0</value>
      <var>Neumann</var>
      <notes>[K] radiation temperature</notes>
    </radiation_temperature>
    <abskg>
      <value>1.0</value>
      <var>Dirichlet</var>
      <notes>[-] wall emissivity</notes>
    </abskg>
    <residence_time>
      <value>0.0</value>
      <var>Neumann</var>
      <notes>[-] net time in reactor</notes>
    </residence_time>
  </gas_phase>
</inputs>
