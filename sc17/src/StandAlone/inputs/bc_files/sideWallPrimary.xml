<?xml version="1.0" encoding="UTF-8"?>
<inputs>
  <CoalFeedRate>
    <!-- Wet coal mass flow rate (as recieved) -->
    <!--<value>1.240</value>-->
    <value>2.6095885116</value>
    <useMassFlowInlet/>
    <var>Dirichlet</var>
    <notes>[kg/s] mass flow rate of coal, var sets the bcs for all of the coal equations </notes>
  </CoalFeedRate>
  <gas_phase>
    <!-- gas flow rate -->
    <mass_inlet>
      <!--<value>2.0703</value>-->
      <value>2.6095885116</value>
      <!--<var>VelocityInlet</var>-->
      <var>MassFlowInlet</var>
      <!--<var>Swirl</var>-->
      <!--<swirl_no>1.0</swirl_no>-->
      <notes>[kg/s] mass flow rate of gas</notes>
    </mass_inlet>
    <!-- Wet coal mass flow rate (as recieved) -->
    <coal_gas_mix_frac>
      <value>.3037</value>
      <var>Dirichlet</var>
      <notes>[-] mixture fraction of coal off gas</notes>
    </coal_gas_mix_frac>
    <!-- Wet coal mass flow rate (as recieved) -->
    <heat_loss>
      <value>0.98</value>
      <var>Dirichlet</var>
      <notes>[-] heat loss</notes>
    </heat_loss>
    <!-- Wet coal mass flow rate (as recieved) -->
    <enthalpy>
      <value>table_enthalpy</value>
      <var>Tabulated</var>
      <notes>[-] heat loss</notes>
    </enthalpy>
    <!-- Wet coal mass flow rate (as recieved) -->
    <radiation_temperature>
      <value>0</value>
      <var>Neumann</var>
      <notes>[K] radiation temperature</notes>
    </radiation_temperature>
    <abskg>
      <value>1.0</value>
      <var>Dirichlet</var>
      <notes>[-] wall emissivity</notes>
    </abskg>
    <residence_time>
      <value>0.0</value>
      <var>Dirichlet</var>
      <notes>[-] net time in reactor</notes>
    </residence_time>
  </gas_phase>
</inputs>
