<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE component PUBLIC '-//SCIRun/Component DTD' '../../SCIRun/Dataflow/XML/component.dtd'>
<?xml-stylesheet href="component.xsl" type="text/xsl"?>
<?cocoon-process type="xslt"?>

<component name="VectorFieldExtractor" category="Selectors">
  <overview>
    <authors>
      <author>Packages/Kurt Zimmerman</author>
    </authors>
    <summary>Select vector data from a Uintah Data Archive.</summary>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>Data Archive</name>
        <datatype>Uintah::Archive</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>Field</name>
        <datatype>SCIRun::Field</datatype>
      </port>
   </outputs>
  </io>
</component>
