  fi
  if test "$with_mpi_include" = "yes"; then
    as_fn_error $? "--with-mpi-include=DIR must specify a DIRectory. " "$LINENO" 5
  fi
  if test "$with_mpi_lib" = "yes"; then
    as_fn_error $? "--with-mpi-lib=DIR must specify a DIRectory. " "$LINENO" 5
  fi
