     echo
     as_fn_error $? "Last parameter of SCI-TRY-LINK for pidx library and headers must be: optional or required or specific.  (You had $_sci_optional.)  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
fi

# If $_sci_inc $INC_MPI_H (the -I paths) is blank, do nothing, else do the for statement.

for inc in $_sci_inc $INC_MPI_H; do

  # Remove any trailing / from inc.
  inc=${inc%/}

  if test "$inc" = "/usr/include" || test "$inc" = "-I/usr/include"; then
     echo ""
     as_fn_error $? "Please do not specify /usr/include as the location for PIDX include files." "$LINENO" 5
  fi

  # Make sure it doesn't have any thing but -I
  #   The following "sed" replaces anything that starts with a '-' with nothing (blank).
  has_minus_faltivec=no
  has_minus=`echo $inc | sed 's/-.*//'`
  if test -z "$has_minus"; then
     has_minus_i=`echo $inc | sed 's/-I.*//'`
     has_minus_faltivec=`echo $inc | sed 's/-faltivec//'`
     if test -n "$has_minus_i" && test -n "$has_minus_faltivec"; then
        # Has some other -?.
        if test "$verbose" = "yes"; then
          echo
          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Only -I options are allowed in arg 4 ($_sci_inc $INC_MPI_H) of PIDX check.  Skipping $inc." >&5
$as_echo "$as_me: WARNING: Only -I options are allowed in arg 4 ($_sci_inc $INC_MPI_H) of PIDX check.  Skipping $inc." >&2;}
        fi
        continue
     fi
  fi

  the_inc=`echo $inc | grep "^\-I"`
  if test -z "$the_inc" && test "$has_minus_faltivec" = "no"; then
     # If the include arg does not already have -I on it.
     if test -d $inc; then
        # If the directory exists
        _sci_includes="$_sci_includes -I$inc"
     fi
  else
     # It already has -I so just add it directly.
     _sci_includes="$_sci_includes $inc"
  fi
done

### Take care of arg 5 (the list of libs)

if test -n "$MPI_LIB_FLAG pidx"; then

   found_framework=no
   for lib in $MPI_LIB_FLAG pidx; do

      if test "$found_framework" = "one"; then
         found_framework=two
      else
         found_framework=no
      fi

      # Make sure it doesn't have any thing but -l
      has_minus=`echo $lib | sed 's/-.*//'`
      if test -z "$has_minus"; then
         has_minus_l=`echo $lib | sed 's/-l.*//'`
         has_minus_framework=`echo $lib | sed 's/-framework.*//'`

         if test -z "$has_minus_framework"; then
            # Two rounds for this loop with respect to frameworks.
            # First round is to skip adding -l to the beginning of -framework.
            # Second round is to not add -l to the framework lib.
            found_framework=one
         fi

         if test -n "$has_minus_l" && test -n "$has_minus_framework"; then
            # Has some other -?.
            if test "$verbose" = "yes"; then
              echo
              { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Only -l options are allowed in arg 5 of PIDX check (disregarding $lib)." >&5
$as_echo "$as_me: WARNING: Only -l options are allowed in arg 5 of PIDX check (disregarding $lib)." >&2;}
            fi
            continue
         fi
      fi

      the_lib=`echo $lib | grep "^\-l"`
      if test -z "$the_lib" && test "$found_framework" = "no"; then
         # If the lib arg does not have -l on it, then add -l.
         if test "$IS_VC" = "yes"; then
           if test -n "`echo $lib | sed 's,.*\.lib,,'`"; then
             final_lib=$lib.lib
           else
             final_lib=$lib
           fi
         else
           final_lib=-l$lib
         fi
      else
         # It already has -l so just add it directly.
         final_lib=$lib
      fi
      _sci_libs="$_sci_libs $final_lib"
   done
fi

### Take care of arg 6 (the list of lib paths)

if test -n "$MPI_LIB_DIR_FLAG $_sci_lib"; then

   for path in $MPI_LIB_DIR_FLAG $_sci_lib; do

      # Make sure it doesn't have any thing but -L
      has_minus=`echo $path | sed 's/-.*//'`
      if test -z "$has_minus"; then
         has_minus_L=`echo $path | sed 's/-L.*//'`
         if test -n "$has_minus_L"; then
            # Has some other -?.
            if test "$verbose" = "yes"; then
              echo
              { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Only -L options are allowed in arg 6 of PIDX check (disregarding $path)." >&5
$as_echo "$as_me: WARNING: Only -L options are allowed in arg 6 of PIDX check (disregarding $path)." >&2;}
            fi
            continue
         fi
      fi

      # Remove the '-L' (if it has one).
      the_path=`echo $path | sed 's/-L//'`
      if test -d "$the_path"; then
         if test "$IS_STATIC_BUILD" = "yes"; then
            _sci_lib_path="$_sci_lib_path -L$the_path"
         else
            _sci_lib_path="$_sci_lib_path $LDRUN_PREFIX$the_path -L$the_path"
         fi
      else
         echo
         as_fn_error $? "The given path \"$the_path\" is not a valid directory..." "$LINENO" 5
      fi
   done
fi

if test "$_sci_optional" = "specific"; then
  # If 'specific' then only one lib is allowed:
  ### Determine if there is only one item in $MPI_LIB_DIR_FLAG $_sci_lib (I don't know of a better way to do this.)
  __sci_pass=false
  __sci_first_time=true

  # Must have the "" for the SGI sh.
  for value in "" $MPI_LIB_DIR_FLAG $_sci_lib; do
    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test $__sci_pass != "true"; then
       as_fn_error $? "For specific SCI-TRY-LINK test for PIDX only one library path may be specified for arg 6 (you had: $MPI_LIB_DIR_FLAG $_sci_lib).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
  fi
  # and only one include path:
  ### Determine if there is only one item in $_sci_inc $INC_MPI_H
  __sci_pass=false
  __sci_first_time=true
  for value in "" $_sci_inc $INC_MPI_H; do
    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test -n "$_sci_inc $INC_MPI_H" && test $__sci_pass != "true"; then
       as_fn_error $? "For specific SCI-TRY-LINK test for PIDX only one include path may be specified for arg 4 (you had: $_sci_inc $INC_MPI_H).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
  fi
