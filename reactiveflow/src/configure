      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test -n "$_sci_inc" && test $__sci_pass != "true"; then
       as_fn_error $? "For specific SCI-TRY-LINK test for PETSC only one include path may be specified for arg 4 (you had: $_sci_inc).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
  fi
fi

