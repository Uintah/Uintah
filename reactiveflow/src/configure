  if test $__sci_pass != "true"; then
       as_fn_error $? "For specific SCI-TRY-LINK test for VISUS only one library path may be specified for arg 6 (you had: $_sci_lib).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." "$LINENO" 5
