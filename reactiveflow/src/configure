     as_fn_error $? "Specifically requested VISUS include file '$_sci_inc/$i' was not found" "$LINENO" 5
    fi
  done
  # Make sure the exact libraries were found
  for i in "" ttmath visusidx curl libz FreeImage tinyxml visusdb visusscenegraph visuskernel curl visusdataflow $VISUS_OSX; do
    if test -z "$i"; then
       continue
    fi
    has_minus=`echo $i | sed 's/-.*//'`
    if test -z "$has_minus"; then
       i=`echo $i | sed 's/-l//g'`
    fi
    if test ! -e $_sci_lib/lib$i.so && test ! -e $_sci_lib/lib$i.a && test ! -e $_sci_lib/$i.lib && test ! -e $_sci_lib/lib$i.dylib; then
     as_fn_error $? "Specifically requested VISUS library file '$_sci_lib/lib$i' (.so, .a, or .dylib) was not found" "$LINENO" 5
    fi
  done
fi


