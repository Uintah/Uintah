    if test ! -e $_sci_lib/lib$i.so && test ! -e $_sci_lib/lib$i.a && test ! -e $_sci_lib/$i.lib && test ! -e $_sci_lib/lib$i.dylib; then
     as_fn_error $? "Specifically requested Z library file '$_sci_lib/lib$i' (.so, .a, or .dylib) was not found" "$LINENO" 5
    fi
  done
fi


