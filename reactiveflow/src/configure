     gperf_lib_list=tcmalloc_and_profiler
  elif test "$IS_STATIC_BUILD" = "yes"; then
     gperf_lib_list="tcmalloc profiler unwind"
  else
     gperf_lib_list="tcmalloc profiler"
  fi

