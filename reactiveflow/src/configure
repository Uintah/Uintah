         has_minus_L=`echo $path | sed 's/-L.*//'`
         if test -n "$has_minus_L"; then
            # Has some other -?.
            if test "$verbose" = "yes"; then
              echo
              { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Only -L options are allowed in arg 6 of VISUS check (disregarding $path)." >&5
$as_echo "$as_me: WARNING: Only -L options are allowed in arg 6 of VISUS check (disregarding $path)." >&2;}
            fi
            continue
         fi
      fi

      # Remove the '-L' (if it has one).
      the_path=`echo $path | sed 's/-L//'`
      if test -d "$the_path"; then
         if test "$IS_STATIC_BUILD" = "yes"; then
            _sci_lib_path="$_sci_lib_path -L$the_path"
         else
            _sci_lib_path="$_sci_lib_path $LDRUN_PREFIX$the_path -L$the_path"
         fi
      else
         echo
         as_fn_error $? "The given path \"$the_path\" is not a valid directory..." "$LINENO" 5
      fi
   done
fi

