# Set GUI variables for the SCIRun->FieldsGeometry->TransformMesh Module
set $m4-function {double phi, r, uu, vv;

const double a = p.x();
const double b = p.y();
const double c = p.z();
  
if (a > -b)
{
  if (a > b)
  {
    r = a;
    phi = (M_PI/4.0) * b/a;
  }
  else
  {
    r = b;
    phi = (M_PI/4.0) * (2 - a/b);
  }
}
else
{
  if (a < b)
  {
    r = -a;
    phi = (M_PI/4.0) * (4 + (b/a));
  }
  else
  {
    r = -b;
    if (b != 0)
    {
      phi = (M_PI/4.0) * (6 - a/b);
    }
    else
    {
      phi = 0;
    }
  }
}

uu = r * cos(phi);
vv = r * sin(phi);
  
result = Point(uu, vv, c);



}

