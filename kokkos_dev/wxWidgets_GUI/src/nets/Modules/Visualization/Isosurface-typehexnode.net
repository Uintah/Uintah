set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {16 9 574 548}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 48 9]

# Create a SCIRun->Visualization->Isosurface Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 165 334]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 379 120]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 334 251]

# Create a SCIRun->FieldsData->TransformData Module
set m5 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 55 165]

# Create a SCIRun->Visualization->ShowField Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 16 418]

# Create a SCIRun->Visualization->ShowField Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 370 419]

# Create a SCIRun->Render->Viewer Module
set m8 [addModuleAtPosition "SCIRun" "Render" "Viewer" 287 504]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m5 0]
set c2 [addConnection $m5 0 $m2 0]
set c3 [addConnection $m5 0 $m6 0]
set c4 [addConnection $m3 0 $m4 0]
set c5 [addConnection $m2 0 $m7 0]
set c6 [addConnection $m6 0 $m8 0]
set c7 [addConnection $m5 0 $m4 1]
set c8 [addConnection $m2 1 $m8 1]
set c9 [addConnection $m4 0 $m2 1]
set c10 [addConnection $m4 0 $m7 1]
set c11 [addConnection $m7 0 $m8 2]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module

# Setup GUI for the SCIRun->Visualization->Isosurface Module
setGlobal $m2-isoval {1.4333}
setGlobal $m2-isoval-min {0.013333333333333327}
setGlobal $m2-isoval-max {3}
setGlobal $m2-isoval-list {0.8 1.4}
setGlobal $m2-active-isoval-selection-tab {2}
setGlobal $m2-active_tab {}

# Setup GUI for the SCIRun->Visualization->GenStandardColorMaps Module

# Setup GUI for the SCIRun->Visualization->RescaleColorMap Module
setGlobal $m4-min {0.013333333333333327}
setGlobal $m4-max {3}

# Setup GUI for the SCIRun->FieldsData->TransformData Module
setGlobal $m5-function {result = x*x + y*y + z*z;
}

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m6-nodes-on {0}
setGlobal $m6-edges-transparency {1}
setGlobal $m6-faces-on {0}
setGlobal $m6-normalize-vectors {}
setGlobal $m6-has_scalar_data {1}
setGlobal $m6-def-color-r {0.50}
setGlobal $m6-def-color-g {0.50}
setGlobal $m6-def-color-b {0.50}
setGlobal $m6-def-color-a {0.80}
setGlobal $m6-active_tab {Faces}
setGlobal $m6-node_scale {0.0300}
setGlobal $m6-edge_scale {0.0150}
setGlobal $m6-scalars_scale {0.300}
setGlobal $m6-show_progress {}

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m7-nodes-on {0}
setGlobal $m7-faces-on {0}
setGlobal $m7-normalize-vectors {}
setGlobal $m7-has_scalar_data {1}
