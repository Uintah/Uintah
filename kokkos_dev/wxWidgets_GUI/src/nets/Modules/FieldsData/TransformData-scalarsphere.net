set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {120 26 523 470}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 120 26]

# Create a SCIRun->Visualization->ShowField Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 136 314]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 138 426]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 328 112]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 275 225]

# Create a SCIRun->FieldsData->TransformData Module
set m6 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 148 115]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m6 0]
set c2 [addConnection $m6 0 $m2 0]
set c3 [addConnection $m4 0 $m5 0]
set c4 [addConnection $m2 0 $m3 0]
set c5 [addConnection $m6 0 $m5 1]
set c6 [addConnection $m5 0 $m2 1]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m2-nodes-on {0}
setGlobal $m2-edges-transparency {1}
setGlobal $m2-faces-on {0}
setGlobal $m2-normalize-vectors {}
setGlobal $m2-has_scalar_data {1}
setGlobal $m2-node_display_type {Spheres}
setGlobal $m2-active_tab {Edges}
setGlobal $m2-node_scale {0.0300}
setGlobal $m2-edge_scale {0.0150}
setGlobal $m2-show_progress {}

