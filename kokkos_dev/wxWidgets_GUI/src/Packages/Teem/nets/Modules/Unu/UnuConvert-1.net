# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {65 9 597 374}
set creationDate {Fri Dec 02 2005}
set creationTime {14:01:58}
set runDate {}
set runTime {}
set notes {}
set geometry {800x800+0+0}

# Create a Teem->UnuAtoM->UnuConvert Module
set m1 [addModuleAtPosition "Teem" "UnuAtoM" "UnuConvert" 65 158]

# Create a SCIRun->DataIO->FieldReader Module
set m2 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 65 9]

# Create a Teem->Converters->FieldToNrrd Module
set m3 [addModuleAtPosition "Teem" "Converters" "FieldToNrrd" 65 84]

# Create a SCIRun->FieldsOther->FieldInfo Module
set m4 [addModuleAtPosition "SCIRun" "FieldsOther" "FieldInfo" 432 317]

# Create a Teem->Converters->NrrdToField Module
set m5 [addModuleAtPosition "Teem" "Converters" "NrrdToField" 65 236]

# Create a Teem->NrrdData->NrrdInfo Module
set m6 [addModuleAtPosition "Teem" "NrrdData" "NrrdInfo" 401 238]

# Create the Connections between Modules
set c1 [addConnection $m2 0 $m3 0]
set c2 [addConnection $m3 2 $m1 0]
set c3 [addConnection $m5 0 $m4 0]
set c4 [addConnection $m1 0 $m6 0]
set c5 [addConnection $m1 0 $m5 2]
set c6 [addConnection $m2 0 $m5 3]

# Set GUI variables for the Teem->UnuAtoM->UnuConvert Module
set $m1-type {10}
set $m1-ui_geometry {314x233+20+117}

# Open the Teem->UnuAtoM->UnuConvert UI
$m1 initialize_ui

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m2-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Set GUI variables for the Teem->Converters->FieldToNrrd Module
set $m3-label {sphere-voltages}

# Set GUI variables for the SCIRun->FieldsOther->FieldInfo Module
set $m4-ui_geometry {682x273+10+71}

# Open the SCIRun->FieldsOther->FieldInfo UI
$m4 initialize_ui

# Set GUI variables for the Teem->Converters->NrrdToField Module
set $m5-datasets {{Points : (none)} {Connections : (none)} {Data : Unknown} {Original Field : Unknown} }

# Set GUI variables for the Teem->NrrdData->NrrdInfo Module
set $m6-name {Unknown}
set $m6-type {double}
set $m6-dimension {1}


::netedit scheduleok
