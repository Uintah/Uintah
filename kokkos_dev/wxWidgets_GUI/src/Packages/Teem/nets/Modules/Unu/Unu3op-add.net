# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {28 4 641 505}
set creationDate {Fri Dec 02 2005}
set creationTime {14:34:25}
set runDate {}
set runTime {}
set notes {}
set geometry {800x800+0+0}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 57 4]

# Create a SCIRun->Render->Viewer Module
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 184 461]

# Create a Teem->Converters->FieldToNrrd Module
set m3 [addModuleAtPosition "Teem" "Converters" "FieldToNrrd" 99 73]

# Create a Teem->Converters->NrrdToField Module
set m4 [addModuleAtPosition "Teem" "Converters" "NrrdToField" 207 228]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 201 395]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 446 236]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 309 311]

# Create a SCIRun->FieldsOther->FieldInfo Module
set m8 [addModuleAtPosition "SCIRun" "FieldsOther" "FieldInfo" 28 322]

# Create a Teem->UnuAtoM->Unu3op Module
set m9 [addModuleAtPosition "Teem" "UnuAtoM" "Unu3op" 143 147]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m3 0]
set c2 [addConnection $m6 0 $m7 0]
