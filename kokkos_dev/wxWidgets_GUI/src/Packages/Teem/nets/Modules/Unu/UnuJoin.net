# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {19 6 474 502}
set creationDate {Fri Dec 02 2005}
set creationTime {14:00:38}
set runDate {}
set runTime {}
set notes {}
set geometry {800x800+0+0}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 156 6]

# Create a SCIRun->FieldsData->Gradient Module
set m2 [addModuleAtPosition "SCIRun" "FieldsData" "Gradient" 19 99]

# Create a Teem->Converters->FieldToNrrd Module
set m3 [addModuleAtPosition "Teem" "Converters" "FieldToNrrd" 122 275]

# Create a Teem->Converters->FieldToNrrd Module
set m4 [addModuleAtPosition "Teem" "Converters" "FieldToNrrd" 209 110]

# Create a Teem->UnuAtoM->UnuJoin Module
set m5 [addModuleAtPosition "Teem" "UnuAtoM" "UnuJoin" 309 355]

# Create a SCIRun->FieldsData->DirectMapping Module
set m6 [addModuleAtPosition "SCIRun" "FieldsData" "DirectMapping" 76 193]

# Create a Teem->NrrdData->NrrdInfo Module
set m7 [addModuleAtPosition "Teem" "NrrdData" "NrrdInfo" 277 445]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m4 0]
set c3 [addConnection $m6 0 $m3 0]
set c4 [addConnection $m2 0 $m6 0]
set c5 [addConnection $m3 2 $m5 0]
set c6 [addConnection $m5 0 $m7 0]
set c7 [addConnection $m1 0 $m6 1]
set c8 [addConnection $m4 0 $m5 1]

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Set GUI variables for the Teem->Converters->FieldToNrrd Module
set $m3-label {gradients}

# Set GUI variables for the Teem->Converters->FieldToNrrd Module
set $m4-label {potentials}

# Set GUI variables for the Teem->UnuAtoM->UnuJoin Module
set $m5-dim {2}

# Set GUI variables for the Teem->NrrdData->NrrdInfo Module
set $m7-name {Unknown}
set $m7-type {double}
set $m7-dimension {2}
set $m7-origin {---}
set $m7-ui_geometry {447x507+10+71}

# Open the Teem->NrrdData->NrrdInfo UI
$m7 initialize_ui


::netedit scheduleok
