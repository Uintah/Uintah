    </BoundaryConditions>
      <!--__________________________________-->
       <Level>
           <Box label = "1">
              <lower>        [-3,0.0,0]       </lower>
              <upper>        [ 3,6.0,0.1]     </upper>         
              <extraCells>   [1,1,1]          </extraCells>
              <patches>      [5,1,1]          </patches>    
           </Box>
           <spacing>         [0.1,0.1,0.1]    </spacing>
        </Level>
<!--
        <Level>
            <Box label = "2"> 
              <lower>        [-2,0,0]         </lower>
              <upper>        [ 2,4,0.1]       </upper>         
              <extraCells>   [1,1,1]          </extraCells>
              <patches>      [1,1,1]          </patches>
           </Box>
           <spacing>         [0.05,0.05,0.1]  </spacing>
       </Level>
-->
    </Grid>
    <!--____________________________________________________________________-->
    <!--    A M R   G R I D                                                 -->
    <!--____________________________________________________________________-->  

    <Regridder>
      <max_levels>2</max_levels>
      <cell_refinement_ratio>    [[2,2,1]]  </cell_refinement_ratio>
      <lattice_refinement_ratio> [[1,1,1]]  </lattice_refinement_ratio>
      <cell_creation_dilation>   [2,2,0]   </cell_creation_dilation>   
      <cell_deletion_dilation>   [1,1,0]   </cell_deletion_dilation>   
      <min_boundary_cells>       [1,1,0]   </min_boundary_cells>
    </Regridder>   
 
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>scalarJet.uda</filebase>
      <!--<outputInterval>0.1</outputInterval> -->
      <outputTimestepInterval> 1</outputTimestepInterval>
      <save label = "delP_Dilatate"/>
      <save label = "press_CC"/>
      <save label = "press_equil_CC"/>
      <save label = "vel_CC"/>
      <save label = "rho_CC"/>
      <save label = "sp_vol_CC"/>
      <save label = "temp_CC"/>
      <save label = "scalar-f"/>
      <save label = "CenterOfMassVelocity"/>
      <save label = "TotalMass"/>
      <save label = "TotalIntEng"/>
      <save label = "sum_scalar_f"/>
      <save label = "mag_grad_scalar-f"/>
      <save label = "refineFlag"/>
      
      <compression>gzip</compression>
      <checkpoint cycle = "2" interval = "0.01"/>
      <outputInitTimestep/>
   </DataArchiver>

