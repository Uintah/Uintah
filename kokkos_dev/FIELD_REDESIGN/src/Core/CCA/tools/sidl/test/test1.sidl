
package Solver {
    interface Vector {
	Vector clone();
	double dot(in Vector y);
	void axpy(in double a, in Vector y);
	void initialize(in array<double, 1> data);
    };

    interface Matrix {
	void apply(out Vector Ax, in Vector x);
    };

    interface RowAddressable {
    };

    class SparseMatrix implements Matrix, RowAddressable {
	void apply(out Vector Ax, in Vector x);
    };

    class Krylov {
    };

    interface AbstractSolver {
    };
    class CG extends Krylov implements AbstractSolver {
	void solve(in Matrix A, inout Vector x, in Vector b);
    };
};
