   { (exit 1); exit 1; }; }
  fi
  # and only one include path:
  ### Determine if there is only one item in /usr/sci/local/include
  __sci_pass=false
  __sci_first_time=true
  for value in "" /usr/sci/local/include; do
    if test "$value" = ""; then
