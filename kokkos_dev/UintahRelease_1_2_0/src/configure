      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test -n "$includes" && test $__sci_pass != "true"; then
       { { echo "$as_me:$LINENO: error: For specific SCI-TRY-LINK test for XML2 only one include path may be specified for arg 4 (you had: $includes).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." >&5
echo "$as_me: error: For specific SCI-TRY-LINK test for XML2 only one include path may be specified for arg 4 (you had: $includes).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." >&2;}
