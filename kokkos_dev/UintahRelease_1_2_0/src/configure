##      F_LIB_DIR_FLAG   => all the -L's
##      F_LIB_FLAG       => all the -l's
##      HAVE_F           => yes or no
##      INC_F_H          => all the -I's
##      HAVE_F_H         => yes or no

if test 9 != 9; then
