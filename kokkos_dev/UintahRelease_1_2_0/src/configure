  for i in "" petsc petscvec petscmat petscdm petscsnes petscts petscksp; do
    if test -z "$i"; then
       continue
    fi
    has_minus=`echo $i | sed 's/-.*//'`
    if test -z "$has_minus"; then
       i=`echo $i | sed 's/-l//g'`
    fi
    if test ! -e $_sci_lib/lib$i.so && test ! -e $_sci_lib/lib$i.a && test ! -e $_sci_lib/$i.lib; then
     { { echo "$as_me:$LINENO: error: Specifically requested PETSC_UNI library file '$_sci_lib/lib$i' (.so or .a) was not found" >&5
echo "$as_me: error: Specifically requested PETSC_UNI library file '$_sci_lib/lib$i' (.so or .a) was not found" >&2;}
   { (exit 1); exit 1; }; }
    fi
  done
fi


