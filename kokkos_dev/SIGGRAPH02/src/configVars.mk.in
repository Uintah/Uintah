	@echo "ACTUALLY COMPILING:"
	$(F77) $(CFLAGS) $(INCLUDES) -c $< -o $@
else
	$(ECHO) Unknown F77_DEPEND_MODE: $(F77_DEPEND_MODE)
	exit 1
endif
endif
endif
endif

.c.o: $<
ifeq ($(CC_DEPEND_MODE),normal)
	$(CC) $(CFLAGS) $(INCLUDES) $(CC_DEPEND_REGEN) -c $< -o $@
else
ifeq ($(CC_DEPEND_MODE),modify)
