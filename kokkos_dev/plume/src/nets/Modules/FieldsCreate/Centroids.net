set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {22 27 411 396}

# Create a SCIRun->FieldsCreate->Centroids Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "Centroids" 202 118]

# Create a SCIRun->Visualization->ShowField Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 22 226]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 93 352]

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m4 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 52 27]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 246 217]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m5 0]
set c2 [addConnection $m4 0 $m1 0]
set c3 [addConnection $m4 0 $m2 0]
set c4 [addConnection $m2 0 $m3 0]
set c5 [addConnection $m5 0 $m3 1]

# Setup GUI for the SCIRun->FieldsCreate->Centroids Module

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m2-nodes-on {0}
setGlobal $m2-faces-on {0}
setGlobal $m2-normalize-vectors {}
setGlobal $m2-has_scalar_data {1}
setGlobal $m2-edge_display_type {Cylinders}
setGlobal $m2-active_tab {Edges}
setGlobal $m2-node_scale {0.0100}
setGlobal $m2-edge_scale {0.00590}
setGlobal $m2-scalars_scale {0.300}
setGlobal $m2-show_progress {}
setGlobal $m2-field-name {Original Plane}

