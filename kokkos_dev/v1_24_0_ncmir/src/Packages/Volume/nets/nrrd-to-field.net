# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {28 20 364 404}
set creationDate {Tue Jun 29 2004}
set creationTime {10:21:46}
set runDate {}
set runTime {}
set notes {}

# Create a SCIRun->FieldsData->NodeGradient Module
set m1 [addModuleAtPosition "SCIRun" "FieldsData" "NodeGradient" 199 235]

# Create a SCIRun->DataIO->FieldWriter Module
set m2 [addModuleAtPosition "SCIRun" "DataIO" "FieldWriter" 199 347]

# Create a Teem->DataIO->NrrdReader Module
set m3 [addModuleAtPosition "Teem" "DataIO" "NrrdReader" 143 20]

# Create a Teem->DataIO->NrrdToField Module
set m4 [addModuleAtPosition "Teem" "DataIO" "NrrdToField" 107 124]

# Create a SCIRun->DataIO->FieldWriter Module
set m5 [addModuleAtPosition "SCIRun" "DataIO" "FieldWriter" 28 347]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m4 0 $m5 0]
set c3 [addConnection $m4 0 $m1 0]
set c4 [addConnection $m3 0 $m4 2]

# Set GUI variables for the SCIRun->DataIO->FieldWriter Module
set $m2-filename "$DATADIR/volume/$DATASET-gradient.fld"
set $m2-types {{{{SCIRun Field File} {.fld} } {{SCIRun Field Any} {.*} } {{TextCurveField} {{.pts} {.edge}} } {{TextHexVolField} {{.pts} {.hex}} } {{TextPointCloudField} {.pts} } {{TextPointCloudString} {.pcs.txt} } {{TextQuadSurfField} {{.pts} {.quad}} } {{TextStructCurveField} {.pts} } {{TextStructHexVolField} {.pts} } {{TextStructQuadSurfField} {.pts} } {{TextTetVolField} {{.pts} {.tet}} } {{TextTriSurfField} {{.pts} {.fac} {.tri}} } }}
set $m2-exporttype {SCIRun Field File (*.fld)}

# Set GUI variables for the Teem->DataIO->NrrdReader Module
set $m3-filename "$DATADIR/volume/$DATASET.nhdr"

# Set GUI variables for the Teem->DataIO->NrrdToField Module
set $m4-struct-unstruct {PointCloud}
set $m4-datasets {{Points : (none)} {Connections : (none)} {Data : Unknown} {Original Field : (none)} }

# Set GUI variables for the SCIRun->DataIO->FieldWriter Module
set $m5-filename "$DATADIR/volume/$DATASET-value.fld"
set $m5-types {{{{SCIRun Field File} {.fld} } {{SCIRun Field Any} {.*} } {{TextCurveField} {{.pts} {.edge}} } {{TextHexVolField} {{.pts} {.hex}} } {{TextPointCloudField} {.pts} } {{TextPointCloudString} {.pcs.txt} } {{TextQuadSurfField} {{.pts} {.quad}} } {{TextStructCurveField} {.pts} } {{TextStructHexVolField} {.pts} } {{TextStructQuadSurfField} {.pts} } {{TextTetVolField} {{.pts} {.tet}} } {{TextTriSurfField} {{.pts} {.fac} {.tri}} } }}
set $m5-exporttype {SCIRun Field File (*.fld)}


::netedit scheduleok
