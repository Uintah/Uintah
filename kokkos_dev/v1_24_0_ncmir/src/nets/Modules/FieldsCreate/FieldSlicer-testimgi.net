set c3 [addConnection $m1 0 $m6 0]
set c4 [addConnection $m1 0 $m4 0]
set c5 [addConnection $m8 0 $m1 0]
set c6 [addConnection $m3 0 $m2 0]
set c7 [addConnection $m7 0 $m5 0]
set c8 [addConnection $m1 0 $m2 1]
