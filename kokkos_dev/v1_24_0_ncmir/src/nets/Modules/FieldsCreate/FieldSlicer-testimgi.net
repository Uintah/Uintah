# Create a SCIRun->FieldsGeometry->ChangeFieldBounds Module
set m8 [addModuleAtPosition "SCIRun" "FieldsGeometry" "ChangeFieldBounds" 67 90]

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m9 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 59 10]

# Create the Connections between Modules
set c1 [addConnection $m6 0 $m7 0]
