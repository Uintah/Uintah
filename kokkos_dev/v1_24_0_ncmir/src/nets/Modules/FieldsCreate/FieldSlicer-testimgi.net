set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {14 10 515 574}

# Create a SCIRun->FieldsData->TransformData Module
set m1 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 58 171]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 285 260]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 320 157]

# Create a SCIRun->Visualization->ShowField Module
