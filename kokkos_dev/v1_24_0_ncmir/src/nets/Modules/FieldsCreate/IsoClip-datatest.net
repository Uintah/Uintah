set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {45 12 526 541}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 53 12]

# Create a SCIRun->FieldsGeometry->HexToTet Module
set m2 [addModuleAtPosition "SCIRun" "FieldsGeometry" "HexToTet" 53 92]

# Create a SCIRun->FieldsData->TransformData Module
set m3 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 53 169]

# Create a SCIRun->FieldsCreate->IsoClip Module
set m4 [addModuleAtPosition "SCIRun" "FieldsCreate" "IsoClip" 45 250]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 208 413]

# Create a SCIRun->Render->Viewer Module
set m6 [addModuleAtPosition "SCIRun" "Render" "Viewer" 208 497]

# Create a SCIRun->FieldsCreate->ClipByFunction Module
set m7 [addModuleAtPosition "SCIRun" "FieldsCreate" "ClipByFunction" 48 330]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m8 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 331 136]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m9 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 239 254]

# Create the Connections between Modules
set c1 [addConnection $m7 0 $m5 0]
set c2 [addConnection $m4 0 $m7 0]
set c3 [addConnection $m1 0 $m2 0]
set c4 [addConnection $m3 0 $m4 0]
set c5 [addConnection $m2 0 $m3 0]
set c6 [addConnection $m8 0 $m9 0]
set c7 [addConnection $m5 0 $m6 0]
set c8 [addConnection $m3 0 $m9 1]
set c9 [addConnection $m9 0 $m5 1]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module
setGlobal $m1-sizex {9}
setGlobal $m1-sizey {9}
setGlobal $m1-sizez {9}

# Setup GUI for the SCIRun->FieldsGeometry->HexToTet Module

# Setup GUI for the SCIRun->FieldsData->TransformData Module
setGlobal $m3-function {result = x*x + y*y + z*z;



}

# Setup GUI for the SCIRun->FieldsCreate->IsoClip Module
setGlobal $m4-isoval {1.2}
$m4 initialize_ui

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m5-nodes-on {0}
