set results [sourceSettingsFile]

if { $results == "failed" } {

    ::netedit scheduleok
    return 

} else {

    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "SCIRun" "DataIO" "MatrixReader" 16 9]
set m1 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 221 10]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowMatrix" 157 125]
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 161 228]

addConnection $m1 0 $m2 0
addConnection $m0 0 $m2 1
addConnection $m2 0 $m3 0

set $m0-notes {}
set $m0-show_status {1}
set $m0-filename $DATADIR/$DATASET/$DATASET-condparams.mat
set $m1-notes {}
set $m1-show_status {1}
set $m1-tcl_status {Calling GenStandardColorMaps!}
set $m1-positionList {}
set $m1-nodeList {}
set $m1-width {1}
set $m1-height {1}
set $m1-mapType {3}
set $m1-minRes {12}
set $m1-resolution {255}
set $m1-realres {255}
set $m1-gamma {0}
set $m2-notes {}
set $m2-show_status {1}
set $m2-color_by_val {1}
set $m2-grid_x {10}
set $m2-grid_y {10}
set $m2-grid_z {10}
set $m2-xpos {-1.000}
set $m2-ypos {-1.0000}
set $m2-xscale {2.00}
set $m2-xscale {2.00}
set $m2-yscale {2.0}
set $m2-displaymode {2D}
set $m2-gmode {1}
set $m2-showtext {1}
set $m2-row_begin {0}
set $m2-row_end {5}
set $m2-col_begin {0}
set $m2-col_end {3}
set $m2-xgap {}
set $m2-ygap {}
set $m2-data_face_centered {}
set $m2-colormapmode {0}
set $m3-notes {}
set $m3-show_status {1}
$m3 ui
set $m3-ViewWindow_0-pos {}
set $m3-ViewWindow_0-caxes {0}
set $m3-ViewWindow_0-raxes {1}
set $m3-ViewWindow_0-iaxes {}
set $m3-ViewWindow_0-have_collab_vis {0}
