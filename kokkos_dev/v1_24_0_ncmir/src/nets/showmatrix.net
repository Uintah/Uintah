set $m3-ViewWindow_0-view-lookat-y {              0.5}
set $m3-ViewWindow_0-view-lookat-z {             -0.5}
set $m3-ViewWindow_0-view-up-x {        -0.136522}
set $m3-ViewWindow_0-view-up-y {         0.983823}
set $m3-ViewWindow_0-view-up-z {        -0.115995}
