<?xml version="1.0" encoding="iso-8859-1"?>


<!-- @version: -->

<Uintah_specification>

   <Meta>
       <title>Advection test</title>
   </Meta>    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            0.001      </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           2.4e-06         </delt_min>
       <delt_max>           2.4e-06         </delt_max>
       <delt_init>          2.4e-06      </delt_init>
       <timestep_multiplier>1.0         </timestep_multiplier>
   </Time>
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face side = "x-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "z+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0,0,0]    </lower>
              <upper>        [2,1,1]    </upper>
                <extraCells>[1,1,1]         </extraCells>
              <patches>    [1,1,1]    </patches>
           </Box>
           <spacing>[0.02,1,1] </spacing>
       </Level>
    </Grid>    
    <!--____________________________________________________________________-->
    <!--      O  U  P  U  T     V  A  R  I  A  B  L  E  S                   -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>riemanSm.uda</filebase>
      <outputInterval>.0001</outputInterval>
      <save label="vel_CC"/>
      <save label="press_CC"/>
      <save label="rho_CC"/>
      <save label = "turb_viscosity_CC"/>
      <checkpoint interval="0.001" cycle="2"/>"
   </DataArchiver>

   <Debug>
       <dbg_timeStart>       0.00   </dbg_timeStart>
       <dbg_timeStop>        0.001  </dbg_timeStop>
       <dbg_outputInterval>  1e-4   </dbg_outputInterval>
       <dbg_Matls>           [0,1]   </dbg_Matls>

       <dbg_GnuPlot>true</dbg_GnuPlot>
       <dbg_BeginIndex>     [-1,0,0] </dbg_BeginIndex>
       <dbg_EndIndex>       [101,1,1] </dbg_EndIndex>
       <dbg_SigFigs>        10      </dbg_SigFigs>

	<debug label = "switchDebug_advance_advect"/>
	<debug label = "switchDebug_explicit_press"/>
    -->
   <!--<debug label = "switchDebug_Initialize"/>             
       <debug label = "switchDebug_equil_press"/>
       <debug label = "switchDebug_vel_FC"/>                   
       <debug label = "switchDebug_explicit_press"/>         
       <debug label = "switchDebug_PressFC"/>                
       <debug label = "switchDebug_LagrangianValues"/>        
       <debug label = "switchDebug_Source_Sink"/>               
       <debug label = "switchDebug_advance_advect"/>   
    -->        
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.5</cfl>
       <ICE>
        <max_iteration_equilibration> 100 </max_iteration_equilibration>
        <advection type = "SecondOrder" />
        <solution technique = "EqForm" />
	 <turbulence model = "Smagorinsky">	    
		<model_constant>0.1</model_constant>		
		<filter_width>1.0</filter_width>
		<turb_Pr>0.4</turb_Pr>				
	 </turbulence>
      </ICE>        
    </CFD>
    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>

    <!--____________________________________________________________________-->
    <!--     Material Properties and Initial Conditions                     -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <ICE>
         <material>
            <burn type = "null" />
           <EOS type = "ideal_gas">                     </EOS>
           <dynamic_viscosity>      1.0                 </dynamic_viscosity>
           <thermal_conductivity>   0.0                 </thermal_conductivity>
           <specific_heat>          717.5               </specific_heat>
           <speed_of_sound>         374.17              </speed_of_sound>
           <gamma>                  1.4                  </gamma>
           <geom_object>
             <difference>
                <box label="wholegrid">
                    <min>           [-1.0, -1.0, -1.0 ]   </min>
                    <max>           [ 3.0,  2.0, 2.0 ]   </max>
                </box> 
                
                <box label="rightpartition">          
                    <min>           [ 1.0, -1.0, -1.0 ]   </min>
                    <max>           [ 3.0, 2.0,  2.0 ]   </max>
                </box>
              </difference>
               <res>                  [1,1,1]                 </res>
               <velocity>       [0.0,0.0,0.0]           </velocity>
               <temperature>    300.0                   </temperature>
               <density>        1.1768292682926831000   </density>
               <pressure>       101325.0                </pressure>
           </geom_object>
           
           
           <geom_object>
                <box label="rightpartition">
                    <min>[ 1.0, -1.0, -1.0 ]</min>
                    <max>[ 3.0, 2.0, 2.0 ]</max>
                </box>
                <res>           [1,1,1]                 </res>
               <velocity>       [0.0,0.0,0.0]           </velocity>
               <temperature>    300.0                   </temperature>
               <density>        .11768292682926831000   </density>
               <pressure>       10132.50                </pressure>
           </geom_object>
         </material>
      </ICE>       
        <exchange_properties> 
         <exchange_coefficients>
            <momentum>           []       </momentum>
            <heat>              []       </heat>
        </exchange_coefficients>
        </exchange_properties> 
    </MaterialProperties>  
                     
</Uintah_specification>
