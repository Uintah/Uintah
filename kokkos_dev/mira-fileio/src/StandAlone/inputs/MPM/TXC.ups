<?xml version="1.0" encoding="iso-8859-1"?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->

<!-- @version: Updated 7/31/00-->

<Uintah_specification>

   <Meta>
       <title>TXC</title>
   </Meta>

   <SimulationComponent type="mpm" />

   <Time>
       <maxTime>2.0</maxTime>
       <initTime>0.0</initTime>
       <delt_min>0.0000001</delt_min>
       <delt_max>0.01</delt_max>
       <timestep_multiplier>0.4</timestep_multiplier>
   </Time>
   <DataArchiver>
       <filebase>TXC.uda</filebase>
       <outputInterval>.05</outputInterval>
       <save label="p.x"/>
       <save label="p.velocity"/>
       <save label="p.volume"/>
       <save label="p.stress"/>
       <save label="p.particleID"/>
       <save label="g.mass"/>
       <save label="g.stressFS"/>
       <save label="p.loadCurveID"/>
       <save label="p.externalforce"/>
   </DataArchiver>

    <MPM>
       <time_integrator>explicit</time_integrator>
       <use_load_curves> true </use_load_curves>
       <interpolator>gimp</interpolator>
    </MPM>

    <PhysicalConstants>
       <gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
       <MPM>
           <material>
              <density>1000.0</density>
                <constitutive_model type="comp_neo_hook">
                    <bulk_modulus> 60000000.0</bulk_modulus>
                    <shear_modulus> 6000000.0</shear_modulus>
                </constitutive_model>
              <thermal_conductivity>1.0</thermal_conductivity>
              <specific_heat>5</specific_heat>
              <geom_object>
                  <box label="gp1">
                     <min>[ 0.0, 0.0,10.0]</min>
                     <max>[10.0,10.0,11.0]</max>
                  </box>
                  <res>[2,2,2]</res>
                  <velocity>[0.0,0.0,0]</velocity>
                  <temperature>12</temperature>
               </geom_object>
           </material>
           <material>
              <density>1000.0</density>
                <constitutive_model type="comp_neo_hook">
                    <bulk_modulus> 60000000.0</bulk_modulus>
                    <shear_modulus> 6000000.0</shear_modulus>
                </constitutive_model>
              <thermal_conductivity>1.0</thermal_conductivity>
              <specific_heat>5</specific_heat>
              <geom_object>
                  <box label="gp2">
                     <min>[ 0.0, 0.0, 0.0]</min>
                     <max>[10.0,10.0,10.0]</max>
                  </box>
                  <res>[2,2,2]</res>
                  <velocity>[0.0,0.0,0]</velocity>
                  <temperature>12</temperature>
               </geom_object>
           </material>

           <contact>
             <type>specified</type>
             <filename>TXC.txt</filename>
             <direction>[1,1,1]</direction>
           </contact>
       </MPM>
    </MaterialProperties>

   <PhysicalBC>
     <MPM>
       <pressure>
         <geom_object>
            <box label="x-face">
              <min>[0.0, 0.0, 0.0]</min>
              <max>[0.025,10.0,10.0]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>1</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5   </time>
             <load> 1.0e5 </load>
           </time_point>
           <time_point>
             <time> 5.0   </time>
             <load> 1.0e5 </load>
           </time_point>
         </load_curve>
       </pressure>

       <pressure>
         <geom_object>
            <box label="x+face">
              <min>[9.99, 0.0, 0.0]</min>
              <max>[10.0,10.0,10.0]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>2</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5   </time>
             <load> -1.0e5 </load>
           </time_point>
           <time_point>
             <time> 5.0   </time>
             <load> -1.0e5 </load>
           </time_point>
         </load_curve>
       </pressure>

       <pressure>
         <geom_object>
            <box label="y-face">
              <min>[0.0, 0.0,   0.0]</min>
              <max>[10.0,0.025,10.0]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>3</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5   </time>
             <load> 1.0e5 </load>
           </time_point>
           <time_point>
             <time> 5.0   </time>
             <load> 1.0e5 </load>
           </time_point>
         </load_curve>
       </pressure>

       <pressure>
         <geom_object>
            <box label="y+face">
              <min>[0.0,  9.99, 0.0]</min>
              <max>[10.0,10.0,  10.0]</max>
            </box>
         </geom_object>
         <load_curve>
           <id>4</id>
           <time_point>
             <time> 0 </time>
             <load> 0 </load>
           </time_point>
           <time_point>
             <time> 0.5   </time>
             <load> -1.0e5 </load>
           </time_point>
           <time_point>
             <time> 5.0   </time>
             <load> -1.0e5 </load>
           </time_point>
         </load_curve>
       </pressure>

