set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {42 12 613 555}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 79 111]

# Create a SCIRun->FieldsData->DirectInterpolate Module
set m2 [addModuleAtPosition "SCIRun" "FieldsData" "DirectInterpolate" 42 190]

# Create a SCIRun->DataIO->FieldReader Module
set m3 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 46 13]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 268 362]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 104 511]

# Create a SCIRun->Visualization->RescaleColorMap Module
