set c3 [addConnection $m3 0 $m2 0]
set c4 [addConnection $m9 0 $m10 0]
set c5 [addConnection $m11 0 $m4 0]
set c6 [addConnection $m2 0 $m11 0]
set c7 [addConnection $m2 0 $m8 0]
set c8 [addConnection $m7 0 $m6 0]
set c9 [addConnection $m8 0 $m5 0]
set c10 [addConnection $m3 0 $m6 1]
set c11 [addConnection $m1 0 $m2 1]
