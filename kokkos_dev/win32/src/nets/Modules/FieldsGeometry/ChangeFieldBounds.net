set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {45 69 521 447}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 140 69]

# Create a SCIRun->FieldsGeometry->ChangeFieldBounds Module
set m2 [addModuleAtPosition "SCIRun" "FieldsGeometry" "ChangeFieldBounds" 282 159]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 45 226]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 356 281]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 153 403]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m3 0]
set c3 [addConnection $m2 0 $m4 0]
set c4 [addConnection $m3 0 $m5 0]
set c5 [addConnection $m4 0 $m5 1]
set c6 [addConnection $m2 1 $m5 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->FieldsGeometry->ChangeFieldBounds Module
setGlobal $m2-outputcenterx {0.8655488232993986}
