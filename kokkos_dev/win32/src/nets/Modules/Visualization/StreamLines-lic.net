set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {45 12 749 542}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 174 12]

# Create a SCIRun->FieldsCreate->SampleField Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleField" 325 102]

# Create a SCIRun->FieldsData->Gradient Module
