set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {58 12 636 557}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 117 12]

# Create a SCIRun->FieldsData->Gradient Module
set m2 [addModuleAtPosition "SCIRun" "FieldsData" "Gradient" 345 234]

# Create a SCIRun->FieldsData->DirectInterpolate Module
set m3 [addModuleAtPosition "SCIRun" "FieldsData" "DirectInterpolate" 58 343]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 289 428]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 233 106]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 162 190]

# Create a SCIRun->Render->Viewer Module
set m7 [addModuleAtPosition "SCIRun" "Render" "Viewer" 474 513]

# Create a SCIRun->FieldsCreate->ClipByFunction Module
set m8 [addModuleAtPosition "SCIRun" "FieldsCreate" "ClipByFunction" 471 103]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m8 0]
set c2 [addConnection $m1 0 $m3 0]
set c3 [addConnection $m8 0 $m2 0]
set c4 [addConnection $m3 0 $m4 0]
set c5 [addConnection $m5 0 $m6 0]
set c6 [addConnection $m4 0 $m7 0]
set c7 [addConnection $m1 0 $m6 1]
set c8 [addConnection $m2 0 $m3 1]
set c9 [addConnection $m6 0 $m4 1]
set c10 [addConnection $m2 0 $m4 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->FieldsData->Gradient Module

# Setup GUI for the SCIRun->FieldsData->DirectInterpolate Module

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m4-nodes-on {0}
setGlobal $m4-edges-on {0}
setGlobal $m4-faces-on {0}
setGlobal $m4-vectors-on {1}
setGlobal $m4-normalize-vectors {1}
setGlobal $m4-has_vector_data {1}
setGlobal $m4-node_display_type {Spheres}
setGlobal $m4-data_display_type {Cones}
setGlobal $m4-active_tab {Vectors}
setGlobal $m4-node_scale {0.0300}
setGlobal $m4-edge_scale {0.0150}
setGlobal $m4-vectors_scale {0.121}
setGlobal $m4-show_progress {}
$m4 initialize_ui

# Setup GUI for the SCIRun->Visualization->GenStandardColorMaps Module
setGlobal $m5-resolution {255}
setGlobal $m5-realres {255}

# Setup GUI for the SCIRun->Visualization->RescaleColorMap Module
setGlobal $m6-min {-5.2531499999999998}
setGlobal $m6-max {4.3192599999999999}

