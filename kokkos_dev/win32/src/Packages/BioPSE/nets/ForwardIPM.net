# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {28 24 673 301}
set creationDate {Mon Apr 11 2005}
set creationTime {11:36:33}
set runDate {}
set runTime {}
set notes {}

# Create a BioPSE->NeuroFEM->ForwardIPM Module
set m1 [addModuleAtPosition "BioPSE" "NeuroFEM" "ForwardIPM" 239 244]

# Create a SCIRun->DataIO->FieldReader Module
set m2 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 28 24]

# Create a SCIRun->DataIO->FieldReader Module
set m3 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 290 32]

# Create a SCIRun->DataIO->FieldReader Module
set m4 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 508 32]

# Create a BioPSE->Forward->IndicesToTensors Module
set m5 [addModuleAtPosition "BioPSE" "Forward" "IndicesToTensors" 78 128]

# Create a SCIRun->FieldsData->ManageFieldData Module
set m6 [addModuleAtPosition "SCIRun" "FieldsData" "ManageFieldData" 500 151]

# Create a SCIRun->FieldsGeometry->ManageFieldMesh Module
set m7 [addModuleAtPosition "SCIRun" "FieldsGeometry" "ManageFieldMesh" 348 140]

# Create the Connections between Modules
set c1 [addConnection $m5 0 $m1 0]
set c2 [addConnection $m2 0 $m5 0]
set c3 [addConnection $m4 0 $m6 0]
set c4 [addConnection $m4 0 $m7 0]
set c5 [addConnection $m3 0 $m1 1]
set c6 [addConnection $m7 1 $m1 2]
set c7 [addConnection $m6 1 $m1 3]

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m2-filename "$DATADIR/$DATASET/$DATASET-mesh.tvt.fld"
set $m2-filetype {SCIRun Field File (*.fld)}

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m3-filename "$DATADIR/$DATASET/$DATASET-electrodes.pcv.fld"
set $m3-filetype {SCIRun Field File (*.fld)}

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m4-filename "$DATADIR/$DATASET/$DATASET-dipole.pcv.fld"
set $m4-filetype {SCIRun Field File (*.fld)}


::netedit scheduleok
