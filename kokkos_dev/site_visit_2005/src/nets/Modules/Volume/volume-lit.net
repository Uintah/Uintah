# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {10 10 406 467}
set creationDate {Tue Jun 29 2004}
set creationTime {10:23:18}
set runDate {}
set runTime {}
set notes {}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 10 10]

# Create a Volume->Visualization->TextureBuilder Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "TextureBuilder" 10 206]

# Create a Volume->Visualization->VolumeVisualizer Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "VolumeVisualizer" 193 313]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 211 195]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 193 423]

# Create a SCIRun->FieldsData->NodeGradient Module
set m6 [addModuleAtPosition "SCIRun" "FieldsData" "NodeGradient" 51 104]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m6 0]
set c2 [addConnection $m1 0 $m2 0]
set c3 [addConnection $m2 0 $m3 0]
set c4 [addConnection $m3 0 $m5 0]
set c5 [addConnection $m6 0 $m2 1]
set c6 [addConnection $m4 0 $m3 1]

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m1-filename "$DATADIR/aneurysm/aneurysm-mra.lvs.fld"
set $m1-types {{{{SCIRun Field File} {.fld} } {{SCIRun Field Any} {.*} } {{TextCurveField} {{.pts} {.edge}} } {{TextHexVolField} {{.pts} {.hex}} } {{TextPointCloudField} {.pts} } {{TextPointCloudString} {.pcs.txt} } {{TextQuadSurfField} {{.pts} {.quad}} } {{TextStructCurveField} {.pts} } {{TextStructHexVolField} {.pts} } {{TextStructQuadSurfField} {.pts} } {{TextTetVolField} {{.pts} {.tet}} } {{TextTriSurfField} {{.pts} {.fac} {.tri}} } {{VTKtoTriSurfField} {.vtk} } {{VTKtoTriSurfField-swap} {.vtk} } }}

