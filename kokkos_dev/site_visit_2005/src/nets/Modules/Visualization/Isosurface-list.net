# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {17 14 558 522}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 47 14]

# Create a SCIRun->Render->Viewer Module
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 82 478]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 17 278]

# Create a SCIRun->Visualization->Isosurface Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 209 280]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 296 389]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 282 114]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 393 275]

# Create a SCIRun->FieldsCreate->FieldBoundary Module
set m8 [addModuleAtPosition "SCIRun" "FieldsCreate" "FieldBoundary" 72 99]

# Create a SCIRun->FieldsData->ApplyInterpMatrix Module
set m9 [addModuleAtPosition "SCIRun" "FieldsData" "ApplyInterpMatrix" 46 184]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m8 0]
set c2 [addConnection $m1 0 $m9 0]
set c3 [addConnection $m9 0 $m4 0]
set c4 [addConnection $m9 0 $m3 0]
set c5 [addConnection $m6 0 $m7 0]
set c6 [addConnection $m4 0 $m5 0]
set c7 [addConnection $m3 0 $m2 0]
set c8 [addConnection $m8 0 $m9 1]
set c9 [addConnection $m9 0 $m7 1]
set c10 [addConnection $m7 0 $m5 1]
set c11 [addConnection $m5 0 $m2 1]
set c12 [addConnection $m8 1 $m9 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

