<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- SCG Tin -->
<!-- 
    From: 
          Steinberg, D.J. Equation of State and Strength Properties of Selected Materials.
           UCRL-MA-106439: Change 1, Feb. 1996.
-->
<Uintah_Include>

    <density>      7287.0 </density>       <!-- kg/m^3 -->
    <thermal_conductivity> 66.8 </thermal_conductivity>  <!-- W/mK -->
    <specific_heat> 220.0 </specific_heat> <!-- J/kgK -->
    <room_temp>     298.0 </room_temp>     <!-- K -->
    <melt_temp>     656.6 </melt_temp>     <!-- K -->

    <!-- Constitutive Model -->
    <constitutive_model type="elastic_plastic">
      <!-- Plasticity Model -->
      <flow_model type="steinberg_cochran_guinan">
        <mu_0>   17.9e9   </mu_0>     <!-- Pa -->
        <sigma_0> 0.16e9  </sigma_0>  <!-- Pa -->
        <Y_max>   0.22e9  </Y_max>    <!-- Pa -->
        <beta>   2000     </beta>
        <n>       0.06    </n>
        <A>       8.66    </A>        <!-- Pa^-1 -->
        <B>       2.12e-3 </B>        <!-- K^-1 -->
        <T_m0>  656.6     </T_m0>     <!-- K -->
        <Gamma_0> 2.27    </Gamma_0>
        <a>       1.7     </a>
        <epsilon_p0> 0.0  </epsilon_p0>
      </flow_model>


      <!-- Shear Modulus Model -->
      <shear_modulus_model type="scg_shear">
        <mu_0> 17.9e9   </mu_0>       <!-- Pa -->
        <A>     8.66    </A>          <!-- Pa^-1 -->
        <B>     2.12e-3 </B>          <!-- K^-1 -->
      </shear_modulus_model>

      <!-- Melting Temperature Model -->
      <melting_temp_model type="scg_Tm">
        <T_m0>  656.6  </T_m0>        <!-- K -->
        <Gamma_0> 2.27 </Gamma_0> 
        <a>       1.7  </a>
      </melting_temp_model>

      <!-- Equation of State -->
      <equation_of_state type="mie_gruneisen">
        <C_0>    5240.0  </C_0>      <!-- m/s -->
        <Gamma_0>   2.27 </Gamma_0>   
        <S_alpha>   1.49 </S_alpha>   
        <S_2>       0.0  </S_2>       
        <S_3>       0.0  </S_3>       
        <!-- <b>         0.54 </b> -->
      </equation_of_state>

      <initial_material_temperature>  298.0 </initial_material_temperature> <!-- K -->
    </constitutive_model>
</Uintah_Include>
