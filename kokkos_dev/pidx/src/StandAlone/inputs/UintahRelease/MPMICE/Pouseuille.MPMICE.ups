<?xml version="1.0" encoding="iso-8859-1"?>

<Uintah_specification> 
<!--Please use a consistent set of units, (mks, cgs,...)-->

   <Meta>
       <title>Poiseuille Flow</title>
   </Meta>

   <SimulationComponent type="rmpmice" />
   
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            15.0        </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1.0         </delt_max>
       <delt_init>          1.0e-9      </delt_init>
       <timestep_multiplier>1.0         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face circle = "x-" origin = "0.0 0.0 0.0" radius = "0.05">
        <BCType id = "0" label = "Pressure"     var = "Dirichlet">
                              <value> 101325.0  </value>
        </BCType>
        <BCType id = "all" label = "Velocity"     var = "Neumann">
                              <value> [0.0,0.0,0.0] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Dirichlet">
                              <value> 300.0  </value>
        </BCType>
        <BCType id = "1"   label = "Density"      var = "Dirichlet">
                              <value> 1.17929469273743 </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value>  0.0 </value>
        </BCType>
      </Face>
      <Face side = "x-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann"> 
                            <value> 0. </value> 
        </BCType> 
        <BCType id = "all" label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature"  var = "Dirichlet"> 
                            <value> 300.0 </value>
        </BCType>
        <BCType id = "all" label = "Density"      var = "Neumann">
                              <value>  0.0 </value>
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0"   label = "Pressure"     var = "Dirichlet">
                              <value> 101324. </value>                
        </BCType>
        <BCType id = "all"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all"   label = "Temperature"  var = "Neumann">
                              <value> 0.0 </value>
        </BCType>
        <BCType id = "all"   label = "Density"      var = "Neumann">
                              <value> 0.0 </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "all"   label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all"   label = "Temperature"  var = "Neumann">
                              <value> 0.0 </value>
        </BCType>
        <BCType id = "all"   label = "Density"      var = "Neumann">
                              <value> 0.0 </value>
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <!--__________________________________________________-->
        <!-- The velocity the upper wall is defined here      -->
        <!-- Pure Poiseuille flow velocity = 0, 0, 0          -->
        <!--__________________________________________________-->
        <BCType id = "all"   label = "Velocity"     var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all"   label = "Temperature"  var = "Neumann">
                              <value> 0.0 </value>
        </BCType>
        <BCType id = "all"   label = "Density"      var = "Neumann">
                              <value> 0.0 </value>       
        </BCType>
      </Face>
      <Face side = "z+">
        <BCType id = "all"   label = "Symmetric" var = "symmetry">
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "all"   label = "Symmetric" var = "symmetry">
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0,-0.08,0]         </lower>
              <upper>        [1, 0.08,0.01]     </upper>
              <extraCells>   [1,1,1]             </extraCells>
              <patches>      [1,1,1]             </patches>
           </Box>
           <spacing>         [0.01,0.005,0.01]  </spacing>
       </Level>
    </Grid>    
    <!--____________________________________________________________________-->
    <!--      O  U  P  U  T     V  A  R  I  A  B  L  E  S                   -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>PoiseuilleMPMICE.uda</filebase>
      <outputInterval>1</outputInterval>
      <save label="p.x"/>
      <save label="p.volume"/>
      <save label="vel_CC"/>
      <save label="press_CC"/>
      <save label="delP_Dilatate"/>
      <save label="rho_CC"/>
      <save label="temp_CC"/>
   </DataArchiver>

    
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
       <cfl>0.4</cfl>
       <ICE>
        <advection type = "SecondOrder" />
        <TimeStepControl>
          <Scheme_for_delT_calc> aggressive   </Scheme_for_delT_calc>
          <knob_for_speedSound>  0.000        </knob_for_speedSound>
        </TimeStepControl>
                                                                                
        <ImplicitSolver>
           <max_outer_iterations>      20    </max_outer_iterations>
           <outer_iteration_tolerance> 1e-8  </outer_iteration_tolerance>
           <iters_before_timestep_restart> 5 </iters_before_timestep_restart>
           <Parameters variable="implicitPressure">
                                                                                
            <!-- CGSolver options -->
              <norm>     LInfinity  </norm>
              <criteria> Absolute   </criteria>
                                                                                
            <!-- Hypre options -->
            <solver>         cg      </solver>
            <preconditioner> pfmg    </preconditioner>
            <tolerance>      1.e-10  </tolerance>
            <maxiterations>  7500    </maxiterations>
            <npre>           1       </npre>
            <npost>          1       </npost>
            <skip>           0       </skip>
            <jump>           0       </jump>
           </Parameters>
        </ImplicitSolver>
      </ICE>        
    </CFD>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>

    <MPM>
        <time_integrator>explicit</time_integrator>
    </MPM>
    
    <!--____________________________________________________________________-->
    <!--      material Properties and Initial Conditions                    -->
    <!--____________________________________________________________________-->                      
  <MaterialProperties>
    <MPM>
      <material name="steel">
        <density>8000.0</density>
        <constitutive_model type="comp_neo_hook">
          <bulk_modulus>207e9</bulk_modulus>
          <shear_modulus>80e9</shear_modulus>
        </constitutive_model>
        <thermal_conductivity>1.0</thermal_conductivity>
        <specific_heat>5</specific_heat>
        <geom_object>
          <union label="walls">
            <parallelepiped label="bottom">
              <p1> [ 0.0, -0.11, 0.0]   </p1>
              <p2> [ 0.0, -0.11, 0.01]  </p2>
              <p3> [ 1.0, -0.08, 0.0]   </p3>
              <p4> [ 0.0, -0.05, 0.0]   </p4>
            </parallelepiped>
            <parallelepiped label="top">
              <p1> [ 0.0, 0.05, 0.0]   </p1>
              <p2> [ 0.0, 0.05, 0.01]  </p2>
              <p3> [ 1.0, 0.08, 0.0]   </p3>
              <p4> [ 0.0, 0.11, 0.0]   </p4>
            </parallelepiped>
<!--
            <box label="bottom">
              <min>[ 0.0,-0.08,0.0]</min>
              <max>[ 1.0,-0.05,0.01]</max>
            </box>
            <box label="top">
              <min>[ 0.0,0.05,0.0]</min>
              <max>[ 1.0,0.08,0.01]</max>
            </box>
-->
           </union>
          <res>[2,2,1]</res>
          <velocity>[0.0,0.0,0]</velocity>
          <temperature>300</temperature>
        </geom_object>
      </material>
      <contact>
        <type>null</type>
      </contact>
    </MPM>
       <ICE>
         <material>
           <EOS type = "ideal_gas">                   </EOS>                   
           <dynamic_viscosity>  1.e-3                 </dynamic_viscosity>     
           <thermal_conductivity>0.0                  </thermal_conductivity>  
           <specific_heat>      716.0                 </specific_heat>         
           <gamma>              1.4                   </gamma>                 
           <geom_object>
             <difference>
              <box label="wholeDomain">
                  <min>       [-1.0, -1.0,-1.0]       </min>
                  <max>       [ 1.0,  1.0, 1.0]       </max>
              </box>
            <union label="walls" />
            </difference>
            <res>           [2,2,1]                  </res>
            <velocity>      [0.,0.,0.]               </velocity>
            <density>       1.1792946927374306000e+00</density>
            <pressure>      101325.0                 </pressure>     
            <temperature>   300.0                    </temperature>
           </geom_object>
         </material>
      </ICE>       

        <exchange_properties>
         <exchange_coefficients>
               <momentum>              [1e15]         </momentum>
               <heat>                  [1e10]         </heat>
         </exchange_coefficients>
        </exchange_properties>

    </MaterialProperties>       
</Uintah_specification>
