# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {13 11 401 462}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 13 11]
setGlobal Notes($m1) {}

# Create a SCIRun->FieldsCreate->FieldBoundary Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "FieldBoundary" 34 96]
setGlobal Notes($m2) {}

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 13 332]
setGlobal Notes($m3) {}

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 206 173]
setGlobal Notes($m4) {}

# Create a SCIRun->Visualization->RescaleColorMap Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 206 253]
setGlobal Notes($m5) {}

# Create a SCIRun->Render->Viewer Module
set m6 [addModuleAtPosition "SCIRun" "Render" "Viewer" 188 418]
setGlobal Notes($m6) {}

# Create a SCIRun->Visualization->Isosurface Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 188 332]
setGlobal Notes($m7) {}

# Create a SCIRun->FieldsData->ApplyInterpMatrix Module
set m8 [addModuleAtPosition "SCIRun" "FieldsData" "ApplyInterpMatrix" 13 175]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m8 0]
set c3 [addConnection $m8 0 $m7 0]
set c4 [addConnection $m8 0 $m3 0]
set c5 [addConnection $m4 0 $m5 0]
set c6 [addConnection $m3 0 $m6 0]
set c7 [addConnection $m2 0 $m8 1]
set c8 [addConnection $m8 0 $m5 1]
set c9 [addConnection $m7 1 $m6 1]
set c10 [addConnection $m5 0 $m7 1]
set c11 [addConnection $m5 0 $m3 1]
set c12 [addConnection $m2 1 $m8 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->FieldsCreate->FieldBoundary Module

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m3-edges-on {0}
setGlobal $m3-faces-on {0}
setGlobal $m3-normalize-vectors {}
setGlobal $m3-has_scalar_data {1}
setGlobal $m3-active_tab {Faces}
setGlobal $m3-show_progress {}

# Setup GUI for the SCIRun->Visualization->GenStandardColorMaps Module
setGlobal $m4-resolution {255}
setGlobal $m4-realres {255}

# Setup GUI for the SCIRun->Visualization->RescaleColorMap Module
setGlobal $m5-min {-0.64964699999999997}
setGlobal $m5-max {0.37080299999999999}

