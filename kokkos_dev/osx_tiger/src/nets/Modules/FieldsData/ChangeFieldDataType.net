# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {47 20 456 505}

# Create a SCIRun->FieldsData->ChangeFieldDataType Module
set m1 [addModuleAtPosition "SCIRun" "FieldsData" "ChangeFieldDataType" 47 210]

# Create a SCIRun->DataIO->FieldReader Module
set m2 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 47 20]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 47 374]

# Create a SCIRun->Render->Viewer Module
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 47 461]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 261 296]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 261 181]

# Create a SCIRun->FieldsData->TransformData Module
set m7 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 47 112]

# Create the Connections between Modules
set c1 [addConnection $m2 0 $m7 0]
set c2 [addConnection $m1 0 $m3 0]
set c3 [addConnection $m7 0 $m1 0]
set c4 [addConnection $m6 0 $m5 0]
set c5 [addConnection $m3 0 $m4 0]
set c6 [addConnection $m1 0 $m5 1]
set c7 [addConnection $m5 0 $m3 1]

# Setup GUI for the SCIRun->FieldsData->ChangeFieldDataType Module
setGlobal $m1-outputdatatype {unsigned char}

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m2-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m3-edges-on {0}
setGlobal $m3-faces-on {0}
setGlobal $m3-normalize-vectors {}
setGlobal $m3-has_scalar_data {1}
setGlobal $m3-node_display_type {Spheres}
setGlobal $m3-node_scale {0.0300}
setGlobal $m3-edge_scale {0.0150}
setGlobal $m3-show_progress {}

# Setup GUI for the SCIRun->Render->Viewer Module
$m4 ui
setGlobal $m4-ViewWindow_0-pos {}
setGlobal $m4-ViewWindow_0-caxes {0}
setGlobal $m4-ViewWindow_0-raxes {1}
setGlobal $m4-ViewWindow_0-iaxes {}
setGlobal $m4-ViewWindow_0-have_collab_vis {0}
