setGlobal $m4-ViewWindow_0-view-eyep-z {         -5.80829}
setGlobal $m4-ViewWindow_0-view-lookat-x {}
setGlobal $m4-ViewWindow_0-view-lookat-y {}
setGlobal $m4-ViewWindow_0-view-lookat-z {}
