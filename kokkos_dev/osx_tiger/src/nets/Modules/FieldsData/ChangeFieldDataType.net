# Setup GUI for the SCIRun->Visualization->RescaleColorMap Module
setGlobal $m5-max {255}

# Setup GUI for the SCIRun->Visualization->GenStandardColorMaps Module
setGlobal $m6-resolution {255}
setGlobal $m6-realres {255}

# Setup GUI for the SCIRun->FieldsData->TransformData Module
setGlobal $m7-function {const double min1 = -5.25315;
const double max1 = 4.31926;
result = (v - min1) / (max1 - min1);
result = result * 255.0;
}

::netedit scheduleok
