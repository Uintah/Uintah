set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {57 27 423 458}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 119 27]

# Create a SCIRun->FieldsData->TransformData Module
set m2 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 96 133]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 57 414]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 80 313]

# Create a SCIRun->FieldsData->VectorMagnitude Module
set m5 [addModuleAtPosition "SCIRun" "FieldsData" "VectorMagnitude" 287 226]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m2 0 $m5 0]
set c3 [addConnection $m2 0 $m4 0]
set c4 [addConnection $m4 0 $m3 0]
set c5 [addConnection $m5 0 $m4 2]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module
setGlobal $m1-sizex {8}
setGlobal $m1-sizey {8}
setGlobal $m1-sizez {8}
$m1 initialize_ui

# Setup GUI for the SCIRun->FieldsData->TransformData Module
setGlobal $m2-function {result = Vector((x+1)*0.5, (y+1)*0.5, (z+1)*0.5);


}
setGlobal $m2-outputdatatype {Vector}
$m2 initialize_ui

