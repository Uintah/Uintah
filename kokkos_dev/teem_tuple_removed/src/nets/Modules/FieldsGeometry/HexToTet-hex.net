set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {24 40 421 457}

# Create a SCIRun->FieldsGeometry->HexToTet Module
set m1 [addModuleAtPosition "SCIRun" "FieldsGeometry" "HexToTet" 159 208]

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 71 40]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 101 413]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 24 306]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 256 306]

# Create a SCIRun->FieldsGeometry->Unstructure Module
set m6 [addModuleAtPosition "SCIRun" "FieldsGeometry" "Unstructure" 123 120]

# Create the Connections between Modules
set c1 [addConnection $m2 0 $m6 0]
set c2 [addConnection $m2 0 $m4 0]
set c3 [addConnection $m1 0 $m5 0]
set c4 [addConnection $m6 0 $m1 0]
set c5 [addConnection $m4 0 $m3 0]
set c6 [addConnection $m5 0 $m3 1]

