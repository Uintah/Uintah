set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {22 17 371 507}

# Create a SCIRun->FieldsGeometry->Unstructure Module
set m1 [addModuleAtPosition "SCIRun" "FieldsGeometry" "Unstructure" 135 178]

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 42 17]

# Create a SCIRun->FieldsCreate->ClipByFunction Module
set m3 [addModuleAtPosition "SCIRun" "FieldsCreate" "ClipByFunction" 206 267]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 22 314]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 196 361]

# Create a SCIRun->Render->Viewer Module
set m6 [addModuleAtPosition "SCIRun" "Render" "Viewer" 103 463]

# Create a SCIRun->FieldsGeometry->ChangeFieldBounds Module
set m7 [addModuleAtPosition "SCIRun" "FieldsGeometry" "ChangeFieldBounds" 42 98]

# Create the Connections between Modules
set c1 [addConnection $m3 0 $m5 0]
set c2 [addConnection $m2 0 $m7 0]
set c3 [addConnection $m7 0 $m1 0]
set c4 [addConnection $m7 0 $m4 0]
set c5 [addConnection $m1 0 $m3 0]
set c6 [addConnection $m4 0 $m6 0]
set c7 [addConnection $m5 0 $m6 1]

# Setup GUI for the SCIRun->FieldsGeometry->Unstructure Module

# Setup GUI for the SCIRun->FieldsCreate->SamplePlane Module
setGlobal $m2-sizex {40}
setGlobal $m2-sizey {40}
setGlobal $m2-axis {2}

# Setup GUI for the SCIRun->FieldsCreate->ClipByFunction Module
setGlobal $m3-clipfunction {(x * x + y * y) < 1.0}

# Setup GUI for the SCIRun->Visualization->ShowField Module
