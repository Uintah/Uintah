set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {26 13 421 427}

# Create a SCIRun->FieldsGeometry->QuadToTri Module
set m1 [addModuleAtPosition "SCIRun" "FieldsGeometry" "QuadToTri" 285 190]

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 70 13]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 26 202]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 220 278]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 102 383]

# Create the Connections between Modules
set c1 [addConnection $m2 0 $m1 0]
set c2 [addConnection $m2 0 $m3 0]
set c3 [addConnection $m1 0 $m4 0]
set c4 [addConnection $m3 0 $m5 0]
set c5 [addConnection $m4 0 $m5 1]

# Setup GUI for the SCIRun->FieldsGeometry->QuadToTri Module

# Setup GUI for the SCIRun->FieldsCreate->SamplePlane Module

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m3-nodes-on {0}
setGlobal $m3-faces-on {0}
setGlobal $m3-normalize-vectors {}
setGlobal $m3-has_scalar_data {1}
setGlobal $m3-def-color-r {0.50}
setGlobal $m3-def-color-g {0.61}
setGlobal $m3-def-color-b {0.50}
setGlobal $m3-def-color-a {0.50}
setGlobal $m3-node_display_type {Spheres}
setGlobal $m3-edge_display_type {Cylinders}
setGlobal $m3-active_tab {Edges}
setGlobal $m3-node_scale {0.0300}
setGlobal $m3-edge_scale {0.0050}
setGlobal $m3-show_progress {}

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m4-nodes-on {0}
setGlobal $m4-faces-on {0}
setGlobal $m4-normalize-vectors {}
setGlobal $m4-has_scalar_data {1}
