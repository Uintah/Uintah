set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {15 11 473 547}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 61 11]

# Create a SCIRun->FieldsGeometry->ManageFieldMesh Module
set m2 [addModuleAtPosition "SCIRun" "FieldsGeometry" "ManageFieldMesh" 237 184]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 84 503]

# Create a SCIRun->Math->LinAlgUnary Module
set m4 [addModuleAtPosition "SCIRun" "Math" "LinAlgUnary" 308 264]

# Create a SCIRun->FieldsGeometry->ManageFieldMesh Module
set m5 [addModuleAtPosition "SCIRun" "FieldsGeometry" "ManageFieldMesh" 213 344]

# Create a SCIRun->FieldsGeometry->Unstructure Module
set m6 [addModuleAtPosition "SCIRun" "FieldsGeometry" "Unstructure" 96 98]

# Create a SCIRun->Visualization->ShowField Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 15 375]

# Create a SCIRun->Visualization->ShowField Module
set m8 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 210 426]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m6 0]
set c2 [addConnection $m2 1 $m4 0]
set c3 [addConnection $m5 0 $m8 0]
set c4 [addConnection $m6 0 $m2 0]
set c5 [addConnection $m6 0 $m5 0]
set c6 [addConnection $m6 0 $m7 0]
set c7 [addConnection $m7 0 $m3 0]
set c8 [addConnection $m4 0 $m5 1]
set c9 [addConnection $m8 0 $m3 1]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module
setGlobal $m1-sizex {5}
