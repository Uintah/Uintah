set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {44 14 595 513}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 44 14]

# Create a SCIRun->FieldsData->ChangeFieldDataType Module
set m2 [addModuleAtPosition "SCIRun" "FieldsData" "ChangeFieldDataType" 167 102]

# Create a SCIRun->FieldsData->TransformVectorData Module
set m3 [addModuleAtPosition "SCIRun" "FieldsData" "TransformVectorData" 116 201]

# Create a SCIRun->Render->Viewer Module
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 71 469]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 50 368]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 400 153]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 339 269]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m5 0]
set c3 [addConnection $m2 0 $m3 0]
set c4 [addConnection $m6 0 $m7 0]
set c5 [addConnection $m5 0 $m4 0]
set c6 [addConnection $m1 0 $m7 1]
set c7 [addConnection $m7 0 $m5 1]
set c8 [addConnection $m3 0 $m5 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->FieldsData->ChangeFieldDataType Module
setGlobal $m2-outputdatatype {Vector}

# Setup GUI for the SCIRun->FieldsData->TransformVectorData Module
setGlobal $m3-functionx {0}
setGlobal $m3-functiony {0}
setGlobal $m3-functionz {1}
$m3 initialize_ui

# Setup GUI for the SCIRun->Render->Viewer Module
$m4 ui
