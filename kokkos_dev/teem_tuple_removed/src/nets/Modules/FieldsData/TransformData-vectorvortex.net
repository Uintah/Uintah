set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {57 7 560 543}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 61 7]

# Create a SCIRun->FieldsData->TransformData Module
set m2 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 57 171]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 79 418]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 365 158]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 305 339]

# Create a SCIRun->Render->Viewer Module
set m6 [addModuleAtPosition "SCIRun" "Render" "Viewer" 124 499]

# Create a SCIRun->FieldsData->VectorMagnitude Module
set m7 [addModuleAtPosition "SCIRun" "FieldsData" "VectorMagnitude" 127 253]

# Create a SCIRun->FieldsCreate->SampleField Module
set m8 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleField" 85 88]

# Create the Connections between Modules
set c1 [addConnection $m8 0 $m2 0]
set c2 [addConnection $m1 0 $m8 0]
set c3 [addConnection $m2 0 $m7 0]
set c4 [addConnection $m2 0 $m3 0]
set c5 [addConnection $m4 0 $m5 0]
set c6 [addConnection $m3 0 $m6 0]
set c7 [addConnection $m7 0 $m5 1]
set c8 [addConnection $m5 0 $m3 1]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module

# Setup GUI for the SCIRun->FieldsData->TransformData Module
setGlobal $m2-function {result=Cross(Vector(x, y, 0), Vector(0, 0, 1));}
