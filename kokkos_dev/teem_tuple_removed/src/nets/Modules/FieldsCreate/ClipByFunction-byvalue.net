set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {28 23 458 400}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 70 23]

# Create a SCIRun->FieldsCreate->ClipByFunction Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "ClipByFunction" 160 126]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 28 239]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 293 241]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 121 356]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m3 0]
set c3 [addConnection $m2 0 $m4 0]
set c4 [addConnection $m3 0 $m5 0]
set c5 [addConnection $m4 0 $m5 1]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/utahtorso-lowres/utahtorso-lowres-mesh.tvt.fld"

# Setup GUI for the SCIRun->FieldsCreate->ClipByFunction Module
setGlobal $m2-clipfunction {v == 4}
$m2 initialize_ui

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m3-nodes-on {0}
setGlobal $m3-edges-on {0}
setGlobal $m3-use-transparency {1}
setGlobal $m3-normalize-vectors {}
setGlobal $m3-has_scalar_data {1}
setGlobal $m3-def-color-r {0.50}
setGlobal $m3-def-color-g {0.50}
setGlobal $m3-def-color-b {0.50}
setGlobal $m3-def-color-a {0.35}
setGlobal $m3-node_display_type {Spheres}
setGlobal $m3-active_tab {Faces}
setGlobal $m3-node_scale {0.0300}
setGlobal $m3-edge_scale {0.0150}
setGlobal $m3-show_progress {}

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m4-nodes-on {0}
setGlobal $m4-edges-on {0}
setGlobal $m4-normalize-vectors {}
setGlobal $m4-has_scalar_data {1}
setGlobal $m4-def-color-r {0.67}
setGlobal $m4-def-color-g {0.50}
