set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {43 30 407 494}

# Create a SCIRun->FieldsCreate->ClipField Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "ClipField" 47 272]

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 101 110]

# Create a SCIRun->DataIO->FieldReader Module
set m3 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 47 30]

# Create a SCIRun->Render->Viewer Module
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 43 450]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 51 361]

# Create a SCIRun->FieldsGeometry->Unstructure Module
set m6 [addModuleAtPosition "SCIRun" "FieldsGeometry" "Unstructure" 96 193]

# Create a SCIRun->Visualization->ShowField Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 242 361]

# Create the Connections between Modules
set c1 [addConnection $m3 0 $m2 0]
set c2 [addConnection $m1 1 $m5 0]
set c3 [addConnection $m1 1 $m7 0]
set c4 [addConnection $m2 0 $m6 0]
set c5 [addConnection $m6 0 $m1 0]
set c6 [addConnection $m5 0 $m4 0]
set c7 [addConnection $m3 0 $m1 1]
set c8 [addConnection $m7 0 $m4 1]

# Setup GUI for the SCIRun->FieldsCreate->ClipField Module
setGlobal $m1-autoinvert {1}
setGlobal $m1-execmode {}
$m1 initialize_ui

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module

