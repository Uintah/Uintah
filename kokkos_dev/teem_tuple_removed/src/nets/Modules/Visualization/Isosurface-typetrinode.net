set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {16 9 574 548}

# Create a SCIRun->Visualization->Isosurface Module
set m1 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 165 334]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 379 120]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 334 251]

# Create a SCIRun->FieldsData->TransformData Module
set m4 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 55 165]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 16 418]

# Create a SCIRun->Visualization->ShowField Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 370 419]

# Create a SCIRun->Render->Viewer Module
set m7 [addModuleAtPosition "SCIRun" "Render" "Viewer" 287 504]

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m8 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 58 9]

# Create a SCIRun->FieldsGeometry->QuadToTri Module
set m9 [addModuleAtPosition "SCIRun" "FieldsGeometry" "QuadToTri" 55 90]

# Create the Connections between Modules
set c1 [addConnection $m8 0 $m9 0]
set c2 [addConnection $m4 0 $m1 0]
set c3 [addConnection $m4 0 $m5 0]
set c4 [addConnection $m9 0 $m4 0]
set c5 [addConnection $m2 0 $m3 0]
set c6 [addConnection $m1 0 $m6 0]
set c7 [addConnection $m5 0 $m7 0]
set c8 [addConnection $m4 0 $m3 1]
set c9 [addConnection $m1 1 $m7 1]
set c10 [addConnection $m3 0 $m1 1]
set c11 [addConnection $m3 0 $m6 1]
set c12 [addConnection $m6 0 $m7 2]

# Setup GUI for the SCIRun->Visualization->Isosurface Module
setGlobal $m1-isoval {0.51831}
setGlobal $m1-isoval-min {0.24999999999999994}
setGlobal $m1-isoval-max {0.75}
setGlobal $m1-isoval-list {0.3 0.4 0.51}
setGlobal $m1-active-isoval-selection-tab {2}
setGlobal $m1-active_tab {}

# Setup GUI for the SCIRun->Visualization->GenStandardColorMaps Module

# Setup GUI for the SCIRun->Visualization->RescaleColorMap Module
setGlobal $m3-min {0.24999999999999994}
setGlobal $m3-max {0.75}

# Setup GUI for the SCIRun->FieldsData->TransformData Module
setGlobal $m4-function {x = x - 0.5;
y = y - 0.5;
z = z - 0.5;
result = x*x + y*y + z*z;
}

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m5-nodes-on {0}
setGlobal $m5-edges-transparency {1}
setGlobal $m5-faces-on {0}
