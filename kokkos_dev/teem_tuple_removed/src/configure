    if test -z "$_sci_includes"; then
      _sci_includes=-I$i
    else
      _sci_includes="$_sci_includes -I$i"
    fi
  fi
done

if test "required" = "specific"; then
  # If 'specific' then only one lib is allowed:
  ### Determine if there is only one item in $_sci_lib (I don't know of a better way to do this.)
  __sci_pass=false
  __sci_first_time=true

  # Must have the "" for the SGI sh.
  for value in "" $_sci_lib; do
    if test "$value" = "/usr/lib"; then
       echo ""
       { { echo "$as_me:$LINENO: error: Please do not specify /usr/lib as the location for libs glut." >&5
echo "$as_me: error: Please do not specify /usr/lib as the location for libs glut." >&2;}
   { (exit 1); exit 1; }; }
    fi
    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test $__sci_pass != "true"; then
       { { echo "$as_me:$LINENO: error: For specific SCI-TRY-LINK test for glut library and headers only one library path may be specified (you had: $_sci_lib).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." >&5
echo "$as_me: error: For specific SCI-TRY-LINK test for glut library and headers only one library path may be specified (you had: $_sci_lib).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." >&2;}
   { (exit 1); exit 1; }; }
  fi
  # and only one include path:
  ### Determine if there is only one item in $_sci_inc
  __sci_pass=false
  __sci_first_time=true
  for value in ""$_sci_inc; do
    if test "$value" = ""; then
      continue
    fi
    if test $__sci_first_time = "true"; then
      __sci_first_time=false
      __sci_pass=true
    else
      __sci_pass=false
    fi
  done
  if test $__sci_pass != "true"; then
       { { echo "$as_me:$LINENO: error: For specific SCI-TRY-LINK test for glut library and headers only one include path may be specified (you had: $_sci_inc).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." >&5
echo "$as_me: error: For specific SCI-TRY-LINK test for glut library and headers only one include path may be specified (you had: $_sci_inc).  This is an internal SCIRun configure error and should be reported to scirun-develop@sci.utah.edu." >&2;}
   { (exit 1); exit 1; }; }
  fi
