  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
elif test "$debug" = "yes"; then
  extra_cflags="-g ${extra_cflags}"
  echo "$as_me:$LINENO: result: yes (-g) " >&5
echo "${ECHO_T}yes (-g) " >&6
else
  extra_cflags="$debug ${extra_cflags}"
