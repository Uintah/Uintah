set results [sourceSettingsFile]

if { $results == "failed" } {

    ::netedit scheduleok
    return 

} else {

    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

set bbox {9 8 639 577}
set Name {Main}

# Create the Modules
set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 9 8]
set m1 [addModuleAtPosition "BioPSE" "Forward" "SetupFEMatrix" 27 87]
set m2 [addModuleAtPosition "BioPSE" "Forward" "ApplyFEMCurrentSource" 45 160]
set m3 [addModuleAtPosition "SCIRun" "Math" "SolveMatrix" 27 224]
set m4 [addModuleAtPosition "SCIRun" "FieldsData" "ManageFieldData" 9 289]
set m5 [addModuleAtPosition "BioPSE" "Visualization" "ShowDipoles" 411 81]
set m6 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 411 10]
set m7 [addModuleAtPosition "SCIRun" "Render" "Viewer" 393 533]
set m8 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 9 455]
set m9 [loadSubnet "ShowElectrodes.net" 228 369]
set m10 [loadSubnet "ShowBoundary.net" 447 325]
set m11 [loadSubnet "ShowStreamlines.net" 465 454]

# Set the Module Notes Dispaly Options

# Create the Connections between Modules
set c0 [addConnection $m1 0 $m3 0]
set c1 [addConnection $m0 0 $m2 0]
set c2 [addConnection $m0 0 $m1 0]
set c3 [addConnection $m0 0 $m4 0]
set c4 [addConnection $m0 0 $m10 0]
set c5 [addConnection $m6 0 $m5 0]
set c6 [addConnection $m4 0 $m8 0]
set c7 [addConnection $m4 0 $m9 0]
set c8 [addConnection $m4 0 $m11 0]
set c9 [addConnection $m8 1 $m7 0]
set c10 [addConnection $m2 0 $m3 1]
set c11 [addConnection $m5 0 $m2 1]
set c12 [addConnection $m3 0 $m4 1]
set c13 [addConnection $m9 0 $m7 1]
set c14 [addConnection $m9 1 $m8 1]
set c15 [addConnection $m9 1 $m11 1]
set c16 [addConnection $m5 1 $m7 2]
set c17 [addConnection $m10 0 $m7 3]
set c18 [addConnection $m11 0 $m7 4]
set c19 [addConnection $m11 1 $m7 5]

# Mark which Connections are Disabled

# Set the Connection Notes and Dislpay Options

# Set the GUI variables for each Module

set $m0-filename $DATADIR/$DATASET/$DATASET-mesh.tvt.fld

set $m3-target_error {0.000001}
set $m3-orig_error {1}
set $m3-current_error {9.84662e-07}
set $m3-iteration {86}
set $m3-maxiter {500}
set $m3-use_previous_soln {1}
set $m3-emit_partial {0}

set $m5-widgetSizeGui_ [expr 0.05 * ${global-scale}]
set $m5-scaleModeGui_ {normalize}
set $m5-showLastVecGui_ {1}
set $m5-showLinesGui_ {0}

set $m6-filename $DATADIR/$DATASET/$DATASET-dipole.pcv.fld

$m7 initialize_ui
set $m7-ViewWindow_0-view-eyep-x ${view-eyep-x}
set $m7-ViewWindow_0-view-eyep-y ${view-eyep-y}
set $m7-ViewWindow_0-view-eyep-z ${view-eyep-z}
set $m7-ViewWindow_0-view-lookat-x ${view-lookat-x}
set $m7-ViewWindow_0-view-lookat-y ${view-lookat-y}
set $m7-ViewWindow_0-view-lookat-z ${view-lookat-z}
set $m7-ViewWindow_0-view-up-x ${view-up-x}
set $m7-ViewWindow_0-view-up-y ${view-up-y}
set $m7-ViewWindow_0-view-up-z ${view-up-z}
set $m7-ViewWindow_0-view-fov ${view-fov}
set $m7-ViewWindow_0-specular-scale {0.4}

set $m8-extract-from-new-field {1}
set $m8-update_type {on release}

::netedit scheduleok
