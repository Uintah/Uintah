set results [sourceSettingsFile]

if { $results == "failed" } {

    ::netedit scheduleok
    return 

} else {

    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

set bbox {9 8 547 548}
set Name {Main}

# Create the Modules
set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 9 8]
set m1 [addModuleAtPosition "SCIRun" "FieldsData" "Gradient" 27 98]
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 346 504]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 184 262]
set m4 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 184 339]
set m5 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleField" 203 98]
set m6 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 9 419]
set m7 [addModuleAtPosition "SCIRun" "Visualization" "StreamLines" 27 187]
set m8 [addModuleAtPosition "SCIRun" "FieldsData" "DirectInterpolate" 9 261]
set m9 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 382 97]

# Set the Module Notes Dispaly Options

# Create the Connections between Modules
set c0 [addConnection $m0 0 $m5 0]
set c1 [addConnection $m0 0 $m8 0]
set c2 [addConnection $m0 0 $m1 0]
set c3 [addConnection $m0 0 $m9 0]
set c4 [addConnection $m8 0 $m6 0]
set c5 [addConnection $m1 0 $m7 0]
set c6 [addConnection $m3 0 $m4 0]
set c7 [addConnection $m6 0 $m2 0]
set c8 [addConnection $m5 0 $m7 1]
set c9 [addConnection $m5 1 $m2 1]
set c10 [addConnection $m8 0 $m4 1]
set c11 [addConnection $m4 0 $m6 1]
set c12 [addConnection $m7 0 $m8 1]
set c13 [addConnection $m9 0 $m2 2]

# Mark which Connections are Disabled

# Set the Connection Notes and Dislpay Options

# Set the GUI variables for each Module
set $m0-notes {}
set $m0-filename $DATADIR/$DATASET/$DATASET-voltage.tvd.fld

set $m1-notes {}

set $m2-notes {}
$m2 ui
set $m2-ViewWindow_0-view-eyep-x ${view-eyep-x}
