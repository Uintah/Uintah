<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- @version: -->
<Uintah_specification>

   <Meta>
       <title> flow through a nozzle using pressure</title>
   </Meta>


    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            0.000547138 </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1.0         </delt_max>
       <delt_init>          1.0e-7      </delt_init>
       <max_iterations>     500         </max_iterations>
       <timestep_multiplier>0.8         </timestep_multiplier>
   </Time>

    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face circle = "x-" origin = "0.0 0.0 0.0" radius = ".03683">
	 <BCType id = "0"   label = "Pressure"     var = "Dirichlet">
			       <value> 4000000.0   </value>
	 </BCType>
	 <BCType id = "1" label = "Velocity"     var = "Dirichlet">
			       <value> [209.,0.,0.] </value>
	 </BCType>
	 <BCType id = "1" label = "Temperature"  var = "Dirichlet">
			       <value> 1000.0  </value>
	 </BCType>
	 <BCType id = "1" label = "Density"	   var = "Dirichlet">
			       <value> 13.937 </value>
	 </BCType>
       </Face>
      <Face side = "x-">
        <BCType id = "0" label = "Pressure" var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity" var = "Dirichlet">
                              <value> [0.0,0.0,0.0] </value>
        </BCType>
      
        <BCType id = "all" label = "Temperature" var = "Dirichlet">
                              <value> 300.0  </value>
        </BCType>
        <BCType id = "all" label = "Density" var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0" label = "Pressure" var = "Dirichlet">
                              <value> 100000.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity" var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature" var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density" var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>

      <Face side = "y-">
        <BCType id = "all" label = "Symmetric" var = "symmetry">
        </BCType>
	</Face>
<!--        <BCType id = "0" label = "Velocity" var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
-->

<!--      <Face side = "y-">
        <BCType id = "0" label = "Pressure" var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity" var = "Dirichlet">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature" var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density" var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>                  
-->
      <Face side = "y+">
        <BCType id = "0" label = "Pressure" var = "Neumann">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" label = "Velocity" var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" label = "Temperature" var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" label = "Density" var = "Neumann">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      
      <Face side = "z-">
        <BCType id = "all" label = "Symmetric" var = "symmetry">
        </BCType>
      </Face>                  
      <Face side = "z+">
        <BCType id = "all" label = "Symmetric" var = "symmetry">
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label = "1">
              <lower>        [0.0,0.0,-0.005]   </lower>
              <upper>        [1.0,0.25,0.005]   </upper>
              <extraCells>   [1,1,1]                  </extraCells>
              <patches>      [1,1,1]                  </patches>
            </Box>
           <spacing>         [0.01,0.01,0.01]       </spacing>
       </Level>
    </Grid>

    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>TRWnoz.uda</filebase>
      <outputTimestepInterval>10</outputTimestepInterval>
      <save label = "press_equil_CC"/>
      <save label = "vol_frac_CC"/>
      <save label = "sp_vol_CC"/>
      <save label = "spVol_source_CC"/>
      <save label = "speedSound_CC"/>
      <save label = "delP_Dilatate"/>
      <save label = "vel_CC"/>
      <save label = "mom_L_CC"/>
      <save label = "rho_CC"/>
      <save label = "temp_CC"/>  
      <save label = "mach"/>  
      <save label = "p.x"/>
      <save label = "p.volume"/>
      <save label = "p.velocity"/>
      <checkpoint cycle = "2" timestepInterval = "50"/>
   </DataArchiver>

