    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.2</cfl>
       <ICE>
        <max_iteration_equilibration> 100</max_iteration_equilibration>
        <advection type = "FirstOrder"/>
        <solution technique = "EqForm"/>
       </ICE>        
    </CFD>
    <MPM>
        <time_integrator>explicit</time_integrator>
        <nodes8or27>8</nodes8or27>
    </MPM>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 100000.0  </reference_pressure>
    </PhysicalConstants>

    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <MPM>
         <material>
                <burn type = "null"/>
               <EOS type = "ideal_gas">
                  <gas_constant>      287             </gas_constant>
               </EOS>
              <density>             8900.0         </density>
              <constitutive_model type = "comp_neo_hook_plastic">
                  <useModifiedEOS>     true          </useModifiedEOS>
                  <bulk_modulus>      117.0e9        </bulk_modulus>
                  <shear_modulus>      43.8e9        </shear_modulus>
                  <yield_stress>       70.0e6        </yield_stress>
                  <hardening_modulus>  43.8e5        </hardening_modulus>
                  <alpha>               0.0          </alpha>
                  <strain_limit>       3.05          </strain_limit>
                  <reduction_factor>   1.0           </reduction_factor>
              </constitutive_model>
              <toughness>               10.e6         </toughness>
                <thermal_conductivity>  400.0         </thermal_conductivity>
                <specific_heat>         386.0         </specific_heat>
               <gamma>                  1.4           </gamma>   
               
                
               <geom_object>
                    <tri>
                      <name>inputs/MPMICE/complex_MPM_geometries/trwnozzlentop3_01mesh</name>
                    </tri>
                    <res>              [2,2,2]          </res>
                    <velocity>         [0.,0.,0.]       </velocity>
                    <temperature>      300.             </temperature>
                </geom_object>
         </material>
            <contact>
              <type>null</type>
              <vel_fields>              [0,0,0]         </vel_fields>
            </contact>
      </MPM>
       <ICE>
         <material>
            <burn type = "null"/>
           <EOS type = "ideal_gas">
              <gas_constant>            287             </gas_constant>
           </EOS>
           <viscosity>                  0.000015        </viscosity>
           <thermal_conductivity>       0.0             </thermal_conductivity>
           <specific_heat>              716.0           </specific_heat>
           <speed_of_sound>             347.0           </speed_of_sound>
           <gamma>                      1.4             </gamma>           
           <geom_object>
              <difference>
                <box label = "the whole computational domain">
                    <min>           [-10.0,-10.0,-10.0]    </min>
                    <max>           [ 10.0, 10.0, 10.0]   </max>
                </box>
              <tri label = "gpicehole">
                   <name>inputs/MPMICE/complex_MPM_geometries/trwnozzlentop3_01mesh</name>
                </tri>
              </difference>
               <res>                      [2,2,2]       </res>
               <velocity>           [0.0,0.0,0.0]    </velocity>
               <temperature>        300.0               </temperature>
               <density>    1.1614402e+00   </density>
               <pressure>           100000.0            </pressure>
           </geom_object>
         </material>
      </ICE>       
       
      <exchange_properties> 
         <exchange_coefficients>
              <momentum>               [1e15]         </momentum>
              <heat>                   [1e15]         </heat>
        </exchange_coefficients>
     </exchange_properties> 
    </MaterialProperties>
            

    <PhysicalBC>
       <MPM>
      </MPM>
    </PhysicalBC>
       
</Uintah_specification>

