<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->
<!-- @version: Updated 7/31/00-->
<Uintah_specification>

  <!-- Impact of a corrugated copper plates with a hypoelastic plate -->

  <Meta>
    <title>Corrugated Plate Impact</title>
  </Meta>

  <Time>
    <maxTime>0.006</maxTime>
    <initTime>0.0</initTime>
    <delt_min>1.0e-10</delt_min>
    <delt_max>1.0</delt_max>
    <timestep_multiplier>0.8</timestep_multiplier>
  </Time>

  <DataArchiver>
    <filebase>test_corrug_plate.uda</filebase>
    <outputTimestepInterval>2</outputTimestepInterval>
    <save label = "KineticEnergy"/>
    <save label = "TotalMass"/>
    <save label = "StrainEnergy"/>
    <save label = "CenterOfMassPosition"/>
    <save label = "CenterOfMassVelocity"/>
    <save label = "p.x"/>
    <save label = "p.velocity"/>
    <save label = "p.mass"/>
    <save label = "p.volume"/>
    <save label = "p.stress"/>
    <save label = "p.deformationMeasure"/>
    <!--
    <save label = "p.strainRate"/>
    <save label = "p.plasticStrain"/>
    <save label = "p.plasticTemp"/>
    <save label = "p.damage"/>
    <save label = "p.porosity"/>
    -->
    <save label = "g.mass"/>
    <save label = "g.volume"/>
    <save label = "g.stressFS"/>
    <checkpoint cycle = "2" interval = "2.0e-6"/>
  </DataArchiver>

  <MPM>
    <time_integrator>explicit</time_integrator>
    <nodes8or27>8</nodes8or27>
    <minimum_particle_mass> 1.0e-8</minimum_particle_mass>
    <maximum_particle_velocity> 1.0e8</maximum_particle_velocity>
    <artificial_damping_coeff> 0.0 </artificial_damping_coeff>
    <artificial_viscosity> false </artificial_viscosity>
    <accumulate_strain_energy> false </accumulate_strain_energy>
    <use_load_curves> false </use_load_curves>
    <erosion algorithm = "none"/>
  </MPM>

  <PhysicalConstants>
    <gravity>[0,0,0]</gravity>
  </PhysicalConstants>

  <MaterialProperties>
    <MPM>
      <material>
        <burn type = "null" />
        <density>1.e9</density>
        <toughness>10.e12</toughness>
        <thermal_conductivity>1.0</thermal_conductivity>
        <specific_heat>400</specific_heat>
        <constitutive_model type="hypo_elastic">
          <G>12.e12</G>
          <K>32.e12</K>
        </constitutive_model>
        <geom_object>
          <corrugated> 
            <xymin>      [1.0,5.0,0.0]    </xymin> 
            <xymax>      [21.0,10.0,0.0]  </xymax> 
            <thickness>  5.0              </thickness>
            <normal>     [0.0,0.0,1.0]    </normal>
            <corr_edge>  y+               </corr_edge>
            <curve>      sin              </curve>
            <wavelength> 2.0              </wavelength>
            <amplitude>  0.5              </amplitude>
          </corrugated>
          <res>[2,2,2]</res>
          <velocity>[0.0,500.0,0.0]</velocity>
          <temperature>294</temperature>
        </geom_object>
        <velocity_field>0</velocity_field>
      </material>
      <material>
        <burn type = "null" />
        <gamma> 1.4 </gamma>           
        <density>2.e3</density>
        <toughness>10.e12</toughness>
        <thermal_conductivity>1.0</thermal_conductivity>
        <specific_heat>400</specific_heat>
        <constitutive_model type="hypo_elastic">
          <G>12.e8</G>
          <K>32.e8</K>
        </constitutive_model>
        <geom_object>
          <corrugated> 
            <xymin>      [1.0,11.0,0.0]   </xymin> 
            <xymax>      [21.0,21.0,0.0]  </xymax> 
            <thickness>  5.0              </thickness>
            <normal>     [0.0,0.0,1.0]    </normal>
            <corr_edge>  y-               </corr_edge>
            <curve>      sin              </curve>
            <wavelength> 2.0              </wavelength>
            <amplitude>  0.5              </amplitude>
          </corrugated>
          <res>[3,3,3]</res>
          <velocity>[0.0,0.0,0.0]</velocity>
          <temperature>294</temperature>
        </geom_object>
        <velocity_field>1</velocity_field>
      </material>
      <contact>
        <type>rigid</type>
        <vel_fields>[0,0,0]</vel_fields>
        <direction>[0,1,0]</direction>
        <stop_time>999999.9</stop_time>
      </contact>
    </MPM>
  </MaterialProperties>
	
  <Grid>
    <Level>
      <Box label = "1">
        <lower>[-1.0,-1.0,-2.5]</lower>
        <upper>[22.0,21.0,2.5]</upper>
        <resolution>[23,23,5]</resolution>
        <patches>[2,2,1]</patches>
        <!--
        <resolution>[92,92,8]</resolution>
        <resolution>[115,115,10]</resolution>
        <patches>[4,4,2]</patches>
        <extraCells>[1,1,1]</extraCells>
        -->
      </Box>
    </Level>
    <BoundaryConditions>
      <Face side = "x-">
        <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "all" var = "Dirichlet" label = "Velocity">
          <value> [0.0,0.0,0.0] </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "all" var = "Dirichlet" label = "Velocity">
          <value> [0.0,0.0,0.0] </value>
        </BCType>
      </Face>                  
      <Face side = "z-">
        <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
      </Face>                  
      <Face side = "z+">
        <BCType id = "all" var = "symmetry" label = "Symmetric"> </BCType>
      </Face>         	 
    </BoundaryConditions>
  </Grid>

  <PhysicalBC>
    <MPM>
    </MPM>
  </PhysicalBC>
    
</Uintah_specification>

