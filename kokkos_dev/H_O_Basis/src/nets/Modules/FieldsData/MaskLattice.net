set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {41 26 504 399}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 108 26]

# Create a SCIRun->Render->Viewer Module
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 93 355]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 41 131]

# Create a SCIRun->FieldsData->MaskLattice Module
set m4 [addModuleAtPosition "SCIRun" "FieldsData" "MaskLattice" 339 126]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 229 220]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m4 0]
set c2 [addConnection $m1 0 $m3 0]
set c3 [addConnection $m4 0 $m5 0]
set c4 [addConnection $m3 0 $m2 0]
set c5 [addConnection $m5 0 $m2 1]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module
setGlobal $m1-sizex {20}
setGlobal $m1-sizey {20}
setGlobal $m1-sizez {20}
setGlobal $m1-data-at {Cells}

