<?xml version='1.0' encoding='ISO-8859-1' ?>

<Uintah_Include>
  <defaultValue name="heat_loss">             0.0</defaultValue>
  <defaultValue name="Fvar">                  0.0</defaultValue>
  <constantValue name="viscosity">            0.000020</constantValue>   
  <constantValue name="thermal_conductivity"> 0.1     </constantValue>   
  <constantValue name="reference_Temp">       298.0   </constantValue>
  
  <derivedValue name="heat_capac_Cv">       [heat_capacity]-8314*[mix_mol_wt]         </derivedValue>
  <derivedValue name="gamma">               [heat_capacity]/[heat_capac_Cv]           </derivedValue>

  <derivedValue name="reference_heat_capac_Cp">1007.34*(1-{mix_frac}) + 2171.5*{mix_frac}           </derivedValue>
  <derivedValue name="reference_mix_mol_wt">    0.0346616*(1-{mix_frac}) + 0.0623334*{mix_frac}     </derivedValue>
  <derivedValue name="reference_heat_capac_Cv">[reference_heat_capac_Cp]-8314*[reference_mix_mol_wt]</derivedValue>
  <derivedValue name="reference_gamma">        [reference_heat_capac_Cp]/[reference_heat_capac_Cv]  </derivedValue>
  <filename>inputs/ARCHES/NewStaticMixingTables/methane/methane_hl_equil.rxn</filename>
</Uintah_Include>


