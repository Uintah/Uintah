set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {64 41 441 447}

# Create a SCIRun->FieldsCreate->GatherFields Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "GatherFields" 142 183]

# Create a SCIRun->Visualization->ShowField Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 113 293]

# Create a SCIRun->DataIO->FieldReader Module
set m3 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 64 41]

# Create a SCIRun->DataIO->FieldReader Module
set m4 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 276 41]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 150 403]

# Create the Connections between Modules
set c1 [addConnection $m3 0 $m1 0]
set c2 [addConnection $m1 0 $m2 0]
set c3 [addConnection $m2 0 $m5 0]
set c4 [addConnection $m4 0 $m1 1]

# Setup GUI for the SCIRun->FieldsCreate->GatherFields Module

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m2-edges-on {0}
setGlobal $m2-faces-on {0}
setGlobal $m2-nodes-usedefcolor {1}
setGlobal $m2-normalize-vectors {}
setGlobal $m2-has_scalar_data {1}
setGlobal $m2-def-color-r {1.00}
setGlobal $m2-def-color-g {1.00}
setGlobal $m2-def-color-b {1.00}
setGlobal $m2-def-color-a {0.50}
setGlobal $m2-node_display_type {Spheres}
setGlobal $m2-node_scale {0.0300}
setGlobal $m2-edge_scale {0.0150}
setGlobal $m2-show_progress {}
setGlobal $m2-show_progress {}

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m3-filename "$DATADIR/$DATASET/$DATASET-dipole.pcv.fld"

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m4-filename "$DATADIR/$DATASET/$DATASET-electrodes.pcd.fld"

