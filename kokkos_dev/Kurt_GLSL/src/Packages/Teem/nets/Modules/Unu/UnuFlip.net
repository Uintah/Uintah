# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {26 6 334 484}
set creationDate {Fri Dec 02 2005}
set creationTime {14:45:14}
set runDate {}
set runTime {}
set notes {}
set geometry {800x800+0+0}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 48 6]

# Create a SCIRun->Render->Viewer Module
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 35 440]

# Create a Teem->Converters->FieldToNrrd Module
set m3 [addModuleAtPosition "Teem" "Converters" "FieldToNrrd" 132 84]

# Create a Teem->Converters->NrrdToField Module
set m4 [addModuleAtPosition "Teem" "Converters" "NrrdToField" 26 285]

# Create a SCIRun->Visualization->Isosurface Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 42 362]

# Create a Teem->UnuAtoM->UnuFlip Module
set m6 [addModuleAtPosition "Teem" "UnuAtoM" "UnuFlip" 169 175]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m3 0]
set c2 [addConnection $m5 1 $m2 0]
set c3 [addConnection $m3 2 $m6 0]
set c4 [addConnection $m4 0 $m5 0]
set c5 [addConnection $m6 0 $m4 2]

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m1-filename "$DATADIR/brain-dmw/brain-dmw-mri-16.lvd.fld"

$m2 addViewer
set $m2-ViewWindow_0-geometry 654x611+627+234

