# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {17 5 498 484}
set creationDate {Sat Mar 05 2005}
set creationTime {14:04:18}
set runDate {}
set runTime {}
set notes {}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 268 5]

# Create a SCIRun->Visualization->Isosurface Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 17 84]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 35 440]

# Create a Teem->Converters->FieldToNrrd Module
set m4 [addModuleAtPosition "Teem" "Converters" "FieldToNrrd" 268 80]

# Create a Teem->Converters->NrrdToField Module
set m5 [addModuleAtPosition "Teem" "Converters" "NrrdToField" 268 291]

# Create a SCIRun->Visualization->Isosurface Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 268 356]

# Create a Teem->UnuNtoZ->UnuPad Module
set m7 [addModuleAtPosition "Teem" "UnuNtoZ" "UnuPad" 333 181]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m4 0]
set c3 [addConnection $m2 1 $m3 0]
set c4 [addConnection $m4 2 $m7 0]
set c5 [addConnection $m5 0 $m6 0]
set c6 [addConnection $m6 1 $m3 1]
set c7 [addConnection $m7 0 $m5 2]

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m1-filename "$DATADIR/brain-dmw/brain-dmw-mri-16.lvd.fld"

