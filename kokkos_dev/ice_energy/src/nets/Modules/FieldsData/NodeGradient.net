# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {34 21 256 494}
set creationDate {Fri Jun 25 2004}
set creationTime {14:32:43}
set runDate {}
set runTime {}
set notes {}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 34 21]

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 91 95]

# Create a SCIRun->FieldsData->DirectInterpolate Module
set m3 [addModuleAtPosition "SCIRun" "FieldsData" "DirectInterpolate" 43 170]

# Create a SCIRun->Render->Viewer Module
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 59 450]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 50 359]

# Create a SCIRun->FieldsData->NodeGradient Module
set m6 [addModuleAtPosition "SCIRun" "FieldsData" "NodeGradient" 46 267]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m3 0]
set c3 [addConnection $m3 0 $m6 0]
set c4 [addConnection $m6 0 $m5 0]
set c5 [addConnection $m5 0 $m4 0]
set c6 [addConnection $m2 0 $m3 1]

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"
set $m1-filetype {SCIRun Field File (*.fld)}

# Set GUI variables for the SCIRun->FieldsData->DirectInterpolate Module
set $m3-exhaustive_search {0}

$m4 addViewer
set $m4-ViewWindow_0-geometry 654x648+569+238

