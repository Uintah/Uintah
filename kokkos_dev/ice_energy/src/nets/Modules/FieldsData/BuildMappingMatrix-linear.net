# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {52 10 530 549}
set creationDate {Tue Jan 25 2005}
set creationTime {22:25:11}
set runDate {}
set runTime {}
set notes {}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 65 10]

# Create a SCIRun->Visualization->ShowField Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 94 423]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 108 505]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 335 147]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 287 345]

# Create a SCIRun->FieldsCreate->SamplePlane Module
