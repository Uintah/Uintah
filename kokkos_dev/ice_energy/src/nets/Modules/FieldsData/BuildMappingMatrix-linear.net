# Create a SCIRun->FieldsData->BuildInterpMatrix Module
set m7 [addModuleAtPosition "SCIRun" "FieldsData" "BuildInterpMatrix" 149 189]

# Create a SCIRun->FieldsData->ApplyInterpMatrix Module
set m8 [addModuleAtPosition "SCIRun" "FieldsData" "ApplyInterpMatrix" 52 266]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m6 0]
set c2 [addConnection $m1 0 $m8 0]
set c3 [addConnection $m1 0 $m7 0]
set c4 [addConnection $m8 0 $m2 0]
set c5 [addConnection $m4 0 $m5 0]
