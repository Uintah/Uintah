set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {20 14 713 542}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 47 14]

# Create a SCIRun->FieldsCreate->Probe Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "Probe" 228 97]

# Create a SCIRun->FieldsData->ManageFieldData Module
set m3 [addModuleAtPosition "SCIRun" "FieldsData" "ManageFieldData" 312 211]

# Create a SCIRun->Render->Viewer Module
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 99 498]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 20 243]

# Create a SCIRun->Visualization->Isosurface Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 208 335]

# Create a SCIRun->Visualization->ShowField Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 366 421]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m8 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 518 150]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m9 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 465 268]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m1 0 $m6 0]
set c3 [addConnection $m1 0 $m5 0]
set c4 [addConnection $m2 1 $m3 0]
set c5 [addConnection $m8 0 $m9 0]
set c6 [addConnection $m6 0 $m7 0]
set c7 [addConnection $m5 0 $m4 0]
set c8 [addConnection $m1 0 $m9 1]
set c9 [addConnection $m2 0 $m4 1]
set c10 [addConnection $m9 0 $m7 1]
set c11 [addConnection $m3 1 $m6 2]
set c12 [addConnection $m7 0 $m4 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

# Setup GUI for the SCIRun->FieldsCreate->Probe Module
