<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE component SYSTEM "component.dtd">
<?xml-stylesheet href="component.xsl" type="text/xsl"?>
<?cocoon-process type="xslt"?>

<component name="MappingMatrixToMaskVector" category="Math">
  <overview>
    <authors>
      <author>McKay Davis</author>
    </authors>
    <summary>
      Converts a Mapping Matrix to a Mask Vector
    </summary>
    <description>
      <p>Converts a Mapping Matrix to a Mask Vector</p>
    </description>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>MappingMatrix</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>MaskVector</name>
        <datatype>SCIRun::Nrrd</datatype>
      </port>
    </outputs>
  </io>
</component>

