<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE component SYSTEM "../../../../Dataflow/XML/component.dtd">
<component name="ParameterListMatrix" category="Script">
  <overview>
    <authors>
      <author>Jeroen Stinstra</author>
    </authors>
    <summary>
      This module gets a matrix from a ParameterList.
    </summary>
    <description>
      This module gets a matrix from a ParameterList. Use the GUI to select the parameter.
    </description>
  </overview>
  <implementation>
  </implementation>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>ParameterList</name>
        <datatype>SCIRun::Bundle</datatype>
      </port>
      <port>
        <name>Matrix</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>ParameterList</name>
        <datatype>SCIRun::Bundle</datatype>
      </port>
      <port>
        <name>Matrix</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </outputs>
  </io>
  <gui>
    <description>
      Use the GUI select which parameter needs to be extracted.
    </description>
    <parameter>
      <widget>Place Widget Name Here</widget>
      <label>Place Label Name Here</label>
    </parameter>
  </gui>
  <testing>
  </testing>
</component>

