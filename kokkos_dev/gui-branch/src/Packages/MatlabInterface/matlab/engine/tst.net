global runTime
set runTime " 10:0:29"

set bbox {10.0 10.0 166 258}

set m0 [addModuleAtPosition "MatlabInterface" "DataIO" "Matlab" 14 95]
set m1 [addModuleAtPosition "SCIRun" "DataIO" "MatrixWriter" 22 201]
