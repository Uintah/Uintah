       <Level>
           <Box label="1">
              <lower>        [-0.0159,-0.0159,0.0]    </lower>
              <upper>        [0.0159,0.0159,0.003]    </upper>
                <extraCells>   [1,1,1]    </extraCells>
              <patches>      [10,10,5]    </patches>
                <resolution>[212,212,20]</resolution>
           </Box>
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>annulus4.0.uda</filebase>
      <outputInterval>1.5e-6</outputInterval>
     <!--
       <compression>gzip</compression>
     -->
      <save label="rho_CC"/>
      <save label="press_CC"/>
      <save label="press_equil_CC"/>
      <save label="temp_CC"/>
      <save label="int_eng_L_CC"/>
      <save label="mom_L_ME_CC"/>
      <save label = "KineticEnergy"/>
      <save label = "TotalMass"/>
      <save label = "StrainEnergy"/>
      <save label = "CenterOfMassPosition"/>
      <save label = "CenterOfMassVelocity"/>
      <save label = "p.x"/>
      <save label = "p.volume"/>
      <save label = "p.mass"/>
      <save label = "p.stress"/>
      <save label = "p.pressure"/>
      <save label = "p.strainEnergy"/>
      <save label = "p.isBroken"/>
      <save label = "g.mass"/>
      <save label = "g.stressFS"/>

        <checkpoint cycle = "2" interval = "6.e-5"/>
   </DataArchiver>

   <Debug>
     <dbg_timeStart>       0.00   </dbg_timeStart>
     <dbg_timeStop>        3.0e-4 </dbg_timeStop>
     <dbg_outputInterval>  1e-9   </dbg_outputInterval>
