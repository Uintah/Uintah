set m11 [addModuleAtPosition "SCIRun" "Render" "Viewer" 611 252]
set m12 [addModuleAtPosition "SCIRun" "Fields" "BuildInterpolant" 398 378]
set m13 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 511 297]
set m14 [addModuleAtPosition "SCIRun" "Fields" "ApplyInterpolant" 380 437]
set m15 [addModuleAtPosition "SCIRun" "Fields" "ManageFieldData" 380 299]
set m16 [addModuleAtPosition "SCIRun" "Fields" "ManageFieldData" 380 496]
