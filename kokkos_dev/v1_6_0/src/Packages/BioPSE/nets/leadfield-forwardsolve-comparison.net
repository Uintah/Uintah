addConnection $m0 0 $m1 0
addConnection $m2 1 $m1 1
addConnection $m3 0 $m2 0
addConnection $m3 0 $m4 0
addConnection $m2 0 $m5 1
addConnection $m7 0 $m8 0
addConnection $m8 0 $m2 1
addConnection $m9 0 $m10 0
addConnection $m3 0 $m9 0
addConnection $m8 1 $m11 0
addConnection $m10 0 $m11 1
addConnection $m1 0 $m6 0
addConnection $m13 0 $m12 1
addConnection $m12 0 $m14 1
addConnection $m3 0 $m15 0
addConnection $m5 0 $m15 1
addConnection $m15 0 $m14 0
addConnection $m14 0 $m16 0
addConnection $m16 1 $m6 1
addConnection $m17 0 $m12 0
addConnection $m4 0 $m18 1
addConnection $m3 0 $m18 0
addConnection $m18 0 $m5 0

set $m0-filename $DATADIR/$DATASET/generated/$DATASET-leadfield.mat
set $m1-row 0
set $m1-row_max 148
set $m1-col 0
set $m1-col_max 18380
set $m1-row_or_col {col}
set $m1-animate 0
set $m3-filename $DATADIR/$DATASET/$DATASET-mesh.tvt.fld
set $m4-UseCondTCL 1
