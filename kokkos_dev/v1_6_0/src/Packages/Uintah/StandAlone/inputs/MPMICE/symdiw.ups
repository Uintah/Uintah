<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- @version: -->
<Uintah_specification>

   <Meta>
       <title>Advection test for a MPM and ICE material</title>
   </Meta>
    
    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            2.0       </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
       <delt_max>           1.0         </delt_max>
       <delt_init>          1.0e-4      </delt_init>
       <timestep_multiplier>0.8         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face side = "x-">
        <BCType id = "0"  var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Dirichlet" label = "Velocity">
                              <value> [10.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0" var = "Dirichlet" label = "Pressure">
                              <value> 101325.0   </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0" var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Dirichlet" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0" var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Dirichlet" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "0" var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Dirichlet" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>                  
      <Face side = "z+">
        <BCType id = "0"   label = "Symmetric" var = "symmetry">
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label = "1">
              <lower>        [0,0,0]    </lower>
              <upper>        [4.5,2.5,1.25]    </upper>
                <extraCells>   [1,1,1]    </extraCells>
              <patches>      [2,2,1]    </patches>
           </Box>
           <spacing>        [0.125,0.125,0.125]  </spacing>
       </Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>symdiw.uda</filebase>
      <outputInterval>.01</outputInterval>
      <save label = "mom_L_ME_CC"/>
      <save label = "vel_CC"/>
      <save label = "press_CC"/>
      <save label = "press_equil_CC"/>
      <save label = "rho_CC"/>
      <save label = "vol_frac_CC"/>
      <save label = "temp_CC"/>
      <save label = "p.x"/>
      <save label = "p.volume"/>
      <save label = "p.velocity"/>
      <save label = "p.temperature"/>
      <save label = "StrainEnergy"/>
   </DataArchiver>

   <Debug>
     <dbg_timeStart>       0.00     </dbg_timeStart>
     <dbg_timeStop>        2.0     </dbg_timeStop>
     <dbg_outputInterval>  1e-9    </dbg_outputInterval>
   <!--
     <dbg_BeginIndex>      [0,0,0] </dbg_BeginIndex> 
     <dbg_EndIndex>        [2,2,2] </dbg_EndIndex>   
     <dbg_SigFigs>         10      </dbg_SigFigs>    
    -->
     <!--
     <debug label = "switchDebugInitialize"/>
     <debug label = "switchDebug_EQ_RF_press"/>
     <debug label = "switchDebug_vel_FC"/>           
     <debug label = "switchDebug_explicit_press"/>   
     <debug label = "switchDebug_PressFC"/>          
     <debug label = "switchDebugLagrangianValues"/> 
     <debug label = "switchDebugMomentumExchange_CC"/> 
     <debug label = "switchDebugSource_Sink"/>       
     <debug label = "switchDebug_advance_advect"/>  
     -->
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.5</cfl>
       <ICE>
        <max_iteration_equilibration> 100 </max_iteration_equilibration>
        <advection type = "FirstOrder" />
        <solution technique = "EqForm" />
      </ICE>        
    </CFD>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>


    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <MPM>
         <material>
                <burn type = "null" />
                <EOS type = "ideal_gas">
                    <gas_constant>      287             </gas_constant>
                </EOS>
                <density>   1.1792946927374306000e+00   </density>
                <constitutive_model type="comp_neo_hook">
                    <bulk_modulus>      40000.0          </bulk_modulus>
                    <shear_modulus>     30000.0          </shear_modulus>
                </constitutive_model>
                <toughness>             10.e6           </toughness>
                <thermal_conductivity>  400.0           </thermal_conductivity>
                <specific_heat>         716.0           </specific_heat>
               <gamma>                 1.4             </gamma>           
                <geom_object>
                    <box label = "gpmpm">
                        <min>           [1.0,0.0,1.00]   </min>
                        <max>           [1.5,1.5,1.25]   </max>
                    </box>
                    <res>               [2,2,2]         </res>
                    <velocity>          [0.0,0.0,0.0]   </velocity>
                    <temperature>       300             </temperature>
                </geom_object>
         </material>

            <contact>
              <type>null</type>
              <vel_fields>              [0,0,0]         </vel_fields>
            </contact>
      </MPM>
       <ICE>
         <material>
            <burn type = "null" />
            <EOS type = "ideal_gas">
                 <gas_constant>      287             </gas_constant>
            </EOS>
           <viscosity>                  0.0             </viscosity>
           <thermal_conductivity>       0.0             </thermal_conductivity>
           <specific_heat>              716.0           </specific_heat>
           <speed_of_sound>             376             </speed_of_sound>
           <gamma>                      1.4             </gamma>           
           <geom_object>
              <difference>
                <box label = "gpice">
                    <min>           [-1.0,-1.0,-1.0]    </min>
                    <max>           [5.0,4.0,4.0 ]      </max>
                </box>
              <box label = "gpicehole">
                    <min>           [1.0,0.0,1.00]       </min>
                    <max>           [1.5,1.5,1.25]       </max>
                </box>
              </difference>
               <res>                      [2,2,2]          </res>
               <velocity>           [0.0,0.0,0.0]      </velocity>
               <temperature>        300.0               </temperature>
               <density>              0.88447102        </density>
               <pressure>           101325.0             </pressure>
           </geom_object>
         </material>
      </ICE>       
       
        <exchange_properties>
         <exchange_coefficients>
               <momentum>               [1e15]         </momentum>
               <heat>                  [1e4]           </heat>
         </exchange_coefficients>
        </exchange_properties>
    </MaterialProperties>
            

    <PhysicalBC>
       <MPM>
      </MPM>
    </PhysicalBC>
       
</Uintah_specification>
