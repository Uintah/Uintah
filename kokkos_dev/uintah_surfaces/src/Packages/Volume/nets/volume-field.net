# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {19 17 486 456}
set creationDate {Tue Jun 29 2004}
set creationTime {10:22:08}
set runDate {}
set runTime {}
set notes {}

# Create a Volume->Visualization->VolumeVisualizer Module
set m1 [addModuleAtPosition "Volume" "Visualization" "VolumeVisualizer" 98 301]

# Create a Volume->Visualization->TextureBuilder Module
set m2 [addModuleAtPosition "Volume" "Visualization" "TextureBuilder" 98 178]

# Create a SCIRun->DataIO->FieldReader Module
set m3 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 19 17]

# Create a SCIRun->DataIO->FieldReader Module
set m4 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 200 17]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 98 412]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 291 177]

# Create the Connections between Modules
set c1 [addConnection $m3 0 $m2 0]
set c2 [addConnection $m2 0 $m1 0]
set c3 [addConnection $m1 0 $m5 0]
set c4 [addConnection $m4 0 $m2 1]
set c5 [addConnection $m6 0 $m1 1]

# Set GUI variables for the Volume->Visualization->VolumeVisualizer Module
set $m1-num_slices {272}
set $m1-alpha_scale {0.0}
set $m1-shading {1}
set $m1-ambient {0.212}
set $m1-diffuse {0.465}
set $m1-specular {0.223}
set $m1-shine {25}

# Open the Volume->Visualization->VolumeVisualizer UI
$m1 initialize_ui

# Set GUI variables for the Volume->Visualization->TextureBuilder Module
set $m2-vmin {0.0}
set $m2-vmax {1300.0}
set $m2-gmin {0.0}
set $m2-gmax {371.835313008}

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m3-filename "$DATADIR/volume/$DATASET-value.fld"
set $m3-types {{{{SCIRun Field File} {.fld} } {{SCIRun Field Any} {.*} } {{TextCurveField} {{.pts} {.edge}} } {{TextHexVolField} {{.pts} {.hex}} } {{TextPointCloudField} {.pts} } {{TextPointCloudString} {.pcs.txt} } {{TextQuadSurfField} {{.pts} {.quad}} } {{TextStructCurveField} {.pts} } {{TextStructHexVolField} {.pts} } {{TextStructQuadSurfField} {.pts} } {{TextTetVolField} {{.pts} {.tet}} } {{TextTriSurfField} {{.pts} {.fac} {.tri}} } {{VTKtoTriSurfField} {.vtk} } {{VTKtoTriSurfField-swap} {.vtk} } }}
set $m3-filetype {SCIRun Field File (*.fld)}

# Set GUI variables for the SCIRun->DataIO->FieldReader Module
set $m4-filename "$DATADIR/volume/$DATASET-gradient.fld"
set $m4-types {{{{SCIRun Field File} {.fld} } {{SCIRun Field Any} {.*} } {{TextCurveField} {{.pts} {.edge}} } {{TextHexVolField} {{.pts} {.hex}} } {{TextPointCloudField} {.pts} } {{TextPointCloudString} {.pcs.txt} } {{TextQuadSurfField} {{.pts} {.quad}} } {{TextStructCurveField} {.pts} } {{TextStructHexVolField} {.pts} } {{TextStructQuadSurfField} {.pts} } {{TextTetVolField} {{.pts} {.tet}} } {{TextTriSurfField} {{.pts} {.fac} {.tri}} } {{VTKtoTriSurfField} {.vtk} } {{VTKtoTriSurfField-swap} {.vtk} } }}
