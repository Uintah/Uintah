set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {38 35 567 320}

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 113 35]

# Create a SCIRun->Render->Viewer Module
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 199 276]

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 38 150]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 214 147]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 402 151]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m3 0]
set c2 [addConnection $m1 0 $m4 0]
set c3 [addConnection $m1 0 $m5 0]
set c4 [addConnection $m3 0 $m2 0]
set c5 [addConnection $m4 0 $m2 1]
set c6 [addConnection $m5 0 $m2 2]

# Setup GUI for the SCIRun->FieldsCreate->SamplePlane Module
setGlobal $m1-sizex {3}
setGlobal $m1-sizey {5}
$m1 initialize_ui

