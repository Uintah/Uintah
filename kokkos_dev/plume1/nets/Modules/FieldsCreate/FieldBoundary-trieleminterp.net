# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {55 16 581 497}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 55 16]

# Create a SCIRun->FieldsCreate->ClipByFunction Module
set m2 [addModuleAtPosition "SCIRun" "FieldsCreate" "ClipByFunction" 64 110]

# Create a SCIRun->FieldsCreate->FieldBoundary Module
set m3 [addModuleAtPosition "SCIRun" "FieldsCreate" "FieldBoundary" 108 193]

# Create a SCIRun->FieldsData->ApplyInterpMatrix Module
set m4 [addModuleAtPosition "SCIRun" "FieldsData" "ApplyInterpMatrix" 86 289]

# Create a SCIRun->Render->Viewer Module
set m5 [addModuleAtPosition "SCIRun" "Render" "Viewer" 220 453]

# Create a SCIRun->Visualization->ShowField Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 220 376]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 386 18]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m8 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 329 108]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m2 0 $m3 0]
set c3 [addConnection $m2 0 $m4 0]
set c4 [addConnection $m4 0 $m6 0]
set c5 [addConnection $m7 0 $m8 0]
set c6 [addConnection $m6 0 $m5 0]
set c7 [addConnection $m1 0 $m8 1]
set c8 [addConnection $m3 0 $m4 1]
set c9 [addConnection $m8 0 $m6 1]
set c10 [addConnection $m3 1 $m4 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/utahtorso-lowres/utahtorso-lowres-mesh.tvt.fld"

# Setup GUI for the SCIRun->FieldsCreate->ClipByFunction Module
setGlobal $m2-clipfunction {z > 350}

