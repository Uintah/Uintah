set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {17 18 651 492}

# Create a SCIRun->FieldsCreate->SampleLattice Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleLattice" 84 18]

# Create a SCIRun->FieldsData->TransformData Module
set m2 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 87 104]

# Create a SCIRun->FieldsCreate->SampleField Module
set m3 [addModuleAtPosition "SCIRun" "FieldsCreate" "SampleField" 486 188]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 17 274]

# Create a SCIRun->Visualization->StreamLines Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "StreamLines" 278 278]

# Create a SCIRun->Visualization->ShowField Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 247 360]

# Create a SCIRun->Render->Viewer Module
set m7 [addModuleAtPosition "SCIRun" "Render" "Viewer" 108 448]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m2 0]
set c2 [addConnection $m2 0 $m3 0]
set c3 [addConnection $m2 0 $m4 0]
set c4 [addConnection $m2 0 $m5 0]
set c5 [addConnection $m4 0 $m7 0]
set c6 [addConnection $m5 0 $m6 0]
set c7 [addConnection $m3 0 $m5 1]
set c8 [addConnection $m6 0 $m7 1]
set c9 [addConnection $m3 1 $m7 2]

# Setup GUI for the SCIRun->FieldsCreate->SampleLattice Module
setGlobal $m1-sizex {5}
setGlobal $m1-sizey {5}
setGlobal $m1-sizez {5}
setGlobal $m1-data-at {Cells}

# Setup GUI for the SCIRun->FieldsData->TransformData Module
setGlobal $m2-function {result = Cross(Vector(0.0, 0.0, 1.0), Vector(x, y, z));
}
setGlobal $m2-outputdatatype {Vector}

# Setup GUI for the SCIRun->FieldsCreate->SampleField Module
setGlobal $m3-endpoints {1}
setGlobal $m3-endpoint0x {-0.8404415378780508}
setGlobal $m3-endpoint0y {-0.448173367565732}
setGlobal $m3-endpoint0z {-0.8566007056821867}
