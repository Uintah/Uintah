setGlobal $m3-endpoint1z {0.7733945241967832}
setGlobal $m3-widgetscale {0.05196152422706631}
setGlobal $m3-ringstate {}
setGlobal $m3-framestate {}
setGlobal $m3-maxseeds {20}

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m4-nodes-on {0}
setGlobal $m4-edges-transparency {1}
setGlobal $m4-faces-on {0}
setGlobal $m4-vectors-on {1}
setGlobal $m4-normalize-vectors {1}
