# Setup GUI for the SCIRun->Visualization->StreamLines Module
setGlobal $m5-maxsteps {20}
setGlobal $m5-direction {2}
setGlobal $m5-remove-colinear {0}
setGlobal $m5-method {5}
$m5 initialize_ui

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m6-nodes-on {0}
