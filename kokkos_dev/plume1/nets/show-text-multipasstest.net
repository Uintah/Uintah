# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {12 11 503 511}

# Create a SCIRun->DataIO->FieldReader Module
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 12 11]
setGlobal Notes($m1) {}

# Create a SCIRun->Render->Viewer Module
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 54 467]
setGlobal Notes($m2) {}

# Create a SCIRun->Visualization->ShowField Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 97 363]
setGlobal Notes($m3) {}

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 161 191]
setGlobal Notes($m4) {}

# Create a SCIRun->Visualization->RescaleColorMap Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 161 272]
setGlobal Notes($m5) {}

# Create a SCIRun->FieldsCreate->FieldBoundary Module
set m6 [addModuleAtPosition "SCIRun" "FieldsCreate" "FieldBoundary" 37 97]
setGlobal Notes($m6) {}

# Create a SCIRun->Visualization->ShowColorMap Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "ShowColorMap" 338 363]
setGlobal Notes($m7) {}

# Create a SCIRun->FieldsData->ApplyInterpMatrix Module
set m8 [addModuleAtPosition "SCIRun" "FieldsData" "ApplyInterpMatrix" 12 192]

# Create the Connections between Modules
set c1 [addConnection $m1 0 $m6 0]
set c2 [addConnection $m1 0 $m8 0]
set c3 [addConnection $m8 0 $m3 0]
set c4 [addConnection $m4 0 $m5 0]
set c5 [addConnection $m5 0 $m7 0]
set c6 [addConnection $m7 0 $m2 0]
set c7 [addConnection $m6 0 $m8 1]
set c8 [addConnection $m8 0 $m5 1]
set c9 [addConnection $m5 0 $m3 1]
set c10 [addConnection $m3 0 $m2 1]
set c11 [addConnection $m6 1 $m8 2]

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m1-filename "$DATADIR/$DATASET/$DATASET-voltage.tvd.fld"

