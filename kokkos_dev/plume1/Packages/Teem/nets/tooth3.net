   set $m4-maxs {nan nan}
   set $m4-type {nrrdTypeFloat}

   # Set GUI variables for the Teem->UnuAtoM->Unu2op Module
   set $m5-operator {+}

   # Set GUI variables for the Teem->UnuAtoM->Unu1op Module
   set $m6-operator {log}
}


