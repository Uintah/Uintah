<!-- FAQ specification -->

<!ELEMENT para (#PCDATA)>
<!ELEMENT pre (#PCDATA)>
<!ELEMENT title (#PCDATA)>
<!ELEMENT subtitle (#PCDATA)>
<!ELEMENT holder (#PCDATA)>
<!ELEMENT year (#PCDATA)>

<!ELEMENT faqbook (title,subtitle,bookinfo,description,faq+)>

<!ELEMENT bookinfo (authorgroup,copyright)>

<!ELEMENT copyright (holder,year)>

<!ELEMENT faq (title,description,entry+)>
<!ELEMENT description (para+)>

<!ELEMENT entry (keywords, question, answer)>

<!ELEMENT keywords (#PCDATA)>
<!ELEMENT question (para|pre)+>
<!ELEMENT answer (para|pre)+>


