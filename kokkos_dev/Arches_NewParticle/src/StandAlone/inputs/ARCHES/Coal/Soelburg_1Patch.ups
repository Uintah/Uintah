                <resolution>[35,12,12]</resolution>
                <patches>[1,1,1]</patches>
                <extraCells>[1,1,1]</extraCells>
            </Box>
        </Level>
        
    </Grid>
    
    <PhysicalConstants>
        <gravity>[-9.8,0,0]</gravity>
        <reference_point>[-1,-1,-1]</reference_point>
        <viscosity>0.000020</viscosity>
    </PhysicalConstants>
    
    <CFD>
        <!-- ARCHES specification -->
        <ARCHES>
 
            <!-- TIME INTEGRATOR -->
            <TimeIntegrator>
                <ExplicitIntegrator order="second"/>
            </TimeIntegrator>
            
            <!-- Extra Transport Equations -->
            <TransportEqns>

                <Eqn label="mixture_fraction_2" type = "CCscalar">
                  <!-- diffusive scalar -->
                  <doDiff>true</doDiff>
                  <doConv>true</doConv>
                  <conv_scheme>upwind</conv_scheme>
                  <use_density_guess/>
                  <initialization type = "constant">
                    <constant>0.0</constant>
                  </initialization>
                </Eqn>

                <Eqn label = "eta" type = "CCscalar">
                    <doDiff>true</doDiff>
                    <doConv>true</doConv>
                    <addSources>true</addSources>
                    <conv_scheme>upwind</conv_scheme>
                    <src label = "eta_source"/>
                    <initialization type="constant">
                        <constant>0.0</constant>
                    </initialization>
                    <Clipping>
                        <low>0.0</low>
                        <high>1.0</high>
                    </Clipping>
                </Eqn>
                
                <Sources>
                    <src label = "eta_source" type = "coal_gas_devol">
                        <devol_model_name>sarofim</devol_model_name>
                    </src>
                </Sources>
            </TransportEqns>

            <Coal_Properties>
                <C>0.75</C>
                <H>0.05</H>
                <N>0.0</N>
                <O>0.2</O>
                <S>0.0</S>
                <initial_ash_mass>[1e-12,2e-12,3e-12]</initial_ash_mass>
            </Coal_Properties>  

            <DQMOM>
                
                <number_quad_nodes>3</number_quad_nodes>

                <LinearSolver>

                    <type>Optimize</type>
                    <tolerance>1e-5</tolerance>
                    <Optimization>
                      <Optimal_abscissas>[1,-1,1,-1,1,1,1,1,-1]</Optimal_abscissas>
                    </Optimization>

                </LinearSolver>
                
                <adiabGas_nonadiabPart>true</adiabGas_nonadiabPart>
                
                <VelModel type="Balachandar">
                    <kinematic_viscosity>.00001</kinematic_viscosity>
                    <rho_ratio>1000.0</rho_ratio>
                    <regime>2</regime>
                    <L>.2</L>
                    <eta>1.0e-5</eta>
                </VelModel>
                
                <Models>
                    
                    <model label="sarofim" type="KobayashiSarofimDevol">
                        <ICVars>
                            <variable label="pT" role="particle_temperature" />
                            <variable label="RCmass" role="raw_coal_mass" />
                        </ICVars>
                    </model>

                    <model label="heat" type="SimpleHeatTransfer">
                        <ICVars>
                            <variable label="length" role="particle_length" />
                            <variable label="RCmass" role="raw_coal_mass" />
                            <variable label="pT"     role="particle_temperature"/>
                        </ICVars>
                        <!--noRadiation /-->
                    </model>
                    
                </Models>



                <Weights>
                    <!-- w [=] #/m^3 -->
                    <doDiff>false</doDiff>
                    <doConv>true</doConv>
                    <initialization type="env_constant">
                        <env_constant qn="0" value="1e4">
                        </env_constant>
                        <env_constant qn="1" value="1.1e4">
                        </env_constant>
                        <env_constant qn="2" value="1.2e4">
                        </env_constant>
                    </initialization>
                    <conv_scheme>upwind</conv_scheme>
                    <scaling_const>1.0e08</scaling_const>
                    <Clipping>
                        <low>0.0</low>
                        <small>1e-7</small>
                    </Clipping>
                </Weights>
                
                <Ic label="length">
                    <!-- l [=] m -->
                    <doDiff>false</doDiff>
                    <doConv>true</doConv>
                    <initialization type="env_constant">
                        <env_constant qn="0" value="1.0e-5">
                        </env_constant>
                        <env_constant qn="1" value="2.0e-5">
                        </env_constant>
                        <env_constant qn="2" value="3.0e-5">
                        </env_constant>
                    </initialization>
                    <conv_scheme>upwind</conv_scheme>
                    <scaling_const>1.0e-5</scaling_const>
                    <Clipping>
                        <low>0.0</low>
                    </Clipping>
                </Ic>
                
                <Ic label="RCmass">
                    <!-- alpha_c [=] kg -->
                    <doDiff>false</doDiff>
                    <doConv>true</doConv>
                    <model label="sarofim" />
                    <initialization type="env_constant">
                        <env_constant qn="0" value="1e-13">
                        </env_constant>
                        <env_constant qn="1" value="0.5e-13">
                        </env_constant>
                        <env_constant qn="2" value="0.7e-13">
                        </env_constant>
                    </initialization>
                    <conv_scheme>upwind</conv_scheme>
                    <scaling_const>1.0e-10</scaling_const>
                    <Clipping>
                        <low>0.0</low>
                    </Clipping>
                </Ic>

                <Ic label="pT">
                    <!--  T [=] K -->
                    <doDiff>false</doDiff>
                    <doConv>true</doConv>
                    <model label="heat" />
                    <initialization type="env_constant">
                        <env_constant qn="0" value="1800">
                        </env_constant>
                        <env_constant qn="1" value="1900">
                        </env_constant>
                        <env_constant qn="2" value="2000">
                        </env_constant>
                    </initialization>
                    <conv_scheme>upwind</conv_scheme>
                    <scaling_const>1000</scaling_const>
                    <Clipping>
                        <low>0.0</low>
                    </Clipping>
                </Ic>


                <Moment><m>[0,0,0]</m></Moment>
                <Moment><m>[0,0,1]</m></Moment>
                <Moment><m>[0,1,0]</m></Moment>
                <Moment><m>[1,0,0]</m></Moment>
                <Moment><m>[0,1,1]</m></Moment>
                <Moment><m>[1,0,1]</m></Moment>
                <Moment><m>[1,1,0]</m></Moment>
                <Moment><m>[0,1,2]</m></Moment>
                <Moment><m>[0,2,0]</m></Moment>
                <Moment><m>[2,0,0]</m></Moment>
                <Moment><m>[0,0,3]</m></Moment>
                <Moment><m>[0,3,0]</m></Moment>

                <save_moments>true</save_moments>


            </DQMOM>
            
            <Turbulence model="compdynamicprocedure">
                <variance_coefficient>5.0</variance_coefficient>
                <turbulentPrandtlNumber>0.4</turbulentPrandtlNumber>
                <dynamicScalarModel>false</dynamicScalarModel>
                <filter_cs_squared>false</filter_cs_squared>
            </Turbulence>
            
            <Properties>

                <use_mixing_model>false</use_mixing_model>

                <TabProps>
                    <inputfile>Soelburg.h5</inputfile>
                    <strict_mode>false</strict_mode>
                    <diagnostic_mode>false</diagnostic_mode>
                </TabProps>

            </Properties>
            
            <BoundaryConditions>

                <!--secondary inlet --> 
                <!--FlowInlet>
                    <Flow_rate>0.0018</Flow_rate>
                    <mixture_fraction>0.0</mixture_fraction>
                    <geom_object>
                        <cylinder label = "secondaryinlet">
                            <bottom>[-5,.1,.1]</bottom>
                            <top>[0,.1,.1]</top>
                            <radius>0.05</radius>
                        </cylinder>
                    </geom_object>
                </FlowInlet-->
                <!-- primary inlet -->
                <FlowInlet>
                    <Flow_rate>0.27</Flow_rate>
                    <mixture_fraction>0.0</mixture_fraction>
                    <geom_object>
                        <cylinder label="primaryinlet">
                            <bottom>[-5,.1,.1]</bottom>
                            <top>[0,.1,.1]</top>
                            <radius>0.04</radius>
                        </cylinder>
                    </geom_object>
                </FlowInlet>
                
                <WallBC>
                    <geom_object>
                        <box label="botWall">
                            <min>[-1,-1,-1]</min>
                            <max>[0,4,4]</max>
                        </box>
                    </geom_object>
                </WallBC>
                
                <OutletBC>
                    <mixture_fraction>0.26</mixture_fraction>
                    <geom_object>
                        <box label="topWall">
                            <min>[.714,-1,-1]</min>
                            <max>[2,4,4]</max>
                        </box>
                        <box label="posY_xzWall">
                        <min>[0, 0.2,-1]</min>
                        <max>[3, 4, 4]</max>
                        </box>
                        <box label="zeroY_xzWall">
                        <min>[0,-1,-1]</min>
                        <max>[3,0,4]</max>
                        </box>
                        <box label="posZ_xyWall">
                        <min>[0, -1, .2]</min>
                        <max>[3, 4, 4]</max>
                        </box>
                        <box label="zeroZ_xyWall">
                        <min>[0,-1,-1]</min>
                        <max>[3,4,0]</max>
                        </box>
                    </geom_object>
                </OutletBC>
                <!--PressureBC>
                    <geom_object>
                        <box label="posY_xzWall">
                            <min>[0, 0.366,-1]</min>
                            <max>[3, 4, 4]</max>
                        </box>
                        <box label="zeroY_xzWall">
                            <min>[0,-1,-1]</min>
                            <max>[3,0,4]</max>
                        </box>
                        <box label="posZ_xyWall">
                            <min>[0, -1, .366]</min>
                            <max>[3, 4, 4]</max>
                        </box>
                        <box label="zeroZ_xyWall">
                            <min>[0,-1,-1]</min>
                            <max>[3,4,0]</max>
                        </box>
                    </geom_object>
                    <mixture_fraction>0.040</mixture_fraction>
                </PressureBC-->
            </BoundaryConditions>
            
            <ExplicitSolver>
                
                <timeIntegratorType>RK2SSP</timeIntegratorType>
                
                <initial_dt>0.05</initial_dt>
                <variable_dt>true</variable_dt>
                <scalarUnderflowCheck>true</scalarUnderflowCheck>
                
                <PressureSolver>
                    <linear_solver>petsc</linear_solver>
                    <parameters>
                        <res_tol>       1.0e-9</res_tol>
                        <solver>        cg    </solver>
                        <preconditioner>jacobi</preconditioner>
                        <max_iter>      75    </max_iter>
                    </parameters>
                </PressureSolver>
                
                <MomentumSolver>
                    <convection_scheme>central</convection_scheme>
                </MomentumSolver>
                
                <MixtureFractionSolver>
                    <convection_scheme>central-upwind</convection_scheme>
                    <initial_value>0.26</initial_value>
                </MixtureFractionSolver>
                
                <EnthalpySolver>
                    <convection_scheme>central-upwind</convection_scheme>
                    <radiationCalcFreq>5</radiationCalcFreq>
                    <radCalcForAllRKSteps>false</radCalcForAllRKSteps>
                    <DORadiationModel>
                     <opl>3.0</opl>
                     <LinearSolver type="petsc">
                     <res_tol>1.0e-10</res_tol>
                     <ksptype>gmres</ksptype>
                     <pctype>blockjacobi</pctype>
                     <max_iter>75</max_iter>
                     </LinearSolver>
                    </DORadiationModel> 
                </EnthalpySolver>
                
            </ExplicitSolver>
            
        </ARCHES>
    </CFD>
</Uintah_specification>
