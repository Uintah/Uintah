<?xml version='1.0' encoding='ISO-8859-1' ?>
<Uintah_Include>

  <CFD>
    <SPATIALOPS                         spec="OPTIONAL NO_DATA">
      <TimeIntegrator                   spec="REQUIRED NO_DATA">
        <tOrder                         spec="REQUIRED INTEGER '1, 3'"/> 
        <ExplicitIntegrator             spec="REQUIRED NO_DATA">
          <alpha                        spec="REQUIRED VECTOR"/>
          <beta                         spec="REQUIRED VECTOR"/>
        </ExplicitIntegrator>
      </TimeIntegrator>

      <TransportEqns                    spec="OPTIONAL NO_DATA">
        <Sources                        spec="OPTIONAL NO_DATA">

          <src                          spec="MULTIPLE NO_DATA"
                                          attribute1="label REQUIRED STRING"
                                          attribute2="type REQUIRED STRING">
            <RequiredVars               spec="OPTIONAL NO_DATA">
              <variable                 spec="REQUIRED NO_DATA"
                                          attribute1="label REQUIRED STRING"/>
            </RequiredVars>
            <!-- For Constant Source Term -->
            <constant                     spec="OPTIONAL DOUBLE"/>

          </src>
        </Sources>

        <Eqn                            spec="MULTIPLE NO_DATA"
                                          attribute1="label REQUIRED STRING"
                                          attribute2="type REQUIRED STRING">
          <doDiff                       spec="OPTIONAL BOOLEAN"/>
          <doConv                       spec="OPTIONAL BOOLEAN"/>
          <addSources                   spec="OPTIONAL BOOLEAN"/>
          <src                          spec="MULTIPLE NO_DATA"
                                          attribute1="label REQUIRED STRING"/>

          <initial_value                spec="OPTIONAL DOUBLE"/>
          <conv_scheme                   spec="OPTIONAL STRING 'upwind,super_bee,central'"/>
        </Eqn>
      </TransportEqns>

      <DQMOM                            spec="OPTIONAL NO_DATA">
        <number_quad_nodes              spec="REQUIRED INTEGER 'positive'"/>
        <Models                         spec="OPTIONAL NO_DATA">
          <model                        spec="MULTIPLE NO_DATA"
                                          attribute1="label REQUIRED STRING"
                                          attribute2="type  REQUIRED STRING">
            <ICVars                     spec="OPTIONAL NO_DATA">
              <variable                 spec="MULTIPLE NO_DATA"
                                          attribute1="label REQUIRED STRING"/>
            </ICVars>
          </model>
        </Models>
        <Weights                        spec="REQUIRED NO_DATA">
          <doDiff                       spec="OPTIONAL BOOLEAN"/>
          <doConv                       spec="OPTIONAL BOOLEAN"/>
          <initial_value                spec="OPTIONAL DOUBLE"/>
          <conv_scheme                   spec="OPTIONAL STRING 'upwind,super_bee,central'"/>
        </Weights>
        <Ic                             spec="MULTIPLE NO_DATA"
                                          attribute1="label REQUIRED STRING">
          <doDiff                       spec="OPTIONAL BOOLEAN"/>
          <doConv                       spec="OPTIONAL BOOLEAN"/>
          <model                        spec="MULTIPLE NO_DATA"
                                          attribute1="label REQUIRED STRING"/>

          <initial_value                spec="OPTIONAL DOUBLE"/>
          <conv_scheme                   spec="OPTIONAL STRING 'upwind,super_bee,central'"/>
        </Ic>

        <Moment                         spec="MULTIPLE NO_DATA" >
          <m                            spec="REQUIRED MULTIPLE_INTEGERS" /> 
        </Moment>

        <VelModel                       spec="REQUIRED NO_DATA">
          <regime                       spec="REQUIRED INTEGER '1,3'"/>
          <new                          spec="REQUIRED DOUBLE 'positive'"/>
          <eta                          spec="REQUIRED DOUBLE 'positive'"/>
          <rho_ratio                    spec="REQUIRED DOUBLE 'positive'"/>
          <eps                          spec="REQUIRED DOUBLE 'positive'"/>
        </VelModel>
      </DQMOM>

      <lambda                           spec="REQUIRED DOUBLE 'positive'"/>
    </SPATIALOPS>

  </CFD>

</Uintah_Include>
