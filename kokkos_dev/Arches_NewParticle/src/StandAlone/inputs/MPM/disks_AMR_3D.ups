<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->
<!-- @version: Updated 7/31/00-->
<Uintah_specification>

   <Meta>
       <title>Colliding Disks</title>
   </Meta>

   <SimulationComponent type="amrmpm" />

   <Time>
       <maxTime>0.1</maxTime>
       <initTime>0.0</initTime>
       <delt_min>1e-10</delt_min>
       <delt_max>0.001</delt_max>
       <max_Timesteps> 1000 </max_Timesteps>
       <timestep_multiplier>0.1</timestep_multiplier>
   </Time>
   
   <DataArchiver>
        <filebase>AMR_2D_3L.uda</filebase>
        <outputInitTimestep/>
       <outputTimestepInterval>20</outputTimestepInterval>

       <!--<save label = "g.zoi"/> -->
       <save label = "KineticEnergy"/>
       <save label = "TotalMass"/>
       <save label = "StrainEnergy"/>
       <save label = "CenterOfMassPosition"/>
       <save label = "TotalMomentum"/>
       
       <save label = "p.x"/>
       <save label = "p.particleID"/>
       <save label = "particleCount"/>
       <save label = "p.volume"/>
       <save label = "p.volume+"/>
       <save label = "p.color"/>
       <save label = "p.stress"/>

       <save label = "p.displacement"/>
       <save label = "p.deformationMeasure"/>
       <save label = "p.velocity"/>

      <!-- <checkpoint cycle = "5" timestepInterval = "1"/> -->
   </DataArchiver>

    <MPM>
       <time_integrator>explicit</time_integrator>
       <interpolator>       linear      </interpolator>
       <withColor>          true      </withColor>
    </MPM>


    <AMR>
      <useLockStep> true </useLockStep>
    </AMR>    
    <PhysicalConstants>
       <gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
       <MPM>
           <material name="disks">
              <density>1000.0</density>
              <constitutive_model type="comp_mooney_rivlin"> 
                 <he_constant_1>100000.0</he_constant_1>
                 <he_constant_2>20000.0</he_constant_2>
                 <he_PR>.49</he_PR>
               </constitutive_model>
              <thermal_conductivity>1.0</thermal_conductivity>
              <specific_heat>5</specific_heat>
               <geom_object>
        <!--
                  <cylinder label = "cylinder">
                     <bottom>           [ -0.35,-0.25, 0.0]   </bottom>
                     <top>              [ 0.35, 0.45, 0.0]   </top>
                     <radius>           0.003                </radius>
                   </cylinder>   
        -->      
                  <sphere label = "sphere">
                     <origin>           [ 0.0, 0.0, 0.0]   </origin>
                     <radius>           0.7                </radius>
                   </sphere>
           
                  <res>         [1,1,1]           </res>
                  <velocity>    [100,100,0.0]    </velocity>
                  <temperature>     12            </temperature>
                  <color>            0            </color>
               </geom_object>              
           </material>

           <contact>
             <type>null</type>
           </contact>
       </MPM>

    </MaterialProperties>
       
    <Grid>

  <!--__________________________________-->
<!--       3D  -->
 <!--  
       <Level>
           <Box label = "0">
              <lower>       [-2, -2, -2]    </lower>
              <upper>       [ 2,  2,  2]    </upper>
              <resolution>  [40, 40, 40]    </resolution>
              <patches>     [2,2,2]         </patches>
              <extraCells>  [0,0,0]         </extraCells>
           </Box>
       </Level>
 
       <Level>
           <Box label = "1">
              <lower>       [0.5, 0.5, 0.5]   </lower>
              <upper>       [1.5, 1.5, 1.5]   </upper>
              <resolution>  [20, 20, 20]      </resolution>
              <patches>     [2,2,2]           </patches>
              <extraCells>  [0,0,0]           </extraCells>
           </Box>
       </Level>       
       <Level>
           <Box label = "2">
              <lower>       [0.8, 0.8, 0.8]   </lower>
              <upper>       [1.2, 1.2, 1.2]   </upper>
              <resolution>  [16,16,16]           </resolution>
              <patches>     [1,1,1]           </patches>
              <extraCells>  [0,0,0]           </extraCells>
           </Box>
       </Level> 
-->

       <BoundaryConditions>
          <Face side = "x-">
              <BCType id = "all" var = "Dirichlet" label = "Velocity">
                     <value> [100, 100, 0] </value>
              </BCType>
          </Face>
          <Face side = "x+">
              <BCType id = "all" var = "Dirichlet" label = "Velocity">
                    <value> [100, 100, 0 ]</value>
              </BCType>
          </Face>
          <Face side = "y-">
              <BCType id = "all" var = "Dirichlet" label = "Velocity">
                     <value> [100, 100, 0] </value>
              </BCType>
          </Face>                  
          <Face side = "y+">
              <BCType id = "all" var = "Dirichlet" label = "Velocity">
                     <value> [100, 100, 0] </value>
              </BCType>
          </Face>
          <Face side = "z-">
              <BCType id = "all" var = "Dirichlet" label = "Velocity">
                     <value> [100, 100, 0] </value>
              </BCType>
          </Face>                  
          <Face side = "z+">
              <BCType id = "all" var = "Dirichlet" label = "Velocity">
                     <value> [100, 100, 0] </value>
              </BCType>
          </Face>                  
       </BoundaryConditions>

       
<include href="inputs/UCF/2DComplexGrid_2.xml"/>

    </Grid>
   
</Uintah_specification>

