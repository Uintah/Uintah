       <Level>
           <Box label="1">
              <lower>        [-0.5,-0.5,-0.05]    </lower>
              <upper>        [0.5,  0.5, 0.05]    </upper>
              <extraCells>   [1,1,1]              </extraCells>
              <patches>      [1,1,1]              </patches>
              <resolution>   [50,50,1]          </resolution>
           </Box>
       </Level>
    </Grid>    
    <!--____________________________________________________________________-->
    <!--      O  U  P  U  T     V  A  R  I  A  B  L  E  S                   -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>mms_1.uda</filebase>
      <outputTimestepInterval>1</outputTimestepInterval>
      <save label="press_equil_CC"/>
      <save label="delP_Dilatate"/>
      <save label="press_CC"/>
      <save label="uvel_FCME"/>
      <save label="vvel_FCME"/>
      <save label="wvel_FCME"/>
      <save label="vol_fracX_FC"/>
      <save label="vol_fracY_FC"/>
      <save label="vol_fracZ_FC"/>      
      <save label="rho_CC"/>
      <save label="vel_CC"/> 
      <save label="temp_CC"/>
      <save label = "rhs"/>
      <save label="scalar-f"/>
      <save label="sum_scalar_f"/>
      <save label="TotalIntEng"/>
      <save label="TotalMass"/>
      <save label="CenterOfMassVelocity"/>
       <outputInitTimestep/>
   </DataArchiver>

  <Debug>
      <dbg_timeStart>       1.00   </dbg_timeStart>
      <dbg_timeStop>        0.01   </dbg_timeStop>
      <dbg_outputInterval>  1e-9   </dbg_outputInterval>
      <dbg_BeginIndex>   [[-1,25,0]] </dbg_BeginIndex>
      <dbg_EndIndex>      [[3,27,1]] </dbg_EndIndex>
      <dbg_Matls>           [0]      </dbg_Matls>
      <dbg_SigFigs>         8        </dbg_SigFigs>
      <debug label = "switchDebug_vel_FC"/>
      <debug label = "switchDebug_Exchange_FC"/>
    </Debug>
    
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.2</cfl>
      <ICE>
        <advection type = "FirstOrder"/>
        <customInitialization>
          <manufacturedSolution>
            <A>   1    </A>
          </manufacturedSolution> 
        </customInitialization>
             
        <TimeStepControl>
          <Scheme_for_delT_calc> conservative   </Scheme_for_delT_calc>
          <knob_for_speedSound>  0.001            </knob_for_speedSound>
        </TimeStepControl>
        
        <ImplicitSolver>
           <max_outer_iterations>      20    </max_outer_iterations>
           <outer_iteration_tolerance> 1e-8 </outer_iteration_tolerance>
           <iters_before_timestep_restart> 5 </iters_before_timestep_restart>
           <Parameters variable="implicitPressure">


              <norm>     LInfinity  </norm>
              <criteria> Absolute   </criteria>

            <solver>         cg      </solver>
            <preconditioner> pfmg    </preconditioner>
            <tolerance>      1.e-10  </tolerance>
            <maxiterations>  7500    </maxiterations>
            <npre>           1       </npre>
            <npost>          1       </npost>
            <skip>           0       </skip>
            <jump>           0       </jump>
           </Parameters>
        </ImplicitSolver> 

      </ICE>
    </CFD>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>
    
    <!--____________________________________________________________________-->
    <!--      material Properties and Initial Conditions                    -->
    <!--____________________________________________________________________-->                      
    <MaterialProperties>
       <ICE>
         <material name = "BackgroundGas">
           <EOS type = "ideal_gas">                     </EOS>
           <dynamic_viscosity>   0.00002                    </dynamic_viscosity>
           <thermal_conductivity>0.0                    </thermal_conductivity>
           <specific_heat>      716.0                   </specific_heat>
           <gamma>              1.4                     </gamma>
           <geom_object>
                <box label="wholeDomain">
                    <min>       [ -1., -1., -1. ]       </min>
                    <max>       [  1.,  1.,  1. ]       </max>
                </box>
               <res>                 [2,2,2]            </res>
               <velocity>      [0.,0.,0.]               </velocity>
               <density>       1.1792946927374306000e+00</density>
               <pressure>      101325.0                 </pressure>     
               <temperature>   300.0                    </temperature>
           </geom_object>
         </material>
      </ICE>       
    </MaterialProperties> 
    <!--____________________________________________________________________-->
    <!--  Use passiveScalar model to advect a scalar around                -->
    <Models>
       <Model type="PassiveScalar">
        <material>BackgroundGas</material>
        <scalar name="f">
        <test_conservation>       true            </test_conservation>
        <constants>
          <diffusivity>              0.0           </diffusivity>
         </constants>
         <geom_object>
            <difference>
            <box label="wholeDomain">
                <min>       [ -1., -1., -1. ]       </min>
                <max>       [  1.,  1.,  1. ]       </max>
            </box>
            <box label="scalarSlug">
              <min>         [-0.1,-0.1,-0.1]       </min>
              <max>         [ 0.1, 0.1, 0.1]       </max>
            </box>
            </difference>
            <scalar>    0.00   </scalar>
         </geom_object>
          <geom_object>
            <box label="scalarSlug">
              <min>          [-0.1,-0.1,-0.1]     </min>
              <max>          [0.1, 0.1, 0.1]      </max>
            </box>
            <scalar>    1.00   </scalar>
         </geom_object>
       </scalar>
      </Model>
    </Models>      
</Uintah_specification>
