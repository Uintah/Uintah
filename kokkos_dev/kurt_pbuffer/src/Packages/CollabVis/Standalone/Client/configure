 case $host in
   *-irix*)
     if test "$is_64bit" = "yes"; then
	 if test -f $LOCAL_GLUT/lib/glut.n64/libglut.a; then
		GLUT_LIB_DIR="$GLUT_LIB_DIR -L$LOCAL_GLUT/lib/glut.n64"
