<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../../../../Dataflow/XML/component.dtd">
<component name="GetSubMatrix" category="Math" optional="false">
  <overview>
    <authors>
      <author>Jeroen Stinstra</author>
    </authors>
    <summary>
      Get a submatrix from a matrix by selecting row and column indices.

    </summary>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>Matrix</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
      <port>
        <name>Rows</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
      <port>
        <name>Columns</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>Matrix</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </outputs>
  </io>
</component>
