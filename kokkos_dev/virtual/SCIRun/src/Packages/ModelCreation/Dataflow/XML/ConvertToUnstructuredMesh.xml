<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE component SYSTEM "../../../../Dataflow/XML/component.dtd">
<component name="ConvertToUnstructuredMesh" category="ChangeMesh">
  <overview>
    <authors>
      <author>Jeroen Stinstra</author>
    </authors>
    <summary>
      This module converts a field with a regular mesh into a field
      with an unstructured mesh.
    </summary>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>Field</name>
        <datatype>SCIRun::Field</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>Field</name>
        <datatype>SCIRun::Field</datatype>
      </port>
    </outputs>
  </io>
</component>

