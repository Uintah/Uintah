<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../../../../Dataflow/XML/component.dtd">
<component name="ClipFieldByMesh" category="CreateField" optional="false">
  <overview>
    <authors>
      <author>Jeroen Stinstra</author>
    </authors>
    <summary>
      This module Clips a field by taking all the elements that are inside the object field.


    </summary>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>Field</name>
        <datatype>SCIRun::Field</datatype>
      </port>
      <port>
        <name>Object</name>
        <datatype>SCIRun::Field</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>Field</name>
        <datatype>SCIRun::Field</datatype>
      </port>
      <port>
        <name>Mapping</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </outputs>
  </io>
</component>
