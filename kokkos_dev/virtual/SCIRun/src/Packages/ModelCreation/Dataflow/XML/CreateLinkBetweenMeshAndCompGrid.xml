<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../../../../Dataflow/XML/component.dtd">
<component name="CreateLinkBetweenMeshAndCompGrid" category="FiniteElements" optional="false">
  <overview>
    <authors>
      <author>Jeroen Stinstra</author>
    </authors>
    <summary>
      This module computes how nodes should be merged over the boundary. It calculates the needed ordering in the computational grid.

    </summary>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>NodeLink</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>GeomToComp</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
      <port>
        <name>CompToGeom</name>
        <datatype>SCIRun::Matrix</datatype>
      </port>
    </outputs>
  </io>
</component>
