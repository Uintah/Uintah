<definition class="ITKBinaryDilateErodeImageFilterToolDialog">
  <object class="Box">
    <var name="color">#AAAAAAAA</var>
    <object class="Grid">
      <var name="rows">10</var>
      <var name="cols">1</var>

      <var name="LabeledSpinnerBox::text_size" propagate="yes">15</var>

      <object class="Root::Text">
        <var name="row">1</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="text"> ITK Binary Dilate->Erode
 Image Filter</var>
        <var name="size">15</var>
        <var name="fgcolor">#000000FF</var>
        <var name="bgcolor">#FFFFFF80</var>
        <var name="anchor">W</var>
        <var name="offsetx">-2</var>
        <var name="offsety">2</var>
        <var name="shadow">1</var>
      </object>
     

      <object class="LabeledSpinnerBox">
        <var name="row">3</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="LabeledSpinnerBox::text" propagate="yes">Dilate Radius: </var>
        <var name="LabeledSpinnerBox::variable" 
             propagate="yes">$ITKBinaryDilateErodeImageFilterTool::DilateRadius</var>
      </object>

      <object class="LabeledSpinnerBox">
        <var name="row">4</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="LabeledSpinnerBox::text" propagate="yes">Erode Radius: </var>
        <var name="LabeledSpinnerBox::variable" propagate="yes">$ITKBinaryDilateErodeImageFilterTool::ErodeRadius</var>
      </object>
      
      <object class="ProgressBar">
        <var name="row">6</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="ProgressBar::progress" propagate="yes">$Filter::percent</var>
      </object>

      <object class="Button">
        <var name="row">7</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="Button::color" propagate="yes">#EEFFEEFF</var>
        <var name="Button::Text::text" propagate="yes">Dilate</var>
        <signal name="Button::clicked" 
                target="Painter::ITKBinaryDilate">
          <var name="threaded" propagate="yes">1</var>
        </signal>
      </object>

      <object class="Button">
        <var name="row">8</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="Button::color" propagate="yes">#EEFFEEFF</var>
        <var name="Button::Text::text" propagate="yes">Erode</var>
        <signal name="Button::clicked" 
                target="Painter::ITKBinaryErode">
          <var name="threaded" propagate="yes">1</var>
        </signal>
      </object>

      <object class="Button">
        <var name="row">9</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="Button::color" propagate="yes">#EEFFEEFF</var>
        <var name="Button::Text::text" propagate="yes">Dilate->Erode</var>
        <signal name="Button::clicked" 
                target="Painter::ITKBinaryDilateErode">
          <var name="threaded" propagate="yes">1</var>
        </signal>
      </object>

      <object class="Button">
        <var name="row">10</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="Button::color" propagate="yes">#EEFFEEFF</var>
        <var name="Button::Text::text" propagate="yes">Done</var>
        
        <signal name="Button::clicked" 
                target="ToolDialog::Done"></signal>
      </object>
    </object>
  </object>
</definition>


