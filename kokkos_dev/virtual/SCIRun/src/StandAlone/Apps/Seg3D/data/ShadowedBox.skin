<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE definition PUBLIC "-//Skinner/Drawable DTD" "skinner.dtd">

<definition class="ShadowedBox"> 
  <object class="Collection">
    
    <var name="ShadowedBox::shadow_N_size" 
         propagate="yes" overwrite="no">3</var>
    <var name="ShadowedBox::shadow_S_size" 
         propagate="yes" overwrite="no">9</var>
    <var name="ShadowedBox::shadow_E_size" 
         propagate="yes" overwrite="no">6</var>
    <var name="ShadowedBox::shadow_W_size" 
         propagate="yes" overwrite="no">6</var>
    <var name="ShadowedBox::outside" propagate="yes" 
         overwrite="no">#00000000</var>
    <var name="ShadowedBox::inside" propagate="yes" 
         overwrite="no">#333333FF</var>

    <var name="border-size1" propagate="yes" overwrite="no">11</var>
    <var name="border-size2" propagate="yes" overwrite="no">11</var>


    <object class="Grid" id="shadowedgrid">
      <var name="rows">3</var>
      <var name="cols">3</var>
      
      <object class="Root::Arc" id="arc1">
        <var name="row">1</var>
        <var name="col">1</var>
        <var name="cell-height">$border-size2</var>
        <var name="cell-width">$border-size2</var>
        <var name="anchor">SE</var>
        <var name="ne-color">$ShadowedBox::outside</var>
        <var name="se-color">$ShadowedBox::inside</var>
        <var name="nw-color">$ShadowedBox::outside</var>
        <var name="sw-color">$ShadowedBox::outside</var>
      </object>
      
      
      <object class="Gradient">
        <var name="row">1</var>
        <var name="col">2</var>
        <var name="cell-height">$border-size2</var>
        <var name="top-color">$ShadowedBox::outside</var>
        <var name="bottom-color">$ShadowedBox::inside</var>
      </object>
      
      <object class="Root::Arc">
        <var name="row">1</var>
        <var name="col">3</var>
        <var name="cell-height">$border-size2</var>
        <var name="cell-width">$border-size1</var>

        <var name="anchor">SW</var>
        <var name="ne-color">$ShadowedBox::outside</var>
        <var name="sw-color">$ShadowedBox::inside</var>
        <var name="nw-color">$ShadowedBox::outside</var>
        <var name="se-color">$ShadowedBox::outside</var>
      </object>
      
      
      
      <object class="Gradient">
        <var name="row">2</var>
        <var name="col">1</var>
        <var name="cell-width">$border-size2</var>
        <var name="left-color">$ShadowedBox::outside</var>
        <var name="right-color">$ShadowedBox::inside</var>
      </object>

      <object class="Box">
        <var name="row">2</var>
        <var name="col">2</var>
        <var name="color">$ShadowedBox::inside</var>
      </object>

      
      
      <object class="Gradient">
        <var name="row">2</var>
        <var name="col">3</var>
        <var name="cell-width">$border-size1</var>
        <var name="right-color">$ShadowedBox::outside</var>
        <var name="left-color">$ShadowedBox::inside</var>
      </object>

      
      <object class="Root::Arc">
        <var name="row">3</var>
        <var name="col">1</var>

        <var name="cell-height">$border-size1</var>
        <var name="cell-width">$border-size2</var>
        <var name="anchor">NE</var>
        <var name="sw-color">$ShadowedBox::outside</var>
        <var name="ne-color">$ShadowedBox::inside</var>
        <var name="nw-color">$ShadowedBox::outside</var>
        <var name="se-color">$ShadowedBox::outside</var>
      </object>

      <object class="Gradient">
        <var name="row">3</var>
        <var name="col">2</var>
        <var name="cell-height">$border-size1</var>
        <var name="bottom-color">$ShadowedBox::outside</var>
        <var name="top-color">$ShadowedBox::inside</var>
      </object>
      
      <object class="Root::Arc">
        <var name="row">3</var>
        <var name="col">3</var>

        <var name="cell-height">$border-size1</var>
        <var name="cell-width">$border-size1</var>
        <var name="anchor">NW</var>
        <var name="se-color">$ShadowedBox::outside</var>
        <var name="nw-color">$ShadowedBox::inside</var>
        <var name="ne-color">$ShadowedBox::outside</var>
        <var name="sw-color">$ShadowedBox::outside</var>
      </object>
    </object>

    <object class="Grid">
      <var name="rows">3</var>
      <var name="cols">3</var>

      <object class="Collection">
        <var name="row">1</var>
        <var name="col">1</var>

        <var name="cell-height">$ShadowedBox::shadow_N_size</var>
        <var name="cell-width">$ShadowedBox::shadow_W_size</var>
      </object>          

      <object class="Collection">
        <var name="row">3</var>
        <var name="col">3</var>

        <var name="cell-height">$ShadowedBox::shadow_S_size</var>
        <var name="cell-width">$ShadowedBox::shadow_E_size</var>
      </object>          

      <object class="ShadowedBox::child">
        <var name="row">2</var>
        <var name="col">2</var>
      </object>
    </object>
    
  </object>
</definition>

