<definition class="CropToolDialog">
  <object class="Frame">
    <var name="LabeledSpinnerBox::text_size" propagate="yes">15</var>
    <var name="invert">1</var>
    <var name="color">#AAAAAAAA</var>
    
    <object class="Texture">
      <var name="file">BackgroundTile04.png</var>
      <var name="color">#AAAAAAFF</var>
      <var name="repeatx">1</var>
      <var name="repeaty">1</var>
    </object>
    
    <object class="Grid">
      <var name="rows">8</var>
      <var name="cols">1</var>

      
      <object class="Root::Text">
        <var name="row">1</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="text"> Crop Tool:</var>
        <var name="size">30</var>
        <var name="fgcolor">#000000FF</var>
        <var name="bgcolor">#FFFFFF80</var>
        <var name="anchor">W</var>
        <var name="offsetx">-2</var>
        <var name="offsety">2</var>
        <var name="shadow">1</var>
      </object>

      <object class="Grid">
        <var name="rows">1</var>
        <var name="cols">2</var>

        <var name="cell-height">35</var>

        <var name="row">3</var>
        <var name="col">1</var>

        <object class="LabeledSpinnerBox">
          <var name="row">1</var>
          <var name="col">1</var>

          <var name="LabeledSpinnerBox::text"
               propagate="yes"
               overwrite="no">Min X:</var>
          <var name="LabeledSpinnerBox::variable"
               propagate="yes" overwrite="no">$Painter::crop::min::x</var>
        </object>

        <object class="LabeledSpinnerBox">
          <var name="row">1</var>
          <var name="col">2</var>
          
          <var name="LabeledSpinnerBox::text"
               propagate="yes"
               overwrite="no">Max X:</var>
          <var name="LabeledSpinnerBox::variable"
               propagate="yes" overwrite="no">$Painter::crop::max::x</var>
        </object>
      </object>


      <object class="Grid">
        <var name="rows">1</var>
        <var name="cols">2</var>

        <var name="cell-height">35</var>
        <var name="row">4</var>
        <var name="col">1</var>

        <object class="LabeledSpinnerBox">
          <var name="row">1</var>
          <var name="col">1</var>
          
          <var name="LabeledSpinnerBox::text"
               propagate="yes"
               overwrite="no">Min Y:</var>
          <var name="LabeledSpinnerBox::variable"
               propagate="yes"
               overwrite="no">$Painter::crop::min::y</var>
        </object>

        <object class="LabeledSpinnerBox">
          <var name="row">1</var>
          <var name="col">2</var>
          
          <var name="LabeledSpinnerBox::text"
               propagate="yes"
               overwrite="no">Max Y:</var>
          <var name="LabeledSpinnerBox::variable"
               propagate="yes"
               overwrite="no">$Painter::crop::max::y</var>
        </object>
      </object>

      <object class="Grid">
        <var name="rows">1</var>
        <var name="cols">2</var>

        <var name="cell-height">35</var>
        <var name="row">5</var>
        <var name="col">1</var>

        <object class="LabeledSpinnerBox">
          <var name="row">1</var>
          <var name="col">1</var>
          
          <var name="LabeledSpinnerBox::text"
               propagate="yes"
               overwrite="no">Min Z:</var>
          <var name="LabeledSpinnerBox::variable"
               propagate="yes"
               overwrite="no">$Painter::crop::min::z</var>
        </object>

        <object class="LabeledSpinnerBox">
          <var name="row">1</var>
          <var name="col">2</var>
          
          <var name="LabeledSpinnerBox::text"
               propagate="yes"
               overwrite="no">Max Z:</var>
          <var name="LabeledSpinnerBox::variable"
               propagate="yes"
               overwrite="no">$Painter::crop::max::z</var>
        </object>
      </object>


      <object class="Button">
        <var name="row">7</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="Button::color" propagate="yes" overwrite="no">#FFEEEEFF</var>
        <var name="Button::Text::text" propagate="yes">Cancel</var>

        <signal name="Button::clicked" 
                target="Painter::CancelTool"/>

        <signal name="Button::clicked" 
                target="ToolDialog::Cancel"/>
      </object>

      <object class="Button">
        <var name="row">8</var>
        <var name="col">1</var>
        <var name="cell-height">35</var>
        <var name="Button::color" propagate="yes" overwrite="no">#EEFFEEFF</var>
        <var name="Button::Text::text" propagate="yes">Crop</var>

        <signal name="Button::clicked" 
                target="Painter::FinishTool"/>

        <signal name="Button::clicked" 
                target="ToolDialog::Done"/>
      </object>

    </object>
  </object>
</definition>
    
