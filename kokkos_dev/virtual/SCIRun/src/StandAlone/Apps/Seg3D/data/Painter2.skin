<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE skinner PUBLIC "-//Skinner/Drawable DTD" "skinner.dtd">

<skinner version="1.0">
  <var name="program_name" propagate="yes">SCI-RENDER 0.1</var>
  <var name="full_program_name" propagate="yes">Raster Explorer and N-Dimensional Editor Revision 0.1</var>


  <var name="ITKCurvatureAnisotropic::numberOfIterations" propagate="yes">5</var>
  <var name="ITKCurvatureAnisotropic::timeStep" propagate="yes">0.075</var>
  <var name="ITKCurvatureAnisotropic::conductanceParameter" propagate="yes">1.0</var>

  <var name="ITKBinaryDilateErode::radius" propagate="yes">3</var>
  <var name="ITKBinaryDilateErode::dilateValue" propagate="yes">255</var>
  <var name="ITKBinaryDilateErode::erodeValue" propagate="yes">255</var>


  <var name="grey" propagate="yes">#444444FF</var>
  <var name="grey2" propagate="yes">#777777FF</var>
  <var name="grey3" propagate="yes">#AAAAAAFF</var>
  <var name="light_red" propagate="yes">#996666DD</var>
  <var name="light_green" propagate="yes">#446644DD</var>
  <var name="dark_blue" propagate="yes">#444466FF</var>
  <var name="light_blue" propagate="yes">#AAAAE0DD</var>

  <var name="button_color" propagate="yes">#BBBBFFFF</var>
  <var name="button_color" propagate="yes">#FFFFFFFF</var>
  <var name="main-color" propagate="yes">#EEEEEEDD</var>

  <var name="grey" propagate="yes">$main-color</var>
  <var name="grey2" propagate="yes">$main-color</var>
  <var name="grey3" propagate="yes">$main-color</var>

  <var name="clear" propagate="yes">#00000000</var>
  <var name="black50" propagate="yes">#00000080</var>
  <var name="black90" propagate="yes">#000000BB</var>
  <var name="Window::borders" propagate="yes">1</var>

  <definition class="ShadowedBox"> 
    <object class="Box"> 
      <var name="color">#00000000</var>
      <var name="focus_mode">1</var>

      <var name="border-size1" propagate="yes" overwrite="no">15</var>
      <var name="border-size2" propagate="yes" overwrite="no">5</var>

      <object class="Grid">
        <var name="rows">3</var>
        <var name="cols">3</var>
        <object class="Gradient">
          <var name="row">1</var>
          <var name="col">2</var>
          <var name="cell-height">$border-size2</var>
          <var name="top-color">$clear</var>
          <var name="bottom-color">$black50</var>
        </object>
        
        <object class="Gradient">
          <var name="row">2</var>
          <var name="col">1</var>
          <var name="cell-width">$border-size2</var>
          <var name="left-color">$clear</var>
          <var name="right-color">$black50</var>
        </object>
        
        <object class="ShadowedBox::child">
          <var name="row">2</var>
          <var name="col">2</var>
        </object>
        
        <object class="Gradient">
          <var name="row">2</var>
          <var name="col">3</var>
          <var name="cell-width">$border-size1</var>
          <var name="right-color">$clear</var>
          <var name="left-color">$black90</var>
        </object>
        
        <object class="Gradient">
          <var name="row">3</var>
          <var name="col">2</var>
          <var name="cell-height">$border-size1</var>
          <var name="bottom-color">$clear</var>
          <var name="top-color">$black90</var>
        </object>

        <object class="Gradient">
          <var name="row">3</var>
          <var name="col">3</var>
          <var name="se-color">$clear</var>
          <var name="nw-color">$black90</var>
          <var name="ne-color">$clear</var>
          <var name="sw-color">$clear</var>
          <var name="backslash">0</var>
        </object>

        <object class="Gradient">
          <var name="row">1</var>
          <var name="col">1</var>
          <var name="ne-color">$clear</var>
          <var name="se-color">$black90</var>
          <var name="nw-color">$clear</var>
          <var name="sw-color">$clear</var>
          <var name="backslash">0</var>
        </object>


        <object class="Gradient">
          <var name="row">1</var>
          <var name="col">3</var>
          <var name="ne-color">$clear</var>
          <var name="sw-color">$black90</var>
          <var name="nw-color">$clear</var>
          <var name="se-color">$clear</var>
          <var name="backslash">1</var>
        </object>


        <object class="Gradient">
          <var name="row">3</var>
          <var name="col">1</var>
          <var name="sw-color">$clear</var>
          <var name="ne-color">$black90</var>
          <var name="nw-color">$clear</var>
          <var name="se-color">$clear</var>
          <var name="backslash">1</var>
        </object>


      </object>
    </object>
  </definition>
  


  <definition class="Button" state="normal">
    <object class="Grid">
      <var name="rows">1</var>
      <var name="cols">3</var>

      <var name="Button::height" propagate="yes" overwrite="no">25</var>
      <var name="Button::color" propagate="yes" overwrite="no">#EEEEFFFF</var>
      <var name="Button::Text::size" propagate="yes" overwrite="no">
        $Button::height
      </var>
      <var name="Button::Text::fgcolor" propagate="yes" overwrite="no">#000000FF</var>
      <var name="Button::Text::bgcolor" propagate="yes" overwrite="no">#FFFFFFFF</var>

      <object class="Texture">
        <var name="row">1</var>
        <var name="col">1</var>

        <var name="cell-width">12</var>
        <var name="cell-height">$Button::height</var>
        <var name="file">button-left-transparent-greyscale-01.png</var>
        <var name="color">$Button::color</var>
      </object>

      <object class="Collection">
        <var name="row">1</var>
        <var name="col">2</var>

        <object class="Box">
          <var name="color">$Button::color</var>
          <signal name="button_1_clicked" target="Button::clicked"></signal>
        </object>

        <object class="Root::Text"> 
          <var name="text">$Button::Text::text</var>
          <var name="size">$Button::Text::size</var>
          <var name="fgcolor">$Button::Text::fgcolor</var>
          <var name="bgcolor">$Button::Text::bgcolor</var>
          <var name="shadow">1</var>
          <var name="offsetx">-1</var>
          <var name="offsety">1</var>
          <var name="reverse">0</var>
          <var name="anchor">c</var>
        </object>


        <object class="Texture">
          <var name="file">button-background-transparent-greyscale-01.png</var>
        </object>        
      </object>
      
      <object class="Texture">
        <var name="row">1</var>
        <var name="col">3</var>
        <var name="cell-width">12</var>
        <var name="file">button-right-transparent-greyscale-01.png</var>
        <var name="color">$Button::color</var>
      </object>
    </object>
  </definition>  
  
  <definition class="Button" state="normal">
    <object class="Frame">
      <var name="width">4</var>
      <var name="height">4</var>
      <var name="color1">$grey3</var>
      <var name="color2">$grey</var>
      <var name="invert">1</var>
      <object class="Collection">
        <object class="Box" id="TargetBox">
          <var name="color">$grey2</var>
          <signal name="button_1_clicked" target="Button::clicked"/>
        </object>
        <object class="Root::Text"> 
          <var name="text">$Button::Text::text</var>
          <var name="row">1</var>
          <var name="col">1</var>
          <var name="cell-height">30</var>
          <var name="size">25.0</var>
          <var name="fgcolor">$text_color</var>
          <var name="shadow">1</var>
          <var name="reverse">1</var>
          <var name="offsetx">1</var>
          <var name="offsety">1</var>
          <var name="anchor">C</var>
        </object>
      </object>
    </object>
  </definition>


  <definition class="SliceFrame" state="normal">
    <object class="Frame">
      <var name="width">5</var>
      <var name="height">5</var>
      <var name="color1">$grey</var>
      <var name="color2">$grey2</var>
      <object class="Frame"> 
        <var name="color1">$grey2</var>
        <var name="color2">$grey</var>
        <object class="Frame1-child"/>
      </object>
    </object>
  </definition>


  <definition class="GradientBackground">
    <object class="Collection">
      <object class="Gradient">
        <var name="bottom-color">$GradientBackground::bottom-color</var>
        <var name="top-color" type="color">$grey</var>
      </object>
      <object class="GradientBackground::child"/>
    </object>
  </definition>

  <definition class="LabeledGradientBackground">
    <object class="Collection">

      <definition class="GradientBackground::child">
        <object class="LabeledGradientBackground::child"/>
      </definition>

      <object class="GradientBackground"/>

      <object class="Root::Text">
        <var name="text">$LabeledGradientBackground::text</var>  
        <var name="size">40</var>  
        <var name="anchor">S</var>
        <var name="shadow" >1</var>
        <var name="offsetx" >-5</var>
        <var name="offsety" >5</var>

      </object>
    </object>
  </definition>


  
  <definition class="NicePainterWindow">
    <object class="Collection">
      <var name="LabeledGradientBackground::text" propagate="yes">$NicePainterWindow::name</var>

      <definition class="LabeledGradientBackground::child">                
        <object class="Painter::SliceWindow"/>
      </definition>

      <object class="LabeledGradientBackground"/>
    </object>
  </definition>



  <definition class="ShadowedPainterWindow">
    <object class="Collection">
      <definition class="ShadowedBox::child">
        <object class="NicePainterWindow"/>
      </definition>
      <object class="ShadowedBox"/>
    </object>
  </definition>




  <definition class="HelpButton">
    <object class="Collection">
      <object class="Box">
        <var name="color">#000000</var>
        <signal name="button_1_clicked"
                target="Quit"/>

      </object>
      <object class="Grid">
        <var name="rows">1</var>
        <var name="cols">2</var>
        
        <var name="Button::color" propagate="yes">#44FF44FF</var>
        <var name="Button::height" propagate="yes">35</var>
        <var name="Button::Text::text" propagate="yes">?</var>
        <var name="Button::Text::size" propagate="yes">43</var>
        <var name="Button::Text::fgcolor" propagate="yes">#003300FF</var>
        <var name="Button::Text::bgcolor" propagate="yes">#FFFFFFFF</var>
        <object class="Button">
          <var name="row">1</var>
          <var name="col">2</var>
          <var name="cell-width">35</var>
          <signal name="Button::clicked"
                  target="Quit"/>
        </object>
      </object>
    </object>
  </definition>
  

  <definition class="FilterDialog">
    <object class="ShadowedBox">
      <var name="FilterDialog::text" 
           propagate="yes" 
           overwrite="no">Filter:</var>
      <definition class="ShadowedBox::child">
        <object class="Box">
          <var name="focus_mode">1</var>
          <var name="color">#AAAAAAAA</var>
          
          <object class="Texture">
            <var name="file">BackgroundTile04.png</var>
            <var name="color">#AAAAAAFF</var>
            <var name="repeatx">1</var>
            <var name="repeaty">1</var>
          </object>
          
          <object class="Texture">
            <var name="file">BackgroundTile04.png</var>
            <var name="color">#000000</var>
            <var name="repeatx">1</var>
            <var name="repeaty">1</var>
          </object>
          
          <object class="Grid">
            <var name="rows">5</var>
            <var name="cols">5</var>


            <object class="Root::Text">
              <var name="row">1</var>
              <var name="col">1</var>
              <var name="text">$FilterDialog::text</var>
              <var name="size">30</var>
              <var name="fgcolor">#000000FF</var>
              <var name="bgcolor">#FFFFFF80</var>
              <var name="anchor">W</var>
              <var name="offsetx">-2</var>
              <var name="offsety">2</var>
              <var name="shadow">1</var>
            </object>

            
            <object class="Button">
              <var name="row">3</var>
              <var name="col">2</var>
              <var name="Button::Text::text" propagate="yes">Cancel</var>
              <signal name="Button::clicked" 
                      target="FilterDialog::Cancel"/>
            </object>
            <object class="Button">
              <var name="row">3</var>
              <var name="col">4</var>
              <var name="Button::Text::text" propagate="yes">Execute</var>
              <signal name="Button::clicked" 
                      target="FilterDialog::Execute"/>
            </object>
          </object>
        </object>
      </definition>
    </object>
  </definition>
  
    


  <definition class="FileLoadBox">
    <object class="ShadowedBox">
      <var name="TextEntry::value" propagate="yes"></var>
      <definition class="ShadowedBox::child">
        <object class="Box">
          <var name="focus_mode">1</var>
          <var name="color">#AAAAAAAA</var>
          
          <object class="Texture">
            <var name="file">BackgroundTile04.png</var>
            <var name="color">#AAAAAAFF</var>
            <var name="repeatx">1</var>
            <var name="repeaty">1</var>
          </object>
          
          <object class="Texture">
            <var name="file">BackgroundTile04.png</var>
            <var name="color">#000000</var>
            <var name="repeatx">1</var>
            <var name="repeaty">1</var>
          </object>
          
          <object class="Grid">
            <var name="rows">3</var>
            <var name="cols">3</var>
            
            <object class="Root::Text">
              <var name="row">1</var>
              <var name="col">1</var>
              <var name="text">$FileLoadBox::text</var>
              <var name="size">30</var>
              <var name="fgcolor">#000000FF</var>
              <var name="bgcolor">#FFFFFF80</var>
              <var name="anchor">W</var>
              <var name="offsetx">-2</var>
              <var name="offsety">2</var>
              <var name="shadow">1</var>
              <var name="cell-width">50</var>
            </object>
            
            <object class="Collection">
              <var name="row">2</var>
              <var name="col">1</var>        
              <var name="cell-width">50</var>
            </object>
            
            <object class="ShadowedBox">
              <var name="row">2</var>
              <var name="col">2</var>        
              <var name="cell-height">40</var>
              <var name="cell-width">600</var>
              <var name="border-size1" propagate="yes">5</var>
              <var name="border-size2" propagate="yes">5</var>
              
              <definition class="ShadowedBox::child">
                <object class="Collection">
                  
                  <object class="Box">
                    <var name="color">#FFFFFFFF</var>
                  </object>
                  
                  
                  
                  <object class="Root::Text">
                    <var name="text">$TextEntry::value</var>
                    <var name="size">30</var>
                    <var name="anchor">W</var>
                    <var name="fgcolor">#000000FF</var>
                    <var name="bgcolor">#FFFFFFFF</var>
                    <var name="bgcolor">#00000080</var>
                    <var name="offsetx">-1</var>
                    <var name="offsety">1</var>
                    <var name="shadow">0</var>
                  </object>
                  
                  <object class="TextEntry">
                    
                  </object>
                </object>
              </definition>
            </object>
            
            <object class="Grid"> 
              <var name="row">3</var>
              <var name="col">2</var>
              <object class="Grid"> 
                <var name="rows">2</var>
                <var name="cols">3</var>
                
                <object class="Collection">
                  <var name="color">#00000000</var>
                  <var name="row">1</var>
                  <var name="col">1</var>
                  <var name="cell-height">5</var>
                </object>
                
                <object class="Button">
                  <var name="row">2</var>
                  <var name="col">1</var>
                  
                  <var name="Button::Text::text" propagate="yes">Close</var>
                  <signal name="Button::clicked" 
                          target="FileLoadBox::CancelButtonPress"/>
                </object>
                <object class="Button">
                  <var name="row">2</var>
                  <var name="col">3</var>
                  
                  <var name="Button::Text::text" propagate="yes">Load</var>
                  <signal name="Button::clicked" 
                          target="FileLoadBox::LoadButtonPress">$TextEntry::value</signal>
                </object>
              </object>
            </object>
          </object>
        </object>
      </definition>
    </object>
  </definition>
  
  <!--
      ***** Definitions Over
      ***** Objects Begin
