<?xml version="1.0" encoding="iso-8859-1"?>


<!-- @version: -->

<Uintah_specification>

   <Meta>
	<title>symetry test</title>
   </Meta>
    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--   maxTime          = ending time for simulation                    -->
    <!--   initTime         = ????????                                      -->
    <!--   delt_init        = initial value for delta time                  -->
    <!--   timestep_mulipler= ???????????                                   -->
    <!--____________________________________________________________________-->
   <Time>
	<maxTime>            0.01        </maxTime>
	<initTime>           0.0         </initTime>
	<delt_min>           0.0         </delt_min>
	<delt_max>           1.0         </delt_max>
	<delt_init>          1.0e-9      </delt_init>
	<timestep_multiplier>1.0         </timestep_multiplier>
   </Time>
    <!--____________________________________________________________________-->
    <!--   B O U N D A R Y   C O N D I T I O N S                            -->
    <!--____________________________________________________________________-->
    <Grid>
		<BoundaryConditions>
		 <Face side = "x-">
                     <BCType label = "Pressure" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Velocity" var = "Neumann">
                           <value> [0.0,0.0,0.0] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
                </Face>
                <Face side = "x+">
			<BCType label = "Pressure" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Velocity" var = "Neumann">
                           <value> [0.0,0.0,0.0] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
                </Face>
                <Face side = "y-">
			<BCType label = "Pressure" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Velocity" var = "Neumann">
                           <value> [0.0,0.0,0.0] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Density"    var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
                </Face>                  
		<Face side = "y+">
			<BCType label = "Pressure"   var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Velocity"   var = "Neumann">
                           <value> [0.0,0.0,0.0] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
		</Face>
		<Face side = "z-">
			<BCType label = "Pressure" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Velocity" var = "Neumann">
                           <value> [0.0,0.0,0.0] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
	        </Face>
		<Face side = "z+">
			<BCType label = "Pressure" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Velocity" var = "Neumann">
                           <value> [0.0,0.0,0.0] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0.0 </value>
                        </BCType>
	        </Face>

	</BoundaryConditions>
    <!--____________________________________________________________________-->
    <!--     P A T C H   D I M E N S I O N S                                -->
    <!--   lower/upper      = lower and upper physical limits               -->
    <!--   extraCells       = number of extra cells in x,y,z                -->
    <!--   spacing          = resolution                                    -->
    <!--____________________________________________________________________-->
	<Level>
	    <Box label="1">
	    	<lower>       [0,0,0]     </lower>
	    	<upper>       [5,5,5]     </upper>
              <extraCells>  [1,1,1]     </extraCells>
		<patches>     [1,1,1]     </patches>
	    </Box>
	    <spacing>[1,1,1] </spacing>
	</Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--   filebase         = output file basenme                           -->
    <!--   outputInterval   = Time between filedumps                        -->
    <!--   save label       = any variable you want to visualize            -->
    <!--____________________________________________________________________-->
   <DataArchiver>
	<filebase>symetry3D.uda</filebase>
	<outputInterval>0.0001</outputInterval>
       <save label="vel_CC"/>
       <save label="temp_CC"/>
       <save label="press_CC"/>
       <save label="delPress_CC"/>
   </DataArchiver>
   
   <Debug>
       <debug label = "switchDebugInitialize"/>        
	<!--<debug label = "switchDebug_equilibration_press"/>   -->
       <!--<debug label = "switchDebug_vel_FC"/>            -->      
       <!--<debug label = "switchDebug_explicit_press"/>    -->    
       <!--<debug label = "switchDebug_PressFC"/>           -->    
       <!--<debug label = "switchDebugLagrangianValues"/>   -->   
       <!--<debug label = "switchDebugSource_Sink"/>        -->   
	<debug label = "switchDebug_advance_advect"/>    	
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--    cfl             = Standard CFL number                           -->
    <!--    time_integrator = [explicit or implicit]                        -->
    <!--    delt_based_on   = [??????]                                      -->
    <!--____________________________________________________________________-->
   <CFD>
  	<cfl>0.5</cfl>
	<ICE>
	    <time_integrator>explicit    </time_integrator>
	    <max_iteration>  100         </max_iteration>
  	    <delt_based_on>  convective  </delt_based_on>
           <max_iteration_equilibration> 50 </max_iteration_equilibration>
	</ICE>	 
    </CFD>
    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--    gravity         = (x, y, z dirs)                                -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
	<gravity>[0,0,0]</gravity>
    </PhysicalConstants>
    <!--____________________________________________________________________-->
    <!--     MATERIAL PROPERTIES AND INITIAL CONDITIONS                     -->
    <!--    density [kg/m^3]             specific_heat [J/kg-K]             -->
    <!--    ideal gas constant [J/kgK]                                      -->
    <!--    geom_object                                                     -->
    <!--        res     = number of particles per cell in each dir          -->
    <!--        min/max = Physical dimensions of overlay                    -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
	<ICE>
	  <material>
	    <ideal_gas>
		<gas_constant>188.92 </gas_constant>
	    </ideal_gas>
           
	    
	    <viscosity>              0.0                   </viscosity>
	    <thermal_conductivity>   0.0                   </thermal_conductivity>
	    <specific_heat>          652.9                 </specific_heat>
	    <speed_of_sound>         270.287               </speed_of_sound>
	    <gamma>                  1.289                 </gamma>
	    <velocity_field>         0.0                   </velocity_field>
           <!--____________________________________ -->
           <!-- How domain gets initialized         -->
           <!--____________________________________ -->
	    <geom_object>
	      <difference>
                <box label="Whole grid">
                    <min>[ -1.0, -1.0, -1.0 ]           </min>
                    <max>[ 6.0, 6.0, 6.0 ]              </max>
                </box>
 
                <box label="hot_spot">
                    <min>[ 2.0, 2.0, 2.0 ]              </min>
                    <max>[ 3.0, 3.0, 3.0 ]              </max>
                </box>
	      </difference>
	        <res>		   [2,2,2]                  </res>
	        <velocity>      [0.0,0.0,0.0]            </velocity>
               <density>       1.7899909957225715000    </density>
               <pressure>      101325.0                 </pressure>
	        <temperature>   300.0                    </temperature>
	    </geom_object>
           
	    <geom_object>
                <box label="hot_spot">
                    <min>[ 2.0, 2.0, 2.0 ]              </min>
                    <max>[ 3.0, 3.0, 3.0 ]              </max>
                </box>
	        <res>		   [2,2,2]                  </res>
	        <velocity>      [0.0,0.0,0.0]            </velocity>
               <density>       1.7899909957225715000     </density>
               <pressure>      135266.039564763254020363         </pressure>
	        <temperature>   400.0                    </temperature>
	    </geom_object>
         </material>
 
 
	  <exchange_coefficients>
	     <momentum>	        [0.0]               </momentum>
	     <heat>                  [0.0]               </heat>
	  </exchange_coefficients>
	</ICE>	
    </MaterialProperties>
                 
	
</Uintah_specification>
