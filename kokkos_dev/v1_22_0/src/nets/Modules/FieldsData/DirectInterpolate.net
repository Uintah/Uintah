set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {14 17 633 500}

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m1 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 217 99]

# Create a SCIRun->DataIO->FieldReader Module
set m2 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 104 17]

# Create a SCIRun->FieldsData->DirectInterpolate Module
set m3 [addModuleAtPosition "SCIRun" "FieldsData" "DirectInterpolate" 178 179]

# Create a SCIRun->FieldsCreate->FieldBoundary Module
set m4 [addModuleAtPosition "SCIRun" "FieldsCreate" "FieldBoundary" 14 201]

# Create a SCIRun->Visualization->ShowField Module
set m5 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 22 363]

# Create a SCIRun->Visualization->ShowField Module
set m6 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 219 360]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m7 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 438 176]

# Create a SCIRun->Visualization->RescaleColorMap Module
set m8 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 380 273]

# Create a SCIRun->Render->Viewer Module
set m9 [addModuleAtPosition "SCIRun" "Render" "Viewer" 72 456]

# Create the Connections between Modules
set c1 [addConnection $m2 0 $m4 0]
set c2 [addConnection $m2 0 $m1 0]
set c3 [addConnection $m2 0 $m3 0]
set c4 [addConnection $m4 0 $m5 0]
set c5 [addConnection $m3 0 $m6 0]
set c6 [addConnection $m7 0 $m8 0]
set c7 [addConnection $m5 0 $m9 0]
set c8 [addConnection $m1 0 $m3 1]
set c9 [addConnection $m3 0 $m8 1]
set c10 [addConnection $m8 0 $m6 1]
set c11 [addConnection $m6 0 $m9 1]

# Setup GUI for the SCIRun->FieldsCreate->SamplePlane Module
setGlobal $m1-sizex {80}
setGlobal $m1-sizey {80}
setGlobal $m1-axis {2}

# Setup GUI for the SCIRun->DataIO->FieldReader Module
setGlobal $m2-filename "$DATADIR/utahtorso-lowres/utahtorso-lowres-voltage.tvd.fld"

# Setup GUI for the SCIRun->FieldsData->DirectInterpolate Module
$m3 initialize_ui

# Setup GUI for the SCIRun->FieldsCreate->FieldBoundary Module

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m5-faces-on {0}
setGlobal $m5-normalize-vectors {}
setGlobal $m5-has_scalar_data {1}
setGlobal $m5-active_tab {Edges}
setGlobal $m5-node_scale {0.0300}
setGlobal $m5-edge_scale {0.0150}
setGlobal $m5-show_progress {}

# Setup GUI for the SCIRun->Visualization->ShowField Module
setGlobal $m6-nodes-on {0}
setGlobal $m6-edges-on {0}
setGlobal $m6-normalize-vectors {}
setGlobal $m6-has_scalar_data {1}
setGlobal $m6-node_display_type {Spheres}
setGlobal $m6-node_scale {0.0300}
setGlobal $m6-edge_scale {0.0150}
setGlobal $m6-show_progress {}

# Setup GUI for the SCIRun->Visualization->GenStandardColorMaps Module
setGlobal $m7-width {335}
setGlobal $m7-height {40}
setGlobal $m7-resolution {255}
setGlobal $m7-realres {255}

# Setup GUI for the SCIRun->Visualization->RescaleColorMap Module
setGlobal $m8-min {-412.74857814679211}
