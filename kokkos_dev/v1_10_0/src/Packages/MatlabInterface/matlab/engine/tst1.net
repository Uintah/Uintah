# SCI Network 1.0

::netedit dontschedule

global userName
set userName "oleg"

global runDate
set runDate " Wed  Jan 30 2002"

global runTime
set runTime " 16:9:42"

set bbox {10.0 10.0 154 147}

set m0 [addModuleAtPosition "MatlabInterface" "DataIO" "Matlab" 10 10]
set m1 [addModuleAtPosition "SCIRun" "DataIO" "MatrixWriter" 10 90]

addConnection $m0 0 $m1 0

set $m0-notes {}
set $m0-show_status {1}
set $m0-hpTCL {127.0.0.1:5517 3}
set $m0-cmdTCL {o1=rand(5,5);

}
set $m1-notes {}
set $m1-show_status {1}
set $m1-filename {rand1.mat}
set $m1-filetype {ASCII}
set $m1-split {0}
$m0 initialize_ui

::netedit scheduleok
