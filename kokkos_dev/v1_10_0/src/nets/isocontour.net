set $m6-ViewWindow_0-aspect_ratio {}
set $m6-ViewWindow_0-unused {1}
set $m6-ViewWindow_0-unused {1}
set $m7-notes {}
set $m7-show_status {1}
set $m7-isoval {-0.11800}
