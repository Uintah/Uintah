# SCI Network 1.0

#######################################################################
# Check environment variables.  Ask user for input if not set:
set results [sourceSettingsFile]
set DATADIR [lindex $results 0]
set DATASET [lindex $results 1]
#######################################################################

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 13 11]
set m1 [addModuleAtPosition "SCIRun" "Fields" "FieldBoundary" 34 96]
set m2 [addModuleAtPosition "SCIRun" "Fields" "ApplyInterpolant" 13 174]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 13 332]
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 206 173]
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 206 253]
set m6 [addModuleAtPosition "SCIRun" "Render" "Viewer" 188 418]
set m7 [addModuleAtPosition "SCIRun" "Visualization" "Isosurface" 188 332]

addConnection $m0 0 $m1 0
addConnection $m1 1 $m2 1
addConnection $m0 0 $m2 0
addConnection $m2 0 $m3 0
addConnection $m4 0 $m5 0
addConnection $m2 0 $m5 1
addConnection $m5 0 $m3 1
addConnection $m3 0 $m6 0
addConnection $m5 0 $m7 1
addConnection $m2 0 $m7 0
addConnection $m7 1 $m6 1

set $m0-notes {}
set $m0-show_status {1}
set $m0-filename $DATADIR/$DATASET/$DATASET-voltage.tvd.fld
set $m1-notes {}
set $m1-show_status {1}
set $m2-notes {}
set $m2-show_status {1}
set $m3-notes {}
set $m3-show_status {1}
set $m3-nodes-on {1}
set $m3-nodes-as-disks {0}
set $m3-edges-on {0}
set $m3-use-normals {0}
set $m3-use-transparency {0}
set $m3-faces-on {0}
set $m3-vectors-on {0}
set $m3-normalize-vectors {}
set $m3-has_vec_data {0}
set $m3-bidirectional {0}
set $m3-arrow-heads-on {1}
set $m3-def-color-r {0.5}
set $m3-def-color-g {0.5}
set $m3-def-color-b {0.5}
set $m3-def-color-a {0.5}
set $m3-node_display_type {Points}
set $m3-edge_display_type {Lines}
set $m3-active_tab {Faces}
set $m3-node_scale {0.03}
set $m3-edge_scale {0.015}
