set $m4-minRes {12}
set $m4-resolution {255}
set $m4-realres {255}
set $m4-gamma {0}
set $m5-notes {}
set $m5-show_status {1}
set $m5-isFixed {0}
set $m5-min {        -0.649647}
set $m5-max {         0.370803}
