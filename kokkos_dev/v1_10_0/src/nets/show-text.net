set $m1-ViewWindow_0-view-up-y {        -0.888243}
set $m1-ViewWindow_0-view-up-z {         0.427766}
set $m1-ViewWindow_0-view-fov {               20}
set $m1-ViewWindow_0-view-eyep_offset-x {}
set $m1-ViewWindow_0-view-eyep_offset-y {}
