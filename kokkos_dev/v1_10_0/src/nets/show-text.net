# SCI Network 1.0

#######################################################################
# Check environment variables.  Ask user for input if not set:
set results [sourceSettingsFile]
set DATADIR [lindex $results 0]
set DATASET [lindex $results 1]
#######################################################################

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 12 11]
set m1 [addModuleAtPosition "SCIRun" "Render" "Viewer" 143 400]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 143 335]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 161 191]
set m4 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 161 272]
set m5 [addModuleAtPosition "SCIRun" "Fields" "FieldBoundary" 37 97]
set m6 [addModuleAtPosition "SCIRun" "Fields" "ApplyInterpolant" 12 191]

addConnection $m2 0 $m1 0
addConnection $m3 0 $m4 0
addConnection $m4 0 $m2 1
addConnection $m0 0 $m5 0
addConnection $m5 1 $m6 1
addConnection $m0 0 $m6 0
addConnection $m6 0 $m2 0
addConnection $m6 0 $m4 1

set $m0-notes {}
set $m0-show_status {1}
set $m0-filename $DATADIR/$DATASET/$DATASET-voltage.tvd.fld
set $m1-notes {}
set $m1-show_status {1}
$m1 ui
set $m1-ViewWindow_0-pos {}
set $m1-ViewWindow_0-caxes {1}
set $m1-ViewWindow_0-iaxes {}
set $m1-ViewWindow_0-view-eyep-x {          8.09284}
