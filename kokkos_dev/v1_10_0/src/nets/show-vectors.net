set $m3-faces-on {0}
set $m3-use-normals {0}
set $m3-use-transparency {0}
set $m3-vectors-on {1}
set $m3-normalize-vectors {1}
set $m3-has_vec_data {1}
set $m3-bidirectional {0}
set $m3-arrow-heads-on {1}
set $m3-text-on {0}
set $m3-text-use-default-color {1}
