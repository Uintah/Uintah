# SCI Network 1.0

#######################################################################
# Check environment variables.  Ask user for input if not set:
set results [sourceSettingsFile]
set DATADIR [lindex $results 0]
set DATASET [lindex $results 1]
#######################################################################

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 117 12]
set m1 [addModuleAtPosition "SCIRun" "Fields" "Gradient" 345 234]
set m2 [addModuleAtPosition "SCIRun" "Fields" "DirectInterpolate" 58 343]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 289 428]
set m4 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 233 106]
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 162 190]
set m6 [addModuleAtPosition "SCIRun" "Render" "Viewer" 474 513]
set m7 [addModuleAtPosition "SCIRun" "Fields" "ClipField" 492 104]

addConnection $m0 0 $m2 0
addConnection $m2 0 $m3 0
addConnection $m1 0 $m3 2
addConnection $m4 0 $m5 0
addConnection $m0 0 $m5 1
addConnection $m5 0 $m3 1
addConnection $m3 0 $m6 0
addConnection $m1 0 $m2 1
addConnection $m7 0 $m6 1
addConnection $m0 0 $m7 0
addConnection $m7 1 $m1 0

set $m0-notes {}
set $m0-show_status {1}
set $m0-filename $DATADIR/$DATASET/$DATASET-voltage.tvd.fld
set $m1-notes {}
set $m1-show_status {1}
set $m2-notes {}
set $m2-show_status {1}
set $m2-use_interp {1}
set $m2-use_closest {1}
set $m2-closeness_distance {1.0e15}
set $m3-notes {}
set $m3-show_status {1}
set $m3-nodes-on {0}
set $m3-nodes-as-disks {0}
