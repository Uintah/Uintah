set results [sourceSettingsFile]

if { $results == "failed" } {

    ::netedit scheduleok
    return 

} else {

    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "SCIRun" "Fields" "ChangeFieldDataType" 47 210]
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 47 20]
set m2 [addModuleAtPosition "SCIRun" "Fields" "TransformScalarData" 47 121]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 47 374]
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 47 461]
set m5 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 261 296]
set m6 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 261 181]

addConnection $m1 0 $m2 0
addConnection $m2 0 $m0 0
addConnection $m3 0 $m4 0
addConnection $m0 0 $m3 0
addConnection $m6 0 $m5 0
addConnection $m0 0 $m5 1
addConnection $m5 0 $m3 1

