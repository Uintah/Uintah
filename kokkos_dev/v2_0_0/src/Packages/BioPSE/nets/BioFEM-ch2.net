set results [sourceSettingsFile]

if { $results == "failed" } {

    ::netedit scheduleok
    return 

} else {

    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

set bbox {9 8 383 295}
set Name {Main}

# Create the Modules
set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 9 8]
set m1 [addModuleAtPosition "SCIRun" "Render" "Viewer" 9 251]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 188 9]
set m3 [addModuleAtPosition "SCIRun" "Visualization" "RescaleColorMap" 188 91]
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 9 174]

# Set the Module Notes Dispaly Options

# Create the Connections between Modules
set c0 [addConnection $m0 0 $m4 0]
set c1 [addConnection $m2 0 $m3 0]
set c2 [addConnection $m4 0 $m1 0]
set c3 [addConnection $m0 0 $m3 1]
set c4 [addConnection $m3 0 $m4 1]

# Mark which Connections are Disabled

# Set the Connection Notes and Dislpay Options

# Set the GUI variables for each Module
set $m0-notes {}
set $m0-filename $DATADIR/$DATASET/$DATASET-electrodes.pcd.fld

set $m1-notes {}
$m1 ui
set $m1-ViewWindow_0-view-eyep-x ${view-eyep-x}
set $m1-ViewWindow_0-view-eyep-y ${view-eyep-y}
set $m1-ViewWindow_0-view-eyep-z ${view-eyep-z}
set $m1-ViewWindow_0-view-lookat-x ${view-lookat-x}
set $m1-ViewWindow_0-view-lookat-y ${view-lookat-y}
set $m1-ViewWindow_0-view-lookat-z ${view-lookat-z}
set $m1-ViewWindow_0-view-up-x ${view-up-x}
set $m1-ViewWindow_0-view-up-y ${view-up-y}
set $m1-ViewWindow_0-view-up-z ${view-up-z}
set $m1-ViewWindow_0-view-fov ${view-fov}
set $m1-ViewWindow_0-specular-scale {0.4}

set $m2-notes {}
set $m2-tcl_status {Calling GenStandardColorMaps!}
set $m2-positionList {}
set $m2-nodeList {}
set $m2-width {1}
set $m2-height {1}
set $m2-mapType {3}
set $m2-minRes {12}
set $m2-resolution {128}
set $m2-realres {128}
set $m2-gamma {0}

set $m3-notes {}
set $m3-isFixed {0}
set $m3-min {-0.0214037}
set $m3-max {0.116037}
set $m3-makeSymmetric {0}

set $m4-notes {}
set $m4-nodes-on {1}
set $m4-edges-on {0}
set $m4-faces-on {0}
set $m4-has_scalar_data {1}
set $m4-text-on {0}
set $m4-def-color-r {0.00}
set $m4-def-color-g {0.00}
set $m4-def-color-b {1.00}
set $m4-def-color-a {0.50}
set $m4-node_display_type {Spheres}
set $m4-active_tab {Nodes}
set $m4-node_scale [expr 0.03 * ${global-scale}]
set $m4-field-name {Electrodes}
set $m4-node-resolution {5}

::netedit scheduleok
