set results [sourceSettingsFile]

if { $results == "failed" } {

    ::netedit scheduleok
    return 

} else {

    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

::netedit dontschedule

set bbox {9 8 174 204}
set Name {Main}

# Create the Modules
set m0 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 9 8]
set m1 [addModuleAtPosition "SCIRun" "Render" "Viewer" 9 160]
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 9 83]

# Set the Module Notes Dispaly Options

# Create the Connections between Modules
set c0 [addConnection $m0 0 $m2 0]
set c1 [addConnection $m2 0 $m1 0]

# Mark which Connections are Disabled

# Set the Connection Notes and Dislpay Options

# Set the GUI variables for each Module
set $m0-notes {}
set $m0-filename $DATADIR/$DATASET/$DATASET-voltage.tvd.fld

set $m1-notes {}
$m1 ui
set $m1-ViewWindow_0-view-eyep-x ${view-eyep-x}
set $m1-ViewWindow_0-view-eyep-y ${view-eyep-y}
set $m1-ViewWindow_0-view-eyep-z ${view-eyep-z}
set $m1-ViewWindow_0-view-lookat-x ${view-lookat-x}
set $m1-ViewWindow_0-view-lookat-y ${view-lookat-y}
set $m1-ViewWindow_0-view-lookat-z ${view-lookat-z}
set $m1-ViewWindow_0-view-up-x ${view-up-x}
set $m1-ViewWindow_0-view-up-y ${view-up-y}
set $m1-ViewWindow_0-view-up-z ${view-up-z}
set $m1-ViewWindow_0-view-fov ${view-fov}
set $m1-ViewWindow_0-specular-scale {0.4}

set $m2-notes {}
set $m2-nodes-on {1}
set $m2-edges-on {0}
set $m2-faces-on {0}
set $m2-has_scalar_data {1}
set $m2-text-on {0}
set $m2-def-color-r {0.00}
set $m2-def-color-g {0.00}
set $m2-def-color-b {1.00}
set $m2-def-color-a {0.50}
set $m2-node_display_type {Spheres}
set $m2-active_tab {Nodes}
set $m2-node_scale [expr 0.01 * ${global-scale}]
set $m2-field-name {UtahTorso}
set $m2-node-resolution {5}

::netedit scheduleok
