# SCI Network 1.0

if {[llength [array get env SCIRUN_DATA]] == 0} {
   puts ""
   puts "*** Error: This net depends on the SCIRUN_DATA enviroment variable."
   puts "*** Please exit, set it in your shell, and rerun."
   puts "*** (e.g. setenv SCIRUN_DATA /local/data/SCIRunData/)"
   puts ""
   ::netedit scheduleok
   return
}

set DATADIR $env(SCIRUN_DATA)
puts "*** Using SCIRUN_DATA $DATADIR"

::netedit dontschedule

global notes
set notes ""

set m0 [addModuleAtPosition "Teem" "Unu" "UnuConvert" 65 158]
set m1 [addModuleAtPosition "SCIRun" "DataIO" "FieldReader" 65 9]
set m2 [addModuleAtPosition "Teem" "DataIO" "FieldToNrrd" 65 84]
set m3 [addModuleAtPosition "SCIRun" "Fields" "FieldInfo" 288 89]
set m4 [addModuleAtPosition "SCIRun" "Fields" "FieldInfo" 432 317]
set m5 [addModuleAtPosition "Teem" "DataIO" "NrrdToField" 65 236]
set m6 [addModuleAtPosition "Teem" "NrrdData" "NrrdInfo" 401 238]

addConnection $m1 0 $m2 0
addConnection $m2 0 $m0 0
addConnection $m0 0 $m5 0
addConnection $m1 0 $m3 0
addConnection $m5 0 $m4 0
addConnection $m0 0 $m6 0

set $m0-notes {}
set $m0-show_status {1}
set $m0-type {10}
set $m1-notes {}
set $m1-show_status {1}
set $m1-filename $DATADIR/sphere/sphere-voltage.tvd.fld
set $m2-notes {}
set $m2-show_status {1}
set $m2-label {sphere-voltages}
set $m3-notes {}
set $m3-show_status {1}
set $m4-notes {}
set $m4-show_status {1}
set $m5-notes {}
set $m5-show_status {1}
set $m6-notes {}
set $m6-show_status {1}
$m0 initialize_ui
$m3 initialize_ui
$m4 initialize_ui

::netedit scheduleok
