<component name="UnuPermute" category="Unu">
  <overview>
    <authors>
      <author>David Weinstein</author>
      <author>Martin Cole</author>
    </authors>
    <summary>Specify axes to be swapped with eachother</summary>
    <description>
      <p>Specify in the UI which axes are to be swapped with
eachother. It is not allowed to swap the Tuple axis from axis 0.</p>
    </description>
    <examplesr>Teem/nets/test/permute.net</examplesr>
  </overview>
  <io>
    <inputs lastportdynamic="no">
      <port>
        <name>Nrrd</name>
	<datatype>Teem::Nrrd</datatype>
      </port>
    </inputs>
    <outputs>
      <port>
        <name>Nrrd</name>
	<datatype>Teem::Nrrd</datatype>
      </port>
    </outputs>
  </io>
  <testing>
  </testing>
</component>
