<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->
<!-- @version: -->
<Uintah_specification>

   <Meta>
       <title>Piston compressing gas adiabatically</title>
   </Meta>
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            1.0      </maxTime>
       <initTime>           0.0      </initTime>
       <delt_min>           0.0      </delt_min>
       <delt_max>           0.006    </delt_max>
       <delt_init>          1e-6     </delt_init>
       <timestep_multiplier>.5       </timestep_multiplier>
   </Time>
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
       <Level>
           <Box label = "1">
              <lower>         [ 0.0,   0.0,   0.0]         </lower>
              <upper>         [ 0.0001,0.0025,0.0001]</upper>
                <extraCells>  [1,1,1]               </extraCells>
              <patches>       [1,2,1]               </patches>
              <resolution>    [1,100,1]            </resolution>
           </Box>
       </Level>
    <BoundaryConditions>
      <Face side = "x-">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0" var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Dirichlet" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0" var = "Neumann" label = "Pressure">
                              <value> 0.0   </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Velocity">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Temperature">
                              <value> 0.0  </value>
        </BCType>
        <BCType id = "all" var = "Neumann" label = "Density">
                              <value> 0.0  </value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>                  
      <Face side = "z+">
        <BCType id = "0" var = "symmetry" label = "Symmetric">
        </BCType>
      </Face>
    </BoundaryConditions>
    </Grid>

    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <DataArchiver>
      <filebase>pistonValidationTest.uda</filebase>
      <outputInterval> 0.01 </outputInterval>
      <save label = "press_CC"/>
      <save label = "vol_frac_CC"/>
      <save label = "rho_CC"/>
      <save label = "sp_vol_CC"/>
      <save label = "delP_Dilatate"/>
      <save label = "speedSound_CC"/>
      <save label = "intE_source_CC"/>  
      <save label = "eng_L_ME_CC"/>
      <save label = "mom_L_ME_CC"/>
      <save label = "vel_CC"/>  
      <save label = "temp_CC"/>
      <save label = "p.x"/>
      <save label = "p.volume"/>
      <checkpoint cycle = "2" timestepInterval = "500"/>
<!--
      <save label = "mom_source_CC"/>    
      <save label = "mom_L_CC"/>
      <save label = "p.mass"/>
      <save label = "p.temperature"/>
      <save label = "g.mass"/>
      <save label = "g.stressFS"/>
-->
    </DataArchiver>
    <Debug>
     <dbg_timeStart>       0.0        </dbg_timeStart>
     <dbg_timeStop>        1.1        </dbg_timeStop>
     <dbg_outputInterval>  0.01       </dbg_outputInterval>
   
     <dbg_BeginIndex>      [0,0,0]    </dbg_BeginIndex>
     <dbg_EndIndex>        [0,100,0]  </dbg_EndIndex>
     <dbg_SigFigs>         16         </dbg_SigFigs>
     <dbg_Matls>           [0,1,2]    </dbg_Matls>
     <dbg_GnuPlot>         true       </dbg_GnuPlot> 
     <debug label = "switchDebug_EQ_RF_press"/> 
     <debug label = "switchDebug_advance_advect"/>   
     <!--
     <debug label = "switchDebugInitialize"/>
     <debug label = "switchDebug_vel_FC"/>           
     <debug label = "switchDebug_explicit_press"/>   
     <debug label = "switchDebug_PressFC"/>          
     <debug label = "switchDebugLagrangianValues"/> 
     <debug label = "switchDebugMomentumExchange_CC"/> 
     <debug label = "switchDebugSource_Sink"/>       
     <debug label = "switchDebug_advance_advect"/>  
     -->
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
       <cfl>0.4</cfl>
       <ICE>
        <max_iteration_equilibration> 100 </max_iteration_equilibration>
        <advection type = "FirstOrderCE"/>
        <solution technique = "EqForm"/>
      </ICE>        
    </CFD>
    <MPM>
       <time_integrator> explicit </time_integrator>
       <nodes8or27>8</nodes8or27>
    </MPM>
    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
      <gravity>                         [0,0,0]         </gravity>
      <reference_pressure>              1.01325         </reference_pressure>
    </PhysicalConstants>
    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!-- changed K from 0.5e-7 to 0.5e-6 -->
    <!--____________________________________________________________________-->

    <MaterialProperties>
       <MPM>
         <material name = " Piston Piston">
                <burn type = "null"/>
                <density>                  1.9e6         </density>
                <constitutive_model type = "comp_neo_hook_plastic">
                  <bulk_modulus>           10.2e6        </bulk_modulus>
                  <shear_modulus>          7.2e6         </shear_modulus>
                  <yield_stress>           .27e6         </yield_stress>
                  <hardening_modulus>      0.1e4         </hardening_modulus>
                  <alpha>                  0.0           </alpha>
                </constitutive_model>
                <toughness>                10.e6         </toughness>
                <thermal_conductivity>     .5e-7         </thermal_conductivity>
                <specific_heat>            1.004e-5      </specific_heat>
              <geom_object>
                  <box label = "Piston">
                     <min>   [ 0.0,   0.0010,0.0   ]     </min>
                     <max>   [ 0.001, 0.0015,0.0001]     </max>
                  </box>
                  <res>                   [2,2,2]         </res>
                  <velocity>              [0.0,-0.001,0.0] </velocity>
                  <temperature>           300             </temperature>
              </geom_object>
         </material>

           <contact>
              <type>rigid</type>
              <vel_fields>[1,0,0]</vel_fields>
              <stop_time>0.8 </stop_time>
           </contact>
      </MPM>
      <!--__________________________________ -->   
       <ICE>
         <material name = "Trapped air">
           <burn type = "null"/>
           <EOS type = "ideal_gas">                     </EOS>
           <viscosity>                  0.0             </viscosity>
           <thermal_conductivity>       0.0             </thermal_conductivity>
           <specific_heat>              716.0e-8        </specific_heat>
           <speed_of_sound>             376.0e-4        </speed_of_sound>
           <gamma>                      1.4             </gamma>           
           <geom_object> 
                <box label = "">                       
                 <min>   [-0.01, -0.005, -0.005]        </min>      
                 <max>   [ 0.01,  0.0010, 0.005]      </max>    
                </box>                                          

               <res>                  [2,2,2]           </res>
               <velocity>             [0.0,0.0,0.0]     </velocity>
               <temperature>          300.0             </temperature>
              <density>               1179.294693       </density>
               <pressure>              1.01325          </pressure>
           </geom_object>
         </material>
         
         <material name = "Air Above piston">
            <burn type = "null"/>
            <EOS type = "ideal_gas">                     </EOS>
           <viscosity>                  0.0             </viscosity>
           <thermal_conductivity>       0.0             </thermal_conductivity>
           <specific_heat>              716.0e-8        </specific_heat>
           <speed_of_sound>             376.0e-4        </speed_of_sound>
           <gamma>                      1.4             </gamma>           
           <geom_object>
                    <box label = "">
                     <min>   [-0.01, 0.0015, -0.005]     </min>
                     <max>   [ 0.01, 0.0025,  0.005]     </max>
                    </box>
               <res>                  [2,2,2]           </res>
               <velocity>             [0.0,-0.001,0.0]  </velocity>
               <temperature>          300.0             </temperature>
              <density>               1179.294693       </density>
               <pressure>              1.01325          </pressure>
           </geom_object>
         </material>
      </ICE>       

       <exchange_properties>
         <exchange_coefficients>
                          <!--  [0-1,   0-2,     1-2]  -->
              <momentum>       [1e15,   1e15,    1e15]     </momentum>
              <heat>           [ 0.001, 0.001,   0.001]     </heat>
          </exchange_coefficients>
          <mass_exchange>               false          </mass_exchange>
       </exchange_properties>
    </MaterialProperties>
       


    <PhysicalBC>
       <MPM>
      </MPM>
    </PhysicalBC>
    
</Uintah_specification>

