<?xml version="1.0" encoding="iso-8859-1"?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->

<!-- @version: -->

<Uintah_specification>

   <Meta>
	<title>Heat conduction problem w/burning</title>
   </Meta>

   <Time>
	<maxTime>.5</maxTime>
	<initTime>0.0</initTime>
	<delt_min>0.001</delt_min>
	<delt_max>0.01</delt_max>
	<timestep_multiplier>0.8</timestep_multiplier>
   </Time>

   <DataArchiver>
        <filebase>twomatburn.uda</filebase>
        <outputInterval>.01</outputInterval>
   </DataArchiver>

   <MPM>
	<time_integrator>explicit</time_integrator>
   </MPM>


    <PhysicalConstants>
	<gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
	<MPM>
	    <material>
                <burn> <type>null</type> </burn>
                <density>1000.0</density>
                <thermal_conductivity>237.0</thermal_conductivity>
                <specific_heat>903</specific_heat>
                <heat_transfer_coefficient>111</heat_transfer_coefficient>
                <comp_neo_hook>
                    <bulk_modulus>2000.0</bulk_modulus>
                    <shear_modulus>1500.0</shear_modulus>
                </comp_neo_hook>
		<geom_object>
		    <box label="gp1">
			<min>[0,0,0]</min>
			<max>[0.004,0.004,0.004]</max>
		    </box>
		    <res>[2,2,2]</res>
		    <velocity>[0.,0.,0.]</velocity>
		    <temperature>100.</temperature>
 		</geom_object>
		<velocity_field>0</velocity_field>
	    </material>

	    <material>
                <burn>
                  <type>simple</type>
                  <a>.000000011</a>
                  <b>200.</b>
                </burn>
                <density>1000.0</density>
                <thermal_conductivity>33.70</thermal_conductivity>
                <specific_heat>903</specific_heat>
                <heat_transfer_coefficient>111</heat_transfer_coefficient>
                <comp_neo_hook>
                    <bulk_modulus>2000.0</bulk_modulus>
                    <shear_modulus>1500.0</shear_modulus>
                </comp_neo_hook>
		<geom_object>
		    <box label="gp2">
			<min>[0.004,0,0]</min>
			<max>[0.012,0.004,0.004]</max>
		    </box>
		    <res>[2,2,2]</res>
		    <velocity>[0.,0.,0.]</velocity>
		    <temperature>0.</temperature>
 		</geom_object>
		<velocity_field>1</velocity_field>
	    </material>

            <contact>
               <type>single_velocity</type>
               <vel_fields>[1,2,3]</vel_fields>
            </contact>

            <heat_conduction></heat_conduction>
            <thermal_contact></thermal_contact>

	</MPM>

    </MaterialProperties>
	
    <Grid>
        <BoundaryConditions>
                 <Face side = "x-">
                        <BCType label = "Velocity" var = "Dirichlet">
                           <value> [0.0,0.0,0.0] </value>
                        </BCType>
                        <BCType label = "Temperature" var = "Dirichlet">
                            <value> 300.0 </value>
                        </BCType>
                </Face>
        </BoundaryConditions>
	<Level>
	    <Box label="1">
		<lower>[0,0,0]</lower>
		<upper>[0.012,0.004,0.004]</upper>
	        <resolution>[12,4,4]</resolution>
		<patches>[1,1,1]</patches>
	    </Box>
	</Level>
    </Grid>

    <PhysicalBC>
	<MPM>
	</MPM>
    </PhysicalBC>
    
</Uintah_specification>
