<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->
<!-- @version: Updated 7/31/00-->
<Uintah_specification>

   <Meta>
	<title>Constitutive Model Tester</title>
   </Meta>

   <Time>
	<maxTime>.05</maxTime>
	<initTime>0.0</initTime>
	<delt_min>2.e-8</delt_min>
	<delt_max>2.e-5</delt_max>
	<timestep_multiplier>0.3</timestep_multiplier>
   </Time>
   <DataArchiver>
        <filebase>const_test_vs.uda</filebase>
	<outputInterval>.01</outputInterval>
	<save label = "NTractionZMinus"/>
	<save label = "KineticEnergy"/>
	<save label = "TotalMass"/>
	<save label = "StrainEnergy"/>
	<save label = "CenterOfMassPosition"/>
	<save label = "CenterOfMassVelocity"/>
	<save label = "p.x"/>
	<save label = "p.velocity"/>
	<save label = "p.volume"/>
	<save label = "p.mass"/>
	<save label = "p.stress"/>
	<save label = "g.mass"/>
	<save label = "g.stressFS"/>
   </DataArchiver>

    <MPM>
	<time_integrator>explicit</time_integrator>
    </MPM>

    <PhysicalConstants>
	<gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
	<MPM>
	    <material>
                <burn>
                  <type>null</type>
                </burn>
		<density>1.e9</density>
		<hypo_elastic> 
		   <G>12.e6</G>
		   <K>32.e6</K>
	        </hypo_elastic>
		<toughness>10.e6</toughness>
		<thermal_conductivity>1.0</thermal_conductivity>
		<specific_heat>5</specific_heat>
		<geom_object>
		    <box label = "piston">
			<min>[0.0,0.0,.01]</min>
			<max>[0.01,0.01,.02]</max>
		    </box>
		    <res>[1,1,1]</res>
		    <velocity>[0.0,0.0,-1.e-2]</velocity>
		    <temperature>12</temperature>
 		</geom_object>
		<velocity_field>0</velocity_field>
	    </material>

	    <material>
                <burn>
                  <type>null</type>
                </burn>
		<density>1828.0</density>
                <visco_scram>
		    <PR>0.3</PR>
		    <CrackParameterA>1.e-3</CrackParameterA>
		    <CrackPowerValue>10.0</CrackPowerValue>
		    <CrackMaxGrowthRate>300.0</CrackMaxGrowthRate>
		    <StressIntensityF>500.0</StressIntensityF>
		    <CrackFriction>0.35</CrackFriction>
		    <InitialCrackRadius>3.e-5</InitialCrackRadius>
		    <CrackGrowthRate>12.</CrackGrowthRate>
		    <G1>3.5e8</G1>
		    <G2>1.739e8</G2>
		    <G3>5.212e8</G3>
		    <G4>9.085e8</G4>
		    <G5>6.875e8</G5>
		    <RTau1>0.0</RTau1>
		    <RTau2>7.32e3</RTau2>
		    <RTau3>7.32e4</RTau3>
		    <RTau4>7.32e5</RTau4>
		    <RTau5>2.e6</RTau5>
		    <Beta>1.5e-4</Beta>
		    <Gamma>1.0</Gamma>
		    <DCp_DTemperature>3.30773</DCp_DTemperature>
                </visco_scram>
		<toughness>10.e6</toughness>
		<thermal_conductivity>1.0</thermal_conductivity>
		<specific_heat>5</specific_heat>
		<geom_object>
		    <box label = "box">
			<min>[0.0,0.0,0.0]</min>
			<max>[0.01,0.01,0.01]</max>
		    </box>
		    <res>[1,1,1]</res>
		    <velocity>[0.0,0.0,-.5e-2]</velocity>
		    <temperature>12</temperature>
 		</geom_object>
		<velocity_field>1</velocity_field>
	    </material>

	    <contact>
	      <type>rigid</type>
	      <vel_fields>[0,1,0]</vel_fields>
              <stop_time>999999.9 </stop_time>
	    </contact>
	</MPM>

    </MaterialProperties>
	
    <Grid>
	<BoundaryConditions>
		 <Face side = "x-">
                        <BCType var = "symmetry" label = "Symmetric"> </BCType>
                </Face>
                <Face side = "x+">
                        <BCType var = "Dirichlet" label = "Velocity">
			     <value> [0.0,0.0,0.0] </value>
                        </BCType>
                </Face>
                <Face side = "y-">
                        <BCType var = "symmetry" label = "Symmetric"> </BCType>
                </Face>                  
		<Face side = "y+">
                        <BCType var = "Dirichlet" label = "Velocity">
     			    <value> [0.0,0.0,0.0] </value>
                        </BCType>
                </Face>                  
                <Face side = "z-">
                        <BCType var = "symmetry" label = "Symmetric"> </BCType>
                </Face>                  
		<Face side = "z+">
                        <BCType var = "Dirichlet" label = "Velocity">
       			    <value> [0.0,0.0,0.0] </value>
                        </BCType>
                </Face>                  
	</BoundaryConditions>
	<Level>
	    <Box label = "1">
		<lower>[0.0,0.0,0.0]</lower>
		<upper>[.02,.02,0.03]</upper>
	        <resolution>[2,2,3]</resolution>
		<patches>[1,1,1]</patches>
	    </Box>
	</Level>
    </Grid>

    <PhysicalBC>
	<MPM>
	</MPM>
    </PhysicalBC>
    
</Uintah_specification>

