<?xml version="1.0" encoding="iso-8859-1"?>


<!-- @version: -->

<Uintah_specification>

   <Meta>
	<title>Advection test</title>
   </Meta>

    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--   maxTime          = ending time for simulation                    -->
    <!--   delt_max         = maximum allowable delta time                  -->
    <!--   delt_init        = initial value for delta time                  -->
    <!--   timestep_mulipler= ???????????                                   -->
    <!--____________________________________________________________________-->
   <Time>
	<maxTime>            0.001      </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           0.0         </delt_min>
	<delt_max>           1.0         </delt_max>
	<delt_init>          1.0e-9      </delt_init>
	<timestep_multiplier>1.0         </timestep_multiplier>
   </Time>
    <!--____________________________________________________________________-->
    <!--   G  R  I  D     V  A  R  I  A  B  L  E  S                         -->
    <!--   lower            = Physical Coordinates (x, y, z)                -->
    <!--   upper            = Physical Coordinates (x, y, z)                -->
    <!--   resolution       = Number of cells in each directon              -->
    <!--   patches          = ????                                          -->
    <!--____________________________________________________________________-->
    <Grid>
		<BoundaryConditions>
		 <Face side = "x-">
                     <BCType label = "Pressure" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Velocity" var = "NegInterior">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0  </value>
                        </BCType>
                </Face>
                <Face side = "x+">
                     <BCType label = "Pressure" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Velocity" var = "NegInterior">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0  </value>
                        </BCType>
                </Face>
                <Face side = "y-">
                     <BCType label = "Pressure" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Velocity" var = "NegInterior">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0  </value>
                        </BCType>
                </Face>                  
		<Face side = "y+">
                     <BCType label = "Pressure" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Velocity" var = "NegInterior">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0  </value>
                        </BCType>
		</Face>
		<Face side = "z-">
                     <BCType label = "Pressure" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Velocity" var = "NegInterior">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0  </value>
                        </BCType>
	        </Face>
		<Face side = "z+">
                     <BCType label = "Pressure" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Velocity" var = "NegInterior">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType label = "Temperature" var = "Neumann">
                           <value> 0. </value>
                        </BCType>
			<BCType label = "Density" var = "Neumann">
                           <value> 0  </value>
                        </BCType>
	        </Face>
	</BoundaryConditions>
	<Level>
	    <Box label="1">
	    	<lower>       [0,0,0]     </lower>
	    	<upper>       [2,1,1]   </upper>
                <extraCells>[1,1,1]	  </extraCells>
		<patches>     [1,1,1]     </patches>
	    </Box>
	    <spacing>[0.04,1,1] </spacing>
	</Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--   filebase         = output file basenme                           -->
    <!--   outputInterval   = Time between filedumps                        -->
    <!--____________________________________________________________________-->
   <DataArchiver>
	<filebase>riemanSm.uda</filebase>
	<outputInterval>.0</outputInterval>
   </DataArchiver>

   <Debug>
   <!--<debug label = "switchDebugInitialize"/>             
	<debug label = "switchDebug_equilibration_press"/>
       <debug label = "switchDebug_vel_FC"/>                   
       <debug label = "switchDebug_explicit_press"/>         
       <debug label = "switchDebug_PressFC"/>                
       <debug label = "switchDebugLagrangianValues"/>        
       <debug label = "switchDebugSource_Sink"/>               
	<debug label = "switchDebug_advance_advect"/>   
    --> 	
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--    Need to comment                                                 -->
    <!--____________________________________________________________________-->
   <CFD>
  	<cfl>0.5</cfl>
	<ICE>
	    <max_iteration_equilibration> 50 </max_iteration_equilibration>
	</ICE>	 
    </CFD>
    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--    gravity         = (x, y, z dirs)                                -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
	<gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <!--____________________________________________________________________-->
    <!--     Material Properties and Initial Conditions                     -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
	<ICE>
	  <material>
	    <ideal_gas>
		<gas_constant>287                         </gas_constant>
	    </ideal_gas>
	    <viscosity>              0.0                 </viscosity>
	    <thermal_conductivity>   0.0                 </thermal_conductivity>
	    <specific_heat>          717.5               </specific_heat>
	    <speed_of_sound>         374.17              </speed_of_sound>
	    <gamma>                  1.4                  </gamma>
	    <velocity_field>         0.0                 </velocity_field>
	    <geom_object>
             <difference>
                <box label="wholegrid">
                    <min>           [-1.0, -1.0, -1.0 ]   </min>
                    <max>           [ 3.0,  2.0, 2.0 ]   </max>
                </box> 
                
                <box label="rightpartition">          
                    <min>           [ 1.0,-1.0, -1.0 ]   </min>
                    <max>           [ 3.0, 2.0,  2.0 ]   </max>
                </box>
              </difference>
               <res>		    [2,2,2]                 </res>
	        <velocity>       [0.0,0.0,0.0]           </velocity>
	        <temperature>    300.0                   </temperature>
               <density>        1.1768292682926831000   </density>
               <pressure>       101325.0                </pressure>
	    </geom_object>
           
           
	    <geom_object>
                <box label="rightpartition">
                    <min>[ 1.0, -1.0, -1.0 ]</min>
                    <max>[ 3.0, 2.0, 2.0 ]</max>
                </box>
                <res>	    [2,2,2]                 </res>
	        <velocity>       [0.0,0.0,0.0]           </velocity>
	        <temperature>    300.0                   </temperature>
               <density>        .11768292682926831000   </density>
               <pressure>       10132.50                </pressure>
	    </geom_object>
	  </material>
         
         
	  <exchange_coefficients>
	     <momentum>	    [0.0]       </momentum>
	     <heat>              [0.0]       </heat>
	 </exchange_coefficients>
	</ICE>	
    </MaterialProperties>  
                     
</Uintah_specification>
