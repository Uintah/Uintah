<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- @version: -->
<Uintah_specification>

   <Meta>
	<title>Advection test for a MPM and ICE material</title>
   </Meta>
    
    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--   maxTime          = ending time for simulation                    -->
    <!--   initTime         = ????????/                                     -->
    <!--   delt_min         = minimum allowable delta time                  -->
    <!--   delt_max         = maximum allowable delta time                  -->
    <!--   delt_init        = initial value for delta time                  -->
    <!--   timestep_mulipler= ???????????                                   -->
    <!--____________________________________________________________________-->
   <Time>
	<maxTime>            0.15       </maxTime>
	<initTime>           0.0         </initTime>
	<delt_min>           0.0         </delt_min>
	<delt_max>           1.0         </delt_max>
	<delt_init>          1.0e-4      </delt_init>
	<timestep_multiplier>0.8         </timestep_multiplier>
   </Time>
   
    <!--____________________________________________________________________-->
    <!--   G  R  I  D     V  A  R  I  A  B  L  E  S                         -->
    <!--   lower            = Physical Coordinates (x, y, z)                -->
    <!--   upper            = Physical Coordinates (x, y, z)                -->
    <!--   resolution       = Number of cells in each directon              -->
    <!--   patches          = ????                                          -->
    <!--____________________________________________________________________-->
    <Grid>
		<BoundaryConditions>
		 <Face side = "x-">
                     <BCType var = "Neumann" label = "Pressure">
                           <value> 0.0   </value>
                        </BCType>
			<BCType var = "Dirichlet" label = "Velocity">
                           <value> [10.,0.,0.] </value>
                        </BCType>
			<BCType var = "Neumann" label = "Temperature">
                           <value> 0.0  </value>
                        </BCType>
			<BCType var = "Neumann" label = "Density">
                           <value> 0.0  </value>
                        </BCType>
                </Face>
                <Face side = "x+">
			<BCType var = "Dirichlet" label = "Pressure">
                           <value> 101325.0   </value>
                        </BCType>
			<BCType var = "Neumann" label = "Velocity">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType var = "Neumann" label = "Temperature">
                           <value> 0.0  </value>
                        </BCType>
			<BCType var = "Neumann" label = "Density">
                           <value> 0.0  </value>
                        </BCType>
                </Face>
                <Face side = "y-">
			<BCType var = "Neumann" label = "Pressure">
                           <value> 0.0   </value>
                        </BCType>
			<BCType var = "Dirichlet" label = "Velocity">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType var = "Neumann" label = "Temperature">
                           <value> 0.0  </value>
                        </BCType>
			<BCType var = "Neumann" label = "Density">
                           <value> 0.0  </value>
                        </BCType>
                </Face>                  
		<Face side = "y+">
			<BCType var = "Neumann" label = "Pressure">
                           <value> 0.0   </value>
                        </BCType>
			<BCType var = "Dirichlet" label = "Velocity">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType var = "Neumann" label = "Temperature">
                           <value> 0.0  </value>
                        </BCType>
			<BCType var = "Neumann" label = "Density">
                           <value> 0.0  </value>
                        </BCType>
		</Face>
                <Face side = "z-">
			<BCType var = "Neumann" label = "Pressure">
                           <value> 0.0   </value>
                        </BCType>
			<BCType var = "Dirichlet" label = "Velocity">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType var = "Neumann" label = "Temperature">
                           <value> 0.0  </value>
                        </BCType>
			<BCType var = "Neumann" label = "Density">
                           <value> 0.0  </value>
                        </BCType>
                </Face>                  
		<Face side = "z+">
			<BCType var = "Neumann" label = "Pressure">
                           <value> 0.0   </value>
                        </BCType>
			<BCType var = "Dirichlet" label = "Velocity">
                           <value> [0.,0.,0.] </value>
                        </BCType>
			<BCType var = "Neumann" label = "Temperature">
                           <value> 0.0  </value>
                        </BCType>
			<BCType var = "Neumann" label = "Density">
                           <value> 0.0  </value>
                        </BCType>
		</Face>
	</BoundaryConditions>
	<Level>
	    <Box label = "1">
	    	<lower>       [0,0,0]     </lower>
	    	<upper>       [3.5,2.5,2.5]     </upper>
               <extraCells> [1,1,1]     </extraCells>
		<patches>     [1,1,1]     </patches>
	    </Box>
	    <spacing>        [0.125,0.125,0.125]  </spacing>
	</Level>
    </Grid>
   
    <!--____________________________________________________________________-->
    <!--   O  U  P  U  T     V  A  R  I  A  B  L  E  S                      -->
    <!--   filebase         = output file basenme                           -->
    <!--   outputInterval   = Time between filedumps                        -->
    <!--   switchDebug      = various debugging output                      -->
    <!--____________________________________________________________________-->
   <DataArchiver>
        <filebase>diw.uda</filebase>
        <outputInterval>.01</outputInterval>
        <save label = "mom_L_ME_CC"/>
        <save label = "vel_CC"/>
        <save label = "press_CC"/>
        <save label = "press_equil_CC"/>
        <save label = "delPress_CC"/>
        <save label = "rho_CC"/>
        <save label = "p.x"/>
        <save label = "p.volume"/>
        <save label = "p.velocity"/>
   </DataArchiver>

   <Debug>
     <!--
     <debug label = "switchDebugInitialize"/>
     <debug label = "switchDebug_equilibration_press"/>
     <debug label = "switchDebug_vel_FC"/>           
     <debug label = "switchDebug_explicit_press"/>   
     <debug label = "switchDebug_PressFC"/>          
     <debug label = "switchDebugLagrangianValues"/> 
     <debug label = "switchDebugMomentumExchange_CC"/> 
     <debug label = "switchDebugSource_Sink"/>       
     <debug label = "switchDebug_advance_advect"/>  
     -->
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--    Need to comment                                                 -->
    <!--____________________________________________________________________-->
   <CFD>
  	<cfl>0.5</cfl>
	<ICE>
	    <max_iteration_equilibration> 50 </max_iteration_equilibration>
	</ICE>	 
    </CFD>

    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--    gravity         = (x, y, z dirs)                                -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
	<gravity>[0,0,0]</gravity>
    </PhysicalConstants>


    <!--____________________________________________________________________-->
    <!--    MATERIAL PROPERTIES INITIAL CONDITIONS                          -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
        <MPM>
            <material>
                <burn>
                  <type>null</type>
                </burn>
	        <ideal_gas>
		    <gas_constant>      287             </gas_constant>
	        </ideal_gas>
                <density>   1.1792946927374306000e+00   </density>
                <comp_neo_hook>
                    <bulk_modulus>      2000.0          </bulk_modulus>
                    <shear_modulus>     1500.0          </shear_modulus>
                </comp_neo_hook>
                <toughness>             10.e6           </toughness>
                <thermal_conductivity>  1.0             </thermal_conductivity>
                <specific_heat>         716.0           </specific_heat>
	        <gamma>                 1.4             </gamma>	    
                <geom_object>
                    <box label = "gpmpm">
                        <min>           [1.0,0.0,1.0]   </min>
                        <max>           [1.5,1.5,1.5]   </max>
                    </box>
                    <res>               [2,2,2]         </res>
                    <velocity>          [0.0,0.0,0.0]   </velocity>
                    <temperature>       300             </temperature>
                </geom_object>
            </material>

            <contact>
              <type>null</type>
              <vel_fields>              [0,0,0]         </vel_fields>
            </contact>
        </MPM>

	<ICE>
	  <material>
	    <ideal_gas>
		<gas_constant>            287             </gas_constant>
	    </ideal_gas>
	    <viscosity>                  0.0             </viscosity>
	    <thermal_conductivity>       0.0             </thermal_conductivity>
	    <specific_heat>              716.0           </specific_heat>
	    <speed_of_sound>             376             </speed_of_sound>
	    <gamma>                      1.4             </gamma>	    
	    <geom_object>
		<difference>
                <box label = "gpice">
                    <min>           [-1.0,-1.0,-1.0]    </min>
                    <max>           [4.0,4.0,4.0 ]      </max>
                </box>
		<box label = "gpicehole">
                    <min>           [1.0,0.0,1.0]       </min>
                    <max>           [1.5,1.5,1.5]       </max>
                </box>
		</difference>
	        <res>		        [2,2,2]             </res>
	        <velocity>           [10.0,0.0,0.0]       </velocity>
	        <temperature>        300.0               </temperature>
	        <density>    1.1792946927374306000e+00   </density>
               <pressure>           101325.0            </pressure>
	    </geom_object>
	  </material>
	</ICE>	
       
      
       <MPMICE>
         <exchange_coefficients>
	       <momentum>	       [1e15, 1e15]         </momentum>
	       <heat>                  [0.0, 0.0]           </heat>
	   </exchange_coefficients>
       </MPMICE>
    </MaterialProperties>
            

    <PhysicalBC>
        <MPM>
        </MPM>
    </PhysicalBC>
	
</Uintah_specification>

