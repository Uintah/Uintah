<?xml version="1.0" encoding="iso-8859-1"?>


<!-- @version: -->

<Uintah_specification>

   <Meta>
       <title>Rieman problem with liquid WP properties</title>
   </Meta>    
    <!--____________________________________________________________________-->
    <!--      T  I  M  E     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
   <Time>
       <maxTime>            0.0001      </maxTime>
       <initTime>           0.0         </initTime>
       <delt_min>           1.0e-08     </delt_min>
       <delt_max>           2.4e-05     </delt_max>
       <delt_init>          1.0e-08     </delt_init>
       <timestep_multiplier>0.5         </timestep_multiplier>
   </Time>
    <!--____________________________________________________________________-->
    <!--      G  R  I  D     V  A  R  I  A  B  L  E  S                      -->
    <!--____________________________________________________________________-->
    <Grid>
    <BoundaryConditions>
      <Face side = "x-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "x+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "y-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>                  
      <Face side = "y+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "z-">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
      <Face side = "z+">
        <BCType id = "0"   label = "Pressure"     var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Velocity"     var = "Neumann">
                              <value> [0.,0.,0.] </value>
        </BCType>
        <BCType id = "0"   label = "Temperature"  var = "Neumann">
                              <value> 0. </value>
        </BCType>
        <BCType id = "0"   label = "Density"      var = "Neumann">
                              <value> 0  </value>
        </BCType>
      </Face>
    </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>        [0,0,0]    </lower>
              <upper>        [2,1,1]    </upper>
              <extraCells>   [1,1,1]    </extraCells>
              <patches>      [1,1,1]    </patches>
           </Box>
           <spacing>[0.02,1,1] </spacing>
       </Level>
    </Grid>    
    <!--____________________________________________________________________-->
    <!--      O  U  P  U  T     V  A  R  I  A  B  L  E  S                   -->
    <!--____________________________________________________________________-->
   <DataArchiver>
      <filebase>riemanGruneisen.uda</filebase>
      <outputInterval>.00001</outputInterval>
      <save label="vel_CC"/>
      <save label="press_CC"/>
      <save label="press_equil_CC"/>
      <save label="delP_Dilatate"/>
      <save label="rho_CC"/>
      <save label="sp_vol_CC"/>
      <save label="speedSound_CC"/>
   </DataArchiver>

   <Debug>
       <dbg_timeStart>       0.00   </dbg_timeStart>
       <dbg_timeStop>        0.001  </dbg_timeStop>
       <dbg_outputInterval>  1e-4   </dbg_outputInterval>
       <dbg_Matls>           [0,1]   </dbg_Matls>

       <dbg_GnuPlot>true</dbg_GnuPlot>
       <dbg_BeginIndex>   [[-1,0,0]] </dbg_BeginIndex>
       <dbg_EndIndex>     [[101,1,1]]</dbg_EndIndex>
       <dbg_SigFigs>        10      </dbg_SigFigs>

       <debug label = "switchDebug_advance_advect"/>
       <debug label = "switchDebug_explicit_press"/>
   <!--<debug label = "switchDebug_Initialize"/>             
       <debug label = "switchDebug_equil_press"/>
       <debug label = "switchDebug_vel_FC"/>                   
       <debug label = "switchDebug_explicit_press"/>         
       <debug label = "switchDebug_PressFC"/>                
       <debug label = "switchDebug_LagrangianValues"/>        
       <debug label = "switchDebug_Source_Sink"/>               
       <debug label = "switchDebug_advance_advect"/>   
    -->        
   </Debug>
    <!--____________________________________________________________________-->
    <!--    I  C  E     P  A  R  A  M  E  T  E  R  S                        -->
    <!--____________________________________________________________________-->
    <CFD>
         <cfl>0.5</cfl>
       <ICE>
        <advection type = "SecondOrder" />
      </ICE>        
    </CFD>
    <!--____________________________________________________________________-->
    <!--     P  H  Y  S  I  C  A  L     C  O  N  S  T  A  N  T  S           -->
    <!--____________________________________________________________________-->   
    <PhysicalConstants>
       <gravity>            [0,0,0]   </gravity>
       <reference_pressure> 101325.0  </reference_pressure>
    </PhysicalConstants>

    <!--____________________________________________________________________-->
    <!--     Material Properties and Initial Conditions                     -->
    <!--____________________________________________________________________-->
    <MaterialProperties>
       <ICE>
         <material>
           <EOS type = "Gruneisen">
             <A>1.3338e9</A>
             <B>7.027e5</B>
             <rho0>1823.3</rho0>
             <P0>101325.0</P0>
             <T0>317.4</T0>
           </EOS>
           <dynamic_viscosity>      0.0                 </dynamic_viscosity>
           <thermal_conductivity>   0.0                 </thermal_conductivity>
           <specific_heat>          820.7               </specific_heat>
           <gamma>                  1.0                 </gamma>
           <geom_object>
             <difference>
                <box label="wholegrid">
                    <min>           [-1.0, -1.0, -1.0 ]   </min>
                    <max>           [ 3.0,   2.0, 2.0 ]   </max>
                </box> 
                
                <box label="rightpartition">          
                    <min>           [ 1.0, -1.0, -1.0 ]   </min>
                    <max>           [ 3.0,  2.0,  2.0 ]   </max>
                </box>
              </difference>
               <res>                  [1,1,1]           </res>
               <velocity>       [0.0,0.0,0.0]           </velocity>
               <temperature>    317.4                   </temperature>
               <density>        1823.3                  </density>
               <pressure>       101325.0                </pressure>
           </geom_object>

           <geom_object>
                <box label="rightpartition">            </box>
                <res>           [1,1,1]                 </res>
               <velocity>       [0.0,0.0,0.0]           </velocity>
               <temperature>    317.4                   </temperature>
               <density>        1823.17534017           </density>
               <pressure>       10132.5                 </pressure>
           </geom_object>
         </material>

      </ICE>       
    </MaterialProperties>  
                     
</Uintah_specification>
