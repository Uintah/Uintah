<Uintah_specification>

  <Meta>
    <title>Lid Driven Cavity Problem</title>
  </Meta>

  <SimulationComponent type="wasatch"/>
   <Solver type="hypre"/>
  <Wasatch>
      <TimeIntegrator> FE </TimeIntegrator>            

    <PostProcessingExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="vel_magnitude" state="STATE_NONE"/>
      <VelocityMagnitude>
      <XVelocity>
        <NameTag name="u" state="STATE_NONE"/>
      </XVelocity>
      <YVelocity>
        <NameTag name="v" state="STATE_NONE"/>
      </YVelocity>
      <ZVelocity>
        <NameTag name="w" state="STATE_NONE"/>
      </ZVelocity>      
      </VelocityMagnitude>
    </PostProcessingExpression>

    <ForceOnGraph tasklist="advance_solution">
      <NameTag name="vel_magnitude" state="STATE_NONE"/>                
    </ForceOnGraph>

    <ParticleTransportEquations>
      <NumberOfInitialParticles>1</NumberOfInitialParticles>
      <ParticleDensity name="p.rho" state="STATE_NONE"/>            
      <ParticleMass name="p.m" state="STATE_DYNAMIC">
      	<SourceTerm name="nothing" state="STATE_NONE"/>	
      </ParticleMass>      
      <ParticleSize name="p.size" state="STATE_DYNAMIC">      
      	<SourceTerm name="nothing" state="STATE_NONE"/>
      </ParticleSize>
			<ParticlePosition x="p.xw" y="p.yw" z="p.zw">
				<Velocity>
					<XVel name="p.u" state="STATE_DYNAMIC"/>
					<YVel name="p.v" state="STATE_DYNAMIC"/>
					<ZVel name="p.w" state="STATE_DYNAMIC"/>	
				</Velocity>
			</ParticlePosition>
			<ParticleMomentum x="p.u" y="p.v" z="p.w">
<!-- 				<DisableDragForce/> -->
				<Gravity direction="Z"/>
				<GasProperties>
					<GasViscosity name="viscosity" state="STATE_NONE"/>
					<GasDensity name="density" state="STATE_NONE"/>					
					<GasVelocity>
						<XVel name="u" state="STATE_NONE"/>
						<YVel name="v" state="STATE_NONE"/>
						<ZVel name="w" state="STATE_NONE"/>		
					</GasVelocity>				
				</GasProperties>
			</ParticleMomentum>
    </ParticleTransportEquations>

    <!-- Momentum transport equations -->
    <MomentumEquations>
 	
      <X-Velocity>u</X-Velocity>
      <X-Momentum>x-mom</X-Momentum>
      <Y-Velocity>v</Y-Velocity>
      <Y-Momentum>y-mom</Y-Momentum>
      <Z-Velocity>w</Z-Velocity>
      <Z-Momentum>z-mom</Z-Momentum>
      <Viscosity>
        <NameTag name="viscosity" state="STATE_NONE"/>
      </Viscosity>
      
      <Pressure>
       <ReferencePressure value="0.0">
              <ReferenceCell>[0,0,0]</ReferenceCell>
        </ReferencePressure>
        <Use3DLaplacian>    true</Use3DLaplacian>

        <Parameters>
 	 	 	 	 	 	 <skip>1</skip>
            <solver>         cg      </solver>
            <preconditioner> pfmg    </preconditioner>
            <tolerance>      1e-10  </tolerance>
            <maxiterations>  100   </maxiterations>
            <relax_type>     2       </relax_type>
           </Parameters>

      </Pressure>
    </MomentumEquations>    
    <Density>
      <Constant value="1.0" name="density"/>
    </Density>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.size" state="STATE_DYNAMIC"/>
       <Constant>0.125</Constant>
<!--        <RandomField low="0.01" high="0.02" seed="11.342"/> -->
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.rho" state="STATE_NONE"/>
       <Constant>1</Constant>
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.xw" state="STATE_DYNAMIC"/>
       <Constant>0.5</Constant>
       
<!-- 
       <ParticlePositionIC coordinate="X" bounds="PATCHBASED">
       		<Random seed="0.1"/>
       </ParticlePositionIC>
 -->
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.yw" state="STATE_DYNAMIC"/>
       <Constant>0.5</Constant>
      <!-- 
       <ParticlePositionIC coordinate="Y" bounds="PATCHBASED">
       		<Random seed="0.23"/>
       </ParticlePositionIC>
 -->
    </BasicExpression>

    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.zw" state="STATE_DYNAMIC"/>
       <Constant>0.9</Constant>
<!-- 
       <RandomField low="0.1" high="0.9" seed="0.03">
				 <ExprLoHi type="ZVOL" name="ZZVOL" state="STATE_NONE"/>       
       </RandomField>
 -->
    </BasicExpression>


    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.u" state="STATE_DYNAMIC"/>
       <Constant>0.0</Constant>
    </BasicExpression>
    
    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.v" state="STATE_DYNAMIC"/>
       <Constant>0.0</Constant>
    </BasicExpression>
    
    <BasicExpression type="PARTICLE">
       <TaskList>initialization</TaskList>
       <NameTag name="p.w" state="STATE_DYNAMIC"/>
       <Constant>0.0</Constant>
    </BasicExpression>


    <!-- Set the initial condition expression for x-momentum -->
    <BasicExpression type="XVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="u" state="STATE_NONE"/>
       <Constant>0.0</Constant>
    </BasicExpression>

    <BasicExpression type="YVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="v" state="STATE_NONE"/>
       <Constant>0.0</Constant>
    </BasicExpression>

    <BasicExpression type="ZVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="w" state="STATE_NONE"/>
       <Constant>0.0</Constant>
    </BasicExpression>

                   
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="viscosity" state="STATE_N"/>
      <Constant>0.01</Constant>
    </BasicExpression>
    
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="viscosity" state="STATE_NONE"/>
      <Constant>0.01</Constant>
    </BasicExpression>

  </Wasatch>


  <Time>
    <maxTime>1</maxTime>
    <initTime>0.0</initTime>
    <delt_min>0.00001</delt_min>
    <delt_max>0.00001</delt_max>
    <max_Timesteps> 10</max_Timesteps>
    <timestep_multiplier>1.0</timestep_multiplier>
  </Time>


  <DataArchiver>
    <filebase>particle-test-free-fall-momsrc.uda</filebase>
    <outputTimestepInterval>1</outputTimestepInterval>
    <outputInitTimestep/>
    <checkpoint cycle="2" interval="0.0001"/>
    <save label="p.x"/>
    <save label="x-mom"/>    
    <save label="vel_magnitude"/>        
    <save label="y-mom"/>        
    <save label="z-mom"/>        
    <save label="p.xw_rhs"/>    
    <save label="p.yw_rhs"/>    
    <save label="p.zw_rhs"/>            

    <save label="p.re"/>    
    <save label="p.cd"/>        
    <save label="p.tau"/>    
    <save label="p.drag_x"/>    
    <save label="p.drag_y"/>    
		<save label="p.drag_z"/>    

		<save label="p.body_z"/>        
    <save label="p.u_rhs"/>    
    <save label="p.v_rhs"/>    
    <save label="p.w_rhs"/>            
    <save label="p.m"/>
    <save label="p.u"/>    
    <save label="p.v"/>        
    <save label="p.w"/>        
    
    <save label="p_momsrc_x"/>    
    <save label="p_momsrc_y"/>    
    <save label="p_momsrc_z"/>        
    
    <save label="p.size"/>    
    <save label="p.xw"/>        
    <save label="p.yw"/>
    <save label="p.zw"/>    
  </DataArchiver> 

	<Grid>
		<BoundaryConditions>
			<DefaultMaterial>all</DefaultMaterial>
			<!-- BOUNDARY CONDITIONS FOR MOMENTUM -->
			<Face side="x-" type="Wall"/>
			<Face side="x+" type="Wall"/>
			<Face side="y-" type="Wall"/>
			<Face side="y+" type="Wall"/>
			<Face side="z-" type="Open"/>			
			<Face side="z+" type="Open"/>			
		</BoundaryConditions>

		<Level>
			<Box label="1">
				<lower>[0,0,-2]</lower>
				<upper>[1,1,1]</upper>
				<resolution>[8,8,24]</resolution>
				<patches>[1,1,1]</patches>
			</Box>
			<periodic>[0,0,0]</periodic>
		</Level>
	</Grid>

</Uintah_specification>
