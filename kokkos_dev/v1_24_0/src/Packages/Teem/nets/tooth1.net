   # Create the Connections between Modules
   set c1 [addConnection $m6 0 $m1 0]
   set c2 [addConnection $m5 0 $m6 0]
   set c3 [addConnection $m2 0 $m3 0]
   set c4 [addConnection $m1 0 $m2 0]
