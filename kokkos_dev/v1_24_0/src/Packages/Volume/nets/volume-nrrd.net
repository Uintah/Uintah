# Ask SCIRun to tell us where the data is
init_DATADIR_and_DATASET

set name {Main}
set bbox {23 27 398 415}
set creationDate {Tue Jun 29 2004}
set creationTime {10:22:38}
set runDate {}
set runTime {}
set notes {}

# Create a Volume->Visualization->NrrdTextureBuilder Module
set m1 [addModuleAtPosition "Volume" "Visualization" "NrrdTextureBuilder" 23 133]

# Create a Volume->Visualization->VolumeVisualizer Module
set m2 [addModuleAtPosition "Volume" "Visualization" "VolumeVisualizer" 23 260]

# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m3 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 203 133]

# Create a SCIRun->Render->Viewer Module
set m4 [addModuleAtPosition "SCIRun" "Render" "Viewer" 23 371]

# Create a Teem->DataIO->NrrdReader Module
set m5 [addModuleAtPosition "Teem" "DataIO" "NrrdReader" 23 27]

# Create a Teem->DataIO->NrrdReader Module
set m6 [addModuleAtPosition "Teem" "DataIO" "NrrdReader" 203 27]

# Create the Connections between Modules
set c1 [addConnection $m5 0 $m1 0]
set c2 [addConnection $m1 0 $m2 0]
set c3 [addConnection $m2 0 $m4 0]
set c4 [addConnection $m3 0 $m2 1]

# Set GUI variables for the Volume->Visualization->VolumeVisualizer Module
set $m2-num_slices {250}
set $m2-alpha_scale {0.03}
set $m2-shading {1}
set $m2-ambient {0.3}
set $m2-diffuse {0.662}
set $m2-shine {30}

# Open the Volume->Visualization->VolumeVisualizer UI
$m2 initialize_ui

# Set GUI variables for the SCIRun->Visualization->GenStandardColorMaps Module
set $m3-positionList {{0 40} {111 40} {151 0} {183 40} {302 40}}
set $m3-nodeList {257 258 259 260 261}
set $m3-width {302}
set $m3-height {40}
set $m3-mapType {17}
set $m3-minRes {3}

$m4 addViewer
set $m4-ViewWindow_0-geometry 679x680+470+93

# Set GUI variables for the SCIRun->Render->Viewer Module
set $m4-ViewWindow_0-pos {}
set $m4-ViewWindow_0-caxes {0}
set $m4-ViewWindow_0-raxes {1}
