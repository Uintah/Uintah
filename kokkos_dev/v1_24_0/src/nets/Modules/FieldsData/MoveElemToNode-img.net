# Create a SCIRun->Visualization->GenStandardColorMaps Module
set m8 [addModuleAtPosition "SCIRun" "Visualization" "GenStandardColorMaps" 335 63]

# Create a SCIRun->FieldsCreate->SamplePlane Module
set m9 [addModuleAtPosition "SCIRun" "FieldsCreate" "SamplePlane" 88 10]

# Create the Connections between Modules
set c1 [addConnection $m9 0 $m6 0]
set c2 [addConnection $m1 0 $m2 0]
set c3 [addConnection $m6 0 $m5 0]
set c4 [addConnection $m5 0 $m1 0]
set c5 [addConnection $m5 0 $m4 0]
set c6 [addConnection $m8 0 $m7 0]
set c7 [addConnection $m4 0 $m3 0]
set c8 [addConnection $m6 0 $m7 1]
set c9 [addConnection $m7 0 $m2 1]
set c10 [addConnection $m7 0 $m4 1]
set c11 [addConnection $m2 0 $m3 1]
set c12 [addConnection $m5 1 $m3 2]

