set results [sourceSettingsFile]

if { $results == "failed" } {
    ::netedit scheduleok
    return
} else {
    set DATADIR [lindex $results 0]
    set DATASET [lindex $results 1]
}

source $DATADIR/$DATASET/$DATASET.settings

global userName
set userName ""

global runDate
set runDate ""

global runTime
set runTime ""

global notes
set notes ""

global Subnet
set Subnet(Subnet$Subnet(Loading)_Name) {Main}
set bbox {40 10 530 546}

# Create a SCIRun->FieldsData->MoveElemToNode Module
set m1 [addModuleAtPosition "SCIRun" "FieldsData" "MoveElemToNode" 196 260]

# Create a SCIRun->Visualization->ShowField Module
set m2 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 354 382]

# Create a SCIRun->Render->Viewer Module
set m3 [addModuleAtPosition "SCIRun" "Render" "Viewer" 206 502]

# Create a SCIRun->Visualization->ShowField Module
set m4 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 40 413]

# Create a SCIRun->FieldsGeometry->ChangeFieldBounds Module
set m5 [addModuleAtPosition "SCIRun" "FieldsGeometry" "ChangeFieldBounds" 52 176]

# Create a SCIRun->FieldsData->TransformData Module
set m6 [addModuleAtPosition "SCIRun" "FieldsData" "TransformData" 71 96]

# Create a SCIRun->Visualization->RescaleColorMap Module
