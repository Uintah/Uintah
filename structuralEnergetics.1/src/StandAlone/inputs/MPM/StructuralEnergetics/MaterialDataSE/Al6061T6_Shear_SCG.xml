<!-- 6061-T6 Aluminum -->
<!-- Aluminum for Structural Energetics -->
<!--  Base Units -->
<!--  mass        - microgram     -->
<!--  time        - microsecond   -->
<!--  length      - millimeter    -->
<!--  temperature - Kelvin        -->

<!--  Pressure Conversion: -->
<!--  1 Pa = 1 kg/(m*s^2) = 1e9 microgram/(1e3 mm * 1e12 microsecond^2) -->
<!--  1 Pa = 1e-6 microgram/(mm*microsecond^2) -->
<!--  1 GPa = 1e+9 Pa = 1e+3 microgram/(mm*microsecond^2) -->
<Uintah_Include>
  <shear_modulus_model type="scg_shear">
    <mu_0> 27.6E+3</mu_0>       <!--  27.6      GPa   -->
    <A> 65.0E-6 </A>            <!--  65.0e-12  1/Pa  -->
    <B> 0.62E-3 </B>            <!--  0.62e-3   1/K   -->
  </shear_modulus_model>
</Uintah_Include>
