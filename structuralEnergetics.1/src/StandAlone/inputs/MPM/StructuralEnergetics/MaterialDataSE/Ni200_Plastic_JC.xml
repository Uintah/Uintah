<!-- Nickel 200 -->
<!--  Base Units -->
<!--  mass        - microgram     -->
<!--  time        - microsecond   -->
<!--  length      - millimeter    -->
<!--  temperature - Kelvin        -->

<!--  Pressure Conversion: -->
<!--  1 Pa = 1 kg/(m*s^2) = 1e9 microgram/(1e3 mm * 1e12 microsecond^2) -->
<!--  1 Pa = 1e-6 microgram/(mm*microsecond^2) -->
<!--  1 GPa = 1e+9 Pa = 1e+3 microgram/(mm*microsecond^2) -->
<Uintah_Include>
  <flow_model type="johnson_cook">
    <A>163.4</A>        <!-- mm-microgram-microsec -->
    <B>648.1</B>        <!-- mm-microgram-microsec -->
    
    <A>163.4E6</A>      <!-- mks -->  
    <B>648.1E6</B>      <!-- mks -->  
        
    <C>0.006</C>
    <n>0.33</n>
    <m>1.44</m>
    <T_r>300.0</T_r>    <!--    300 K   -->
    <T_m>1728.0</T_m>   <!--   1728 K   -->
  </flow_model>
</Uintah_Include>
