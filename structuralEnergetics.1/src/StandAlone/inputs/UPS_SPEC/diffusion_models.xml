<?xml version='1.0' encoding='ISO-8859-1' ?>
<Uintah_Include>

  <diffusion_model         spec="OPTIONAL NO_DATA"
                           attribute1="type REQUIRED STRING 'jg, rf1, null, 
                                       non_linear1, non_linear2, constant_rate,
                                       bazant, activated, activated2, cahn-hilliard'">                                       
                                       
    <diffusivity           spec="REQUIRED DOUBLE" />
    <max_concentration     spec="REQUIRED DOUBLE 'positive' " />
    <min_concentration     spec="OPTIONAL DOUBLE " />
    <initial_concentration spec="OPTIONAL DOUBLE " />
    <conc_tolerance        spec="OPTIONAL DOUBLE 'positive' " />
    
    <BoltzmannConstant          spec="REQUIRED DOUBLE" need_applies_to="type bazant, non_linear2" />
    
    <!--  Variables specific to the Cahn-Hilliard test model -->
    <gamma                      spec="REQUIRED DOUBLE " need_applies_to="type cahn-hilliard" />
    <a                          spec="REQUIRED DOUBLE 'positive' " need_applies_to="type cahn-hilliard" />
    <b                          spec="REQUIRED DOUBLE 'positive' " need_applies_to="type cahn-hilliard" />
    <random_scale               spec="OPTIONAL DOUBLE 'positive' " need_applies_to="type cahn-hilliard" />
    
    <!-- Variables specific to the Bazant model (for now) -->
    <regularSolutionParameter   spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <penaltyCahnHilliard        spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <intercolantSiteDensity     spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <molecularWeight            spec="REQUIRED DOUBLE" need_applies_to="type bazant" />
    <externalChemicalPotential  spec="OPTIONAL DOUBLE" />
    <mismatchStrain             spec="OPTIONAL NO_DATA" >
      <v1                       spec="REQUIRED VECTOR" />
      <v2                       spec="REQUIRED VECTOR" />
      <v3                       spec="REQUIRED VECTOR" />
    </mismatchStrain>
    <!-- Bazant model variables -->

    <!-- Activated diffusion model parameters -->
    <gasConstant           spec="REQUIRED DOUBLE" need_applies_to="type activated" />
    <activationEnergy      spec="REQUIRED DOUBLE" need_applies_to="type activated" />
    <!--  Activated diffusion models -->

    <!-- Required for constant_rate model -->    
    <constant_rate         spec="REQUIRED DOUBLE" need_applies_to="type constant_rate" />

    <!-- Required for non-linear diffusion models -->
    <tuning1               spec="REQUIRED DOUBLE" need_applies_to="type non_linear1" />
    <tuning2               spec="REQUIRED DOUBLE" need_applies_to="type non_linear1" />

    <!-- Non-linear 1 implemented only -->
    <use_pressure          spec="OPTIONAL BOOLEAN" />
    <tuning3               spec="OPTIONAL DOUBLE" />
    <tuning4               spec="OPTIONAL DOUBLE" />
    <tuning5               spec="OPTIONAL DOUBLE" />

    <!-- Diff curves only implemented for type non_linear1 -->
    <diff_curve            spec="OPTIONAL NO_DATA" need_applies_to="type non_linear1" >
      <time_point          spec="MULTIPLE NO_DATA" >
        <time              spec="REQUIRED DOUBLE" />
        <flux_direction    spec="REQUIRED STRING 'in, out, transition'" />
      </time_point>
    </diff_curve>
    
    <!-- non-linear diffusion models -->
    
    <!-- Tags not currently used -->
    <ramp_time             spec="OPTIONAL DOUBLE" />
    <partial_atomic_vol    spec="OPTIONAL DOUBLE" />
    
    <!-- For conductivity model based diffusion (non-linear 2) -->
    <unit_charge           spec="REQUIRED DOUBLE" need_applies_to="type non_linear2" />
    <operating_temp        spec="REQUIRED DOUBLE" need_applies_to="type non_linear2" />
        
    <conductivity_equation   spec="OPTIONAL NO_DATA"
                             attribute1="type REQUIRED STRING 'fixed, binary'">
      <min_conc              spec="OPTIONAL DOUBLE" />
      <max_conc              spec="OPTIONAL DOUBLE" />
      <conductivity          spec="OPTIONAL DOUBLE" />
      <min_conductivity      spec="OPTIONAL DOUBLE" />
      <max_conductivity      spec="OPTIONAL DOUBLE" />
    </conductivity_equation>
  </diffusion_model>
</Uintah_Include>
