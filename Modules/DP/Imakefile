LIBNAME = DP

DPHOME = /home/sci/local/apps/diffpack
DPINCS = -I$(DPHOME)/dp/include -I$(DPHOME)/la/include \
	-I$(DPHOME)/bt/include
DPLTYPE = lib/IRIX64/nopt
DPLIBS = -L$(DPHOME)/dp/$(DPLTYPE) -L$(DPHOME)/la/$(DPLTYPE) \
	 -L$(DPHOME)/bt/$(DPLTYPE) \
	 -lenvir -lprep -ldpK -llineq -larr2 -lbt2_no_x -larr1 -lbt1
XLIBS =

SO_LIBS =
SO_LDFLAGS = -Wl,-woff,15 -L$(TOP)/lib $(DPLIBS) -lDatatypes -lDataflow -lGeom -lGeometry \
