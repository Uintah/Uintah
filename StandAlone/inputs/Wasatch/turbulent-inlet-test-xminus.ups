<Uintah_specification>

  <Meta>
    <title>Momentum Test with Boundary Conditions</title>
  </Meta>

  <SimulationComponent type="wasatch"/>
   <Solver type="hypre" />
  <Wasatch>
    <Density>
      <IsConstant>true</IsConstant>
      <NameTag name="density" state="STATE_NONE" />
    </Density>
    <TimeIntegrator>FE</TimeIntegrator>
    <Turbulence>
       <KolmogorovScale>1000000000000</KolmogorovScale>
      <EddyViscosityConstant>0.1</EddyViscosityConstant>
      <TurbulenceModel> VREMAN </TurbulenceModel>
    </Turbulence>

    <BCExpression type="XVOL">
      <TaskList>initialization advance_solution</TaskList>
      <NameTag name="x-turbulent-inlet" state="STATE_NONE" />
      <TurbulentInlet period="2" component="X">
				<InputFile> inputs/Wasatch/turbulent-inlet-test-xminus-data.txt.gz </InputFile>
      </TurbulentInlet>
    </BCExpression>

    <BCExpression type="YVOL">
      <TaskList>initialization advance_solution</TaskList>
      <NameTag name="y-turbulent-inlet" state="STATE_NONE" />
      <TurbulentInlet period="2" component="Y">
				<InputFile> inputs/Wasatch/turbulent-inlet-test-xminus-data.txt.gz </InputFile>
      </TurbulentInlet>
    </BCExpression>

    <BCExpression type="ZVOL">
      <TaskList>initialization advance_solution</TaskList>
      <NameTag name="z-turbulent-inlet" state="STATE_NONE" />
      <TurbulentInlet period="2" component="Z">
				<InputFile> inputs/Wasatch/turbulent-inlet-test-xminus-data.txt.gz </InputFile>
      </TurbulentInlet>
    </BCExpression>
    
    
    <!-- Set the initial condition expression for x-momentum -->
    <BasicExpression type="XVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="x-mom" state="STATE_N" />
       <Constant>0.01</Constant>
    </BasicExpression>

    <BasicExpression type="YVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="y-mom" state="STATE_N" />
       <Constant>0.0</Constant>
    </BasicExpression>

    <BasicExpression type="ZVOL">
       <TaskList>initialization</TaskList>
       <NameTag name="z-mom" state="STATE_N" />
       <Constant>0.0</Constant>
    </BasicExpression>
               
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="viscosity" state="STATE_NONE" />
      <Constant>1.983e-5</Constant>
    </BasicExpression>

    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="density" state="STATE_NONE" />
      <Constant>1.0</Constant>
    </BasicExpression>

    <!-- Momentum transport equations -->
    <MomentumEquations>
      <X-Velocity>u</X-Velocity>
      <X-Momentum>x-mom</X-Momentum>
      <Y-Velocity>v</Y-Velocity>
      <Y-Momentum>y-mom</Y-Momentum>
      <Z-Velocity>w</Z-Velocity>
      <Z-Momentum>z-mom</Z-Momentum>      
      <Viscosity>
        <NameTag name="viscosity" state="STATE_NONE" />
      </Viscosity>
      
      <Pressure>
        <Parameters>
            <solver>         cg      </solver>
            <preconditioner> pfmg    </preconditioner>
            <tolerance>      1e-10  </tolerance>
            <maxiterations>  50    </maxiterations>
            <relax_type>     2       </relax_type>
            <setupFrequency>0</setupFrequency>
           </Parameters>
      </Pressure>
    </MomentumEquations>

  </Wasatch>


  <Time>
    <maxTime>0.5</maxTime>
    <initTime>0.0</initTime>
    <delt_min>1e-4</delt_min>
    <delt_max>1e-4</delt_max>
    <max_Timesteps>22</max_Timesteps>
    <timestep_multiplier>1.0</timestep_multiplier>
  </Time>


  <DataArchiver>
    <filebase>turbulent-inlet-test-xmiuns.uda</filebase>
    <outputTimestepInterval>1</outputTimestepInterval>
    <outputInitTimestep/>
    <checkpoint cycle = "2" interval = "1e-4"/>
    <compression>gzip</compression>
    <save label="x-mom"/>
    <save label="y-mom"/>
    <save label="z-mom"/>    
    <save label="u"/>
    <save label="v"/>    
    <save label="w"/>        
    <save label="pressure"/>
    <save label="dilatation"/>
  </DataArchiver> 

  <Grid>
      <BoundaryConditions>
      <!-- BOUNDARY CONDITIONS FOR MOMENTUM -->
<!-- 
      <Face circle = "x-" origin = "0.0 0.0 0.0" radius = "0.1">
 -->
    <Face rectangle="x-" lower="0.0 -0.025 -0.025" upper="0 0.025 0.025">
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
          <functor_name>x-turbulent-inlet</functor_name>
        </BCType>

        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
          <functor_name>y-turbulent-inlet</functor_name>
        </BCType>

        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
          <functor_name>z-turbulent-inlet</functor_name>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="x-mom_rhs_partial">
          <value> 0.0 </value>
        </BCType>                                                     
      </Face>      
            
      <Face side="x-">

        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>

        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="x-mom_rhs_partial">
          <value> 0.0 </value>
        </BCType>                                               

      </Face>
      
      <Face side="x+">

        <BCType id="all" var="Dirichlet" label="pressure">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Neumann" label="y-mom">
          <value> 0.0 </value>
        </BCType>        
        <BCType id="all" var="Neumann" label="v">
          <value> 0.0 </value>
        </BCType>        


        <BCType id="all" var="Neumann" label="z-mom">
          <value> 0.0 </value>
        </BCType>        
        <BCType id="all" var="Neumann" label="w">
          <value> 0.0 </value>
        </BCType>        
        
        <BCType id="all" var="Dirichlet" label="x-mom_rhs_partial">
          <value> 0.0 </value>
        </BCType>                       
        
      </Face>
      
      <Face side="y-">
      
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>

        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom_rhs_partial">
          <value> 0.0 </value>
        </BCType>        
                        
      </Face>
      
      <Face side="y+">
      
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>

        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom_rhs_partial">
          <value> 0.0 </value>
        </BCType>     
                        
      </Face>



      <Face side="z-">
      
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>

        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="z-mom_rhs_partial">
          <value> 0.0 </value>
        </BCType>        
                        
      </Face>
      
      <Face side="z+">
      
        <BCType id="all" var="Dirichlet" label="x-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="y-mom">
          <value> 0.0 </value>
        </BCType>

        <BCType id="all" var="Dirichlet" label="z-mom">
          <value> 0.0 </value>
        </BCType>
        
        <BCType id="all" var="Dirichlet" label="z-mom_rhs_partial">
          <value> 0.0 </value>
        </BCType>     
                        
      </Face>

      </BoundaryConditions>
      <Level>
      <Box label="1">
        <lower>       [0,-0.1,-0.1]   </lower>
        <upper>       [.6,.1,.1]   </upper>
        <resolution>  [72,24,24]</resolution>
        <patches>     [3,2,2]   </patches>
      </Box>
      <periodic>[0,0,0]</periodic>
    </Level>
  </Grid>

</Uintah_specification>
