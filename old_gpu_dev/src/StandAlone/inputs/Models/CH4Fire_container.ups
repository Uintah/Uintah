        </BCType>
        <BCType id = "4" label = "Density"      var = "Dirichlet">
                              <value> 1.1792946927374306  </value>
        </BCType>
        <BCType id = "4" label = "scalar-f"     var = "Dirichlet">
                              <value> 0.0</value>
        </BCType>
        <BCType id = "4" label = "cumulativeEnergyReleased" var = "Dirichlet">
