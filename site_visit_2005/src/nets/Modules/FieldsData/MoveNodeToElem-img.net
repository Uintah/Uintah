# Create a SCIRun->Visualization->ShowField Module
set m1 [addModuleAtPosition "SCIRun" "Visualization" "ShowField" 352 380]

# Create a SCIRun->Render->Viewer Module
set m2 [addModuleAtPosition "SCIRun" "Render" "Viewer" 239 508]

# Create a SCIRun->Visualization->ShowField Module
