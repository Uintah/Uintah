<?xml version="1.0" encoding="ISO-8859-1"?>
<Uintah_specification>
  <Meta>
   </Meta>

  <SimulationComponent type="arches"/>

  <Solver type="hypre"/>
  <Time>
    <maxTime>         10.0        </maxTime>
    <initTime>        0.0         </initTime>
    <delt_min>        0.00001     </delt_min>
    <delt_max>        0.01        </delt_max>
    <max_Timesteps>   50          </max_Timesteps>
    <timestep_multiplier>0.35     </timestep_multiplier>
  </Time>

  <DataArchiver>
    <filebase>helium_RT.uda</filebase>
    <outputTimestepInterval>5</outputTimestepInterval>
    <save label="totalKineticEnergy"/>
    <save label="pressurePS"/>
    <save label="CCVelocity"/>
    <save label="mixture_fraction"/>
    <save label="density"/>
    <save label="viscosityCTS"/>
    <save label="cellType"/>
    <checkpoint cycle="2" interval="0.01"/>
  </DataArchiver>

  <Grid>
    <Level>
      <Box label="1">
        <lower>       [0,0,0]   </lower>
        <upper>       [3,3,3]   </upper>
        <resolution>  [21,21,21]</resolution>
        <extraCells>  [1,1,1]   </extraCells>
        <patches>     [1,1,1]   </patches>
      </Box>
    </Level>
    <BoundaryConditions>
      <!-- Secondary, Co-Flow Inlet --> 
      <Face side="x-">
        <!-- Momentum BCs --> 
        <BCType id="0" label="co-flow inlet" var="MassFlowInlet">
          <value>0.01</value>
        </BCType>
        <!-- Table BCs --> 
        <BCType id="0" label="mixture_fraction" var="Dirichlet">
          <value>0.0</value>
        </BCType>
      </Face>
      <!-- Bottom Inlet -->
      <Face circle="x-" origin="0.0 1.5 1.5" radius="0.5">
        <!-- Momentum BCs --> 
        <BCType id="0" label="helium inlet" var="MassFlowInlet">
          <value>0.351</value>
        </BCType>
        <!-- Table BCs --> 
        <BCType id="0" label="mixture_fraction" var="Dirichlet">
          <value>1.0</value>
        </BCType>
      </Face>
      <!-- Pressure BC's --> 
      <Face side="y-">
        <!-- Momentum BCs --> 
        <BCType id="0" label="y- pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <!-- Table BCs --> 
        <BCType id="0" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
      </Face>
      <Face side="y+">
        <!-- Momentum BCs --> 
        <BCType id="0" label="y+ pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <!-- Table BCs --> 
        <BCType id="0" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
      </Face>
      <Face side="z-">
        <!-- Momentum BCs --> 
        <BCType id="0" label="z- pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <!-- Table BCs --> 
        <BCType id="0" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
      </Face>
      <Face side="z+">
        <!-- Momentum BCs --> 
        <BCType id="0" label="z+ pressure" var="PressureBC">
          <value>0.0</value>
        </BCType>
        <!-- Table BCs --> 
        <BCType id="0" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
      </Face>
      <!-- Outlet --> 
      <Face side="x+">
        <!-- Momentum BCs --> 
        <BCType id="0" label="outlet" var="OutletBC">
          <value>0.0</value>
        </BCType>
        <!-- Table BCs --> 
        <BCType id="0" label="mixture_fraction" var="Neumann">
          <value>0.0</value>
        </BCType>
      </Face>
    </BoundaryConditions>
  </Grid>

  <PhysicalConstants>
    <gravity>         [-9.8,0,0]    </gravity>
    <reference_point>       [-1,-1,-1]    </reference_point>
    <viscosity>       0.000020      </viscosity>
  </PhysicalConstants>

  <CFD>
    <ARCHES>

      <TimeIntegrator>
        <ExplicitIntegrator order="second"/>
      </TimeIntegrator>

      <Turbulence model="compdynamicprocedure"/>

      <TransportEqns>
        <Eqn label="mixture_fraction" type="CCscalar">
          <doDiff>true</doDiff>
          <doConv>true</doConv>
          <conv_scheme>super_bee</conv_scheme>
          <determines_properties/>
          <initialization type="constant">
            <constant>.0</constant>
          </initialization>
          <Clipping>
            <low>0.0</low>
            <high>1.0</high>
          </Clipping>
        </Eqn>
      </TransportEqns>

      <Properties>
        <ColdFlow>
          <mixture_fraction label="mixture_fraction"/>
          <stream_0 density="0.133" temperature="298"/>
          <stream_1 density="1.18434" temperature="298"/>
          <reference_density value="1.18434"/>
        </ColdFlow>
      </Properties>

<!--__________________________________-->
      <BoundaryConditions>
        <suppress_corner_recirculation/>
      </BoundaryConditions>

<!--__________________________________-->
      <ExplicitSolver>
        <PressureSolver>
          <Parameters>
            <solver>           cg     </solver>
            <preconditioner>  pfmg    </preconditioner>
            <tolerance>      1.0e-10  </tolerance>
            <maxiterations>    75     </maxiterations>
            <skip>              1     </skip>
            <setupFrequency>    0     </setupFrequency>
            <jump>              0     </jump>
          </Parameters>
        </PressureSolver>

        <MomentumSolver>
          <convection_scheme>central</convection_scheme>
        </MomentumSolver>

      </ExplicitSolver>
    </ARCHES>

  </CFD>

</Uintah_specification>
