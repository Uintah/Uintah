<?xml version="1.0"?>
<Uintah_specification>
  <Meta>
    <title>Compressible flow in 2D</title>
  </Meta>
  <SimulationComponent type="wasatch"/>
  <Solver type="hypre"/>
  <Wasatch>
    <TimeIntegrator> FE </TimeIntegrator>
    <Density method="COMPRESSIBLE">
      <NameTag name="density"/>
    </Density>
    <!-- Set the initial condition expression for mixture fraction -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f0"/>
      <GaussianFunction amplitude="40.0" deviation="0.5" mean="0" baseline="0.0">
        <NameTag name="XSVOL"/>
      </GaussianFunction>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f1"/>
      <GaussianFunction amplitude="40.0" deviation="0.5" mean="0" baseline="0.0">
        <NameTag name="YSVOL"/>
      </GaussianFunction>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f01" state="STATE_NONE"/>
      <ExprAlgebra algebraicOperation="PRODUCT">
        <!-- specify the list of tags of the source expressions -->
        <NameTag name="f0" state="STATE_NONE"/>
        <NameTag name="f1" state="STATE_NONE"/>
      </ExprAlgebra>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="f3"/>
      <Constant>273.0</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="T" state="STATE_NONE"/>
      <ExprAlgebra algebraicOperation="SUM">
        <!-- specify the list of tags of the source expressions -->
        <NameTag name="f01" state="STATE_NONE"/>
        <NameTag name="f3" state="STATE_NONE"/>
      </ExprAlgebra>
    </BasicExpression>
    <!-- Set the initial condition expression for mixture fraction -->
    <!-- 
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="T"/>
      <GaussianFunction amplitude="1000.0" deviation="1.0" mean="0.0" baseline="273.0">
        <NameTag name="XSVOL"/>
      </GaussianFunction> 
    </BasicExpression>
 -->
    <!-- Set the initial condition expression for x-velocity -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="u"/>
      <Constant>0</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="v"/>
      <Constant>0</Constant>
    </BasicExpression>
    <!-- Set the initial condition expression for x-velocity -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="pressure"/>
      <Constant>101325</Constant>
    </BasicExpression>
    <!-- Set the initial condition expression for x-velocity -->
    <BasicExpression type="SVOL">
      <TaskList>initialization</TaskList>
      <NameTag name="MW"/>
      <Constant>0.028966</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="MW"/>
      <Constant>0.028966</Constant>
    </BasicExpression>
    <BasicExpression type="SVOL">
      <TaskList>advance_solution</TaskList>
      <NameTag name="viscosity"/>
      <Constant>1.0e-5</Constant>
    </BasicExpression>
    <!-- Momentum transport equations -->
    <MomentumEquations>
      <EnergyEquation>
        <SolutionVariable>rhoet</SolutionVariable>
        <PrimitiveVariable>et</PrimitiveVariable>
        <Temperature><NameTag name="T"/></Temperature>
        <MolecularWeight>
          <NameTag name="MW"/>
        </MolecularWeight>
        <DiffusiveFlux direction="XY" coefficient="0.001"/>
        <ConvectiveFlux direction="X" method="CENTRAL">
          <AdvectiveVelocity>
            <NameTag name="u"/>
          </AdvectiveVelocity>
        </ConvectiveFlux>
        <ConvectiveFlux direction="Y" method="CENTRAL">
          <AdvectiveVelocity>
            <NameTag name="v"/>
          </AdvectiveVelocity>
        </ConvectiveFlux>
      </EnergyEquation>
      <X-Velocity>u</X-Velocity>
      <X-Momentum>x-mom</X-Momentum>
      <Y-Velocity>v</Y-Velocity>
      <Y-Momentum>y-mom</Y-Momentum>
      <Viscosity>
        <NameTag name="viscosity"/>
      </Viscosity>
    </MomentumEquations>
  </Wasatch>
  <Time>
    <maxTime>5.0</maxTime>
    <initTime>0.0</initTime>
    <delt_min>1e-7</delt_min>
    <delt_max>1e-7</delt_max>
    <max_Timesteps> 100 </max_Timesteps>
    <timestep_multiplier>1.0</timestep_multiplier>
  </Time>
  <DataArchiver>
    <filebase>compressible-2d-test.uda</filebase>
    <outputTimestepInterval>1</outputTimestepInterval>
    <outputInitTimestep/>
    <checkpoint cycle="2" timestepInterval="20"/>
    <save label="T"/>
    <save label="u"/>
    <save label="v"/>
    <save label="pressure"/>
    <save label="density"/>
    <save label="et"/>
    <save label="rhoet"/>
  </DataArchiver>
  <Grid>
    <BoundaryConditions>
      <DefaultMaterial>all</DefaultMaterial>
    </BoundaryConditions>
    <Level>
      <Box label="1">
        <lower>[-2,-2,0]</lower>
        <upper>[2,2,1]</upper>
        <resolution>[200,200,1]</resolution>
        <patches>[2,2,1]</patches>
      </Box>
      <periodic>[1,1,1]</periodic>
    </Level>
  </Grid>
</Uintah_specification>
